translation.add(0, "Sábado por la mañana.", "");
translation.add(1, "Gracias por la comida. Hasta luego.", "Kouhei");
translation.add(2, "Termino de desayunar antes de lo habitual,\ncojo mi bolsa y me levanto listo para marcharme.", "");
translation.add(3, "¿Vas a salir, Kouhei?", "Papá");
translation.add(4, "Sí, he quedado con alguien.", "Kouhei");
translation.add(5, "Ah, llegaré tarde.", "Kouhei");
translation.add(6, "De acuerdo. Que te lo pases bien.", "Mamá");
translation.add(7, "Eso haré. Nos vemos.", "Kouhei");
translation.add(8, "Estoy a punto de ponerme las zapatillas\ncuando aparece Aya.", "");
translation.add(9, "Al menos podrías limpiar los platos\nque has ensuciado antes de irte.", "Aya");
translation.add(10, "Lo dice totalmente en serio.", "");
translation.add(11, "Hermanita, hermanita...\nUn hombre tiene que establecer prioridades.", "Kouhei");
translation.add(12, "¿...?", "Aya");
translation.add(13, "En otras palabras, ¡que te diviertas limpiando!", "Kouhei");
translation.add(14, "¡...!", "Aya");
translation.add(15, "Saludo con la mano y salgo por la puerta.\nAya se pone roja de furia.", "");
translation.add(16, "¡Jajajá! El hermano mayor siempre tiene razón.\n¡Hasta luego!", "Kouhei");
translation.add(17, "¿Por qué eres tan egoísta?... ¡Vuelve aquí!", "Aya");
translation.add(18, "El autobús me deja en la plaza de la estación\ncinco minutos antes de la cita.", "");
translation.add(19, "Esta plaza es un punto de reunión muy habitual.", "");
translation.add(20, "(Nunca pensé que quedaría aquí con alguien... Oh).", "Kouhei");
translation.add(21, "Distingo a Aeka entre las parejas\nque están atravesando la plaza.", "");
translation.add(22, "Buenos días, Aeka.", "Kouhei");
translation.add(23, "¡Oh! ¡Kouhei-kun!", "Aeka");
translation.add(24, "¡Buenos días!", "Aeka");
translation.add(25, "Su reacción es tan exagerada como la de ayer.\nUna amplia sonrisa se dibuja en su rostro al verme.", "");
translation.add(26, "No me sorprendería que le aparecieran orejas\nde perrito y una cola agitándose detrás de ella.", "");
translation.add(27, "¿Hace mucho que me esperas?", "Kouhei");
translation.add(28, "No... He llegado hace poco.", "Aeka");
translation.add(29, "Vaya, vaya...\nA este paso va a resultar tan aburrido como parece.", "");
translation.add(30, "Sonrío con desgana,\naunque ella parece estar verdaderamente contenta.", "");
translation.add(31, "Está sonriendo desde lo más profundo de su corazón.\nNo he dicho nada especial, pero aun así\ntrata mis palabras como si fueran un tesoro.", "");
translation.add(32, "Mira cuánta gente hay,\ny eso que estamos en fin de semana.", "Aeka");
translation.add(33, "Me mira entre una mezcla de felicidad y confusión.\nSe comporta como una niña en un festival.", "");
translation.add(34, "En realidad, ¿no crees que es\nprecisamente por ser fin de semana?", "Kouhei");
translation.add(35, "Ah, claro...", "Aeka");
translation.add(36, "Bueno... ¿adónde vamos?", "Aeka");
translation.add(37, "Está tan emocionada\nque le hacen chiribitas los ojos.", "");
translation.add(38, "¿Eh?", "Kouhei");
translation.add(39, "(¡Mierda! ¡Ni se me había ocurrido!).", "Kouhei");
translation.add(40, "Me ha pillado totalmente desprevenido.", "");
translation.add(41, "Argh... ¿Qué debería decirle?\nNo se me ocurrió planear nada para esta cita.", "");
translation.add(42, "Creo que el único que se siente incómodo\naquí soy yo...", "");
translation.add(43, "Pero se supone que íbamos a salir,\ny eso es precisamente lo que vamos a hacer.", "");
translation.add(44, "Bueno, por ahora...", "Kouhei");
translation.add(45, "¿Sí?", "Aeka");
translation.add(46, "¿Qué tal si vamos a un karaoke?", "Kouhei");
translation.add(47, "¿Eh? ¿a un karaoke? Bueno...", "Aeka");
translation.add(48, "Suponía que ir al karaoke\nme daría más tiempo para pensar otro plan.", "");
translation.add(49, "Pero parece que Aeka\nno está muy entusiasmada con la idea.", "");
translation.add(50, "Creo que estaba por aquí...", "Kouhei");
translation.add(51, "¡Ahh!", "Kouhei");
translation.add(52, "De repente, alguien me tira de la ropa por detrás.", "");
translation.add(53, "Mmm. ¿Vienes muy a menudo a sitios así?", "Aeka");
translation.add(54, "Me detiene frente a la puerta giratoria\nrepleta de pósteres.", "");
translation.add(55, "¿Por qué está tan nerviosa? Solo es un karaoke,", "");
translation.add(56, "La verdad es que no, solo he venido\nun par de veces con los de la clase... Oh.", "Kouhei");
translation.add(57, "(Esto no va bien).", "Kouhei");
translation.add(58, "No digo que no quiera ir pero, bueno...", "Aeka");
translation.add(59, "¿Una cita en un lugar donde podría\naparecer gente que pudiera reconocernos?", "");
translation.add(60, "Es evidente que no le hace mucha gracia.", "");
translation.add(61, "De repente, yo también me siento avergonzado.", "");
translation.add(62, "Es que, el karaoke... No se me da bien cantar.", "Aeka");
translation.add(63, "L-Lo siento. ¿Quieres que vayamos a otro sitio?", "Kouhei");
translation.add(64, "Vale.", "Aeka");
translation.add(65, "Respira aliviada.", "");
translation.add(66, "Muy bien, nos vamos de aquí.", "Kouhei");
translation.add(67, "Sí.", "Aeka");
translation.add(68, "Me fijo en un tipo que está intentando\natraer clientes a una sala de cine.", "");
translation.add(69, "El sol corona el cielo\ny las calles están empezando a llenarse.", "");
translation.add(70, "(Bueno, no me importa).", "Kouhei");
translation.add(71, "No destacas si estás rodeado de gente.", "");
translation.add(72, "Pero eso solamente se aplica si vas solo...", "");
translation.add(73, "¡Qué bien huele! Oh, mira eso, ¡qué mono!", "Aeka");
translation.add(74, "Aeka corre hasta el escaparate de una pastelería.", "");
translation.add(75, "¿Mmm? ¿Quieres que comamos algo?", "Kouhei");
translation.add(76, "El hecho de que hoy Aeka esté conmigo me hace\nser más consciente de las miradas de los demás.", "");
translation.add(77, "A veces me preguntaba cómo era\neso de pasear por la calle con tu novia.", "");
translation.add(78, "Y esa novia es Aeka.", "");
translation.add(79, "Es mona desde casi todos los puntos de vista.", "");
translation.add(80, "Mirar los escaparates con tu novia... es agradable.", "");
translation.add(81, "Entonces, ¿por qué me siento incómodo?", "");
translation.add(82, "No, no es eso...", "Aeka");
translation.add(83, "Se sonroja un poco.", "");
translation.add(84, "No tengo hambre.", "Aeka");
translation.add(85, "Pero es que me gusta mirar.", "Aeka");
translation.add(86, "Oh...", "Kouhei");
translation.add(87, "La sigo mientras va de tienda en tienda,\nde vendedor a vendedor.\nmaravillada por todo lo que ve.", "");
translation.add(88, "Por lo visto ha olvidado completamente\nel riesgo que corremos de ser descubiertos.", "");
translation.add(89, "(Aeka me recuerda a Aya de pequeña).", "Kouhei");
translation.add(90, "Era tan ingenua por aquel entonces.", "");
translation.add(91, "Bueno, y yo también...", "");
translation.add(92, "Me daba vergüenza salir con una chica en público.", "");
translation.add(93, "Me portaba mal con Aya por eso.\nTodo el mundo sabe cómo son los críos...", "");
translation.add(94, "Pero lo que siento ahora es... diferente.", "");
translation.add(95, "Quizá desde lo más profundo de mi ser,\ntodavía no haya aceptado a Aeka como mi novia.", "");
translation.add(96, "Pero si poco a poco, con el tiempo,\nvoy intimando más a ella, quizá algún día...", "");
translation.add(97, "... seré capaz de verla como mi amor verdadero.", "");
translation.add(98, "La curiosidad de Aeka nos lleva por toda la ciudad.", "");
translation.add(99, "Hasta que nos detenemos\ndelante del conbini donde trabajo.", "");
translation.add(100, "(Pues sí que hemos caminado).", "Kouhei");
translation.add(101, "¿Es que estaba tan tenso\nque he perdido la noción del tiempo?", "");
translation.add(102, "¿Quieres comprar algo, Kouhei-kun?", "Aeka");
translation.add(103, "Aeka se acerca a mí cuando se da cuenta\nde que estoy mirando la tienda.", "");
translation.add(104, "Ah... no. Es que aquí es donde trabajo.", "Kouhei");
translation.add(105, "¿De verdad?\nA veces paso por esta calle en bicicleta.", "Aeka");
translation.add(106, "¿En serio? ¡Pues nunca te había visto!", "Kouhei");
translation.add(107, "Se pone roja.", "");
translation.add(108, "Sí que te has sorprendido, Kouhei-kun.", "Aeka");
translation.add(109, "(Qué mona).", "Kouhei");
translation.add(110, "Argh... Ahora yo también me estoy sonrojando.", "");
translation.add(111, "¿Pasa algo?", "Aeka");
translation.add(112, "Nuestros ojos se cruzan un instante,\npara después volver a perder el contacto.", "");
translation.add(113, "Oh... Nada. Continuemos.", "Kouhei");
translation.add(114, "(No vale la pena arriesgarse a toparse con ÉL).", "Kouhei");
translation.add(115, "¿Eh? ¿No quieres entrar?", "Aeka");
translation.add(116, "¡No! Por cierto, ¿no empiezas a tener apetito?", "Kouhei");
translation.add(117, "Se lleva la mano al estómago\ncomo si fuera el personaje de un manga.\nA estas alturas ya me he acostumbrado.", "");
translation.add(118, "Ahora que lo dices...", "Aeka");
translation.add(119, "¡Bien! Entonces vámonos.", "Kouhei");
translation.add(120, "Vemos a un montón de gente comiendo\ncuando volvemos a la plaza de la estación.", "");
translation.add(121, "Espera aquí, ahora vuelvo.", "Kouhei");
translation.add(122, "La dejo en la mesa de una hamburguesería\ny parto dispuesto a aprovisionarnos.", "");
translation.add(123, "Ah, es verdad. ¿Quieres algo en especial?", "Kouhei");
translation.add(124, "No... Cualquier cosa me va bien.", "Aeka");
translation.add(125, "Como quieras.", "Kouhei");
translation.add(126, "Me uno a la cola. Veo a varios estudiantes\nataviados con el uniforme de nuestra escuela.", "");
translation.add(127, "(¿Y si me cruzo con alguien que conozco?).", "Kouhei");
translation.add(128, "Aunque el restaurante está bastante lleno.\nNo debería haber ningún problema mientras no\nllame la atención.", "");
translation.add(129, "(Pero aun así debería andarme con ojo).", "Kouhei");
translation.add(130, "Al final termino por comprar dos hamburguesas.", "");
translation.add(131, "Ya estoy aquí.", "Kouhei");
translation.add(132, "Me siento en la mesa.", "");
translation.add(133, "Gracias... Qué aproveche.", "Aeka");
translation.add(134, "Empezamos a comer.", "");
translation.add(135, "(Son bastante grandes...\nNo sé si Aeka podrá con todo).", "Kouhei");
translation.add(136, "¿Qué ocurre? ¿No te estás divirtiendo?", "Aeka");
translation.add(137, "Frunce el ceño.", "");
translation.add(138, "¿Eh? ¿Por qué lo preguntas?", "Kouhei");
translation.add(139, "Has estado todo el día con la mirada perdida.", "Aeka");
translation.add(140, "Eso no es verdad...", "Kouhei");
translation.add(141, "¿Le estás dando vueltas a algo?", "Aeka");
translation.add(142, "Bueno, quizá.", "Kouhei");
translation.add(143, "Mm... ¿Te molesta que esté contigo?", "Aeka");
translation.add(144, "Parece un poco abatida; voy a animarla un poco.", "");
translation.add(145, "En absoluto. Fui yo quien te invitó, ¿recuerdas?", "Kouhei");
translation.add(146, "...", "Aeka");
translation.add(147, "Es que no puedo evitar estar alerta por\ndar la sensación de que estamos saliendo juntos.", "Kouhei");
translation.add(148, "Y entonces, por fin se da cuenta.", "");
translation.add(149, "Oh. Ahora que lo dices,\nla escuela no está muy lejos de aquí.", "Aeka");
translation.add(150, "Exacto.\nEs un milagro que aún no nos haya visto nadie.", "Kouhei");
translation.add(151, "Mmmm. Entiendo.", "Aeka");
translation.add(152, "Después me gustaría ver tu casa.", "Kouhei");
translation.add(153, "Mmmm...", "Aeka");
translation.add(154, "Pero si no quieres, lo entend... ¿Eh?", "Kouhei");
translation.add(155, "(¿Me está escuchando?).", "Kouhei");
translation.add(156, "Está jugueteando con la comida,\ncavilando sobre vete a saber qué.", "");
translation.add(157, "¿Aeka?", "Kouhei");
translation.add(158, "Oh. Sé de un sitio que te encantará, Kouhei-kun.", "Aeka");
translation.add(159, "Levanta la vista y me sonríe.", "");
translation.add(160, "¿En serio?", "Kouhei");
translation.add(161, "Me interesa cualquier lugar secreto\nque Aeka pueda recomendarme.", "");
translation.add(162, "Pues llévame allí cuando terminemos de comer.", "Kouhei");
translation.add(163, "¡Vale!", "Aeka");
translation.add(164, "Aeka recobra los ánimos cuando ve\nque demuestro algo de interés.\nEmpieza a devorar la hamburguesa de nuevo.", "");
translation.add(165, "El lugar al que me lleva\nsupera todas mis expectativas.", "");
translation.add(166, "Aquí deberíamos estar bien, ¿no?", "Aeka");
translation.add(167, "(Eh... ¿Este es un sitio adecuado para una cita?).", "Kouhei");
translation.add(168, "Me ha llevado hasta la escalera desierta\nde un centro comercial.", "");
translation.add(169, "Todos los clientes usan las escaleras mecánicas\no los ascensores; a nadie se le ocurre subir aquí.", "");
translation.add(170, "Me preocupa que piense que esta es la mejor\nforma de ocultarnos de la gente que conocemos.", "");
translation.add(171, "Eh... ¿Kouhei-kun?", "Aeka");
translation.add(172, "Bueno... seguro que no nos verá nadie.", "Kouhei");
translation.add(173, "¿A que sí?", "Aeka");
translation.add(174, "Pero aquí uno no puede hacer muchas cosas...", "Kouhei");
translation.add(175, "Es cierto que te puedes sentar a hablar; hay\nmáquinas expendedoras y hasta un servicio cerca.", "");
translation.add(176, "Pero esto no está bien.", "");
translation.add(177, "¿He... vuelto a hacer algo que te ha molestado?", "Aeka");
translation.add(178, "No es que esté molesto... ¡No!\nSi además tiene hasta aire acondicionado.\n¡Este lugar secreto es estupendo!", "Kouhei");
translation.add(179, "Me comporto como si me encantara estar aquí.", "");
translation.add(180, "¿En serio?", "Aeka");
translation.add(181, "...", "Estudiante (chica)");
translation.add(182, "(¡Mierda!).", "Kouhei");
translation.add(183, "¡¿Qué hace una estudiante en un sitio así?!", "");
translation.add(184, "Una chica ha aparecido por las escaleras\ny se ha detenido en seco en cuanto nos ha visto.", "");
translation.add(185, "Su mirada me deja helado.", "");
translation.add(186, "(¿La conozco? Creo que nunca la he visto...).", "Kouhei");
translation.add(187, "¿La conoces?", "Aeka");
translation.add(188, "No, creo que no. Pero será mejor que nos vayamos.", "Kouhei");
translation.add(189, "De repente me han entrado\nunas ganas locas de respirar aire fresco.", "");
translation.add(190, "Ah...", "Aeka");
translation.add(191, "Salimos afuera y caminamos sin rumbo fijo.\nAl cabo de un rato, Aeka se detiene.", "");
translation.add(192, "(¿Habrá visto algo?).", "Kouhei");
translation.add(193, "Está mirando el letrero de una agencia de viajes.\nEn él aparece una isla bañada por un mar de color\nverde esmeralda.", "");
translation.add(194, "¿Te gustaría ir allí?", "Kouhei");
translation.add(195, "¿Eh? Ah, bueno sí, un poco. Creo.", "Aeka");
translation.add(196, "Parece que hoy ha sido un día horrible.", "Kouhei");
translation.add(197, "No quería decir eso. ¿Tan seria parecía?", "Aeka");
translation.add(198, "Pues sí. Y mucho.", "Kouhei");
translation.add(199, "¿De verdad? Oh, qué vergüenza.", "Aeka");
translation.add(200, "Parece sentirse tan tímida como patética.", "");
translation.add(201, "No soy como esa clase de chicas descaradas\nque pedirían un crucero por el océano...", "Aeka");
translation.add(202, "Pero al menos crees que podría estar bien, ¿no?", "Kouhei");
translation.add(203, "Sí... Sería muy bonito.", "Aeka");
translation.add(204, "¿Qué te parece si la próxima vez\nbuscamos una foto sobre eso en Internet?", "Kouhei");
translation.add(205, "¿Eh?", "Aeka");
translation.add(206, "Si encuentro alguna, te la daré.", "Kouhei");
translation.add(207, "¿Puedes hacer eso? ¡Vaya!", "Aeka");
translation.add(208, "Aunque solo será una impresión.", "Kouhei");
translation.add(209, "Guau... Sabes usar ordenadores. Es increíble.", "Aeka");
translation.add(210, "Eh... Creo que cualquiera con\nun ordenador sabría hacer algo así.", "");
translation.add(211, "Ahora soy yo el avergonzado.", "");
translation.add(212, "Gracias. Tengo muchas ganas de verla.", "Aeka");
translation.add(213, "Lamento no haberte podido llevar a ningún sitio.", "Kouhei");
translation.add(214, "¿Por qué dices eso? Me lo he pasado muy bien.", "Aeka");
translation.add(215, "Me ha encantado el simple hecho de pasear contigo\npor tantos sitios y haber visto tantas cosas.", "Aeka");
translation.add(216, "No parece estar mintiendo.\n¿Debería considerarlo simplicidad o inocencia?", "");
translation.add(217, "Pero bueno, aunque haya metido la pata,\nsi ella está contenta significa que la cita\nha sido un éxito, ¿verdad?", "");
translation.add(218, "¿Algún día podré ver tu casa?", "Kouhei");
translation.add(219, "Decido insistir sobre ese tema.", "");
translation.add(220, "Podemos ir a su habitación\ny relajarnos sin que nadie nos moleste.", "");
translation.add(221, "¿M-Mañana?", "Aeka");
translation.add(222, "No necesariamente. Cuando quieras.", "Kouhei");
translation.add(223, "Mmmm. Bueno, quizá por la tarde...", "Aeka");
translation.add(224, "Parece que no le importa.", "");
translation.add(225, "Entonces, ¿nos vemos aquí a la una de la tarde?", "Kouhei");
translation.add(226, "Sí... Gracias, Kouhei-kun.", "Aeka");
translation.add(227, "Igualmente. Hasta mañana.", "Kouhei");

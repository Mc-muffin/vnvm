translation.add(0, "¿Ya has terminado de rebuscar en las tiendas?", "Kouhei");
translation.add(1, "No, todavía no.", "Aeka");
translation.add(2, "Hay un montón de establecimientos a las que\naún no he ido. Creo que seguiré investigando.", "Aeka");
translation.add(3, "Es divertido descubrir muchas cosas nuevas.", "Aeka");
translation.add(4, "Entiendo.", "Kouhei");
translation.add(5, "Puedo acompañarte, si quieres.", "Kouhei");
translation.add(6, "¿Vendrías conmigo?...", "Aeka");
translation.add(7, "Pero solo a mirar, ¿eh? No conseguirás nada\npor mucho que me halagues, así que ni se te\nocurra intentarlo.", "Kouhei");
translation.add(8, "No, no. Yo nunca...", "Aeka");
translation.add(9, "Ni tampoco trates de engañarme.", "Kouhei");
translation.add(10, "¿Tan materialista te parezco?\nIncluso hablas de engaños...", "Aeka");
translation.add(11, "Se está tomando está conversación muy a pecho.", "");
translation.add(12, "Que no, que era una broma. Ja, ja, ja...", "Kouhei");
translation.add(13, "Esto es lo que se llama un corazón puro.", "");
translation.add(14, "Bueno, ¿te importa si te acompaño?", "Kouhei");
translation.add(15, "Claro, si tú quieres.", "Aeka");
translation.add(16, "Perfecto, vamos adonde quieras.", "Kouhei");
translation.add(17, "Vale. Gracias.", "Aeka");
translation.add(18, "No pongas esa cara.\nSolo estamos dando un paseo, ¿verdad?", "Kouhei");
translation.add(19, "No sé por qué, pero mi corazón está palpitando\ncon fuerza.", "");
translation.add(20, "«Boutique Selecta Platina. ¡Entre en nuestra\ntienda y pregúntenos todo lo que quiera sobre\nmoda!».", "");
translation.add(21, "¡Qué broche más bonito!", "Aeka");
translation.add(22, "Está mirando un broche con forma de corazón\ndel tamaño de mi pulgar.", "");
translation.add(23, "¡Guau, es carísimo!", "Kouhei");
translation.add(24, "Iría muy bien con mi reloj.", "Aeka");
translation.add(25, "Tu reloj es un poco raro, ¿no crees?", "Kouhei");
translation.add(26, "La cara es elíptica y no tiene ningún símbolo.", "");
translation.add(27, "¿Puedes saber cuál es la hora exacta con eso?", "Kouhei");
translation.add(28, "No mucho...\nPero lo llevo sobre todo para estar a la moda.", "Aeka");
translation.add(29, "Así que se trata de eso...", "Kouhei");
translation.add(30, "(Anda que no hay que documentarse para poder\nsalir de compras).", "Kouhei");
translation.add(31, "(La moda no es precisamente mi punto fuerte).", "Kouhei");
translation.add(32, "Es un poco tarde, pero no me importa.", "");
translation.add(33, "(Supongo que tendré que improvisar algo\nsobre la marcha).", "Kouhei");
translation.add(34, "¿Qué ocurre, Kagami-kun?", "Aeka");
translation.add(35, "Ah, estaba abstraído en mis cosas.\nNo estoy acostumbrando a los complementos.", "Kouhei");
translation.add(36, "Es que los chicos no suelen hacer esto.", "Aeka");
translation.add(37, "¿Vamos a la siguiente tienda?", "Aeka");
translation.add(38, "¿Ya? ¿No quieres entrar?", "Kouhei");
translation.add(39, "No, me conformo con mirar los escaparates.", "Aeka");
translation.add(40, "No haría más que molestar a los vendedores,\ny no puedo permitirme algo tan caro.", "Aeka");
translation.add(41, "El cliente es el rey, ¿no?", "Kouhei");
translation.add(42, "Si te preocupas por cosas así,\nno llegarás a ninguna parte.", "Kouhei");
translation.add(43, "Mi madre también dice lo mismo.\nAun así, estoy un poco asustada...", "Aeka");
translation.add(44, "No pasará nada. Echemos un vistazo.", "Kouhei");
translation.add(45, "Atravieso con decisión las puertas automáticas.", "");
translation.add(46, "¿Qué puedo hacer por vosotros?", "Vendedor");
translation.add(47, "Un anciano miope ataviado con un ajustado\npantalón azul, una camiseta blanca\ny una corbata a rayas surge de la nada.", "");
translation.add(48, "Me mira de tal forma que tengo la sensación\nde que me comerá vivo si no compro nada.", "");
translation.add(49, "Vámonos a otra tienda, Shiraki-san.", "Kouhei");
translation.add(50, "V-Vale... Vamos...", "Aeka");
translation.add(51, "Maldita sea, parece que está disgustada.", "");
translation.add(52, "«Dulce de la Zona Este. La tienda de moda\npara las jovencitas más modernas».", "");
translation.add(53, "Qué blusa de seda tan bonita.", "Aeka");
translation.add(54, "A mí me gusta la corbata azul.", "Kouhei");
translation.add(55, "Nos quedamos embobados mirando la ropa\nque llevan encima los maniquís.", "");
translation.add(56, "Los adultos están muy bien hechos...", "Aeka");
translation.add(57, "Sí, es verdad.", "Kouhei");
translation.add(58, "La mirada de Shiraki está fija en una falda\ncon una raja en un lado.", "");
translation.add(59, "Para conjuntar, tiene una chaqueta diseñada\npara enseñar algo de carne al moverse.", "");
translation.add(60, "*suspiro*", "Aeka");
translation.add(61, "Detrás del cristal posa un maniquí rígido\ncomo un árbol.", "");
translation.add(62, "¿Qué tal esto? Seguro que te quedaría bien.", "Kouhei");
translation.add(63, "El maniquí lleva una chaqueta color crema\ny unos pantalones marrones.", "");
translation.add(64, "Pero... bueno...", "Aeka");
translation.add(65, "Vale, esto no debe gustarte. Perdona.", "Kouhei");
translation.add(66, "No es eso. Pero...", "Aeka");
translation.add(67, "¿Pero?", "Kouhei");
translation.add(68, "Bueno, mmm, es un poco corto\nalrededor de la cintura.", "Aeka");
translation.add(69, "¿En serio?", "Kouhei");
translation.add(70, "Miro al maniquí y después a ella.", "");
translation.add(71, "No lo creo.", "");
translation.add(72, "Kagami, mmm, por favor, no...", "Aeka");
translation.add(73, "Por favor, no me mires así.", "Aeka");
translation.add(74, "La he jodido.", "");
translation.add(75, "Kagami-kun, eres un pervertido.", "Aeka");
translation.add(76, "No, esto..., yo no quería decir...", "Kouhei");
translation.add(77, "Menudo fallo. Ahora pensará mal de mí.", "");
translation.add(78, "Alguien como Antoinette hubiese disfrutado\nque la mirasen, pero Shiraki obviamente se\nsiente incómoda.", "");
translation.add(79, "Lo siento, Shiraki-san...", "Kouhei");
translation.add(80, "No pasa nada... No estoy enfadada.", "Aeka");
translation.add(81, "¿Nos vamos?", "Aeka");
translation.add(82, "De la que me he librado.", "");
translation.add(83, "Menos mal que tiene un corazón de oro.", "");
translation.add(84, "(Estoy seguro de que tendré la ocasión\nde arreglarlo. Aún no debo rendirme).", "Kouhei");
translation.add(85, "No es que quiera mejorar mi relación con ella,\npero... Bueno, es difícil de explicar.", "");
translation.add(86, "Es que simplemente me lo paso bien a su lado.", "");
translation.add(87, "«Tienda de animales.\n¡Contratamos adiestradores y veterinarios!».", "");
translation.add(88, "¡Un gatito! ¡Qué mono!", "Aeka");
translation.add(89, "Esos hámsteres de ahí también lo son.", "Kouhei");
translation.add(90, "Mira a ese loro sacudiéndose las alas.\nMe pregunto si querrá que lo observemos.", "Aeka");
translation.add(91, "También venden peces tropicales.\nNo me importaría tener uno de esos.", "Kouhei");
translation.add(92, "¿Te gustan los animales, Kagami-kun?", "Aeka");
translation.add(93, "Sí, mucho. Aunque no tenemos ninguna mascota\nen casa... Ey, ¿eso no es una iguana?", "Kouhei");
translation.add(94, "¿Te gustan los reptiles? Creo que tienen un\nno sé qué que los hacen muy interesantes.", "Kouhei");
translation.add(95, "¿Qué opinas de las iguanas?", "Kouhei");
translation.add(96, "...", "Aeka");
translation.add(97, "¿Shiraki-san?", "Kouhei");
translation.add(98, "Ah, sí... No, no me gustan las iguanas.", "Aeka");
translation.add(99, "Pero me encantan los zorros.\nMe gustaría criar uno alguna vez.", "Aeka");
translation.add(100, "¿Un zorro? ¿De dónde cree que sacaría uno?", "");
translation.add(101, "A veces dice cosas muy extrañas.", "");
translation.add(102, "Dudo que se haya dado cuenta, pero ese es uno\nde los motivos por los que más destaca en clase.", "");
translation.add(103, "Aunque eso no me importa...", "");
translation.add(104, "(Un zorro... Veamos...\n¿Los fenecs no son parecidos?).", "Kouhei");
translation.add(105, "¿Tan raro es que quiera tener uno?", "Aeka");
translation.add(106, "Bueno, los fenecs ahora son bastante populares.\nMe suena que en un manga un tío tenía uno.", "Kouhei");
translation.add(107, "¿Sí? Tendría que pasarme\npor una librería de vez en cuando.", "Aeka");
translation.add(108, "Es que creo que son tan encantadores...", "Aeka");
translation.add(109, "Shiraki se pone a fantasear.", "");
translation.add(110, "Se está planteando seriamente\nconseguir una cría de zorro.", "");
translation.add(111, "Regresa repentinamente al mundo real.", "");
translation.add(112, "Oh, lo siento... Supongo que debe ser\nmuy extraño eso de querer criar un zorro.", "Aeka");
translation.add(113, "Tampoco es para tanto.", "Kouhei");
translation.add(114, "Después de todo, yo quiero una iguana.", "Kouhei");
translation.add(115, "Pero a mí las iguanas no me asustan.", "Aeka");
translation.add(116, "Bueno, vámonos de aquí.", "Kouhei");
translation.add(117, "¿Descansamos un rato?", "Kouhei");
translation.add(118, "Vale; se me están cansando las piernas.", "Aeka");
translation.add(119, "Yo también estoy agotado,\nhacía tiempo que no caminaba tanto.", "Kouhei");
translation.add(120, "Quiero detenerme por varios motivos.", "");
translation.add(121, "Por aquí cerca hay una tienda bastante popular.\n¿Has oído hablar alguna vez del Antoinette?", "Kouhei");
translation.add(122, "Sí, sé cuál es.", "Aeka");
translation.add(123, "Vale, pues vamos allí.", "Kouhei");
translation.add(124, "Shiraki pide un suflé y una crema de soda.\nYo un café negro americano.", "");
translation.add(125, "...", "Aeka");
translation.add(126, "Está buenísimo.", "Kouhei");
translation.add(127, "Sí...", "Aeka");
translation.add(128, "No decimos nada mientras bebemos.", "");
translation.add(129, "Hace un rato que está como perdida.", "");
translation.add(130, "¿Estás cansada, Shiraki?", "Kouhei");
translation.add(131, "¿Eh? Oh, no, para nada.", "Aeka");
translation.add(132, "¿Y tú? ¿Lo estás?", "Aeka");
translation.add(133, "Sí, un poco.", "Kouhei");
translation.add(134, "(Mmm. Bueno, por ahora...).", "Kouhei");
translation.add(135, "Estaba pensando a qué nuevas tiendas ir luego.", "Kouhei");
translation.add(136, "Lo mejor sería comprar una guía de la ciudad.", "Aeka");
translation.add(137, "Sí, sería lo más rápido.", "Kouhei");
translation.add(138, "Los panfletos locales que distribuyen\núltimamente traen un montón de información.", "");
translation.add(139, "No está mal deambular de aquí para allá, pero\ntambién se agradece tener una idea de adónde ir.", "");
translation.add(140, "¿Kagami-kun?", "Aeka");
translation.add(141, "¿Sí?", "Kouhei");
translation.add(142, "¿Qué opinas de mí?", "Aeka");
translation.add(143, "No es la primera vez que me pregunta esto.\nAyer también hizo lo mismo, en la azotea.", "");
translation.add(144, "Bueno..., mantienes un ritmo muy peculiar.", "Kouhei");
translation.add(145, "¿Un ritmo?", "Aeka");
translation.add(146, "Sí... Lo siento, no puedo explicarlo bien.\nEs como un aura propia o algo así.", "Kouhei");
translation.add(147, "No es que no me guste ni nada de eso.", "Kouhei");
translation.add(148, "Lo he dicho con calma, como si la cosa no fuera\nconmigo, pero por dentro estoy muy nervioso.", "");
translation.add(149, "Me juego lo que sea a que ha sonado\ncomo si estuviera flirteando con ella.", "");
translation.add(150, "Gracias...", "Aeka");
translation.add(151, "Parece que le da un poco de vergüenza.", "");
translation.add(152, "Hoy te noto algo tensa.", "Kouhei");
translation.add(153, "¿En serio?", "Aeka");
translation.add(154, "Se ruboriza ligeramente y me sonríe.", "");
translation.add(155, "Una cosa.", "Aeka");
translation.add(156, "¿Mmm?", "Kouhei");
translation.add(157, "Gracias por haber estado hoy conmigo.", "Aeka");
translation.add(158, "Es como si hubiese tenido una cita con mi novio.\nMe lo he pasado muy bien.", "Aeka");
translation.add(159, "Me alegra oír eso.", "Kouhei");
translation.add(160, "Supongo que hubiera sido mas divertido\nsi hubiera venido mas gente.", "Aeka");
translation.add(161, "Entonces, ¿qué tal si la próxima vez\nvamos a algún sitio con mis amigos?", "Kouhei");
translation.add(162, "¿Eh? No creo que pudiese.", "Aeka");
translation.add(163, "¿Te supondría un problema?", "Kouhei");
translation.add(164, "La verdad es que sí... Lo siento.\nPero me alegro de que me lo hayas ofrecido.", "Aeka");
translation.add(165, "*suspiro* Qué envidia me dan los demás...\nMe gustaría que yo...", "Aeka");
translation.add(166, "Vamos, no te deprimas. Si no te va bien\nla semana que viene, podemos hacerlo en\ncualquier otro momento.", "Kouhei");
translation.add(167, "*suspiro*", "Aeka");
translation.add(168, "Suspira mientras juguetea con la pajita.", "");
translation.add(169, "Qué triste. Me deprimo con tanta facilidad...", "Aeka");
translation.add(170, "Creo que no podría hablar con tus amigos así.", "Aeka");
translation.add(171, "Si fueses un poco más valiente, la gente\nse daría cuenta de cómo eres en realidad.", "Kouhei");
translation.add(172, "Ojalá. No espero tener un montón de amigos,\npero aun así...", "Aeka");
translation.add(173, "No te preocupes. Lo único que necesitas es\ntener un poco más de confianza en ti misma.", "Kouhei");
translation.add(174, "Vale...", "Aeka");
translation.add(175, "Aeka consulta su reloj de corte sencillo.\nEl sol está empezando a ponerse.", "");
translation.add(176, "Pero mira qué hora es. Sí que ha pasado rápido\nel tiempo. ¿Tus padres no estarán preocupados?", "Kouhei");
translation.add(177, "No, no pasa nada.", "Aeka");
translation.add(178, "Coge la cuenta.", "");
translation.add(179, "Ya pago yo. Será mi agradecimiento\npor haberme acompañado de compras.", "Aeka");
translation.add(180, "No, no puedo dejar que lo hagas. Yo me encargo.", "Kouhei");
translation.add(181, "Intento atrapar el papel de la cuenta,\npero Aeka sonríe y lo aparta de mí.", "");
translation.add(182, "No es más que un trozo de pastel y un café,\ny aun así me sentiría mal.", "");
translation.add(183, "Intento convencerla, pero antes de que pueda\nhacer nada, se escabulle y paga la cuenta.", "");
translation.add(184, "Qué rápida... Bueno, gracias.\nYa pagaré yo la próxima vez.", "Kouhei");
translation.add(185, "¿Ya estás pensando en invitarme?\nNo, soy yo quien debe darte las gracias.", "Aeka");
translation.add(186, "Dudo que pueda olvidar el día de hoy.", "Aeka");
translation.add(187, "Ja, ja. Qué exagerada.", "Kouhei");
translation.add(188, "Quizá...", "Aeka");
translation.add(189, "¿Quieres que te acompañe a casa?", "Kouhei");
translation.add(190, "No, gracias. Estaré bien.", "Aeka");
translation.add(191, "Como quieras. Ten cuidado al volver.", "Kouhei");
translation.add(192, "Igualmente.", "Aeka");
translation.add(193, "Pues nada, hasta la próxima.", "Kouhei");
translation.add(194, "Hasta la próxima.", "Aeka");

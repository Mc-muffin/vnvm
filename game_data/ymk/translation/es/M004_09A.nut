translation.add(0, "Domingo, fiesta nacional.", "");
translation.add(1, "Estiro los brazos.", "");
translation.add(-1, "", "");
translation.add(3, "¿Por qué tengo que seguir en el hospital?", "");
translation.add(4, "Me levanto.", "");
translation.add(5, "Hmmmm...", "Kouhei");
translation.add(6, "Inclino la cabeza de un lado a otro,\nproduciendo chirridos con las articulaciones.", "");
translation.add(7, "......", "");
translation.add(8, "............", "");
translation.add(9, "Hmm... ¿Ya estoy bien?", "");
translation.add(10, "Después de que Aya se fuese ayer, aquel\ndoctor vino en su ronda.", "");
translation.add(11, "Llevaba una serie de internos consigo.\nDebe ser alguna persona importante del hospital.\nMe da un poco de miedo.", "");
translation.add(12, "Lo que vuelve a demostrar que no puedes juzgar\nun libro por su portada.", "");
translation.add(13, "Cuando vino a la habitación, me trajo un\ncertificado en el que ponía que era libre\nde marcharme del hospital el lunes.", "");
translation.add(14, "Pero para asegurarse, debería estar aquí\nhasta la mañana.", "");
translation.add(15, "Eso fue ayer.\nHoy, me encuentro estupendamente.", "");
translation.add(16, "Y tengo la sensación de que si me quedo aquí\nmás tiempo, mi cuerpo empezará a echar raíces.", "");
translation.add(17, "Ojalá Aya me hubiese traído un libro\no cualquier cosa para leer.", "");
translation.add(18, "......", "");
translation.add(19, "............", "");
translation.add(20, "Enciendo la televisión.", "");
translation.add(21, "Están poniendo anuncios.\nUna chica exhibe algún producto con una\nsonrisa barata trazada en la cara.", "");
translation.add(22, "Hago un poco de zapping, pero todos\nlos canales me parecen iguales.", "");
translation.add(23, "En uno de ellos echan un programa viejo.\nSe deduce por la calidad de la imagen.", "");
translation.add(24, "Pero ya está empezado.\nNo sabría decir lo que está pasando.", "");
translation.add(25, "Los domingos no hacen nada decente por la tele.", "");
translation.add(26, "La apago.", "");
translation.add(27, "Me tumbo.", "");
translation.add(28, "El techo ocupa enteramente mi campo de visión.", "");
translation.add(29, "Ah...", "");
translation.add(30, "Me perdí el capítulo de la semana pasada.", "");
translation.add(31, "......", "");
translation.add(32, "............", "");
translation.add(33, "De hecho,\nque no me haya dado cuenta\nhasta ahora dice un montón.", "");
translation.add(34, "......", "");
translation.add(35, "............", "");
translation.add(36, "Ahora me doy cuenta de que me han dejado\naquí sin nada que hacer.", "");
translation.add(37, "Maldición... Tengo demasiado tiempo libre.", "");
translation.add(38, "Me doy la vuelta. Ojalá tuviera mi móvil.", "");
translation.add(39, "Se lo comenté a mamá, pero el personal del\nhospital le dijo que no estaban permitidos.", "");
translation.add(40, "No es que fuera a utilizarlo mucho, pero es\nuna de esas cosas que echas en falta cuando\nno las tienes.", "");
translation.add(41, "Parece que estoy falto de opciones...", "");
translation.add(42, "No tengo nada que hacer. Me aburro.", "");
translation.add(43, "Hace tan solo unos días estaba tan ocupado que\napenas tenía tiempo de respirar.\nY ahora estoy desesperado por algo que hacer.", "");
translation.add(44, "Todo es por culpa de Kirimiya.", "");
translation.add(45, "Me ha dejado desastroso esta última semana.", "");
translation.add(46, "Mmm... Aunque ha sido una semana increíble.", "");
translation.add(47, "Aquel incidente con el yakuza ha sido una\nexperiencia única en la vida.", "");
translation.add(48, "Y luego cuando nos fuimos a aquel casino\nde Hong Kong.", "");
translation.add(49, "No es el tipo de cosas que ocurran normalmente.", "");
translation.add(50, "Y resulta que lo he disfrutado extrañamente.", "");
translation.add(51, "Bueno...\nSin contar la parte en la que casi muero.", "");
translation.add(52, "Ains. Esa Kirimiya...", "");
translation.add(53, "¿Qué clase de magia usa para\nacabar irritándome tanto?", "");
translation.add(54, "¡Ahhh!", "");
translation.add(55, "Me encuentro aferrándome a las sábanas y\nretorciéndome en la cama.", "");
translation.add(56, "Quizá ya no pueda volver a tener una vida\nnormal.", "");
translation.add(57, "Aún así, la alucinación auditoria ya no ha\nvuelto.", "");
translation.add(58, "¿Deseaba muy en mi interior\nalejar esa monotonía?", "");
translation.add(59, "Cuando estoy junto a Kirimiya,\nno tengo tiempo para pensar en esas cosas.", "");
translation.add(60, "Por otra parte, no sé si volverán las\nalucinaciones, si dejo de verla.", "");
translation.add(61, "Oh, no...\nEstoy empezando a volverme adicto a ella,\nmás que lo que pudiese volverme con cualquier\ndroga.", "");
translation.add(62, "Aunque he acabado en el hospital por esforzarme\ndemasiado, el estrés de no estar con ella\npuede perjudicarme la salud.", "");
translation.add(63, "......", "");
translation.add(64, "............", "");
translation.add(65, "¡Perfecto!\nCREO que el médico no hace rondas los domingos.", "");
translation.add(66, "Miro el reloj.", "");
translation.add(67, "Si regreso por la noche; no, al atardecer,\nnadie se enterará de que me he ido.\nDeseo creer fervientemente.", "");
translation.add(68, "Salgo de la cama. Cuando me levanto,\nme encuentro un poco rígido.", "");
translation.add(69, "Supongo que todavía no estoy del todo bien...", "");
translation.add(70, "Pero no estoy tan mal como para no moverme.\nAsí que empiezo a desvestirme.", "");
translation.add(71, "¿Qué haces aquí?", "Mizuki");
translation.add(72, "Suena fría.", "");
translation.add(73, "Pero cuando abrí la puerta y levantó la mirada,\nparecía sorprendida.", "");
translation.add(74, "Y cuando lo que vio, fue a mí, su cara\nse iluminó como una flor de primavera...", "");
translation.add(75, "Pero entonces se calmó y se puso hecha\nuna fiera.", "");
translation.add(76, "Vaya, parece que sí has cumplido tu promesa.", "Kouhei");
translation.add(77, "No me he dado prisa. He venido aquí con calma,\ncon cuidado de no esforzarme demasiado.", "");
translation.add(78, "Aun así, estoy sin respiración,\ny tengo sudor en la frente.", "");
translation.add(79, "No me había recuperado tanto como pensaba.", "");
translation.add(80, "Parece que pensé que encontrarme un poco mejor\nera lo mismo que haberme recuperado por\ncompleto.", "");
translation.add(81, "Pero de todos formas, aquí estoy.", "");
translation.add(82, "Por supuesto que sí.\nEstoy haciendo mi trabajo.", "Mizuki");
translation.add(83, "Jajaja. Aunque ahora es MI trabajo.", "Kouhei");
translation.add(84, "Me siento en frente de ella.", "");
translation.add(85, "Noto que estoy sudando.", "");
translation.add(86, "Tu trabajo es estar en el hospital y descansar.", "Mizuki");
translation.add(87, "Está ocupada, encargándose de los papeles\ncon suma tranquilidad.", "");
translation.add(88, "Tiene una bonita letra.\nUna chica bella con una letra bonita se me\nantoja casi como demasiado perfecto.", "");
translation.add(89, "Supongo que tienes razón...", "Kouhei");
translation.add(90, "Aunque en cierta manera,\nno pareces muy convencida.", "Kouhei");
translation.add(91, "¡No digas tonterías!", "Mizuki");
translation.add(92, "Me mira ferozmente.", "");
translation.add(93, "Pero no voy a dejarme acobardar por solo eso.", "");
translation.add(94, "Estoy realmente contento de tenerla en frente\nmío.", "");
translation.add(95, "Esto es abochornante...", "Mizuki");
translation.add(96, "Parece haberse relajado un poco.", "");
translation.add(97, "Quizá se ha dado cuenta de que he visto\na través de su severa expresión. Rápidamente\nvuelve a sus papeles.", "");
translation.add(98, "Coge otro papel de una pila cercana.", "");
translation.add(99, "Kagami... A veces juegas muy sucio.", "Mizuki");
translation.add(100, "¿Por quñe me miras así...?\nHe tratado de aguantarlo.", "Mizuki");
translation.add(101, "Tampoco es que tú juegues limpio, senpai.", "Kouhei");
translation.add(102, "No te burles de mí...", "Mizuki");
translation.add(103, "Mi respiración vuelve a la normalidad.\nEcho un vistazo a una pila de papeles.", "");
translation.add(104, "......", "");
translation.add(105, "............", "");
translation.add(106, "Vaya...\nEsto supera con creces mis expectativas.", "");
translation.add(107, "¡Ya ha terminado todos estos formularios\ncon los que yo tenía tantos problemas!", "");
translation.add(108, "¡Y ya se ha encargado de todo lo que llegó\nel pasado viernes!", "");
translation.add(109, "¿No usas la calculadora?", "Kouhei");
translation.add(110, "Solo son números; puedo hacerlos de cabeza.", "Mizuki");
translation.add(111, "Me pregunto como es que se escaquea siempre\nde su trabajo, aún con todo el talento que\ntiene.", "");
translation.add(112, "Espera, no. No debo pensar así.\nEso solo la presiona.", "");
translation.add(113, "Aún no te encuentras bien, ¿verdad?", "Mizuki");
translation.add(114, "La miro.", "");
translation.add(115, "Me está mirando con una con una\nexpresión pensativa trazada en su cara.", "");
translation.add(116, "Pues claro que sí. Estoy perfectamente.", "Kouhei");
translation.add(117, "No me mientas.", "Mizuki");
translation.add(118, "El tono de su voz no admite discusión.", "");
translation.add(119, "Pensaba que estaba bien antes de irme del\nhospital, pero...", "Kouhei");
translation.add(120, "El sudor chorrea por toda mi cara.", "");
translation.add(121, "Debo estar palidísimo.", "");
translation.add(122, "Parece que venir aquí no ha servido más\nque para preocuparla.", "");
translation.add(123, "Me llevo una mano a la cara y reflexiono.", "");
translation.add(124, "En fin... ¿Y por qué estás aquí?", "Mizuki");
translation.add(125, "No sé qué decirle.", "");
translation.add(126, "Ahora me está mirando de una forma bastante\nespeluznante.", "");
translation.add(127, "Está enfadada...", "");
translation.add(128, "Está muy enfadada... porque se preocupa por mí.", "");
translation.add(129, "Lo siento.", "Kouhei");
translation.add(130, "Lo único que puedo hacer es disculparme.", "");
translation.add(131, "Si ahora vas a disculparte...\nDeberías haberte quedado donde estabas.", "Mizuki");
translation.add(132, "Su expresión no cambia.\nY una disculpa no va a lograrlo.", "");
translation.add(133, "El ambiente es pesado.", "");
translation.add(134, "Sé que no puedo enfrentarme a ella.", "");
translation.add(135, "¿Qué debería hacer?", "");
translation.add(136, "¡¿Qué debería decir?!", "");
translation.add(137, "«Estaba aburrido».", "");
translation.add(138, "Esa es la única razón.", "");
translation.add(139, "Si le digo eso,\nentonces sí que la habré liado bien.", "");
translation.add(140, "(Y es una de esas chicas que no\ndemuestran ninguna clase de compasión).", "");
translation.add(141, "......", "");
translation.add(142, "............", "");
translation.add(143, "¿Eh?", "");
translation.add(144, "Sinceramente, ¿por qué he venido aquí?", "");
translation.add(145, "Ahora que lo pienso...", "");

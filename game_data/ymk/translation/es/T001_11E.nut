translation.add(0, "No puedo sacarme a Shiraki de la cabeza,\npero hoy no tengo más remedio que ir a trabajar.", "");
translation.add(1, "Y también estoy preocupado por Aya...\nQuizá debería salir antes.", "");
translation.add(-1, "", "");
translation.add(3, "......", "");
translation.add(4, "............", "");
translation.add(5, "............", "");
translation.add(6, "......", "");
translation.add(7, "Llamo a Aya; no parece encontrarse peor.", "");
translation.add(8, "Su enfermedad no debe ser de esas que empeoran.", "");
translation.add(9, "Entonces le comento a Tsubaki que me iré antes,\ny me pongo a trabajar.", "");
translation.add(10, "Ocurre mientras estoy limpiando\nla malla metálica de las frituras.", "");
translation.add(11, "¡T-Tienes que esconderme!", "Voz");
translation.add(12, "Una figura entra precipitadamente por la puerta.", "");
translation.add(13, "Y yo que creía hasta entonces que estaba mal...\nAhora se me cae el alma a los pies.", "");
translation.add(14, "Es Nekoko.\nEstá apoyada en el mostrador, sin aliento.", "");
translation.add(15, "Lo siento, pero las hadas no tiene permiso\npara entrar en esta tienda. Me temo que voy\na tener que llamar a la policía...", "Kouhei");
translation.add(16, "¡Cómo! ¡¿Qué tienes en contra de las hadas?!", "Nekoko");
translation.add(17, "¡M-Me está persiguiendo la brigada\nde supresión feérica! ¡Es una situación tan\ngrave como la fusión de un reactor nuclear!", "Nekoko");
translation.add(18, "¿Cómo puedes abandonarme? ¡¿Cómo puedes?!", "Nekoko");
translation.add(19, "¡Las normas son las normas!", "Kouhei");
translation.add(20, "Ahora mismo Tsubaki está sacando la basura.", "");
translation.add(21, "No tiene nadie más a quien recurrir.", "");
translation.add(22, "Aquí que lo que haré es...", "");

translation.add(0, "Uggh... Hacerlo así, aquí mismo...", "");
translation.add(1, "Es demasiado arriesgado.", "");
translation.add(2, "Me da igual que se comporte como una reina;\nno me importa que me use como a un juguete.", "");
translation.add(3, "Pero tanta indiscreción no me gusta.", "");
translation.add(4, "Eres muy mono, Kagami...", "Mizuki");
translation.add(5, "Esboza una sonrisa felina.", "");
translation.add(6, "Siento como si tuviera una diosa frente a mí.", "");
translation.add(7, "¿O es que esto no te es suficiente?", "Mizuki");
translation.add(8, "¡No, lo es! ¡Estoy a punto de reventar!", "");
translation.add(9, "Mentiría si dijera que no me quiero correr,\npero mi instinto me advierte de que no lo haga.", "");
translation.add(10, "Si eyaculase aquí y ahora...\nno sé qué me podría ocurrir.", "");
translation.add(11, "Los fluidos que recorren mi glande\nme causan un gran dolor.", "");
translation.add(12, "Lo cierto es que me duele todo.\nA este paso creo que eyacularé sangre.", "");
translation.add(13, "He perdido la cuenta de todas las veces\nque me he corrido durante estos últimos días.", "");
translation.add(14, "Mi cuerpo grita que me estoy pasando de la raya.", "");
translation.add(15, "Le pediré amablemente que lo deje...\no mi vida podría correr otra vez grave peligro.", "");
translation.add(16, "¿O... es que no quieres correrte?", "Mizuki");
translation.add(17, "Ciega ante mi situación, continúa su ataque.", "");
translation.add(18, "¿Qué tal si hago... esto?", "Mizuki");
translation.add(19, "Sus caricias se vuelven aún más contundentes.", "");
translation.add(20, "Empieza a frotar mi pene con los pies.", "");
translation.add(21, "La sensación se acentúa por\nlos fluidos que cubren mi glande.", "");
translation.add(22, "Me encuentro entre el placer y el dolor.", "");
translation.add(23, "¡Córrete, Kagami! ¡Córrete!", "Mizuki");
translation.add(24, "Pero es el placer quien termina venciendo.", "");

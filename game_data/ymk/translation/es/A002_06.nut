translation.add(0, "A la mañana siguiente...", "");
translation.add(1, "Gracias por la comida.", "Kouhei");
translation.add(2, "Como es costumbre en Japón, junto las palmas\nde las manos y doy gracias por la comida.", "");
translation.add(3, "¿Kouhei?... Estás actuando de forma MUY extraña.", "Aya");
translation.add(4, "Oh, Aya. ¿Qué ocurre?", "Mamá");
translation.add(5, "Mamá, mamá,\nKouhei ha estado comportándose de forma muy rara.", "Aya");
translation.add(6, "Por lo visto, Aya quiere que todos crean su\npredicción de anoche. Muy bien, entraré al trapo.", "");
translation.add(7, "Mamá, Aya está últimamente muy rara.", "Kouhei");
translation.add(8, "¡Eh, Kouhei, eso no es justo!", "Aya");
translation.add(9, "Cree que va pasar algo solo\nporque ayer me vio tatareando.", "Kouhei");
translation.add(10, "¡Es verdad!\nEs un mal augurio que estés tan contento.", "Aya");
translation.add(11, "Ja, ja. Menudo par.", "Mamá");
translation.add(12, "Parece que la hemos hecho reír.", "");
translation.add(13, "Je...\nCreo que ninguna de las dos se daría cuenta\nde que estoy a punto de ser acosado en clase.", "");
translation.add(14, "Hasta a mí me resulta hasta surrealista.", "");
translation.add(15, "¡Pero es verdad! ¡Créeme!", "Aya");
translation.add(16, "¡Lo siento, pero me tengo que ir!", "Kouhei");
translation.add(17, "Dejo a Aya con sus ardides y me marcho.", "");
translation.add(18, "(Me pregunto qué pasará hoy...\nPor lo pronto, actuaré con normalidad).", "Kouhei");
translation.add(19, ".........", "");
translation.add(20, "Todos enmudecen cuando me ven entrar en clase.", "");
translation.add(21, "Buenos días, Sayama-san...", "Kouhei");
translation.add(22, "...", "Misaki");
translation.add(23, "Sayama es una de las pocas personas\nque saludo por las mañanas.", "");
translation.add(24, "Pero ahora mira hacia otro lado.\nY también ha alejado su pupitre del mío.", "");
translation.add(25, "(Guau. Blanco y en botella).", "Kouhei");
translation.add(26, "Esto parece una telenovela.", "");
translation.add(27, "Es como si portase un escudo invisible que\npreviniese cualquier tipo de contacto social.", "");
translation.add(28, "(Si así está las cosas,\nno tiene sentido que siga fingiendo).", "Kouhei");
translation.add(29, "Doy media vuelta y me dirijo hacia Aeka.", "");
translation.add(30, "Kouhei-kun...", "Aeka");
translation.add(31, "(No puedo demostrar tristeza delante de ella).", "Kouhei");
translation.add(32, "Parece muy consternada por lo que me está pasando.", "");
translation.add(33, "Así que tengo que sonreír para tranquilizarla.", "");
translation.add(34, "Buenos días, Aeka.", "Kouhei");
translation.add(35, "Buenos días... Mmm, ¿Kouhei-kun?", "Aeka");
translation.add(36, "Tranquila, no pasa nada.", "Kouhei");
translation.add(37, "A partir de ahora nos va pasar de todo.\nPero deberemos afrontarlo sin la más mínima duda.", "Kouhei");
translation.add(38, "V-Vale...", "Aeka");
translation.add(39, "Parece que los estudiantes ha tenido que\nhacerse a la idea de que ahora hay dos objetivos.", "");
translation.add(40, "(Hay más silencio de lo que esperaba...).", "Kouhei");
translation.add(41, "(Además, Antoinette aún no ha venido.\nA saber lo que pasará cuando aparezca).", "Kouhei");
translation.add(42, "Mientras las aguas sigan calmas, puedo ignorar\nlas miradas de los demás y estar con Aeka.", "");
translation.add(43, "¿Entiendes esto, Aeka?", "Kouhei");
translation.add(44, "No... Y el profesor ha borrado la pizarra...\n¿Tú lo entiendes, Kouhei-kun?", "Aeka");
translation.add(45, "Veamos, lo que tienes que hacer es...", "Kouhei");
translation.add(46, "Vale. Espera un momento...", "Aeka");
translation.add(47, "Aeka empieza a copiar mis apuntes en el cuaderno.", "");
translation.add(48, "¿Quieres que comamos juntos?", "Kouhei");
translation.add(49, "¿Eh?", "Aeka");
translation.add(50, "Aeka deja de escribir.", "");
translation.add(51, "Casi siempre comemos bocadillos.\n¿Qué te parece si hoy vamos a la cafetería?", "Kouhei");
translation.add(52, "Vaya... Pero no sé qué sirven...", "Aeka");
translation.add(53, "¿Por qué no vamos allí y lo averiguamos?", "Kouhei");
translation.add(54, "Por lo visto, aún no tiene\nmuy claro que deban vernos juntos.", "");
translation.add(55, "Así que creo que la mejor forma de acostumbrarla\nes que comamos en la cafetería.", "");
translation.add(56, "V-Vale...", "Aeka");
translation.add(57, "Asiente.\nMe levanto antes de que pueda cambiar de opinión.", "");
translation.add(58, "Antoinette aparece en la cuarta hora,\npero aparte de las miradas de odio de ella\ny sus compinches, no ha hecho nada.", "");
translation.add(59, "Probablemente esté analizando la situación.", "");
translation.add(60, "(Y, además, los profesores me tienen aprecio).", "Kouhei");
translation.add(61, "Tienen ojos, y no es tan fácil atacar\na un estudiante modelo como yo.", "");
translation.add(62, "Aunque no sabría decir lo que deparará el futuro.", "");
translation.add(63, "(Sería caer bastante bajo pero,\nsi todo se desmadra, podría acudir a ellos).", "Kouhei");
translation.add(64, "Dudo que sirviera para terminar con el acoso,\npero menos daría una piedra.", "");
translation.add(65, "Deberíamos resolver nuestras diferencias\nsin ayuda de ningún profesor.", "");
translation.add(66, "¡Shiraki-san! ¡Espera un momento!", "Kyoka");
translation.add(67, "¿Eh?", "Aeka");
translation.add(68, "(Ah... Ahí vamos otra vez).", "Kouhei");
translation.add(69, "Vas a comprar comida, ¿no?\n¿Por qué siempre tengo que avisarte yo, estúpida?", "Kyoka");
translation.add(70, "Normalmente intenta fingir amistad con ella,\npero parece que ahora eso ya le da igual.", "");
translation.add(71, "Eh... Pero...", "Aeka");
translation.add(72, "¿Qué? ¿Tienes algo que decir?", "Kyoka");
translation.add(73, "La actitud amenazante de Antoinette\nestá asustando a Aeka.", "");
translation.add(74, "Lo siento, pero hoy estoy...", "Aeka");
translation.add(75, "No pienso permitir que esto continúe.\nNo quiero que Aeka tenga que tratar con ella.", "");
translation.add(76, "Vamos, Aeka. Vayamos a la cafetería.", "Kouhei");
translation.add(77, "¡Kouhei-kun!", "Aeka");
translation.add(78, "Me acerco a ella y le ofrezco la mano.", "");
translation.add(79, "¡Shiraki-san!", "Kyoka");
translation.add(80, "La cara de Antoinette se contrae por la furia.", "");
translation.add(81, "Me lo habías prometido, ¿no?", "Kouhei");
translation.add(82, "Sí...", "Aeka");
translation.add(83, "Pues adelante.", "Kouhei");
translation.add(84, "Ignoramos las miradas y abandonamos la clase.", "");
translation.add(85, "Llegamos un poco tarde,\npero todavía quedan algunos asientos libres.", "");
translation.add(86, "Yo pido curry y Aeka el menú del día.", "");
translation.add(87, "Ñam, ñam...", "");
translation.add(88, "Eh, esto no está nada mal. Qué raro.", "Kouhei");
translation.add(89, "Es el menú de la escuela: barato y copioso,\npero no suele destacar por su sabor.", "");
translation.add(90, "Y aun así...\nlo que tengo delante es realmente apetecible.", "");
translation.add(91, "Quizá sea porque está recién hecho y calentito...", "Aeka");
translation.add(92, "En parte, sí.", "Kouhei");
translation.add(93, "Y... estamos comiendo juntos...\nOh, no, ¡pero qué estoy diciendo!", "Aeka");
translation.add(94, "Eso es. Ahora lo entiendo.", "Kouhei");
translation.add(95, "Tienes toda la razón. La comida sabe\nmucho mejor si se come con alguien como tú.", "Kouhei");
translation.add(96, "Oh... Qué vergüenza que digas eso...", "Aeka");
translation.add(97, "Pese a sus palabras,\nparece muy contenta por el cumplido.", "");
translation.add(98, "(Quizá deberíamos hacer esto más a menudo).", "Kouhei");
translation.add(99, "No tenemos mucho más que decirnos,\nasí que seguimos comiendo en silencio.", "");
translation.add(100, "(La atmósfera resulta un tanto extraña...).", "Kouhei");
translation.add(101, "Aeka y yo volvemos de la comida.\nSin duda, el aire ha cambiado desde esta mañana.", "");
translation.add(102, "¡Mmpf!", "Kyoka");
translation.add(103, "En cuanto Antoinette me ve,\ndesvía la cabeza para mirar por la ventana.", "");
translation.add(104, "(Me gustaría saber en qué estará pensando...).", "Kouhei");
translation.add(105, "Mmm... ¿Qué deberíamos hacer\ndespués de clase, Kouhei-kun? ¿Kouhei-kun?...", "Aeka");
translation.add(106, "Oh, te llevaré a casa, como siempre.\nNos veremos donde están las bicicletas.", "Kouhei");
translation.add(107, "Le hablo con delicadeza,\npara disipar cualquier duda que pudiese tener.", "");
translation.add(108, "Vale... ¡Gracias!", "Aeka");
translation.add(109, "Por la tarde no pasa nada digno de mención.", "");
translation.add(110, "(Pero mientras ella ronde por aquí,\nno debo bajar la guardia).", "Kouhei");
translation.add(111, "Recuerdo la expresión de su cara cuando me invitó\nhace unos días y la que ha puesto cuando nos hemos\nvisto en clase.", "");
translation.add(112, "(Ha cambiado completamente... Me pregunto si solo\npretende ser una chica buena, o todo lo contrario).", "Kouhei");
translation.add(113, "(Aún sale con Jingai, aunque no sé si tiene\nplaneado algo para evitar que se líe con otras).", "Kouhei");
translation.add(114, "Pienso sobre ello mientras me dirijo\nhacia el aparcamiento de bicicletas.", "");

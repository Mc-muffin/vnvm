translation.add(0, "¿Kouhei, vas a la cafetería?", "Misaki");
translation.add(1, "Sí, aunque solo a comprar un poco de merienda.", "Kouhei");
translation.add(2, "Hoy no he participado en la batalla campal.\nBueno, seguro que encontraré algo decente\nsalvo que me pusiera a pedir según qué cosas.", "");
translation.add(3, "¿Me podrías comprar algo también?", "Misaki");
translation.add(4, "¿Yo? No sé si podré conseguir lo que quieras.", "Kouhei");
translation.add(5, "No pasa nada, no te supondrá ningún problema.", "Misaki");
translation.add(6, "Me gustaría un pan de melón chocolateado,\nun bocadillo con doble de huevo\ny un zumo de fresa.", "Misaki");
translation.add(7, "¿Un bocadillo con doble de huevo?", "Kouhei");
translation.add(8, "Tengo que hacerle unas cosas a mi novio\nahora mismo. ¿Podrías comprarlo, por favor?", "Misaki");
translation.add(9, "(¿Eh? ¿Por qué no va a buscarlo él, entonces?).", "Kouhei");
translation.add(10, "Bueno... ¿Te lo traigo aquí?", "Kouhei");
translation.add(11, "Sí. ¡Gracias!", "Misaki");
translation.add(12, "Ahora estoy en la misma situación que Shiraki.", "");
translation.add(13, "Frente al establecimiento, descubro por qué\nSayama decía que no iba a ser un problema...", "");
translation.add(14, "Serán 1 680 yenes.", "Vendedor");
translation.add(15, "(¡¿Que?! ¡El bocadillo con doble de huevo\ncuesta 580 yenes y el pan de melón 620!).", "Kouhei");
translation.add(16, "Son terriblemente caros...", "");
translation.add(17, "El pedido de Sayama cuesta dos veces más\nque mi propia comida.", "");
translation.add(18, "Tienen un precio tan elevado que mucha gente\nse lo piensa dos veces antes de comprarlos.", "");
translation.add(19, "Y lo más sorprendente es que quedan muy pocos.\n¿Es que son famosos o algo?", "");
translation.add(20, "¡Gracias por su compra!", "Vendedor");
translation.add(21, "Lo siento, Kouhei... Mi novio me ha dicho\nque me va a invitar por haberle ayudado.", "Misaki");
translation.add(22, "¡¿De verdad?! Menudo chasco...", "Kouhei");
translation.add(23, "(¿Y ahora qué hago con este bocadillo?).", "Kouhei");
translation.add(24, "Lo lamento. Me comeré lo que has comprado\ndurante las prácticas de mi club, tranquilo.", "Misaki");
translation.add(25, "¿Cuánto te ha costado?", "Misaki");
translation.add(26, "(No sería muy cortés darle la factura ahora).", "Kouhei");
translation.add(27, "No pasa nada, ya me lo comeré yo todo.", "Kouhei");
translation.add(28, "Lo siento mucho. Te debo una, ¿vale?\nNo dudes en pedirme lo que sea cuando quieras.", "Misaki");
translation.add(29, "Vale.", "Kouhei");
translation.add(30, "Muy bien, ¡hasta luego!", "Misaki");
translation.add(31, "Se dirige hasta otro chico. Su novio, supongo.", "");
translation.add(32, "*suspiro* (Supongo que iré a comer).", "Kouhei");
translation.add(33, "La azotea me da la bienvenida con una\nfresca brisa y la brillante luz del sol.\nVeo a Shiraki.", "");
translation.add(34, "(Estaba seguro de que estaría aquí).", "Kouhei");
translation.add(35, "Comprendo que no quiera ir a clase después\nde todas las burlas que ha recibido.", "");
translation.add(36, "Zzz... Zzz...", "Aeka");
translation.add(37, "Parece que está durmiendo plácidamente.", "");
translation.add(38, "Me siento y miro la comida.", "");
translation.add(39, "¿Debería empezar con mi bocadillo de cerdo?\nMe encantan.", "");
translation.add(40, "(El placer de un manjar se decide\nen el primer mordisco.)", "Kouhei");
translation.add(41, "El envoltorio hace un montón de ruido\ncuando empiezo a quitarlo del bocadillo.", "");
translation.add(42, "Nnnnn...", "Aeka");
translation.add(43, "Lo siento. ¿Te he despertado?", "Kouhei");
translation.add(44, "No, no pasa nada. Buenos días, Kagami-kun.", "Aeka");
translation.add(45, "Lo siento...", "Kouhei");
translation.add(46, "Vaya, ya es hora de comer. Tengo hambre.", "Aeka");
translation.add(47, "Grrrrrrrr...", "");
translation.add(48, "El oso de mi estómago está gruñendo...\nQué vergüenza...", "Aeka");
translation.add(49, "Ja, ja. Al menos es señal de buena salud.", "Kouhei");
translation.add(50, "¿Quieres un poco de mi comida?", "Kouhei");
translation.add(51, "¿Yo? No, no podría...", "Aeka");
translation.add(52, "En realidad era para una amiga mía,\npero al final no lo quiso.", "Kouhei");
translation.add(53, "Es demasiado para una sola persona.", "Kouhei");
translation.add(54, "Vaya...", "Aeka");
translation.add(55, "Es imposible que me lo termine, así que sírvete.\nTambién tengo zumo de fresa.", "Kouhei");
translation.add(56, "Le ofrezco la bolsa a Shiraki.", "");
translation.add(57, "¡Gracias! Tiene muy buena pinta.", "Aeka");
translation.add(58, "Quizá sea por la luz del sol,\npero tiene la cara radiante.", "");
translation.add(59, "¡Buen provecho!", "Aeka");
translation.add(60, "No te contengas por mí.", "Kouhei");
translation.add(61, "Vale... Entonces cogeré esto.", "Aeka");
translation.add(62, "Saboreo mi bocadillo de cerdo al tiempo que\nextiendo la mano para atrapar mi lata de café.", "");
translation.add(63, "Está realmente bueno.", "Aeka");
translation.add(64, "Me alegro, así no lo habré comprado para nada.", "Kouhei");
translation.add(65, "...", "Aeka");
translation.add(66, "(Mmm... ¿Debería hablar con ella sobre algo?).", "Kouhei");
translation.add(67, "Está concentradísima en su comida.", "");
translation.add(68, "Aunque parece animada,\nsus ojos observan en la distancia.", "");
translation.add(69, "Aunque el sol brilla con fuerza,\nuna sombra se cierne sobre ella.", "");
translation.add(70, "Dime, Kagami, ¿qué opinas de mí?", "Aeka");
translation.add(71, "¿Eh? ¿Qué?", "Kouhei");
translation.add(72, "Me pilla por sorpresa.", "");
translation.add(73, "Bueno... Mmm...", "Aeka");
translation.add(74, "Nada, olvídalo.", "Aeka");
translation.add(75, "Lo siento.", "Aeka");
translation.add(76, "Ahora parece sentirse incómoda.", "");
translation.add(77, "Gracias por compartir tu comida conmigo.", "Aeka");
translation.add(78, "¡La próxima vez seré yo quien te traiga algo!", "Aeka");
translation.add(79, "Sonríe con inmensa alegría y sale corriendo.", "");
translation.add(80, "Eh... Claro.", "Kouhei");
translation.add(81, "Será mejor que no la siga.", "");
translation.add(82, "El sentido común me dice que no es buena idea\nmeterse en problemas que pueden evitarse.", "");
translation.add(83, "(Pero me alegra ver que se trata de una chica\ncon actitud positiva).", "Kouhei");
translation.add(84, "Cuando se comporta así,\nse parece a cualquier otra chica mona.", "");
translation.add(85, "Mis ensoñaciones se ven interrumpidas por el\ntimbre de la campana que indica el inicio de\nla siguientes clases.", "");

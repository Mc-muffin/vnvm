translation.add(0, "............", "");
translation.add(1, "......", "");
translation.add(2, "Llego bastante cansado a casa.\nTal como imaginaba, mamá y papá no están aquí.", "");
translation.add(3, "Subo para comprobar cómo se encuentra Aya.\nEstá tumbada sobre la cama de su habitación;\nme mira como un diablillo cuando entro.", "");
translation.add(4, "Largo...", "Aya");
translation.add(5, "Qué ojos más terribles...", "");
translation.add(6, "Es una mirada que helaría la sangre\ndel más atroz y sádico de los criminales.", "");
translation.add(7, "Me tiemblan las piernas...\nSalgo de ahí como alma que lleva el diablo.", "");
translation.add(8, "Hoy duermo con la cabeza debajo de la almohada.", "");
translation.add(9, "Una única pared me separa de ESO... de Aya.", "");
translation.add(10, "No sabía que la noche pudiera asustarme tanto...", "");

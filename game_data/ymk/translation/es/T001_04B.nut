translation.add(0, "No tengo elección...", "");
translation.add(1, "Soy como esos chicos que darían de comer a un\ncachorrito hambriento abandonado en una calle.", "");
translation.add(2, "Está bien, espera aquí.", "Kouhei");
translation.add(3, "Dirijo mis pasos hacia la tienda.\nNekoko se lamenta detrás de mí.", "");
translation.add(4, "Si no vuelves... mi poder feérico...\nhará que te hierva la sangre...", "Nekoko");
translation.add(5, "Me pregunto si todas las hadas son capaces\nde proferir maldiciones tan terribles como esa.", "");
translation.add(6, "Tsubaki está atendiendo a unos clientes.", "");
translation.add(7, "¡Muchas gracias!", "Hirofumi");
translation.add(8, "Vaya, Kouhei. Sí que te ha costado sacar una\nbolsa de basura. ¿Le has dado de comer a los\ncuervos?", "Hirofumi");
translation.add(9, "Bueno, algo así.", "Kouhei");
translation.add(10, "Le pregunto si puedo descansar un poco más.\nNo me gusta nada tener que hacer eso, pero\nnecesito más de tiempo.", "");
translation.add(11, "Menos mal que aún no ha venido nadie.", "");
translation.add(12, "¿Puedo coger algo de comida caducada?", "Kouhei");
translation.add(13, "Sí, claro. No me importa.", "Hirofumi");
translation.add(14, "Vale, es gay, pero también una buena persona.", "");
translation.add(15, "Agradezco muchísimo que sea tan comprensivo.", "");
translation.add(16, "¿De verdad le estás dando de comer a algo?", "Hirofumi");
translation.add(17, "No es que me importe mucho...\nEspera, ¡si es un perro o un gato, entonces...!", "Hirofumi");
translation.add(18, "¿Entonces?", "Kouhei");
translation.add(19, "¡Quizá se transforme en una chica gato o perro\ny te haga una mamada como muestra de gratitud!", "Hirofumi");
translation.add(20, "A veces creo que no está bien de la cabeza.", "");
translation.add(21, "Dudo mucho que eso pase en el mundo real.", "Kouhei");
translation.add(22, "En fin, ahora vuelvo.", "Kouhei");
translation.add(23, "Lo dejo con sus fantasías y corro al almacén.", "");
translation.add(24, "Cojo dos cajas de comida y regreso al callejón.", "");
translation.add(25, "Es un espectáculo ver comer a Nekoko...", "");
translation.add(26, "¡Ah! Ahh...", "Nekoko");
translation.add(27, "¡Wunderbar!", "Nekoko");
translation.add(28, "Empieza a balbucear en alemán\ny se pone a bailotear como una retrasada.", "");
translation.add(29, "Puede que esta sea la forma que tiene de\ndemostrar su alegría, aunque se parezca a la\ndanza del fuego de los indígenas americanos.", "");
translation.add(30, "O quizá sea como esos antiguos bailes indios,\ndonde los participantes se pintarrajeaban la\ncara y se afeitaban la cabeza.", "");
translation.add(31, "¿Qué estás haciendo?", "Kouhei");
translation.add(32, "¿Es que no lo ves?\nEs el baile de agradecimiento de las hadas.", "Nekoko");
translation.add(33, "Vaya...", "Kouhei");
translation.add(34, "Sacude la cabeza como un gato con una bolsa\nde plástico en la cabeza.", "");
translation.add(35, "Menea el trasero y levanta la falda\nde tal forma que puedo verle las bragas.\nNo hay nada que destacar.", "");
translation.add(36, "La buena suerte acompaña\na todas las personas que lo ven.", "Nekoko");
translation.add(37, "Sacarás tres décimas más en las notas.", "Nekoko");
translation.add(38, "Y en el viaje de estudios saldrás con la chica\nque más te gusta. Bueno, en la foto de grupo.", "Nekoko");
translation.add(39, "Guau, qué afortunado...", "Kouhei");
translation.add(40, "Cuando termina de bailar,\nse sienta en el suelo y abre una de las cajas.", "");
translation.add(41, "Decido irme cuando la veo escurriendo\nun poco de salsa sobre unas chuletas.", "");
translation.add(42, "Bueno, tengo que volver al trabajo.\nNo vuelvas a rebuscar entre la basura.", "Kouhei");
translation.add(43, "¡Ah, Kouhei, espera!", "Nekoko");
translation.add(44, "Me indica que me acerque.\nLe hago caso, pero con cautela.", "");
translation.add(45, "Mmm.", "Nekoko");
translation.add(46, "Antes de que pueda evitarlo, Nekoko se inclina\nhacia mí. Siento una extraña sensación en la\nmejilla.", "");
translation.add(47, "Eh... ¿Qué?", "Kouhei");
translation.add(48, "Una sensación que se acentúa con el paso\ndel tiempo.", "");
translation.add(49, "¿Acaba... de besarme?", "");
translation.add(50, "Je, je. Eso vale un punto, ¿no crees, Kouhei?", "Nekoko");
translation.add(51, "Aún estoy desconcertado; nadie me había hecho\neso antes. Asiento vagamente.", "");
translation.add(52, "La dejo tranquila con su comida y entro en\nla tienda. Presiono una mano contra la mejilla.", "");
translation.add(53, "Aún puedo sentir el calor de sus labios.", "");
translation.add(54, "Pero, ¿a qué se refería con lo de un punto?", "");
translation.add(55, "¿Los estará contando? ¿Podré\ncanjearlos por un premio si consigo muchos?", "");

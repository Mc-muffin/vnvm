translation.add(0, "Hadas...", "Kouhei");
translation.add(1, "Paso distraídamente las páginas del libro.", "");
translation.add(2, "Están decoradas con unas pequeñas alas.\nAlgunos nombres están destacados en cursiva.", "");
translation.add(3, "Esto es lo que uno suele imaginarse\ncuando piensa en hadas.", "");
translation.add(4, "Pero si las comparamos con la real...", "Kouhei");
translation.add(5, "Me dirijo hastiado al índice.", "");
translation.add(6, "Seguro que no sale ningún\nestúpido nombre como «hada gato».", "");
translation.add(7, "Guau, pues sí.", "Kouhei");
translation.add(8, "«"Hada gato". Un hada con características felinas».", "");
translation.add(9, "Voy a su descripción.", "");
translation.add(10, "Mmm... «Un hada de las Tierras Altas Escocesas».", "Kouhei");
translation.add(11, "No pega mucho con ella.", "");
translation.add(12, "«Se desplaza bajo el amparo de la noche.\nEs muy astuta y escurridiza».", "");
translation.add(13, "Sí, esto ya sí.", "Kouhei");
translation.add(14, "«Suele rehuir a los humanos\ny es totalmente inofensiva».", "");
translation.add(15, "......", "");
translation.add(16, "¡Esto no tiene nada que ver!", "Kouhei");
translation.add(17, "No he podido evitar pegar un grito.", "");
translation.add(18, "Pe... perdón...", "Bibliotecaria");
translation.add(19, "¿Mmm?", "Kouhei");
translation.add(20, "Alguien se encuentra detrás de mí.", "");
translation.add(21, "Mierda, es verdad, estoy en una biblioteca.", "");
translation.add(22, "Mm... Bueno...", "Bibliotecaria");
translation.add(23, "Una plana y tímida bibliotecaria\nse encuentra de pie, detrás de mí.", "");
translation.add(24, "Lo siento. No debería haber gritado.", "Kouhei");
translation.add(25, "Oh, no se trata de eso...\nAunque bueno, tampoco deberías gritar...", "Bibliotecaria");
translation.add(26, "Es que tengo cosas que hacer\ny me gustaría cerrar ya...", "Bibliotecaria");
translation.add(27, "¿Eh? ¿Tan tarde es?", "Kouhei");
translation.add(28, "Compruebo la hora en el móvil.\nDebería irme ya si no quiero llegar...", "");
translation.add(29, "¡ H O S T I A !", "");
translation.add(30, "¡Gracias por avisarme! ¡Hasta luego!", "Kouhei");
translation.add(31, "Devuelvo los libros al estante y\nparto raudo hacia el distrito empresarial.", "");

translation.add(36, "Una chica que hasta ahora no había considerado\ncomo una mujer...", "");
translation.add(37, "Ahh, ahh... ¿En qué piensas, chico?", "Mizuki");
translation.add(38, "Respira pesadamente, mucho más que antes.", "");
translation.add(39, "Pero puedo sentir la calidez de su voz.", "");

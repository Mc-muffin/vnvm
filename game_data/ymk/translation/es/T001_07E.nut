translation.add(11, "Debo mostrarme desesperado.", "");
translation.add(12, "Ella, en cambio, luce increíblemente atractiva.", "");
translation.add(13, "No voy a comerte.", "Mizuki");
translation.add(14, "Y te he repetido muchas veces que estoy\nmás que dispuesta.", "Mizuki");

translation.add(0, "¡Por fin he terminado las estimaciones!", "");
translation.add(1, "Coloco la hoja de papel encima\nde la montaña que se alza junto a mí.", "");
translation.add(2, "Vale, estos son los presupuestos\nde los clubes para el próximo festival.", "");
translation.add(3, "Ahora tocan los informes de los presidentes\nde los clubes para los profesores.", "");
translation.add(-1, "", "");
translation.add(5, "Además de un ligero dolor de cabeza.", "");
translation.add(6, "Echo un vistazo al calendario que cuelga\nde la pared. Me cuesta mucho concentrarme.", "");
translation.add(7, "Oh, no... Todavía tengo que calcular\nlos gastos de los clubes secundarios.", "");
translation.add(8, "Son grupos de personas dedicados\na lo mismo, como correr o investigar.\nTambién tengo que hacer esos.", "");
translation.add(9, "Estoy reventado...", "Kouhei");
translation.add(10, "Joder... Este trabajo del consejo\nestá destrozando mi rutina diaria.", "");
translation.add(11, "¡¿De qué van los ayudantes?!\n¡Esta persona también tiene que\nir a un trabajo a tiempo parcial!", "");
translation.add(12, "......", "");
translation.add(13, "Sí.", "");
translation.add(14, "Una queja inexistente de una persona inexistente.", "");
translation.add(15, "Me postro sobre la mesa.\nMi frente toca la superficie.", "");
translation.add(16, "Ah. Está fresquita...", "");
translation.add(17, "Creo que ya estoy preparado para entrar\nen una compañía de contabilidad.", "");
translation.add(18, "*Bostezo*", "Kouhei");
translation.add(19, "Me dejo caer otra vez sobre la silla.", "");
translation.add(20, "Me desperezo. Me crujen las articulaciones.", "");
translation.add(21, "¿Kirimiya hacía todo esto ella sola?", "Kouhei");
translation.add(22, "He venido aquí como buen presidente del consejo,\npero todavía no ha aparecido ni Dios.", "");
translation.add(23, "No me extraña. Después de la descarada espantada\nde Kirimiya, han perdido las ganas de trabajar.", "");
translation.add(24, "El próximo presidente del consejo...", "");
translation.add(25, "El título no suena nada mal.", "");
translation.add(26, "Pero me preocupa perder el favor de Kirimiya\nahora que está libre de responsabilidades.", "");
translation.add(27, "¿Ya solo quedas tú, senpai?", "Voz");
translation.add(28, "¡Ah!", "Kouhei");
translation.add(29, "Una chica cabizbaja aparece ante mí.", "");
translation.add(30, "Eh... Esto...", "Kouhei");
translation.add(31, "¿Ella otra vez?", "");
translation.add(32, "Sí... Mmm. Eres la nueva del\nclub del estudio del arte, ¿no?", "Kouhei");
translation.add(33, "Sí. Me alegra que me recuerdes.\nToma, nuestro calendario del festival escolar.", "Chica");
translation.add(34, "Acepto el trozo de papel que me ofrece.\nPero qué letra más pequeña...", "");
translation.add(35, "Está lleno de columnas de números y\nkanjis bastante difíciles de entender.", "");
translation.add(36, "Así que vais a exponer copias de ilustraciones...", "Kouhei");
translation.add(37, "Si no recuerdo mal, el tema del festival es China.", "");
translation.add(38, "No es un club muy conocido,\npero sus miembros son extremadamente aplicados.\nSus actividades están al nivel de la universidad.", "");
translation.add(39, "Así que copias de ilustraciones históricas...\nSegún esta hoja hay un buen número de ellas.", "Kouhei");
translation.add(40, "Eso es porque también necesitamos firmas\nque nos muestren cuándo se hizo el original...", "Chica");
translation.add(41, "Comprendo...", "Kouhei");
translation.add(42, "Lo siento, pero no lo pillo.", "");
translation.add(43, "Solo finjo que lo hago.\nPero me está sonriendo, ¿qué puedo hacer?", "");
translation.add(44, "Seguro que este no será el último documento\nespecializado que me traigan.\nFinjo que le echo un vistazo.", "");
translation.add(45, "Mmm... ¿Y la presidenta?", "Chica");
translation.add(46, "Oh... Ha salido a comprar algo para beber.", "Kouhei");
translation.add(47, "Ahora que lo menciona... Miro el reloj.", "");
translation.add(48, "Ha pasado casi una hora desde que se fue.", "");
translation.add(49, "Vaya... Qué raro. Siempre suele estar por aquí.", "Chica");
translation.add(50, "La chica se despide y se marcha.", "");
translation.add(51, "......", "");
translation.add(52, "............", "");
translation.add(53, "Otra vez no...", "Kouhei");
translation.add(54, "Coloco el informe de la chica en la pila.", "");
translation.add(55, "La habitación del consejo queda en silencio.\n¿Cuántas veces van con esta?", "");
translation.add(56, "Cierro los ojos leeentamente...", "");
translation.add(57, "Me duelen hasta los párpados.\nEstaba tan concentrado en el trabajo\nque ni me había dado cuenta.", "");
translation.add(58, "Ha pasado una hora desde que ha salido.", "");
translation.add(59, "No era más que una excusa para dejarme\ncon todo el trabajo. Me siento desamparado.", "");
translation.add(60, "Estaba tan inmerso en esta tarea tan cansina\nque había perdido la noción del tiempo.", "");
translation.add(61, "¿Cómo puedo esforzarme tanto\nen algo así por voluntad propia?", "");
translation.add(62, "......", "");
translation.add(63, "............", "");
translation.add(64, "Me froto los ojos. Ah, qué gustirrinín.", "");
translation.add(65, "¿En qué estará pensando Kirimiya?", "");
translation.add(66, "Antes no salía nunca de esta habitación.\nPrácticamente vivía aquí.", "");
translation.add(67, "Mmm... Claro que si siempre estaba aquí...", "");
translation.add(68, "Era porque ella sola se encargaba de todo.", "");
translation.add(69, "Mmm. No es moco de pavo.", "");
translation.add(70, "El consejo se encarga de asuntos así cada mes.", "");
translation.add(71, "Y creo que también tiene que decidir\nlas fechas de las excursiones.", "");
translation.add(72, "Por mucho que trabajes no acaba nunca.\nSiempre habrá nuevos eventos.", "");
translation.add(73, "Cuando te paras a pensarlo...\nSe trata de un cargo que te ata a esta sala.", "");
translation.add(74, "......", "");
translation.add(75, "............", "");
translation.add(76, "Para el carro...\nAunque comprendo por qué siempre se escapa...", "");
translation.add(77, "Hay algo que no encaja.", "");
translation.add(78, "¿Por qué tengo que hacer todo esto yo solo?", "");
translation.add(79, "¿Y... cómo sabías has conseguido encontrarme?", "Mizuki");
translation.add(80, "No ha sido tan sencillo como te crees.", "Kouhei");
translation.add(81, "Ah. Y no te olvides el informe ese de ahí.", "Kouhei");
translation.add(82, "Mmf.", "Mizuki");
translation.add(83, "Me he pateado la ciudad en busca de Kirimiya.", "");
translation.add(84, "O mejor dicho, tengo la sensación de\nhaber seguido el mismo camino que ella.", "");
translation.add(85, "Primero he buscado la\nmáquina expendedora más cercana.", "");
translation.add(86, "Hacía un buen día.", "");
translation.add(87, "Y eso me ha incitado a vagar por ahí.", "");
translation.add(88, "Después de hacer todo el camino hasta la máquina,\nlas puertas de salida resultaban muy tentadoras.", "");
translation.add(89, "Y he salido.", "");
translation.add(90, "Entonces, un restaurante que hay justo al\ncruzar la calle me ha llamado la atención.", "");
translation.add(91, "Así que he ido hasta allí...", "");
translation.add(92, "Y entonces he visto un templo sintoísta...", "");
translation.add(93, "He pensado: ¿si fuera Kirimiya, adónde iría?\nY he empezado a vagar por la ciudad.", "");
translation.add(94, "No albergaba muchas esperanzas.", "");
translation.add(95, "Pero la he acabado encontrando.", "");
translation.add(96, "Debo tener talento para la investigación.", "");
translation.add(97, "......", "");
translation.add(98, "............", "");
translation.add(99, "La he encontrado en un paso de cebra.\nQuería llamarla, pero al final he dudado.", "");
translation.add(100, "La luz se ha puesto en verde y todo\nel tropel de gente ha empezado a moverse.", "");
translation.add(101, "Vamos, como cada día.", "");
translation.add(102, "Salvo porque... Kirimiya no se movía de allí.", "");
translation.add(103, "Simplemente miraba caminar a las personas.", "");
translation.add(104, "¿No estaba esperando para cruzar?", "");
translation.add(105, "Su mirada parecía reflejar algo completamente\ndistinto a la realidad que tenía delante.", "");
translation.add(106, "¿Kirimiya-senpai?...", "Kouhei");
translation.add(107, "Nunca la había visto con esa cara.", "");
translation.add(108, "Decido llamarla por su nombre,\nen vista de que no parecía hacer nada.", "");
translation.add(109, "Oh, eres tú...", "Mizuki");
translation.add(110, "Me había mirado un instante antes de volverse\nde nuevo hacia la miríada de gente.", "");
translation.add(111, "La gente terminaría por darse cuenta si seguía así.", "");
translation.add(112, "Pero si me veían pensarían que me estaba esperando.", "");
translation.add(113, "Aunque nuestra relación difiriera algo de eso.", "");
translation.add(114, "Mientras permanecía ahí, de pie, no podía evitar\npensar en la distancia que separa nuestros mundos.", "");
translation.add(115, "Aún queda trabajo por hacer, senpai...", "Kouhei");
translation.add(116, "Dudo que mis palabras quedaran\nregistradas en su cerebro.", "");
translation.add(117, "Tenía la mirada totalmente perdida.", "");
translation.add(118, "Dime, chico. ¿Lo has tenido en cuenta alguna vez?", "Mizuki");
translation.add(119, "¿Tenido en cuenta qué?", "Kouhei");
translation.add(120, "Sigue sin mirarme. Continúa hablando...", "");
translation.add(121, "El mundo está lleno de humanos.", "Mizuki");
translation.add(122, "Tanto en esta ciudad como en cualquier otra parte.", "Mizuki");
translation.add(123, "Hay tantos. Y, sin excepción, todos...", "Mizuki");
translation.add(124, "... todos ellos morirán.", "Mizuki");
translation.add(125, "¿Eh?", "Kouhei");
translation.add(126, "Los hilos de mis pensamientos se detienen en seco.", "");
translation.add(127, "Es una afirmación corriente, una verdad innegable;\npero lo ha dicho con tanta sencillez\nque no se me ocurre qué puedo decirle.", "");
translation.add(128, "La muerte.\nEn efecto, todos los seres humanos... no,\ntodos los seres vivos, morirán algún día.", "");
translation.add(129, "No sabemos cuándo, pero en algún momento,\nlos humanos desaparecerán de este mundo.", "");
translation.add(130, "Eso también va por ti y por mí.", "Mizuki");
translation.add(131, "Algún día, la gente morirá y desaparecerá.", "Mizuki");
translation.add(132, "Para cualquier observador casual, debemos\nparecer una pareja hablando de camino a casa.", "");
translation.add(133, "Lo que ha dicho es un simple hecho,\nno una divagación filosófica.", "");
translation.add(134, "Es solo que mi mente estaba centrada en\ntratar de llevarla otra vez a la escuela.", "");
translation.add(135, "Y ahora esto.", "");
translation.add(136, "No va a ser fácil tratar de quitarle ese tema\nde la cabeza y conseguir que vuelva al consejo.", "");
translation.add(137, "Además, siento que no sería\nbuena idea zanjar el tema sin más.", "");
translation.add(138, "Quizá sea por la expresión de su cara.", "");
translation.add(139, "Parece triste y desesperada,\ncomo si se hubiera rendido al desaliento.", "");
translation.add(140, "Perdona si me equivoco, pero...", "Kouhei");
translation.add(141, "¿Es posible que tengas miedo de la muerte?", "Kouhei");
translation.add(142, "Es una idea absurda.", "");
translation.add(143, "De hecho, la muerte es como una sombra\nque se yergue sobre ti, estés donde estés.", "");
translation.add(144, "La verdad, no tengo muy claro qué es exactamente.", "");
translation.add(145, "Y no creo que nadie lo sepa con certeza.", "");
translation.add(146, "Pero eso no es lo importante.\nLo importante es que no tienes que\nvivir siempre pensando en ella.", "");
translation.add(147, "Cuando lo haces, pues sí, es normal tener miedo.\nPero para darle a eso ya tenemos a los filósofos.", "");
translation.add(148, "¿Por qué ha sacado este tema?", "");
translation.add(149, "Si tengo que elegir entre estar asustada o no,\nentonces, por supuesto que lo estoy.", "Mizuki");
translation.add(150, "Pero eso es inherente a todos los seres vivos.", "Mizuki");
translation.add(151, "No es eso. Es que estoy... inquieta.", "Mizuki");
translation.add(152, "Voy a morir, haga lo que haga.\nEse simple hecho me inquieta.", "Mizuki");
translation.add(153, "No termino de entenderlo,\npor eso intento ponerme en su posición.", "");
translation.add(154, "Voy a morir.\nCualquier día de estos, mi vida llegará a su final.", "");
translation.add(155, "Pero, ¿cuándo? Si llego a viejo,\npuede ser dentro de mucho tiempo.", "");
translation.add(156, "Pero también podría morir en un accidente.\nO de un ataque al corazón.", "");
translation.add(157, "Si vamos más allá, hasta podría caer\nredondo en mitad de una conversación.", "");
translation.add(158, "Podría atropellarme un coche fuera de control,\no ser aplastado por un meteorito.", "");
translation.add(159, "Si lo piensas de esa manera, entonces puedo\ncomprender más o menos que se sienta inquieta.", "");
translation.add(160, "Pero solo un poco...", "");
translation.add(161, "No es algo en lo que piense muy a menudo.", "");
translation.add(162, "Un pensamiento siniestro cruza por mi mente.", "");
translation.add(163, "¿No será...? ¿Padeces una enfermedad incurable?", "Kouhei");
translation.add(164, "Eso lo explicaría todo.", "");
translation.add(165, "Tengo una salud de hierro.\nNuestro médico particular puede dar fe de ello.", "Mizuki");
translation.add(166, "¡Guau! ¡Tienen un médico particular!...\nVale, olvidémoslo. Siguiente.", "");
translation.add(167, "Entonces, ¿has perdido a alguien recientemente?", "Kouhei");
translation.add(168, "¿Qué?", "Mizuki");
translation.add(169, "Ahora es ella la desconcertada.", "");
translation.add(170, "Parece que no tiene respuesta a mi pregunta.", "");
translation.add(171, "¿Qué puedo hacer? No se me ocurren más opciones.", "");
translation.add(172, "¿Qué otro motivo puede tener alguien tan joven\ncomo ella se ponga a pensar en la muerte?", "");
translation.add(173, "No, no. No tiene nada que ver con eso.", "Mizuki");
translation.add(174, "No tengo intención de vivir\ntodo el tiempo con la incertidumbre.", "Mizuki");
translation.add(175, "Entonces, ¿a qué ha venido eso tan de repente?", "Kouhei");
translation.add(176, "Tan de repente, ¿eh?", "Mizuki");
translation.add(177, "Creo que todo el mundo\nen algún momento piensa en eso.", "Mizuki");
translation.add(178, "Vale, sí en algún momento, pero no todo el rato.", "Kouhei");
translation.add(179, "Entonces debo pertenecer a esa minoría.", "Mizuki");
translation.add(180, "Puede ser, pero seguro que también\ntiene otras inquietudes en la cabeza.", "");
translation.add(181, "Otro pensamiento siniestro cruza por mi mente.", "");
translation.add(182, "Antes de que nos vayamos por las ramas,\ntengo una pregunta quizá algo rara para ti...", "Kouhei");
translation.add(183, "¿Tienes pensado suicidarte?", "Kouhei");
translation.add(184, "¿Y eres tú quien me acusa de cambiar de tema?", "Mizuki");
translation.add(185, "Por fin se gira y me mira.", "");
translation.add(186, "No la entiendo.\nY sus ojos me dicen que es recíproco.", "");
translation.add(187, "Siempre me he preocupado por ella,\npero esto es excesivo.", "");
translation.add(188, "Es como si una sirena y un troglodita\nse hubieran visto por primera vez.", "");
translation.add(189, "Entiendo que pueda ser un tema un tanto raro.\nY posiblemente no sea el que tenga más éxito\nen las conversaciones.", "Mizuki");
translation.add(190, "Pero tengo un presentimiento.", "Mizuki");
translation.add(191, "Que dentro de poco, en alguna parte...", "Mizuki");
translation.add(192, "Voy a morir.", "Mizuki");
translation.add(193, "El color y el sonido se desvanecen del mundo...", "");
translation.add(194, "No quedamos más que Kirimiya y yo.", "");
translation.add(195, "Y ella está tan distante que no puedo alcanzarla.", "");
translation.add(196, "Es una chica con la que tuve sexo hace unos días,\npero ahora ni siquiera puedo tocarla.", "");
translation.add(197, "No me malinterpretes. No creo que mi vida esté\nen peligro. Y por supuesto no pienso suicidarme.", "Mizuki");
translation.add(198, "Es solo un presentimiento.", "Mizuki");
translation.add(199, "...", "Kouhei");
translation.add(200, "Mierda, no sé qué responderle.", "");
translation.add(201, "No está enferma, ni ha perdido a nadie.\nTampoco tiene intención de suicidarse.", "");
translation.add(202, "Entonces, ¿por qué cree que la muerte\nse cierne sobre ella? ¡Está desvariando!", "");
translation.add(203, "Y eso mismo ya es peligroso...", "");
translation.add(204, "Así que pretendes confundirme con temas así\npara conseguir escaquearte y dejarme todo el\ntrabajo a mí, ¿eh?", "Kouhei");
translation.add(205, "Creo que me he pasado.\nNo quería que sonara tan duro.", "");
translation.add(206, "¿Escaquearme? Ah... Es cierto.", "Mizuki");
translation.add(207, "Sonríe.", "");
translation.add(208, "Es una sonrisa distraída cuya meta es enredarme.", "");
translation.add(209, "Tenía asuntos que atender,\nasí que he salido de la escuela un momento...", "Mizuki");
translation.add(210, "No, no me lo creo.", "Kouhei");
translation.add(211, "¡Esto no se hace, senpai!\n¡No puedo hacer todo el trabajo yo solo!", "Kouhei");
translation.add(212, "¿Quééé?", "Mizuki");
translation.add(213, "Retornamos a nuestros respectivos papeles:\nyo como el aprendiz reprensor de su senpai\ny ella como la vaga que no quiere currar.", "");
translation.add(214, "Nos olvidamos del tema que tratábamos.", "");
translation.add(215, "Ella no vuelve a mencionarlo, así que yo tampoco.", "");
translation.add(216, "«Pronto podría morir»...", "");
translation.add(217, "Esas palabras resuenan en mi cabeza\nmientras continúo con el trabajo.", "");
translation.add(218, "Una y otra vez.", "");
translation.add(219, "Por lo menos ha vuelto a la sala del consejo.\nMe preocupaba que viniera con alguna\nexcusa chorra y se largara de nuevo.", "");
translation.add(220, "Me alegro mucho.", "");
translation.add(221, "... ¡Eh, no tendría por qué!\nNo debería pirarse ya desde un principio.", "");
translation.add(222, "Pero bueno, dejemos esta incómoda conversación\ny centrémonos en terminar este montón de informes.", "");
translation.add(223, "Para serte sincero, creo que he sido\nel único que se ha sorprendido.", "Kouhei");
translation.add(224, "Intento reírme. La verdad es que no sé qué decir.", "");
translation.add(225, "Sí... Yo no estaba tan sorprendida como impactada.", "Mizuki");
translation.add(226, "Su voz es seca, despectiva. ¿Habré dicho\nalgo que no debía? Ahora me siento un poco mal.", "");
translation.add(227, "Hoy es la tercera vez que me encuentras.", "Mizuki");
translation.add(228, "¿Sí? Vaya...", "Kouhei");
translation.add(229, "No se refería a la conversación del paso de cebra.\nLástima. Pero bueno, el caso es que...", "");
translation.add(230, "Ha intentado escapar tres veces\nmediante excusas de lo más variopintas.", "");
translation.add(231, "La primera vez la he visto en el patio.", "");
translation.add(232, "La segunda en la pastelería cercana a la escuela.", "");
translation.add(233, "Y la tercera vez en el paso de cebra...\nCada vez se aleja más y más de la escuela.", "");
translation.add(234, "No sé cómo explicarlo, pero de algún modo\npuedo suponer adónde has ido. Supongo que\nnuestros patrones de pensamiento son similares.", "Kouhei");
translation.add(235, "No te eches tantas flores.\nHa sido solo una coincidencia.", "Mizuki");
translation.add(236, "La primera vez puede que sí,\n¿pero tres veces seguidas?", "Kouhei");
translation.add(237, "Mmm... Tienes razón.\nLa primera es posible, pero la segunda vez...", "Mizuki");
translation.add(238, "No ha sido una coincidencia,\nsino una coincidencia increíble.", "Mizuki");
translation.add(239, "Está que no puede más con las coincidencias, ¿eh?", "");
translation.add(240, "¿Por qué no habla del vínculo que\npueden compartir algunas personas?", "");
translation.add(241, "¿Y qué me dices de la tercera vez?", "Kouhei");
translation.add(242, "La tercera vez ha sido...\nuna coincidencia extraordinaria.", "Mizuki");
translation.add(243, "¡Noooooooouuuu!", "Kouhei");
translation.add(244, "Parecemos una pareja cómica.", "");
translation.add(245, "Pero al menos es entretenido.\nHace que pulsar botones sea menos tedioso.", "");
translation.add(246, "Trabajar de esta forma\nincrementa nuestra eficiencia.", "");
translation.add(247, "¿Te estás divirtiendo?", "Mizuki");
translation.add(248, "¿Eh? Pues sí, un poco. ¿Por qué lo dices?", "Kouhei");
translation.add(249, "Alzo la mirada hacia ella.", "");
translation.add(250, "¡Oye, senpai, no estás haciendo nada!", "Kouhei");
translation.add(251, "Me encuentro terriblemente abatido.", "");
translation.add(252, "Está ahí sentada, con los brazos cruzados.", "");
translation.add(253, "¿Qué te ocurre, chico?", "Mizuki");
translation.add(254, "He sufrido un repentino ataque desmoralizador...", "Kouhei");
translation.add(255, "Exhalo un profundo suspiro.\nYa debería estar acostumbrado a todo esto.", "");
translation.add(256, "¿No te gusta este trabajo?", "Mizuki");
translation.add(257, "Al principio sí... porque podía hablar contigo.", "Kouhei");
translation.add(258, "Pero ahora me siento como un estúpido.", "Kouhei");
translation.add(259, "Así que te gusta hablar conmigo.", "Mizuki");
translation.add(260, "Y a ti, ¿te gusta hablar conmigo?", "Kouhei");
translation.add(261, "Mmmm...", "Mizuki");
translation.add(262, "Se lo está pensando.", "");
translation.add(263, "Es una pregunta bien sencilla...", "");
translation.add(264, "También podría estar fingiendo, pero no lo creo.", "");
translation.add(265, "Bueno... Por lo menos no es aburrido.", "Mizuki");
translation.add(266, "Tanto pensar para decir eso. Exhalo otro suspiro.", "");
translation.add(267, "¿Podrías al menos dejar de llamarme «chico»?", "Kouhei");
translation.add(268, "No me gusta cuando me trata como a un crío.", "");
translation.add(269, "Vale...", "Mizuki");
translation.add(270, "¿Ya? Qué fácil.", "");
translation.add(271, "Kagami...", "Mizuki");
translation.add(272, "Vaya, ¿sin sufijo? Bueno, por lo menos\neso hace que me sienta más apegado a ella.", "");
translation.add(273, "¿Se escribe así?", "Mizuki");
translation.add(274, "Se está esforzando mucho por recordar cómo\nse escriben los caracteres de mi nombre.", "");
translation.add(275, "Me dejo caer sobre la mesa.", "");
translation.add(276, "No puedo creer que saliera conmigo\nsin siquiera recordar mi nombre.", "");
translation.add(277, "Olvídalo... Llámame como quieras:\nchico, novato, subordinado. Me da igual.", "Kouhei");
translation.add(278, "Recojo la calculadora con desgana\ny empiezo a introducir números.", "");
translation.add(279, "*Suspiro*", "");
translation.add(280, "Ya me veo condenado a terminar\nesta montaña de trabajo yo solo.", "");
translation.add(281, "Pero tú solo te diriges a mí como «senpai».\nAsí que también podrías aplicarte el cuento.", "Mizuki");
translation.add(282, "Pero es que tú eres mi senpai.", "Kouhei");
translation.add(283, "Sí... Supongo que tengo ese título.", "Mizuki");
translation.add(284, "No es un título. Vas a un curso superior\nal mío y eres la presidenta del consejo.", "Kouhei");
translation.add(285, "Tienes razón, pero...", "Mizuki");
translation.add(286, ".........", "");
translation.add(287, "..................", "");
translation.add(288, "¿Eso quiere decir que...\npara ti no soy más que tu senpai?", "Mizuki");
translation.add(289, "Está hablando más consigo misma que conmigo.", "");
translation.add(290, "Eh, espera... ¿Qué?", "");
translation.add(291, "............", "");
translation.add(292, "¡¿Qué?!", "");
translation.add(293, "Alzo la cabeza y la miro.", "");
translation.add(294, "Está MUY cerca de mí.", "");
translation.add(295, "Es muy guapa incluso de tan cerca.\nEl corazón me empieza a latir con fuerza.", "");
translation.add(296, "Ya veo. Bien, menos mal.", "Mizuki");
translation.add(297, "Eh. ¿Qué?", "Kouhei");
translation.add(298, "¿Qué? ¿Te creías que podrías robarme un beso?", "Mizuki");
translation.add(299, "Tengo que tranquilizarme cuanto antes,\nespecialmente en algunas zonas.", "");
translation.add(300, "¡Ja, ja, ja! ¡Pues claro que no!\n¡Qué tonterías estás diciendo!\n¿Y tú qué? ¿Por qué pareces tan aliviada?", "Kouhei");
translation.add(301, "Vale, voy a fingir que estoy calmado\naunque por dentro no pueda estar más nervioso.", "");
translation.add(302, "Qué tranquilo estás.\nTe comportas como si nunca hubiera ocurrido.", "Mizuki");
translation.add(303, "¿Qué?", "Kouhei");
translation.add(304, "¿Como si nunca hubiera ocurrido QUÉ?", "");
translation.add(305, "Leí por ahí que algunos hombres que han\ntenido relaciones sexuales con una chica,\nla consideran como de su posesión.", "Mizuki");
translation.add(306, "Oh, vaya...", "Kouhei");
translation.add(307, "Se refiere a que me comporto\ncomo si nunca lo hubiéramos hecho.", "");
translation.add(308, "Recuerdo su pálida piel de aquella noche.\n¡Cómo podría olvidar aquel tacto!", "");
translation.add(309, "Temía que pasara lo mismo contigo,\npero veo que no has cambiado nada.", "Mizuki");
translation.add(310, "Una chica de tu posesión...", "");
translation.add(311, "Es cierto que algunas personas son así,\npero yo no soy de ese tipo.", "");
translation.add(312, "Me viene a la mente un chico de nuestra clase\nque se echó novia; desde entonces se convirtió\nen un bastardo.", "");
translation.add(313, "No me gusta nada verla de esa forma.", "");
translation.add(314, "Nada bueno puede salir cuando\nconsideras a tu novia de tu propiedad.", "");
translation.add(315, "Tampoco puedo afirmarlo con seguridad, aunque\naun así mantuve las distancias con aquel chico.", "");
translation.add(316, "Además, solo por haberlo hecho con ella una o dos\nveces, no me da derecho a considerarla como mía.", "");
translation.add(317, "Eso sería de una soberbia estúpida.", "");
translation.add(318, "Bueno... Eh...", "Kouhei");
translation.add(319, "No logro sacarme de la cabeza su tacto,\npero consigo mantenerme impasible.", "");
translation.add(320, "Aunque una cámara de infrarrojos hubiera percibido\nel incremento de mi calor corporal.", "");
translation.add(321, "Eso significa que no eres como esos, ¿verdad?", "Mizuki");
translation.add(322, "Así que para ti, yo soy...", "Mizuki");
translation.add(323, "............", "");
translation.add(324, "............", "");
translation.add(325, "............", "");
translation.add(326, "Silencio.", "");
translation.add(327, "Trago saliva. Me está mirando fijamente.", "");
translation.add(328, "Soy la presidenta del consejo...\ny tú mi subordinado. Eso es todo.", "Mizuki");
translation.add(329, "De nuevo lo dice más para ella que para mí.", "");
translation.add(330, ".........", "");
translation.add(331, "..................", "");
translation.add(332, "....................................", "");
translation.add(333, "Silencio.", "");
translation.add(334, "¿Está esperando algo?", "");
translation.add(335, "¿Está esperando que hable?\n¿Qué se supone que voy a decir?", "");
translation.add(336, "No puedo engañarla.\nTengo que decirle lo que siente mi corazón.", "");
translation.add(337, "Debo contarle lo que realmente siento.", "");

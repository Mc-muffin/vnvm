translation.add(71, "No me lamas...", "Kouhei");
translation.add(72, "Ah... ¿He hecho algo mal?", "Aeka");
translation.add(73, "Sus ojos, repletos de angustia, se preguntan\nsi me he enfadado por haber sido objeto de algo\ntan atrevido.", "");

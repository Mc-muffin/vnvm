translation.add(62, "Su respiración se acelera conforme nos catamos.", "");
translation.add(63, "Ahhh...", "Kouhei");
translation.add(64, "Mmm...", "Aeka");
translation.add(65, "Cada vez me cuesta más respirar. Nos separamos.", "");
translation.add(66, "Slurp...", "Aeka");

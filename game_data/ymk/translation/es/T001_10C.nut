translation.add(287, "Probablemente.", "Kouhei");
translation.add(288, "Supongo que será verdad...", "Mizuki");
translation.add(289, "Se sonroja un poco\ny contrae los músculos vaginales.", "");
translation.add(290, "Uhh... Ahh...", "Kouhei");
translation.add(291, "Ja, ja... Estás incluso más sensible que yo.", "Mizuki");
translation.add(292, "Me estoy derritiendo de placer.", "");
translation.add(293, "Aún no estoy acostumbrado a esto...", "Kouhei");
translation.add(294, "Ahora no te hagas el sueco...", "Mizuki");
translation.add(295, "Pero si es verdad...", "Kouhei");
translation.add(296, "Me desplazo lentamente adentro y afuera.", "");
translation.add(297, "Ahhh... Nnnh...", "Mizuki");
translation.add(298, "Sus tetas se sacuden dulcemente con el bamboleo.", "");
translation.add(299, "Senpai...", "Kouhei");
translation.add(300, "Cada vez que la saco, su cálida\npegajosidad me invita a entrar de nuevo.", "");
translation.add(301, "Ahhh... Creo...\ncreo que aguantaré si vas más rápido...", "Mizuki");
translation.add(302, "Vamos, disfruta conmigo.", "Mizuki");
translation.add(303, "S-Sí, senpai...", "Kouhei");
translation.add(304, "Más, más...", "Kouhei");
translation.add(305, "Los ruidos húmedos que profieren nuestros\ngenitales resuenan por toda la habitación.", "");
translation.add(306, "Chico... te siento...", "Mizuki");
translation.add(307, "Te siento dentro de mi vientre...", "Mizuki");
translation.add(308, "Me inclino hacia ella y le pellizco los pezones.", "");
translation.add(309, "Ayy... Ve con cuidado...", "Mizuki");
translation.add(310, "Es increíble, senpai.\nMira lo tiesos que los tienes ahora.", "Kouhei");
translation.add(311, "Cállate... Me da mucha vergüenza...", "Mizuki");
translation.add(312, "Jugueteo con sus pezones un poco más.", "");
translation.add(313, "M-Me estoy quedando sin fuerzas... Uff... Uff...", "Mizuki");
translation.add(314, "Tienes el coño muy prieto, senpai...\nMe cuesta mucho moverme...", "Kouhei");
translation.add(315, "Es porque lo metes como una mala bestia...\nSe engancha él solo... No puedo... respirar...", "Mizuki");
translation.add(316, "Siento sus vibraciones a través de las tetas\nque abarcan mis manos.", "");
translation.add(317, "Es como si mis manos se fundieran contigo...", "Kouhei");
translation.add(318, "Pese a todo, las dejo donde están.", "");
translation.add(319, "Ahh... Ojalá no dejaras nunca de tocarme...\nQué sensación tan extraña...", "Mizuki");
translation.add(320, "Uff... Uff...", "Mizuki");
translation.add(321, "Está empezando a sudar.", "");
translation.add(322, "¡Senpai, voy a correrme!", "Kouhei");
translation.add(323, "Ahhh... Hazlo... Yo también voy a...", "Mizuki");
translation.add(324, "Aplico una marcha más. Nuestros fluidos\ncorporales tiñen completamente el sofá.", "");
translation.add(325, "De hecho, están volando por todas partes.", "");
translation.add(326, "Ahh... Qué... placer...", "Kouhei");
translation.add(327, "Yo también... Yo también... ¡Ahh!", "Mizuki");
translation.add(328, "Veo estallar fuegos artificiales ante mis ojos.", "");
translation.add(329, "Ahh... Mi cuerpo... Me siento muy rara...\n¡Ahhhh!", "Mizuki");
translation.add(330, "Mi pene empieza a palpitar.\nSolo unas pocas embestidas más...", "");
translation.add(331, "Senpai... No puedo aguantarlo... ¡¡Me corro!!", "Kouhei");
translation.add(332, "Siento que llega el clímax.", "");
translation.add(333, "¡¡Aaaaaaaahhhhhhhh!!", "Mizuki");
translation.add(334, "¡Ahhhh!", "Kouhei");
translation.add(335, "La abrazo con fuerza\nal tiempo que libero mi cartucho en su interior.", "");
translation.add(336, "Ahhhh... Lo siento dentro de mí...\nEs tan... tan caliente...", "Mizuki");
translation.add(337, "Mi pene expulsa semen a borbotones.\nEstoy sudando como un cerdo.", "");
translation.add(338, "Ah... Ah... Ah...", "Mizuki");
translation.add(339, "Unhh...", "Kouhei");
translation.add(340, "Compartimos un abrazo y un beso.\nPermanecemos así hasta que Kirimiya se calma.", "");
translation.add(341, "Por cierto, lo del afrodisíaco era mentira.", "Mizuki");
translation.add(342, "¡¿Cómo?! Pero si ha funcionado, ¿no?\nEstaba muy cachondo y tenía la vista borrosa...", "Kouhei");
translation.add(343, "Pues claro que sí:\nte eché una pastilla de sudoración en el té.", "Mizuki");
translation.add(344, "¿De sudoración?", "Kouhei");
translation.add(345, "Una especie de placebo.", "Mizuki");
translation.add(346, "Respecto a la vista borrosa, estoy segura\nde que has sido tú mismo quien ha hecho\nque terminase así.", "Mizuki");
translation.add(347, "Maldita sea, me ha engañado como a un crío.", "");
translation.add(348, "Quería tirármela y resulta que ella ya\nlo había planeado desde el principio.\nSoy un ingenuo.", "");

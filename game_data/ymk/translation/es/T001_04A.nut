translation.add(-1, "", "");
translation.add(3, "Sé que debería levantarme, pero la cama está\ntan mullidita que me da una pereza tremenda.", "");
translation.add(4, "Lo único que quiero es dormir un poco más.", "");
translation.add(5, "Cinco minutos más no harán daño a nadie...\nNo quiero ni mirar la hora. Me limitaré\na dejar pasar el tiempo.", "");
translation.add(6, "¿Kouhei?...", "Aya");
translation.add(7, "Ah... Es una lástima que tenga que levantarme.", "");
translation.add(8, "Aya viene para despertarme.", "");
translation.add(9, "Pero me parece absurdo hacer eso sólo\nporque alguien esté llamando a la puerta.\nVuelvo a cerrar los ojos.", "");
translation.add(10, "Mamá dice que deberías levantarte ya.\nVoy a entrar...", "Aya");
translation.add(11, "Aya vacila un instante antes de abrir la puerta.", "");
translation.add(12, "(Quizá debería hacerle caso...).", "Kouhei");
translation.add(13, "(Nah, esperaré a que intente despertarme).", "Kouhei");
translation.add(14, "Creo que voy a gastarle una broma.\nEntreabro ligeramente los ojos.", "");
translation.add(15, "Saltaré de la cama cuando se acerque.\nEso debería darle un buen susto.", "");
translation.add(16, "Sé que no es nada del otro mundo, pero debería\nbastar para ser la primera travesura del día.", "");
translation.add(17, "Cierro los ojos, atento a los pasos de Aya...", "");
translation.add(18, "Ayer te fuiste a dormir tarde otra vez, ¿eh?\nHay que ver...", "Aya");
translation.add(19, "Cada vez está más cerca. Bien, ya falta menos...", "");
translation.add(20, "Mmsa, Mmsa...", "Aya");
translation.add(21, "¿...? ¿Qué está haciendo?", "");
translation.add(22, "Bah, qué importa. Está casi a tiro...", "");
translation.add(23, "Hermanito...", "Aya");
translation.add(24, "Antes de que pueda decir «¡Despierta!»,\nme levanto de un salto, apareciendo de debajo\nde las sábanas como una caja de sorpresas.", "");
translation.add(25, "¡Toma!", "Aya");
translation.add(26, "¡Ja, ja, ja! Ya estaba despier...", "Kouhei");
translation.add(27, "*¡CRASH!*", "");
translation.add(28, "Un objeto muy pesado se estampa contra la cama.", "");
translation.add(29, "Los muelles del colchón me elevan tres\ncentímetros en el aire antes de caer de nuevo.", "");
translation.add(30, "¡¿...?!", "");
translation.add(31, "¡Ahhh! ¡¿Pero qué coño?!", "Kouhei");
translation.add(32, "Todo el sueño que pudiera tener ha desaparecido.\nMiro detrás de mí.", "");
translation.add(33, "Ahí, donde hace un momento estaba mi cabeza...", "");
translation.add(34, "Yace la estatua de un mapache que tenemos por\ncasa, una que se tambalea hasta cuando no hay\nviento. La llamamos Dangly.", "");
translation.add(35, "Que por cierto, pesa 5,2 kilos.", "");
translation.add(36, "Esta no es forma de empezar bien el día.", "");
translation.add(37, "¿Ya estabas despierto? La próxima vez\nme lo dices antes de hacerme perder el tiempo.", "Aya");
translation.add(38, "Eh...", "Kouhei");
translation.add(39, "Mamá estaba preocupada porque decía que\nllegarías tarde a clase si seguías durmiendo.", "Aya");
translation.add(40, "¡No hagas como si no hubiese pasado nada!", "Kouhei");
translation.add(41, "¿Qué es esto?", "Kouhei");
translation.add(42, "¿Qué va a ser? Es Dangly.", "Aya");
translation.add(43, "Vale... Dejemos las cosas claras.", "Kouhei");
translation.add(44, "¿Ibas a despertarme lanzándome a Dangly\nsobre la cabeza?", "Kouhei");
translation.add(45, "Me estremezco al pensar lo que podría haber\npasado si no hubiera saltado en ese momento.", "");
translation.add(46, "¿Por qué intentas confirmar algo tan obvio?", "Aya");
translation.add(47, "De repente tengo una visión.", "");
translation.add(48, "Mongolia. Las llanuras. La hierba\nse extiende hasta donde alcanza la vista.", "");
translation.add(49, "Soy un cazador a galope.", "");
translation.add(50, "El caballo se llama Aya.", "");
translation.add(51, "Está desnuda, y estoy cabalgando sobre ella.", "");
translation.add(52, "Uso su cola como rienda y le golpeo el trasero\npara instarla a avanzar.", "");
translation.add(53, "La visión desaparece.", "");
translation.add(54, "Sooo, caballo, sooo.", "Kouhei");
translation.add(55, "Mi mano derecha se acerca a Aya,\npero logro detenerla a tiempo.", "");
translation.add(56, "No sabía que mi cuerpo reaccionase\ncon tanta facilidad a las fantasías.", "");
translation.add(57, "Tranquilo, relájate.\nNo ha sido más que una broma mañanera.", "Kouhei");
translation.add(58, "Sí. No es para tanto.", "");
translation.add(59, "Aya intentaba ayudar. No tengo que enfadarme\ncon ella. Soy un chico perfectamente razonable.", "");
translation.add(60, "Ah, otra cosa, Kouhei.", "Aya");
translation.add(61, "Como tardabas tanto en bajar,\nme he comido tu yogur.", "Aya");
translation.add(62, "Lo siento.", "Aya");
translation.add(63, "Se dice que las personas empiezan a reír\ncuando su furia sobrepasa cierto límite.", "");
translation.add(64, "Ja... ¡Ja, ja! ¡¡Jajajajajá!!", "Kouhei");
translation.add(65, "¡¿Que ella, ELLA, se ha comido MI yogur?!", "");
translation.add(66, "Esa suavidad y textura.\nEsa exquisita mezcla agridulce.", "");
translation.add(67, "Esa maravillosa combinación de calcio\ny vitaminas.", "");
translation.add(68, "Estaba muy bueno.", "Aya");
translation.add(69, "¡¿Y ella...?!", "");
translation.add(70, "¡¡Aaarrrggghhh!!", "Kouhei");
translation.add(71, "Me vuelvo loco.\nSalto de la cama y me lanzo a por ella.", "");
translation.add(72, "¡Ah! ¡Tienes los ojos inyectados en sangre!", "Aya");
translation.add(73, "El sueño que aún arrastro hace que mis\nmovimientos sean algo torpes. Aya logra\nesquivarme con facilidad.", "");
translation.add(74, "Me engancho el pie con una pata de la cama\ny me desplomo contra el suelo.", "");
translation.add(75, "¡Ahh!", "Kouhei");
translation.add(76, "Me levanto, pero enseguida me tropiezo hasta\nchocar irremediablemente contra las estanterías.", "");
translation.add(77, "Y encima, el golpe hace que algunos\nde los libros caigan sobre mi cabeza.", "");
translation.add(78, "¡Ay!", "Kouhei");
translation.add(79, "Argh, acabo de levantarme pero estoy a punto\nde caer inconsciente de nuevo.", "");
translation.add(80, "¿A qué viene este numerito a estas horas?\n¿Has tomado suficiente calcio?", "Aya");
translation.add(81, "¡¡No!! ¡¡Y es porque te has comido mi yogur!!", "Kouhei");
translation.add(82, "Aparto los libros y me levanto.", "");
translation.add(83, "Vamos, date prisa, mamá quiere limpiar ya.", "Aya");
translation.add(84, "*suspiro* ¿Por qué tengo que meterme\nen todo este lío porque tú seas un dormilón?", "Aya");
translation.add(85, "Aya se marcha, sinceramente disgustada.", "");
translation.add(86, "Baja las escaleras, murmurando para sí. Es un\nrumor mucho más animado de lo que cabría esperar\npor sus palabras.", "");
translation.add(87, "Debe haberse quedado bien a gusto\ndespués de todo lo que ha visto.", "");
translation.add(88, "Y eso que aún colea el incidente\nde la ropa interior...", "");
translation.add(89, "¿Habrá mejorado su temperamento\nel trozo de pastel que le di ayer?", "");
translation.add(90, "......", "");
translation.add(91, "............", "");
translation.add(92, "............", "");
translation.add(93, "......", "");
translation.add(94, "Hoy no ocurre nada interesante en la escuela.", "");
translation.add(95, "No he tenido la oportunidad de hablar\ncon Shiraki, y Nanjou también se ha ido.", "");
translation.add(96, "Incluso Antoinette parece comprender\nque tengo que ir a trabajar.", "");
translation.add(97, "No puedo arriesgarme a quedarme en clase y\nser reclutado de nuevo por Kirimiya. Me dirijo\ndirecto al conbini.", "");
translation.add(98, "......", "");
translation.add(99, "............", "");
translation.add(100, "............", "");
translation.add(101, "......", "");
translation.add(102, "Otro duro día de trabajo.", "");
translation.add(103, "Aunque no está mal si te da dinero\npara pasarlo bien.", "");
translation.add(104, "Aprovecho cuando hay pocos clientes para\nexaminar el escaparate junto con Tsubaki.", "");
translation.add(105, "Por cierto, Kouhei-san.\n¿Probaste el juego que te dejé el otro día?", "Hirofumi");
translation.add(106, "Ah, el juego. No me acordaba de él.\nNi siquiera lo saqué de la bolsa donde lo puso.", "");
translation.add(107, "Pero me sentiría mal si le dijera eso.", "");
translation.add(108, "Eh, no. Solo lo instalé.", "Kouhei");
translation.add(109, "No es verdad,\npero es que sencillamente se me olvidó.", "");
translation.add(110, "Aunque siento que cruzaría cierta línea\nsi lo probase.", "");
translation.add(111, "Con eso no basta.\nEs una verdadera joya, perfecta para un novato.", "Hirofumi");
translation.add(112, "Supone una gran responsabilidad escoger\nel primer juego hentai de alguien.", "Hirofumi");
translation.add(113, "¿En serio?", "Kouhei");
translation.add(114, "Oh. Estoy tan centrado en Tsubaki\nque he dejado de trabajar.", "");
translation.add(115, "Recobro la atención sobre lo que tenía entre\nmanos: introducir números en el ordenador.", "");
translation.add(116, "Pues sí. Sería muy, muy mala idea dejar que un\nprincipiante probara un juego con violaciones\ntentaculares.", "Hirofumi");
translation.add(117, "¿Violaciones tentaculares?", "");
translation.add(118, "Eso más que erótico suena asqueroso.", "");
translation.add(119, "Acabaría traumatizado.\nQuizá hasta tendría pesadillas.", "Hirofumi");
translation.add(120, "Y entonces, al despertarse, descubriría\nque se ha convertido en un pene gigante.", "Hirofumi");
translation.add(121, "¡Pero de qué estás hablando!\n¡Deja de mezclar churras con merinas!", "Kouhei");
translation.add(122, "Y NO hables sobre penes gigantes en una tienda,\naunque no haya clientes.", "");
translation.add(123, "El caso es que tu primer juego hentai\nes muy importante. Tanto como la primera vez\nque tienes sexo.", "Hirofumi");
translation.add(124, "No estoy muy seguro de a qué refiere,\npero suena razonable... Mmm, acabo de oír\nun ruido que provenía de ahí atrás.", "");
translation.add(125, "Me costó mucho trabajo encontrar uno perfecto\npara ti, y ni siquiera lo has probado.", "Hirofumi");
translation.add(126, "Si no lo juegas como un chico bueno,\ntendré que hacerte ESTO.", "Hirofumi");
translation.add(127, "Me está tocando...", "");
translation.add(128, "Empieza por el muslo y, antes de que me dé\ntiempo a reaccionar, se desplaza hacia arriba.", "");
translation.add(129, "Es un masaje indescriptible.", "");
translation.add(130, "¡Ahhhh!", "Kouhei");
translation.add(131, "Menos mal que ya no quedan clientes,\no todos me hubieran oído.", "");
translation.add(132, "¡Noooo! ¡¡Dios mío!!", "Kouhei");
translation.add(133, "Salto como un estudiante en un viaje\nde estudios a Nueva York, que se ve manoseado\npor un pervertido en un aseo público.", "");
translation.add(134, "Lo peor de todo es que no es desagradable,\nincluso me está empezando a...", "");
translation.add(135, "Eres muy bueno, Tsu-Tsuba... ¡No!\n¡No era eso lo que quería decir!", "Kouhei");
translation.add(136, "¡Tsubaki-san, me dijiste que no eras el tipo\nde chico que tocaba a aquellos que no les iba\nel tema!", "Kouhei");
translation.add(137, "Lástima, ahora que estaba empezando a ponerse\ninteresante. Pero aun así, si no pruebas el\njuego...", "Hirofumi");
translation.add(138, "Me mira de arriba a abajo. Incluso ahora,\ncon esta actitud lujuriosa, sigue manteniendo\nun aire de elegancia.", "");
translation.add(139, "Parece un coleccionista de arte evaluando\nuna pieza.", "");
translation.add(140, "Entonces tendré que jugar contigo en mi mente.", "Hirofumi");
translation.add(141, "¡¿Qué?! ¡Eso es un ataque imbloqueable!", "Kouhei");
translation.add(142, "Puedo defenderme en el mundo físico,\n¡pero en el ilusorio no puedo hacer nada!", "");
translation.add(143, "¡Lo siento! ¡Lo siento, de verdad!\n¡Lo lamento! ¡Perdóname!", "Kouhei");
translation.add(144, "¡Además, ¿no deberíamos seguir trabajando?!", "Kouhei");
translation.add(145, "Yo ya he terminado.", "Hirofumi");
translation.add(146, "¡¿Eh?!", "Kouhei");
translation.add(147, "Es cierto. Ha acabado todos\nsus quehaceres mientras hablábamos.", "");
translation.add(148, "Je, je, je. Parece que no vendrá nadie\ndurante un rato. Hasta entonces...", "Hirofumi");
translation.add(149, "Te echaré un vistazo en profundidad.", "Hirofumi");
translation.add(150, "¡Ni hablar!", "Kouhei");
translation.add(151, "Tsubaki regresa al mostrador.", "");
translation.add(152, "Trato de huir de sus continuas miradas\nmientras me apresuro en acabar el inventario.", "");
translation.add(153, "Argh. No quiero estar en la misma tienda que él.", "");
translation.add(154, "¡Voy a tirar la basura!", "Kouhei");
translation.add(155, "Vale.\nMientras tanto dejaré volar la imaginación...", "Hirofumi");
translation.add(156, "¡No fantasees!", "Kouhei");
translation.add(157, "Cojo el saco de basura y me escabullo\npor la puerta de atrás.", "");
translation.add(158, "No es un mal tipo, de verdad que no. Pero...", "");
translation.add(159, "......", "");
translation.add(160, "............", "");
translation.add(161, "............", "");
translation.add(162, "......", "");
translation.add(163, "He conseguido escapar de su abuso mental.", "");
translation.add(164, "Bueno...", "Kouhei");
translation.add(165, "Estoy en la parte trasera de la tienda,\ndonde acumulamos todos los desperdicios.", "");
translation.add(166, "¿Quién es esta?", "Kouhei");
translation.add(167, "Aaahhh...", "Chica");
translation.add(168, "Esa ropa tan escandalosa hace daño a la vista.", "");
translation.add(169, "Me suena haberla visto en alguna carátula\nde los juegos porno de Tsubaki.", "");
translation.add(170, "Pero ahí está, boca arriba, tumbada encima\nde un montón de bolsas de basura.", "");
translation.add(171, "Ja, ja, ja...", "");
translation.add(172, "Mierda. ¿Tanto se ha metido ese maricón\nen mi mente que ahora tengo alucinaciones\nvisuales?", "");
translation.add(173, "Ya tengo bastante con aquel tren;\nsolo faltaría que también me fallase la vista.", "");
translation.add(174, "Creo que tengo un problema serio.", "");
translation.add(175, "Sonrío para mí mientras lanzo la bolsa en un\nrincón, haciendo todo lo posible por ignorarla.", "");
translation.add(176, "Seguramente sea una alucinación causada por\nuna frustración sexual o algo por el estilo.", "");
translation.add(177, "Eh, tú, humano...", "Chica");
translation.add(178, "Su voz no es más que fruto de mi imaginación...", "");
translation.add(179, "Tranquilo, corazón. Es una ilusión.\nMira, la ignoraremos y entraremos, ¿vale?", "");
translation.add(180, "¡Te estoy hablando a ti, humano!", "Chica");
translation.add(181, "Algo me golpea el cogote. El lúgubre callejón\nse ilumina repentinamente con unas lucecitas\nque centellean delante de mis ojos.", "");
translation.add(182, "¡Auu!", "Kouhei");
translation.add(183, "Me pongo de cuclillas, protegiéndome la cabeza\ncon las manos. Veo un objeto rodar hasta chocar\ncontra mis zapatos.", "");
translation.add(184, "Es un bastón de esos que llevan en los animes\nlas chicas que tardan media hora en cambiarse.\nCreo que se llaman varitas mágicas.", "");
translation.add(185, "O eso parece. Es cilíndrico, y está coronado\npor una forma que no logro ubicar. Alguien debe\nhabérmelo lanzado.", "");
translation.add(186, "¡¿Desde cuándo las alucinaciones pueden atacar\nfísicamente a la gente?!", "Kouhei");
translation.add(187, "¡A quién crees que llamas alucinación!\nNo soy la personificación de tus frustraciones, \n¡soy muy real!", "Chica");
translation.add(188, "Pero desde el punto de vista de mi libido,\nese objeto podría ser un ****** o una *****.", "Kouhei");
translation.add(189, "Bueno, ya me entiendes.", "");
translation.add(190, "Guau, eso ha sonado muy obsceno.", "Kouhei");
translation.add(191, "¡...!", "Chica");
translation.add(192, "Parece que ella también lo ha pillado.\nSe le sonrojan las mejillas.", "");
translation.add(193, "Esta es la primera vez que ves a un hada,\n¿y lo único que se te ocurre es acosarla\nsexualmente? ¡Argh!", "Chica");
translation.add(194, "Se enfada y brinca a por mí como un saltamontes.", "");
translation.add(195, "¡Ah!", "Kouhei");
translation.add(196, "Pero no logra alcanzarme, pierde el equilibrio\ny se la pega contra el suelo.", "");
translation.add(197, "Uuh... Qué mala suerte.\nNo tengo suficiente poder feérico para luchar...", "Chica");
translation.add(198, "Está ahí tirada, con el culo al aire.\nEs una posición bastante extraña.", "");
translation.add(199, "Aunque más extrañas son las cosas que dice.", "");
translation.add(200, "¿Qué quieres decir... con un «hada»?", "Kouhei");
translation.add(201, "Un hada es un hada.", "Chica");
translation.add(202, "¿Te refieres a esos soldados que participan\nen conflictos a cambio de un estipendio?", "Kouhei");
translation.add(203, "¡Exacto! La Legión extranjera francesa es muy\nfamosa. ¡Somos soldados de fortuna que haríamos\ncualquier cosa por dinero!", "Chica");
translation.add(204, "Eh... ¡No, nada de eso!", "Chica");
translation.add(205, "Se pone a saltar como una pulga a mi alrededor,\nen un intento por atacarme de nuevo.", "");
translation.add(206, "Pero esta vez la esquivo apartándome a un lado.", "");
translation.add(207, "Ah, ah, ah. Me mareo...", "Chica");
translation.add(208, "Acaba otra vez con los huesos en el suelo.\nLe cuesta respirar. Alza la vista y me echa\nuna mirada furiosa.", "");
translation.add(209, "¡Te digo que soy un hada! Un elfo. Un duende.\n¡No me digas que es la primera vez que lo oyes!", "Chica");
translation.add(210, "Pues... vale.", "");
translation.add(211, "Me examino cuidadosamente.\nCuanto más la miro, más rara me parece.", "");
translation.add(212, "Dirijo la mirada hasta el punto de recogida\nde residuos.", "");
translation.add(213, "Basura incinerable los lunes, no incinerable\nlos miércoles y reciclable los jueves...", "Kouhei");
translation.add(214, "¿Qué día tocaría tirar a las hadas?", "Kouhei");
translation.add(215, "Que lo decida el basurero. La meteré dentro\nde una de esas bolsas con cierre hermético.", "");
translation.add(216, "No pienso darle más vueltas al asunto.\nMe desharé de ella y haré como si nada\nde esto hubiera ocurrido.", "");
translation.add(217, "¡¡Ni se te ocurra hacer eso!!\n¡Debes tener muchas agallas si te atreves a\nlanzar a la basura a la gran hada gato Nekoko!", "Chica");
translation.add(218, "Hada gato... ¿Qué?", "Kouhei");
translation.add(219, "Es mi seudónimo. No puedo darte mi nombre\nverdadero porque eso te permitiría controlarme.", "Nekoko");
translation.add(220, "Aunque yo sí sé el tuyo, Kagami Kouhei.", "Nekoko");
translation.add(221, "¡Agh! ¡¿Cómo lo has descubierto?! ¿Ha sido tu\npoder feérico? ¡Sal de mi mente ahora mismo!", "Kouhei");
translation.add(222, "Nop. Lo llevas escrito ahí.", "Nekoko");
translation.add(223, "Lo pone en la etiqueta del uniforme del trabajo.\nIntento ocultarla, pero ya es demasiado tarde.", "");
translation.add(224, "Je, je, je... ¡Ya no tienes adónde huir!", "Nekoko");
translation.add(225, "Y mi poder feérico me ha revelado que tu\ncompañero de trabajo se arrima mucho a ti...", "Nekoko");
translation.add(226, "¡¿Qué?! ¡Aléjate de mí!", "Kouhei");
translation.add(227, "Le echaré veneno a tu comida siempre que salgas\na trabajar. Es una maldición terrible que solo\nlas hadas podemos realizar.", "Nekoko");
translation.add(228, "¿No crees que eso se aleja demasiado\nde la imagen que se supone que tenéis?", "Kouhei");
translation.add(229, "Maldigo el día en que conocí\na esta estrafalaria hada.", "");
translation.add(230, "Bah, haz lo que quieras.\nPero deja la basura en paz, ¿vale?", "Kouhei");
translation.add(231, "Me está entrando dolor de cabeza.\nDecido entrar en la tienda.", "");
translation.add(232, "¡Ah, espera! ¡Espera!", "Nekoko");
translation.add(233, "¡¿Vas a abandonar a un hada en este sucio\ny mugriento callejón?!", "Nekoko");
translation.add(234, "Se engancha a mi pierna, impidiendo que me vaya.\nSe parece más a una garrapata que a un hada.", "");
translation.add(235, "Lo siento, pero estoy ocupado.", "Kouhei");
translation.add(236, "Intento desembarazarme de ella...", "");
translation.add(237, "Cuando oigo un gruñido que resuena debajo de mí.", "");
translation.add(238, "Proviene del estómago del hada gato Nekoko.\nDura por lo menos siete segundos. La miro\nestupefacto. Parece que está muerta de hambre.", "");
translation.add(239, "Mira lo que has conseguido.\nHe perdido más poder feérico por tu culpa.", "Nekoko");
translation.add(240, "Refunfuña para sus adentros, a punto de llorar.", "");
translation.add(241, "Las hadas necesitan energía para permanecer\nen este mundo. ¡Deberías saberlo!", "Nekoko");
translation.add(242, "Especialmente comida.", "Nekoko");
translation.add(243, "No me he echado a la boca nada más que agua\ny microbios desde hace casi veinte horas.", "Nekoko");
translation.add(244, "Ahora comprendo por qué ha estado dándome\nla tabarra todo este tiempo.", "");
translation.add(245, "¿Qué debería hacer?", "");

translation.add(0, "*Suspiro*", "");
translation.add(1, "Ahora sí que estoy realmente cansado.", "");
translation.add(2, "Yo me he puesto nervioso y yo\nhe terminado reventado. Todo queda en casa.", "");
translation.add(3, "¿He dicho algo que te deprimiera?", "Mizuki");
translation.add(4, "Sí... Bueno, no...", "Kouhei");
translation.add(5, "¿O quizá... estás preocupado?", "Mizuki");
translation.add(6, "¿Eh? ¡No, no! ¡No lo estoy! ¡Para nada!", "Kouhei");
translation.add(7, "¿De verdad que no? Qué soso.", "Mizuki");
translation.add(8, "Me dejo caer otra vez sobre la mesa.\nMi cabeza rebota contra la superficie.", "");
translation.add(9, "Si esto fuera un manga, ahora tendría\nuna tirita bien grande en la frente.", "");
translation.add(10, "Pero no lo es. Es la realidad, y lo único\nque he conseguido es sentirme aún más agotado.", "");
translation.add(11, "Me duele la cabeza del golpe. Nunca me\nhabía sentido tan cerca de mis antepasados.", "");
translation.add(12, "De en medio de la desesperanza\nasoma el horrible semblante de la ira.", "");
translation.add(13, "De hecho, hay una cosa que me preocupa. Y mucho.", "Kouhei");
translation.add(14, "¿Mmm?", "Mizuki");
translation.add(15, "Me preocupa toda esta montaña de trabajo.", "Kouhei");
translation.add(16, "Tengo la sensación de que nunca más\nme levantaré de esta mesa.", "");
translation.add(17, "Pues dejémoslo para mañana.", "Mizuki");
translation.add(18, "¿Eh? Pero mañana será domingo.", "Kouhei");
translation.add(19, "Exacto. Nos veremos aquí a primera hora.", "Mizuki");
translation.add(20, "Ahh... Vale.", "Kouhei");
translation.add(21, "No es que tenga muchas ganas de volver...", "");

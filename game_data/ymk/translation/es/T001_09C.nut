translation.add(0, "Parece que le duele, aunque es comprensible.", "");
translation.add(1, "No he visto lo que ha pasado,\npero seguro que ha dado muchas vueltas.", "");
translation.add(2, "No puedo contener el impulso de ofrecerle\nla mano. Como haría cualquier persona normal.", "");
translation.add(3, "Kouhei...", "Takeshi");
translation.add(4, "Iogi clava su mirada en mí.\n«Déjala en paz», parece decir.", "");
translation.add(5, "Sus ojos reflejan el sentir de toda la clase.", "");
translation.add(6, "Aléjate de ella. No te involucres.", "");
translation.add(7, "Ah...", "Aeka");
translation.add(8, "Finge que no le duele nada,\naunque sé que no es verdad.", "");
translation.add(9, "Nadie podría estar bien en una atmósfera\ntan fría como esta.", "");
translation.add(10, "Varias preguntas me rondan por la cabeza.", "");
translation.add(11, "¿Por qué nadie se preocupa por ella?\n¿Por qué nadie ha intentado ayudarla?", "");
translation.add(12, "No tiene mucho sentido, ¿verdad?", "");
translation.add(13, "Ja, ja... Me he caído.", "Aeka");
translation.add(14, "Y aun así, yo...", "");
translation.add(15, "Me iba a lavar las manos, cuando me caí...", "Aeka");
translation.add(16, "No intentes ayudarla...", "");
translation.add(17, "Lo siento, Kagami-kun. Espero no haberte\nasustado cuando me he chocado contigo.", "Aeka");
translation.add(18, "Y aun así se preocupa por mí.", "");
translation.add(19, "Eh, sí... Estoy bien.", "Kouhei");
translation.add(20, "¿De verdad? Menos mal...", "Aeka");
translation.add(21, "Se levanta cabizbaja y abandona el aula.", "");
translation.add(22, "No parece haberse hecho nada.\nQuizá haya sido solo el susto.", "");
translation.add(23, "Pero de todas formas, algo ha quedado.", "");
translation.add(24, "Debe estar más herida por dentro que por fuera.", "");
translation.add(25, "En mi ensimismamiento, el timbre anuncia\nel comienzo de la siguiente clase.", "");

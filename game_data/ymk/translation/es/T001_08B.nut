translation.add(0, "No puedo dejar de pensar en Shiraki.", "");
translation.add(1, "No sé cómo pudo convertirse en el principal\nobjetivo de Antoinette y su banda casi desde\nel primer día en que la transfirieron.", "");
translation.add(2, "Prefiero mil veces a Shiraki antes que a\nuna chica tan cargante e infame como ella.", "");
translation.add(3, "(Además, se ha olvidado el cuaderno).", "Kouhei");
translation.add(4, "Supongo que debería devolvérselo.", "");
translation.add(5, "Aprovecho para recogerlo de su pupitre\ncuando nadie está mirando y me marcho.", "");
translation.add(6, "Seguro que Shiraki está en la azotea.", "");
translation.add(7, "De paso le echaré un vistazo al cuaderno.", "");
translation.add(8, "Quiero comprobar si la hipótesis\nque me planteé en clase es correcta.", "");
translation.add(9, "Hojeo el libro hasta encontrar la página que\ntuvo que leer. En principio no noto nada raro.", "");
translation.add(10, "Pero...", "");
translation.add(11, "(Lo que me temía).", "Kouhei");
translation.add(12, "La primera página que leyó está intacta,\npero la siguiente...", "");
translation.add(13, "... está arrancada.", "");
translation.add(14, "(¿Esto no es delito?).", "Kouhei");
translation.add(15, "Siento una fuerte presión en el pecho.\nNo creo que Antoinette lo haya hecho\ncon sus propias manos.", "");
translation.add(16, "Lo más probable es que mandara a otra chica\npara que lo hiciera por ella.", "");
translation.add(17, "(Debería decírselo a un profesor... Mejor no).", "Kouhei");
translation.add(18, "No tendrían ni idea de cómo intervenir\nen un asunto como este.", "");
translation.add(19, "Solo empeoraría las cosas.", "");
translation.add(20, "Abro la puerta de la azotea.", "");
translation.add(21, "¿Por qué soy el único que se siente solo?", "");
translation.add(22, "(Me pregunto si estará por aquí).", "Kouhei");
translation.add(23, "Pues sí; está con su manta, como de costumbre.", "");
translation.add(24, "Mmm...", "Aeka");
translation.add(25, "Shiraki-san, ya han terminado las clases.", "Kouhei");
translation.add(26, "Ya...", "Aeka");
translation.add(27, "Parece que he dormido demasiado tiempo...", "Aeka");
translation.add(28, "Me sonríe radiante, pero todavía no soy capaz\nde olvidar lo ocurrido en clase.", "");
translation.add(29, "De hecho, eso hace su sonrisa aún más dolorosa.", "");
translation.add(30, "Bueno... Shiraki-san, te olvidaste el cuaderno.", "Kouhei");
translation.add(31, "Gracias... Muchas gracias.", "Aeka");
translation.add(32, "Se lo doy.", "");
translation.add(33, "Una expresión inquisitiva se perfila en su cara.", "");
translation.add(34, "Como si me estuviese preguntando\nsi he rebuscado en el interior.", "");
translation.add(35, "Pero no digo nada.", "");
translation.add(36, "Y ella no saca el tema a colación.", "");
translation.add(37, "Ambos tememos que cualquiera de los dos\nlo mencione.", "");
translation.add(38, "Un pesado silencio cae sobre nuestros hombros.", "");
translation.add(39, "¡Ah, claro!", "");
translation.add(40, "Una cosa...", "Kouhei");
translation.add(41, "Quizá sea por la lástima que siento por ella,\npero...", "");
translation.add(42, "¿Sí?", "Aeka");
translation.add(43, "¿Te gustaría que te acompañara a casa?", "Kouhei");
translation.add(44, "Me siento obligado a no dejarla sola.", "");
translation.add(45, "Vale...", "Aeka");
translation.add(46, "¡¿De verdad?!", "Kouhei");
translation.add(47, "Dirige la mirada al patio.", "");
translation.add(48, "Los del club de béisbol entrenan cada día\nmuy duro.", "Aeka");
translation.add(49, "Bueno, viven en un mundo donde solo existe\nel espíritu de lucha. Está en su naturaleza.", "Kouhei");
translation.add(50, "Me pregunto si debería comportarme igual.", "Aeka");
translation.add(51, "¿No practicas ningún deporte?", "Kouhei");
translation.add(52, "No soy muy buena moviendo el cuerpo.", "Aeka");
translation.add(53, "Vaya... Supongo que yo tampoco soy\nel más indicado para recomendar eso.", "Kouhei");
translation.add(54, "¿Y qué te parece el club del té, el de lectura\no el de cocina?", "Kouhei");
translation.add(55, "No sé... Estar en un club parece cansar mucho.", "Aeka");
translation.add(56, "Ojalá pudiera apuntarme al club de la siesta,\no el de salir de compras.", "Aeka");
translation.add(57, "No creo que exista algo como eso...", "Kouhei");
translation.add(58, "Bueno...", "");
translation.add(59, "Supongo que no... *suspiro*", "Aeka");
translation.add(60, "Sabes...", "Aeka");
translation.add(61, "¿Sí?", "Kouhei");
translation.add(62, "Creo que dormiré un poco más antes de volver\na casa. Lo siento.", "Aeka");
translation.add(63, "¡¿Qué?! ¡Pero si pillarás un resfriado!", "Kouhei");
translation.add(64, "Estaré bien; esta manta es muy calentita.", "Aeka");
translation.add(65, "Mmmm...", "Kouhei");
translation.add(66, "¿No puedo hacer nada para sacarla de aquí?", "");
translation.add(67, "Kagami-kun.", "Aeka");
translation.add(68, "Me coge por sorpresa.", "");
translation.add(69, "¿Sí? ¿Qué?", "Kouhei");
translation.add(70, "¿Estás apuntando en algún club?", "Aeka");
translation.add(71, "Pues no; además, trabajo a tiempo parcial.", "Kouhei");
translation.add(72, "¿En serio? Eres increíble, Kagami-kun.", "Aeka");
translation.add(73, "No se lo cuentes a los profesores, ¿vale?", "Kouhei");
translation.add(74, "¿Por? ¿Es algo malo?", "Aeka");
translation.add(75, "Pues sí. Solo los estudiantes con problemas\neconómicos en casa o los que deben trabajar\ndespués de clase pueden hacerlo.", "Kouhei");
translation.add(76, "Oh, Kagami-kun, eres un chico muy malo.", "Aeka");
translation.add(77, "Así que no digas ni una palabra de esto a nadie.", "Kouhei");
translation.add(78, "Solo Iogi y Sayama lo saben.", "Kouhei");
translation.add(79, "O así es como se supone que debería ser.\nPero tengo la sensación de que todo cristo\nestá al corriente.", "");
translation.add(80, "Espero que a nadie se le ocurra chivarse.", "");
translation.add(81, "¿Iogi-kun? ¿Sayama-san?", "Aeka");
translation.add(82, "Sí, están en nuestra clase.\nAunque tampoco es que seamos TAN amigos.", "Kouhei");
translation.add(83, "Antoinette también lo sabe, supongo...", "Kouhei");
translation.add(84, "Mierda, se me ha escapado.", "");
translation.add(85, "Er, no... Acércate, te confiaré un secreto.\nCuando hablo de Antoinette, no me refiero a\naquella tienda...", "Kouhei");
translation.add(86, "No necesito huir o esconderme.", "");
translation.add(87, "¿Eh?", "Aeka");
translation.add(88, "Vale, escucha, esto es confidencial...", "Kouhei");
translation.add(89, "Me acerco a ella y se lo susurro al oído.", "");
translation.add(90, "Me refiero a Nanjou, la de nuestra clase.", "Kouhei");
translation.add(91, "¡¿Qué?! ¿Le has puesto un apodo?", "Aeka");
translation.add(92, "Ell también habla en voz baja.", "");
translation.add(93, "Sí. Porque aunque parezca una chica agradable,\nen realidad es insoportable.", "Kouhei");
translation.add(94, "¿No crees que es apropiado para ella?", "Kouhei");
translation.add(95, "Sí..., supongo que sí, si lo piensas bien.\nAunque si alguna vez lo descubre, te meterás\nen un buen lío.", "Aeka");
translation.add(96, "Lo sé, y por eso no debes decírselo a nadie,\n¿de acuerdo? Eres la primera persona a quien\nse lo he contado.", "Kouhei");
translation.add(97, "Oh, ¿solo a mí? Vaya...\nPrometo mantener la boca cerrada.", "Aeka");
translation.add(98, "Je, je, je.", "Aeka");
translation.add(99, "...", "Kouhei");
translation.add(100, "...", "Aeka");
translation.add(101, "¿Quieres que te acompañe hasta casa?\nDe paso podríamos tomar un té en el Antoinette.", "Kouhei");
translation.add(102, "Podríamos hablar allí si tuviéramos algo\nque contarnos.", "");
translation.add(103, "¿Shiraki-san? ¿Nos vamos?", "Kouhei");
translation.add(104, "D-De acuerdo...", "Aeka");
translation.add(105, "¿Qué te pasa, Shiraki-san?", "Kouhei");
translation.add(106, "Na... nada...", "Aeka");
translation.add(107, "Mmm...", "Aeka");
translation.add(108, "Parece que no se siente a gusto.", "");
translation.add(109, "No pienso obligarte a ir si no quieres...", "Kouhei");
translation.add(110, "Er, mmm...", "Aeka");
translation.add(111, "¿Qué te ocurre?", "Kouhei");
translation.add(112, "Mmm...", "Aeka");
translation.add(113, "¿Estará indispuesta?", "");
translation.add(114, "¿Shiraki, te encuentras mal?", "Kouhei");
translation.add(115, "...", "Aeka");
translation.add(116, "Antes dijiste que tenías que ir a la enfermería.\nPodemos quedarnos aquí, si quieres.", "Kouhei");
translation.add(117, "No, no...\nMentí cuando dije que no me encontraba bien.", "Aeka");
translation.add(118, "Solo huía. Me escapé, y no quería ir a casa.\nNo tenía ningún sitio al que ir excepto este.", "Aeka");
translation.add(119, "Y pensé que si subía aquí...", "Aeka");
translation.add(120, "Entonces, quizá, tú también vendrías.", "Aeka");
translation.add(121, "Shiraki no me permite decir nada.", "");
translation.add(122, "Porque solo tú...", "Aeka");
translation.add(123, "Parece tan desesperada...", "");
translation.add(124, "Se ha echado a reír, aunque parece\nque está a punto de romper a llorar.", "");
translation.add(125, "Espera un momento... ¿Shiraki-san?", "Kouhei");
translation.add(126, "La única persona que me habla así...", "Aeka");
translation.add(127, "Eres tú, Kagami-kun...", "Aeka");
translation.add(128, "No, no puede ser verdad...", "Kouhei");
translation.add(129, "Es por eso... Es por eso...", "Aeka");
translation.add(130, "¿Está temblando?", "");
translation.add(131, "Mmm... Yo...", "Aeka");
translation.add(132, "(¡Eh! ¿Shiraki? Espera un momento... Ah...).", "Kouhei");
translation.add(133, "Un torrente de lágrimas mana por sus mejillas.", "");
translation.add(134, "*snif*... *sollozo*", "Aeka");
translation.add(135, "¡Creía que me bastaba con poder hablar contigo!", "Aeka");
translation.add(136, "Pero ya no es suficiente...\nEres tan bueno. ¡No puedo aguantarlo más!", "Aeka");
translation.add(137, "¡Sé que es egoísta por mi parte,\npero tú has sido quien me ha hecho serlo!", "Aeka");
translation.add(138, "Esta es la primera vez que la veo así.", "");
translation.add(139, "Nunca había visto en ella tantos sentimientos.", "");
translation.add(140, "Y, en cierto modo, es por mi culpa.", "");
translation.add(141, "Se abalanza hacia mí, aferrándose a mi pecho.", "");
translation.add(142, "Mis mejillas se encienden. ¿Debería apartarla?", "");
translation.add(143, "Ya basta...", "Aeka");
translation.add(144, "¡¿Mmmm?!", "Kouhei");
translation.add(145, "Mmm...", "Aeka");
translation.add(146, "Mis labios tocan los suyos.", "");
translation.add(147, "Está temblando.", "");
translation.add(148, "Mmff... ¡¿Shiraki-san?!", "Kouhei");
translation.add(149, "Ahhh...", "Aeka");
translation.add(150, "Mmm...", "Aeka");
translation.add(151, "Tiene los labios tan suaves.", "");
translation.add(152, "Son como nubes de algodón.", "");
translation.add(153, "Algo en mi interior se derrite cuando me toca.", "");
translation.add(154, "El aroma del perfume que la rodea es tan dulce\ncomo ella.", "");
translation.add(155, "Mmmff... N-No puedo respirar, Shiraki-san...", "Kouhei");
translation.add(156, "¡¡...!!", "Aeka");
translation.add(157, "Aprieta ligeramente la cabeza contra mi pecho.", "");
translation.add(158, "Por favor, no te vayas...", "Aeka");
translation.add(159, "Me da igual que me odies, pero por favor,\nquédate conmigo un rato...", "Aeka");
translation.add(160, "No digas eso, sería incapaz de odiarte.", "Kouhei");
translation.add(161, "Por favor...", "Aeka");
translation.add(162, "Por favor, aleja esta soledad que siento\ndurante al menos un instante...", "Aeka");
translation.add(163, "Shiraki-san...", "Kouhei");
translation.add(164, "Lo siento...", "Aeka");
translation.add(165, "Estoy siendo tan egoísta...", "Aeka");
translation.add(166, "No me queda otra opción que hacer lo que dice.", "");
translation.add(167, "No me importa. Hablaré con ella cuando se calme.", "");
translation.add(168, "Kagami-kun...", "Aeka");
translation.add(169, "¿Qué ocurre, Shiraki-san?", "Kouhei");
translation.add(170, "Bésame...", "Aeka");
translation.add(171, "Sí..., claro.", "Kouhei");
translation.add(172, "Ahora me toca a mí.", "");
translation.add(173, "Mmmm...", "Aeka");
translation.add(174, "Se acurruca aún más a mí.", "");
translation.add(175, "Espero que no la canse estar en esa posición.", "");
translation.add(176, "No sabía que fuera tan lanzada.", "");
translation.add(177, "Mmm... Mmmmm...", "Aeka");
translation.add(178, "¿Ya estás más tranquila?", "Kouhei");
translation.add(179, "Sí... Ahora ya puedo...", "Aeka");
translation.add(180, "Se separa un poco de mí.", "");
translation.add(181, "¿Te encuentras mejor?", "Kouhei");
translation.add(182, "Sí, ahora sí.", "Aeka");
translation.add(183, "Sus ojos, resplandecientes por la luz de\nla puesta de sol, ven a través de mi alma.", "");
translation.add(184, "Pero, Kagami-kun... Mm...", "Aeka");
translation.add(185, "No me importa si tienes que obligarte a hacerlo.", "Aeka");
translation.add(186, "No me importa si acabas por odiarme...", "Aeka");
translation.add(187, "Pero... me gustaría que continuáramos\na partir de aquí.", "Aeka");
translation.add(188, "¿Lo dices en serio, Shiraki-san?...", "Kouhei");
translation.add(189, "Lo siento...", "Aeka");
translation.add(190, "Te pido tanto..., y ni siquiera somos amantes.", "Aeka");
translation.add(191, "¿Estás segura de que quieres hacerlo? ¿Conmigo?", "Kouhei");
translation.add(192, "Sí.", "Aeka");
translation.add(193, "Entonces que no se hable más.\nEra todo lo que necesitaba saber.", "Kouhei");
translation.add(194, "Si fuera Antoinette,\nla hubiera rechazado como la peste.", "");
translation.add(195, "Abrazo a Shiraki de nuevo.", "");
translation.add(196, "Bésame una vez más.", "Aeka");
translation.add(197, "Uno con delicadeza mis labios con los suyos.", "");
translation.add(198, "Mmm...", "Aeka");
translation.add(199, "Siento el ardor que desprende bajo su uniforme.", "");
translation.add(200, "Lo sabía. Se congelará si se queda aquí.", "");
translation.add(201, "La froto con las manos.", "");
translation.add(202, "Te vas a resfriar.", "Kouhei");
translation.add(203, "Estaré bien.", "Aeka");
translation.add(204, "Desenrollo la manta y me siento encima,\njunto a ella.", "");
translation.add(205, "Es una manta calentita.", "Aeka");
translation.add(206, "Empecemos...", "Kouhei");
translation.add(207, "Vale...", "Aeka");
translation.add(208, "Me besa como si quisiera tomar la iniciativa.", "");
translation.add(209, "Mmm...", "Aeka");
translation.add(210, "Aprovecho el momento para apoyar su espalda\ncontra el tanque de agua.", "");
translation.add(211, "Le desabrocho la camisa para poder acceder\na sus senos.", "");
translation.add(212, "Nnnaa...", "Aeka");
translation.add(213, "Se pone tensa de la sorpresa.", "");
translation.add(214, "Voy a tocarte.", "Kouhei");
translation.add(215, "Estoy... bastante asustado.", "");
translation.add(216, "Empiezo a masajearle lentamente los pechos.", "");
translation.add(217, "Ja, ja... Me haces cosquillas.", "Aeka");
translation.add(218, "Siento la textura del sujetador mientras sobo\ncon delicadeza su carne. ¡Qué tersa que es!", "");
translation.add(219, "Tienes los pechos muy suaves, Shiraki-san.", "Kouhei");
translation.add(220, "No digas esas cosas...", "Aeka");
translation.add(221, "Coloca una mano sobre la mía.", "");
translation.add(222, "No me importa que me hagas cosquillas.\nPor favor, continúa.", "Aeka");
translation.add(223, "Empiezo a acariciar con ambas manos sus senos.", "");
translation.add(224, "Ja, ja... Es como un masaje.", "Aeka");
translation.add(225, "¿No te gusta?", "Kouhei");
translation.add(226, "Es... extraño.", "Aeka");
translation.add(227, "Me da vergüenza y es un poco agobiante,\npero... creo que me gusta mucho...", "Aeka");
translation.add(228, "Se está poniendo colorada.", "");
translation.add(229, "Yo tampoco soy de piedra.\nMi respiración se ha acelerado un poco.", "");
translation.add(230, "Nnn...", "Aeka");
translation.add(231, "Se echa a mis brazos.", "");
translation.add(232, "Puedo sentir sus apasionados jadeos,\nsus labios, sus hombros, sus tetas...", "");
translation.add(233, "Le pellizco los pezones.", "");
translation.add(234, "Ahhh... Ja, ja...", "Aeka");
translation.add(235, "Qué forma más graciosa tiene de juntar\nlas rodillas.", "");
translation.add(236, "Presiono sus pezones con un poco más de fuerza.", "");
translation.add(237, "Estas... cosquillas acabarán conmigo...", "Aeka");
translation.add(238, "¿Te duele? ¿Quieres que pare?", "Kouhei");
translation.add(239, "Aaaahh... Estoy bien...", "Aeka");
translation.add(240, "Paso las manos suavemente por sus mejillas.", "");
translation.add(241, "Nff...", "Aeka");
translation.add(242, "Está intentando disimular la vergüenza que pasa.", "");
translation.add(243, "Paso la mano por su vientre. Está muy caliente.", "");
translation.add(244, "Ahh... Detente... ¡Jajajaja!\n¡Quieto! ¡Me haces cosquillas!", "Aeka");
translation.add(245, "Me levanta los brazos.", "");
translation.add(246, "¡Ahora verás lo que es bueno!", "Aeka");
translation.add(247, "Parece que poco a poco se va soltando.", "");
translation.add(248, "Se abalanza sobre mí y desliza los dedos\npor mis costados.", "");
translation.add(249, "¡Jajajaja! ¡Para ya, Shiraki-san!\n¡No te lo haré más, lo prometo!", "Kouhei");
translation.add(250, "¿A que chincha?", "Aeka");
translation.add(251, "¡Jajaja! ¡No seas tan dura conmigo!", "Kouhei");
translation.add(252, "Je, je...", "Aeka");
translation.add(253, "Apóyate otra vez contra la pared, Shiraki-san.", "Kouhei");
translation.add(254, "...", "Aeka");
translation.add(255, "Vale...", "Aeka");
translation.add(256, "Al principio se lo piensa,\npero termina por hacerme caso.", "");
translation.add(257, "Le quito las bragas y\nme quedo observando su entrepierna.", "");
translation.add(258, "Ahh... Pervertido...", "Aeka");
translation.add(259, "Alza las rodillas, cortando así cualquier\nposible vía hacia su coño.", "");
translation.add(260, "No tienes por qué avergonzarte de mí.", "Kouhei");
translation.add(261, "No puedo evitarlo.", "Aeka");
translation.add(262, "En ese caso...", "Kouhei");
translation.add(263, "Agarro sus tobillos y\nlos levanto por encima de ella.", "");
translation.add(264, "No, en esta posición no...", "Aeka");
translation.add(265, "Eso es porque tu conejito está escondido.\n¡Muéstrame el conejito!", "Kouhei");
translation.add(266, "¡Deja de decir eso!", "Aeka");
translation.add(267, "¡Vamos, haz que salga de su madriguera!", "Kouhei");
translation.add(268, "Eres un tonto, Kagami-kun...", "Aeka");
translation.add(269, "Acerco la mano hasta su vagina\ny deslizo un dedo en el interior.", "");
translation.add(270, "Ahh... Nnngg...", "Aeka");
translation.add(271, "Vaya, parece que te está empezando a gustar...", "Kouhei");
translation.add(272, "Tengo el dedo untado por un fluido pegajoso.", "");
translation.add(273, "*schlup* *schlup* *schlup*", "");
translation.add(274, "Aaahh... Uhh... Q-Qué vergüenza...", "Aeka");
translation.add(275, "Le hago dar vueltas de un lado a otro.\nSu cuerpo está empezando a reaccionar.", "");
translation.add(276, "Estás muy caliente, ¿eh, Shiraki-san?", "Kouhei");
translation.add(277, "N-No es verdad. No estoy caliente...", "Aeka");
translation.add(278, "¿Cómo que no? Pero mira lo mojada que estás.\nEstá manando a chorros.", "Kouhei");
translation.add(279, "Vas a dejar la manta perdida si sigues así.", "Kouhei");
translation.add(280, "Iaaaaa... ¡N-No!", "Aeka");
translation.add(281, "Empiezo a lamer lentamente ahí abajo.", "");
translation.add(282, "Aaannn... Aahhh...", "Aeka");
translation.add(283, "No... chupes ahí... Nngg...", "Aeka");
translation.add(284, "¿No te gusta?", "Kouhei");
translation.add(285, "Mmmm... Sí. Pero creía que los chicos\nno se lo hacían a las chicas...", "Aeka");
translation.add(286, "Ja, ja, ja. ¿Dónde te enteraste de eso?", "Kouhei");
translation.add(287, "¡Cómo podría acordarme! Y tú, ¿dónde\naprendiste a hacerlo? Eres un degenerado.", "Aeka");
translation.add(288, "No es más que algo que saben los chicos.", "");
translation.add(289, "Y esta debe ser su zona más sensible, ¿verdad?", "");
translation.add(290, "¡¡Auuu!! No, ahí no; es demasiado... Ahí no...", "Aeka");
translation.add(291, "Nunca había visto a nadie chorrear tanto.", "");
translation.add(292, "A pesar de sus protestas, sigo lamiendo.", "");
translation.add(293, "Salpica tanto que me ha empapado la nariz.", "");
translation.add(294, "Ahh... Ahh...", "Aeka");
translation.add(295, "No puedo aguantarlo más.\nEmpiezo a desabrocharme los pantalones.", "");
translation.add(296, "Ah, esto... ¡Kagami-kun, espera!", "Aeka");
translation.add(297, "Libero a mi miembro viril de su cautiverio.", "");
translation.add(298, "Iiic...", "Aeka");
translation.add(299, "¿Qué te pasa, Shiraki-san?", "Kouhei");
translation.add(300, "Mmm... Bueno...", "Aeka");
translation.add(301, "Si... si pudieras...", "Aeka");
translation.add(302, "..................", "");
translation.add(303, "«Ir a una farmacia a comprar un anticonceptivo».", "");
translation.add(304, "Qué mala pata; debería haber guardado\naquellos preservativos que nos dieron\nen clase de educación sexual.", "");
translation.add(305, "Vale, al final he podido encontrar una farmacia.", "");
translation.add(306, "Espero que baste con esto.", "");
translation.add(307, "Ya estoy aquí, Shiraki-san...", "Kouhei");
translation.add(308, "Es capaz de haberse quedado dormida...", "");
translation.add(309, "Aunque mis temores son infundados.", "");
translation.add(310, "Hola otra vez, Kagami-kun.", "Aeka");
translation.add(311, "Aquí traigo lo que querías. ¿Te va bien así?", "Kouhei");
translation.add(312, "¿Son estos?", "Aeka");
translation.add(313, "Sí. El farmacéutico me dijo\nque eran los más seguros.", "Kouhei");
translation.add(314, "¿De verdad? Qué señor más raro.", "Aeka");
translation.add(315, "Vamos, acércate.", "Kouhei");
translation.add(316, "Abro la caja, extraigo el supositorio\ny se lo meto por el coño, bien adentro.", "");
translation.add(317, "¡Nooo! ¡No tan rápido!", "Aeka");
translation.add(318, "Saco mi dedo chorreante.", "");
translation.add(319, "¿Has estado mojada todo este tiempo?", "Kouhei");
translation.add(320, "Esta es la primera vez que hago algo así.\nNo podía dejar de pensar en cosas sucias...", "Aeka");
translation.add(321, "Estoy muy caliente...\nY tengo una sensación muy extraña ahí abajo.", "Aeka");
translation.add(322, "No te preocupes. El anticonceptivo\nfuncionará mejor de esta forma.", "Kouhei");
translation.add(323, "Dicen que esto se carga los espermatozoides.\nSupongo que será más efectivo si no me pongo\nningún preservativo.", "");
translation.add(324, "El tipo me dijo que el contacto directo\nproducía mayor satisfacción tanto para el\nhombre como para la mujer.", "");
translation.add(325, "También me recomendó que leyera atentamente\nlas instrucciones.", "");
translation.add(326, "No sé, no termino de calmarme.", "Aeka");
translation.add(327, "La mano que cubre su coño está pegajosa,\ntotalmente embadurnada por su propio fluido\nde amor.", "");
translation.add(328, "Shiraki-san.", "Kouhei");
translation.add(329, "¿Kagami-kun?", "Aeka");
translation.add(330, "Aparto cuidadosamente su mano.", "");
translation.add(331, "Ya no debería haber ningún problema, ¿verdad?", "Kouhei");
translation.add(332, "Supongo...", "Aeka");
translation.add(333, "Trátame con delicadeza.", "Aeka");
translation.add(334, "No te preocupes, lo haré.", "Kouhei");
translation.add(335, "Me saco la polla de nuevo.", "");
translation.add(336, "Increíble...", "Aeka");
translation.add(337, "Ja, ja...", "Kouhei");
translation.add(338, "El pene de un hombre puede ser muy grande... \n¿No me dolerá?", "Aeka");
translation.add(339, "Ya está a punto de caramelo.\nSi esperamos un poco más me reventará.", "Kouhei");
translation.add(340, "Y a ti también se te ve con ganas.\n¿A que ya no puedes aguantarte más?", "Kouhei");
translation.add(341, "Pues no; es que tú estás demasiado cachondo.", "Aeka");
translation.add(342, "¡Pero si eras tú quien quería hacerlo!", "");
translation.add(343, "Pervertido...", "Aeka");
translation.add(344, "Ja, ja, ja. Bueno, allá voy, Shiraki-san.", "Kouhei");
translation.add(345, "Ahhh... Unnnnnnnnnn...", "Aeka");
translation.add(346, "Se la meto lentamente.", "");
translation.add(347, "(Guau... ¡Esto es increíble!).", "Kouhei");
translation.add(348, "La entrada está tan apretada\nque creo que no podré sacarla.", "");
translation.add(349, "Ahh... Nnrgg...", "Aeka");
translation.add(350, "*shlrrp* *shlrrp*", "");
translation.add(351, "¿Estás bien?", "Kouhei");
translation.add(352, "S-Sí... Uaaahhh...", "Aeka");
translation.add(353, "Algo está bloqueando mi avance.", "");
translation.add(354, "Allá voy, Shiraki-san...", "Kouhei");
translation.add(355, "Sí...", "Aeka");
translation.add(356, "Presiono con más ahínco hasta romper su barrera.", "");
translation.add(357, "Ahhh... Duele...", "Aeka");
translation.add(358, "¿De verdad que estás bien?", "Kouhei");
translation.add(359, "Du... duele... Ahh... Ungghh...", "Aeka");
translation.add(360, "Abrazo a Shiraki con la máxima delicadeza.", "");
translation.add(361, "Ahh... Hnngg...", "Aeka");
translation.add(362, "Kagami-kun... Estoy... bien...", "Aeka");
translation.add(363, "Podré soportarlo...", "Aeka");
translation.add(364, "Le seco las lágrimas con los pulgares.", "");
translation.add(365, "Dímelo enseguida si no puedes aguantarlo.", "Kouhei");
translation.add(366, "Espera un momento, por favor...", "Aeka");
translation.add(367, "Está pálida.", "");
translation.add(368, "Me pregunto si le dolerá mucho...\nMe aterro solo con imaginármelo.", "");
translation.add(369, "Lo siento, Shiraki-san. Tómate tu tiempo.", "Kouhei");
translation.add(370, "Asiente con la cabeza.", "");
translation.add(371, "Soy tan débil... Lamento no poder aguantarlo.", "Aeka");
translation.add(372, "Trata de mostrarme la mejor de sus sonrisas.", "");
translation.add(373, "Me compadezco sinceramente de ella.", "");
translation.add(374, "Hazme lo que quieras, Kagami-kun... Por favor.", "Aeka");
translation.add(375, "Estoy segura de que todo irá bien.", "Aeka");
translation.add(376, "Como quieras. Empezaré a moverme ahora mismo.\nSi te duele, dímelo, ¿vale?", "Kouhei");
translation.add(377, "Vale... Uff... Uff...", "Aeka");
translation.add(378, "Aprovecha el tiempo que le doy para\napaciguar su respiración y prepararse.", "");
translation.add(379, "Es lo que ambos deseamos.", "");
translation.add(380, "Despacio, despacio.", "");
translation.add(381, "Inicio mi bombeo poco a poco,\ncomo las bielas de una antigua locomotora.", "");
translation.add(382, "Gnnhh... Ahh... Auhhh...", "Aeka");
translation.add(383, "Oh, sí...", "Kouhei");
translation.add(384, "Esto es el paraíso... Es como si hubiera\nacabado de recibir una descarga eléctrica.", "Kouhei");
translation.add(385, "Es la primera vez que siento algo así... Ahh...", "Aeka");
translation.add(386, "Cada vez estoy más y más cachondo.", "");
translation.add(387, "La presión que sentía antes\nse convierte en un suave vaivén.", "");
translation.add(388, "Ahhh... Qué placer... Ser...", "Aeka");
translation.add(389, "Ser una con Kagami-kun...", "Aeka");
translation.add(390, "Estoy unida... a ti, ¿verdad?", "Aeka");
translation.add(391, "Así es, Shiraki-san.", "Kouhei");
translation.add(392, "Estoy... Ahh... Estoy tan contenta...", "Aeka");
translation.add(393, "Ohhh...", "Kouhei");
translation.add(394, "Algo empieza a brotar en mi interior.", "");
translation.add(395, "Ahh, Shiraki-san... Me voy a...", "Kouhei");
translation.add(396, "Me voy a correr...", "Kouhei");
translation.add(397, "Muy bien, Kagami-kun. ¡Desata todo el placer\nque quieras gracias a mí! ¡Por favor!", "Aeka");
translation.add(398, "Shlap, shlrrp, shlshh...", "");
translation.add(399, "Siento que mi miembro está a punto de reventar.", "");
translation.add(400, "Está agrandándose hasta límites insospechados.", "");
translation.add(401, "Y en un estallido, mi semen sale a borbotones.", "");
translation.add(402, "¡¡M-Me corro!!", "Kouhei");
translation.add(403, "¡Hazlo, Kagami-kun! ¡Hazlo gracias a mí!", "Aeka");
translation.add(404, "¡¡Urrggghhhh!!", "Kouhei");
translation.add(405, "¡¡Uaaaaaaaahhhhhhh!!", "Kouhei");
translation.add(406, "¡¡Bum, bum!!", "");
translation.add(407, "¡¡Bum, bum!!", "");
translation.add(408, "¡Bum, bum, bum!", "");
translation.add(409, "El repentino placer que inunda mi cuerpo\nanula completamente cualquier estímulo\ndel exterior.", "");
translation.add(410, "Soy incapaz de pensar en otra cosa.", "");
translation.add(411, "Uhhhh...", "Kouhei");
translation.add(412, "Mi polla sigue palpitando mucho después\nde haber liberado mi semen.", "");
translation.add(413, "Uff... Uff... Uff...", "Aeka");
translation.add(414, "Ah, ah, ah...", "Kouhei");
translation.add(415, "Está muy caliente, Kagami-kun...", "Aeka");
translation.add(416, "Y hay tanto...", "Aeka");
translation.add(417, "Recoge algo de fluido con los dedos y lo mira.", "");
translation.add(418, "Estoy... muy contenta.", "Aeka");
translation.add(419, "¿Estás bien, Shiraki-san? ¿Aún te duele?", "Kouhei");
translation.add(420, "Sí, un poquito, pero estoy bien.", "Aeka");
translation.add(421, "Menos mal.", "Kouhei");
translation.add(422, "Esto... A mí me ha encantado.", "Kouhei");
translation.add(423, "Eres un tonto, Kagami-kun.", "Aeka");
translation.add(424, "Pero... gracias.", "Aeka");
translation.add(425, "Nos abrazamos\ny compartimos un largo y apasionado beso.", "");
translation.add(426, "Me pregunto cuánto tiempo llevamos aquí.", "");
translation.add(427, "El sol está a punto de ponerse y todos\nlos clubes han dado por finalizadas sus\nactividades. La escuela está en silencio.", "");
translation.add(428, "Me encantaría quedarme junto a ella,\npero falta poco para que cierren las puertas...", "");
translation.add(429, "Eh, bueno, hasta luego.", "Aeka");
translation.add(430, "¿Cómo? Pero si es muy tarde.\nDeja que te acompañe a casa.", "Kouhei");
translation.add(431, "¡Muchísimas gracias por todo!", "Aeka");
translation.add(432, "¿Eh?", "Kouhei");
translation.add(433, "Ya ha desaparecido. ¿Tan avergonzada estaba?", "");
translation.add(434, "Bueno, ya la veré mañana.", "");

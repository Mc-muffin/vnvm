translation.add(0, "(Aprovecharé que me he levantado más temprano\npara ir antes a clase. ¡Vamos!).", "Kouhei");
translation.add(1, "Creía que por hacer esto me toparía\ncon algo nuevo y emocionante pero...", "");
translation.add(2, "(Bah, qué decepción).", "Kouhei");
translation.add(3, "No veo a mis amigos por ningún sitio.", "");
translation.add(4, "Me molesta que las cosas\nno hayan salido como esperaba.", "");
translation.add(5, "Decido comer una hamburguesa\npara quitarme esta sensación de malestar.", "");
translation.add(6, "¿Eh? Ey, Kouhei.", "Takeshi");
translation.add(7, "Oigo la voz de Iogi cuando me uno a la cola.\nParece que ya se va.", "");
translation.add(8, "Vaya, qué raro verte por aquí.", "Kouhei");
translation.add(9, "¿No te parece extraño que nos\nencontremos en un lugar como este?", "Takeshi");
translation.add(10, "Puede... Bueno, ¿qué estabas haciendo aquí?", "Kouhei");
translation.add(11, "Es que Nanjou ha convocado una reunión.", "Takeshi");
translation.add(12, "No lo dice muy emocionado,\npero está claro que le tiene ganas.", "");
translation.add(13, "Je... No puede estarse quieta.", "Kouhei");
translation.add(14, "¿Tú también vas a ir?", "Takeshi");
translation.add(15, "Lo siento, pero he quedado.", "Kouhei");
translation.add(16, "Y es verdad. Una excusa caída del cielo.", "");
translation.add(17, "Vaya... Pues nada, ya nos veremos.", "Takeshi");
translation.add(18, "Llegó un pelín tarde;\nno veo a Aeka por ningún lado.", "");
translation.add(19, "(Quizá se haya entretenido con algo).", "Kouhei");
translation.add(20, "Pero por mucho que espero, no aparece.", "");
translation.add(21, "(Llamaré a su casa).", "Kouhei");
translation.add(22, "Saco el móvil y marco su número.", "");
translation.add(23, "...", "Kouhei");
translation.add(24, "¿Sí?", "Voz de hombre");
translation.add(25, "La áspera voz de un hombre responde a la llamada.", "");
translation.add(26, "(¡Mierda! ¡Debe ser su padre!).", "Kouhei");
translation.add(27, "¡L-Lo siento, me he confundido!", "Kouhei");
translation.add(28, "Cuelgo rápidamente.", "");
translation.add(29, "(Uff... ¿Pero qué tonterías estoy haciendo?).", "Kouhei");
translation.add(30, "(No creo que vaya a venir... Me iré a casa).", "Kouhei");
translation.add(31, "Me consuelo pensando en que la vi ayer\ny que mañana lo volveré a hacer.", "");
translation.add(32, "Tardo un tiempo en darme cuenta de\nlo terriblemente equivocado que estaba...", "");
translation.add(33, "Pasan los días y sigue sin haber rastro de Aeka.", "");
translation.add(34, "Me planteo muchas veces ir a la azotea,\npero nunca termino por hacerlo.", "");
translation.add(35, "Entonces, durante un breve descanso,\nel rumor de unas voces captan mi atención.", "");
translation.add(36, "¡¿En serio?! ¿Shiraki deja la escuela?", "Takeshi");
translation.add(37, "El salto la ha dejado en estado crítico.\nLa gente normalmente suele morir por esto,\npero ella debe ser una cucaracha o algo así.", "Kyoka");
translation.add(38, "¡Jajajá! ¡Qué bueno, Nanjou-san!", "Misaki");
translation.add(39, "(Ah. Así que se trataba de eso...).", "Kouhei");
translation.add(40, "Ahora entiendo por qué nadie respondía\na las llamadas que hice estos últimos días.", "");
translation.add(41, "(Pero, ¿no debería haberme afectado más?).", "Kouhei");
translation.add(42, "Pasan el tiempo sin que ocurra nada de interés.", "");
translation.add(43, "Un día me doy cuenta de que han\nalejado los pupitres que rodean el mío.", "");
translation.add(44, "Parece que quieren hacerme pagar\nla cercanía que tuve con Aeka.", "");
translation.add(45, "(Vaya... Así que ahora soy el siguiente).", "Kouhei");
translation.add(46, "Sinceramente, no me importa demasiado.", "");
translation.add(47, "Contaminas el aire que respiro, Kagami-kun.", "Kyoka");
translation.add(48, "La gente de alrededor se ríe disimuladamente.", "");
translation.add(49, "Entonces, un sonido familiar me visita de nuevo.", "");
translation.add(50, "Cachún... Cachún... Cachún...", "");
translation.add(51, "Me concentro en él, el único factor\nconocido en este sueño que nunca había tenido.", "");
translation.add(52, "El mundo entero está formado por líneas difusas.\nNo podría decir nada de nada de lo que me rodea.\nDebería estar haciendo algo, pero no reacciono.", "");
translation.add(53, "Cachún... Cachún... Cachún...", "");
translation.add(54, "(¿Me habré echado a perder? Pero...).", "Kouhei");
translation.add(55, "Me rindo.\nMe tumbo sobre la cama, custodiando esta paz.", "");
translation.add(56, "(Así ya está bien...).", "Kouhei");
translation.add(57, "Cachún... Cachún... Cachún...\nCachún... Cachún... Cachún...", "");

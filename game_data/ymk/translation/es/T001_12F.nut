translation.add(0, "......", "");
translation.add(1, "............", "");
translation.add(2, "............", "");
translation.add(3, "......", "");
translation.add(4, "¿No estaba buscando a Kirimiya?", "");
translation.add(5, "¿Por qué no puedo simplemente\nocuparme de mis propios asuntos?", "");
translation.add(6, "¿Por qué siempre acabo en estos tinglados?", "");
translation.add(7, "Tsubaki tenía razón,\nla estación está llena de gente.", "");
translation.add(8, "Y ahí está, subida a un árbol.", "");
translation.add(9, "Es ella, como no podía ser de otra manera.", "");
translation.add(10, "Es el hada gato Nekoko.", "");
translation.add(11, "Está intentado alcanzar la copa,\nignorando el hecho de que va con falda.", "");
translation.add(12, "¡Eh, tú! ¡Baja de ahí ahora mismo!", "Policía");
translation.add(13, "¡Guau, ha venido hasta la policía!", "");
translation.add(14, "Pero Nekoko le ignora\ny se dedica a escalar más y más.", "");
translation.add(15, "Tengo que... subir más arriba. ¡Más arriba!", "Nekoko");
translation.add(16, "La multitud la observa como si esperaran que\nle fueran a aparecer alas y saliera volando.", "");
translation.add(17, "Alguien ha dejado sueltos a varios perros\nque se han puesto a ladrar bajo el árbol.", "");
translation.add(18, "La muchedumbre circunvala el área acordonada.", "");
translation.add(19, "Es la misma imagen del Infierno.", "");
translation.add(20, "De acuerdo, de acuerdo. Vamos, ¿quieres bajar?", "Policía");
translation.add(21, "Te llevaré a un sitio lleno de amiguitos\ncon los que podrás jugar.", "Policía");
translation.add(22, "Parece que Nekoko pasa olímpicamente de él.\nEl policía se planta debajo del árbol,\ndispuesto a usar la fuerza si fuera necesario.", "");
translation.add(23, "¡Cállate, policía violento! ¡Lo que quieres es\nmeterme en una cámara con paredes acolchadas!", "Nekoko");
translation.add(24, "¡Me encerraríais en una celda de aislamiento\ndurante meses y me lavaríais el cerebro hasta\nque no recordara ni mi nombre!", "Nekoko");
translation.add(25, "¡Eso es lo que hacen en todos los países!", "Nekoko");
translation.add(26, "Se está yendo completamente por las ramas,\naunque el policía no parece haberse alterado.", "");
translation.add(27, "¿De verdad hacen esas cosas?", "Transeúnte A");
translation.add(28, "A mí me contaron que arrestan\na los alborotadores sin ningún motivo,\n¡y que después los lobotomizan!", "Transeúnte B");
translation.add(29, "¿Es verdad que el servicio secreto acecha\na todo aquel que piensa mal del Gobierno?", "Transeúnte C");
translation.add(30, "La multitud empieza a cuchichear alentada por\nlas palabras de Nekoko. Parece que ahora ven\nal policía como un feroz sicario del Gobierno.", "");
translation.add(31, "¿Q-Qué? ¡Eso está totalmente prohibido por la\nConstitución! ¡Todo eso es mentira, ciudadanos!", "Policía");
translation.add(32, "Yo solo soy un funcionario público.\nEstoy aquí para mantener la paz para todos.", "Policía");
translation.add(33, "¡Lo que significa que tú, la del árbol,\neres la causante de perturbar a la gente!", "Policía");
translation.add(34, "¡Ja, ja, ja! ¡Eres un bocazas, agente!\n¡Jamás podrías arrestar a un hada\nsi no eres capaz de hacer nada por ti mismo!", "Nekoko");
translation.add(35, "¡Métete en tu comisaría llena de residuos\ntóxicos y conviértete en un policía zombi!", "Nekoko");
translation.add(36, "Como de costumbre, se ha puesto a largar sobre\nalgo que no tiene nada que ver con el tema.", "");
translation.add(37, "¡Ey, pero qué estás diciendo! Te haré saber...", "Policía");
translation.add(38, "¡Que me esforcé muchísimo para lograr entrar\nen el escuadrón anti disturbios!", "Policía");
translation.add(39, "Pero mi familia procede de un linaje\nde sacerdotes shinto...\n¡Fui rechazado por mi estatus social!", "Policía");
translation.add(40, "¡Porque la religión que practicamos ni siquiera\nte da la oportunidad de presentarte al examen!", "Policía");
translation.add(41, "*Snif* ¡¡Buaaahhhh!!", "Policía");
translation.add(42, "Parece que Nekoko ha reabierto viejas heridas.", "");
translation.add(43, "La gente ahora divide su atención entre Nekoko\ny el sollozante policía.", "");
translation.add(44, "¿Eh? ¿Pero qué estoy haciendo? ¡Eh, tú! Primero\ncometes vandalismo, luego interfieres con las\ntareas de un funcionario, ¿y ahora me insultas?", "Policía");
translation.add(45, "El policía vuelve en sí. Ha sacado\nsu porra de paseo y se ha quitado los guantes.", "");
translation.add(46, "¡Jajajajá! ¿Crees que me alcanzarás con ese\npalito? ¡Es como intentar coger un plátano\nsubido a una silla!", "Nekoko");
translation.add(47, "Me sorprende su absoluta carencia de miedo.", "");
translation.add(48, "Ya debería haber bajado del árbol pero,\nen vez de eso, se dedica a pasear la mirada\nmientras hace muecas al policía.", "");
translation.add(49, "Parece que le gusta contemplar a la muchedumbre.\nEntonces, de repente, se fija en mí.", "");
translation.add(50, "Nuestros ojos se cruzan.", "");
translation.add(51, "¡Ahhhh! ¡¡Es Kouhei!!", "Nekoko");
translation.add(52, "¡¡Ahhh!!", "");
translation.add(53, "¡Zorra tarada!", "");
translation.add(54, "¡Por qué has tenido que gritar mi nombre así!", "");
translation.add(55, "Desvío la mirada hacia otra parte.", "");
translation.add(56, "Pero ya es demasiado tarde.", "");
translation.add(57, "Todos los ojos se giran en mi dirección.", "");
translation.add(58, "Ah, esto... Qué cosas más raras se encuentra\nuno en los árboles. Es evidente que no conozco\na nadie que pueda subirse a uno de esta forma.", "Kouhei");
translation.add(59, "Hago lo que puedo por defenderme, pero Nekoko...", "");
translation.add(60, "¡Aquíííí, Kouhei!\nEscucha, ¡acabo de descubrir algo increíble!", "Nekoko");
translation.add(61, "No me importa que la gente sonría de oreja\na oreja, pero no en una situación como esta.", "");
translation.add(62, "¡Ah! ¡No te acerques a mí!", "Kouhei");
translation.add(63, "Jejejé. ¡No tienes por qué avergonzarte!\n¡Mi instinto feérico\nte ha distinguido de entre el populacho!", "Nekoko");
translation.add(64, "Baja de un salto, esquiva las zarpas del policía\ny corre hacia mí.", "");
translation.add(65, "Pretendo hacer como que no la he visto,\npero ya no puedo hacer nada para escaquearme.", "");
translation.add(66, "¡Te he pillado, Kouhei!\n¡Vámonos de aquí cuanto antes!", "Nekoko");
translation.add(67, "Me abraza la cintura con muchísima fuerza.", "");
translation.add(68, "¡A mí no me metas en este fregado!\n¡Déjame en paz, por favor!", "Kouhei");
translation.add(69, "¡Soy un hada, no tienes por qué reprimirte!", "Nekoko");
translation.add(70, "¡Pues eres un hada muy molesta!", "Kouhei");
translation.add(71, "¡Eres como una marabunta enfurecida!", "Kouhei");
translation.add(72, "Pero si ni siquiera me está escuchando...", "");
translation.add(73, "Me coge del brazo y me arrastra entre la gente.", "");
translation.add(74, "......", "");
translation.add(75, "............", "");
translation.add(76, "............", "");
translation.add(77, "......", "");
translation.add(78, "Nekoko se detiene cuando llegamos a un callejón.", "");
translation.add(79, "¡Ya estamos a salvo!\n¡Mis sensores feéricos no detectan más enemigos!", "Nekoko");
translation.add(80, "Las hadas os conocéis\nestos callejones al dedillo...", "Kouhei");
translation.add(81, "Nos ha estado persiguiendo la policía,\npero parece que se han dado por vencidos.", "");
translation.add(82, "Oteo en una esquina. Sí, hemos logrado escapar.", "");
translation.add(83, "Vale, olvidémonos del asunto de la huida...", "Kouhei");
translation.add(84, "Pero, ¡¿por qué me has traído contigo?!", "Kouhei");
translation.add(85, "Esos humanos podrían haberte contaminado\nsi te hubiera dejado con ellos.", "Nekoko");
translation.add(86, "¿Cómo que contaminarme? Además, ¿de quién\nes la culpa de que se juntase toda esa gente?", "Kouhei");
translation.add(87, "¡Ah sí, escucha, escucha!", "Nekoko");
translation.add(88, "Empieza a bailar una pequeña danza de alegría.", "");
translation.add(89, "Si es que a eso se le puede llamar bailar.\nCurva el cuerpo hacia adelante y hacia atrás\nal son de unos pasos aleatorios.", "");
translation.add(90, "Parece una oscura danza ritual\nofrecida a un dios de las tinieblas.", "");
translation.add(91, "Al cabo de un rato, cuando termina...", "");
translation.add(92, "¡He encontrado el hogar de las hadas!", "Nekoko");
translation.add(93, "Otra vez no...", "");
translation.add(94, "Creo que ya me había hablado de eso.", "");
translation.add(95, "¿Así que ese sitio está en aquel árbol?", "Kouhei");
translation.add(96, "¿Eh? ¿Es que no me crees?", "Nekoko");
translation.add(97, "¿Es que no lo has visto?\nYo lo podía ver perfectamente.\nEstaba tan cerca que casi podía tocarlo.", "Nekoko");

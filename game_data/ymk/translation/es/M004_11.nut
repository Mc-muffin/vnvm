translation.add(0, "Abro lentamente los ojos.", "");
translation.add(1, "Busco a tientas a mi alrededor.", "");
translation.add(2, "No hay nadie.", "");
translation.add(3, "Me siento como si mi propia esencia me hubiera\nabandonado. He perdido todo aquello en lo que\ncreía.", "");
translation.add(4, "Me recuesto sobre la cama.\nNoto el fluir del tiempo por todo mi cuerpo.", "");
translation.add(5, "Miro a mi lado. Kirimiya...", "");
translation.add(6, "La chica que debería haber estado junto a mí\ntoda la vida...", "");
translation.add(7, "No está aquí.", "");
translation.add(8, "He pasado por esto cada mañana desde hace medio\naño.", "");
translation.add(9, "¿Cuándo llegaré a aceptarlo?", "");
translation.add(10, "No importa cuántas veces lo repita,\nsu recuerdo nunca se desvanecerá...", "");
translation.add(11, "Me hundo entre las sábanas.", "");
translation.add(12, "Incluso ahí, su calidez ha desaparecido.", "");
translation.add(13, "Su aroma desapareció con ella.\nEl único olor que perdura es el mío.", "");
translation.add(14, "Miro la ventana por donde entró.", "");
translation.add(15, "Sé que ocurrió hace medio año...", "");
translation.add(16, "Cuando me desperté esa mañana, se había ido.", "");
translation.add(17, "La promesa que hicimos antes de dormirnos...", "");
translation.add(18, "«Mañana nos veremos en la escuela».\nSonó algo dubitativa, pero asintió.", "");
translation.add(19, "Sé que ocurrió hace medio año...", "");
translation.add(20, "No pudo responderme con franqueza, y aun así\nme besó.", "");
translation.add(21, "Sé que ocurrió hace medio año...", "");
translation.add(22, "Cuando nuestros caminos se separaron.", "");
translation.add(23, "¿Es que aquella noche no fue más que un sueño?", "");
translation.add(24, "No, no lo fue.", "");
translation.add(25, "Sé que es una tontería, pero cuando me desperté\nla escultura del perrito había desaparecido.", "");
translation.add(26, "Kirimiya debió llevársela, aunque no sé por qué.", "");
translation.add(27, "......", "");
translation.add(28, "............", "");
translation.add(29, "Me levanto y cojo el móvil de la mesa.", "");
translation.add(30, "No hay ningún mensaje.", "");
translation.add(31, "No esperaba otra cosa, pero de todas formas\nlo compruebo cada mañana.", "");
translation.add(32, "Pulso un botón y, durante un instante, veo su\nnúmero de teléfono en la pantalla.", "");
translation.add(33, "La luz se desvanece, oscureciéndolo.", "");
translation.add(34, "Marcar su número no tendría sentido. Lo sé...", "");
translation.add(35, "Kirimiya-senpai... ya no está aquí.", "");
translation.add(36, "Intenté muchas cosas; incluso la busqué\npor toda la ciudad. No sirvió de nada.", "");
translation.add(37, "Traté de averiguar dónde vivía, pero nadie\nquiso decírmelo.", "");
translation.add(38, "Parece que incluso allí desapareció.", "");
translation.add(39, "Me doy cuenta de que estoy constriñendo\nmi teléfono móvil.", "");
translation.add(40, "Con tanta fuerza que me duelen los dedos...", "");
translation.add(41, "...", "");
translation.add(42, "......", "");
translation.add(43, "Oigo a alguien que me llama.", "");
translation.add(44, "¡... sidente...!", "Voz");
translation.add(45, "¡Presidente...!", "Voz");
translation.add(46, "¡Presidente del consejo!", "Miembro del consejo");
translation.add(47, "¿Hmm?", "Kouhei");
translation.add(48, "Me despierto titubeante.", "");
translation.add(49, "Me ha sentado muy bien echarme una siestecita\nen mi silla.", "");
translation.add(50, "¡Lamento interrumpirle en un momento como este!", "Miembro del consejo");
translation.add(51, "Abro mis cansados ojos.", "");
translation.add(52, "Qué quieres...", "Kouhei");
translation.add(53, "¡He finalizado nuestras valoraciones!\n¡Por favor, deme su visto bueno!", "Miembro del consejo");
translation.add(54, "Pone una hoja de papel delante de mí.", "");
translation.add(55, "Trabajas demasiado. No es que me queje, claro,\npero tampoco hace falta que te esfuerces tanto.", "Kouhei");
translation.add(56, "¡Sí, señor!\n¡Muchísimas gracias, presidente del consejo!", "Miembro del consejo");
translation.add(57, "Está sonriendo de oreja a oreja.", "");
translation.add(58, "Le he dicho mil veces que no hace falta\nque sea tan formal, pero no me hace caso.\nEmpiezo a firmar los papeles.", "");
translation.add(59, "Hmm... Todo está en orden. Buen trabajo.", "Kouhei");
translation.add(60, "¡Sí, señor!\n¡Es muy amable, presidente del consejo!", "Miembro del consejo");
translation.add(61, "Tráeme una fotocopia de este.", "Kouhei");
translation.add(62, "Mi petición es realizada con presteza.", "");
translation.add(63, "Los del primer año son tan adorables.", "");
translation.add(64, "¡Presidente!", "Miembro del consejo");
translation.add(65, "Argh. ¿Qué pasa ahora?\n¿No podrías dejar de llamarme de esa forma?", "Kouhei");
translation.add(66, "¡No, no, no, no, no!", "Miembro del consejo");
translation.add(67, "Sacude violentamente la cabeza.", "");
translation.add(68, "¿Qué piensa que significa para esta escuela?", "Miembro del consejo");
translation.add(69, "No soy más que un alumno cualquiera.", "Kouhei");
translation.add(70, "¡¿Pero qué dice?! ¿Es que no sabe lo mucho\nque ha cambiado todo desde que usted es el\npresidente?", "Miembro del consejo");
translation.add(71, "No, otra vez no...", "");
translation.add(72, "Los demás miembros ríen entre dientes.\nSe nota que ya han pasado por esto.", "");
translation.add(73, "Unos graciosillos empiezan a aplaudir.", "");
translation.add(74, "Suelta el mismo discursito día sí, día no.", "");
translation.add(75, "Me he convertido en alguien bastante famoso por\naquí. Me cuesta bastante acostumbrarse a ello.", "");
translation.add(76, "Me gustaría que no exagerara tanto las cosas.", "");
translation.add(77, "¡En primer lugar!", "Miembro del consejo");
translation.add(78, "Alza la voz para sobreponerse al clamor\nde la multitud.", "");
translation.add(79, "Respecto al tema del acoso escolar,\ndejó bien claro que el silencio no\nera más que otra forma de contribuir a...", "Miembro del consejo");
translation.add(80, "¡¡Oh, mierda!!", "Kouhei");
translation.add(81, "Me levanto de un salto.", "");
translation.add(82, "La sala del consejo se queda en silencio.", "");
translation.add(83, "¡No me acordaba de que hoy tenía que reunirme\ncon Tsukamoto!", "Kouhei");
translation.add(84, "¿Qué?", "Miembro del consejo");
translation.add(85, "¡Lo siento!\nYa escucharé tu historia otro día, ¿de acuerdo?", "Kouhei");
translation.add(86, "Va... vale.", "Miembro del consejo");
translation.add(87, "¡Encárgate de todo!", "Kouhei");
translation.add(88, "Salgo corriendo de la sala.", "");
translation.add(89, "«¿No se suponía que Tsukamoto tenía dolor\nde barriga y que hoy no había podido venir?».", "");
translation.add(90, "«Sí. Habían suspendido todas sus clases».", "");
translation.add(91, "El tiempo se detiene a mi alrededor.", "");
translation.add(92, "Todo el mundo se gira en mi dirección.", "");
translation.add(93, "Un par de chicas se tapan la boca con las manos.", "");
translation.add(94, "El miembro del consejo está ahí, de pie,\ncon la boca completamente abierta.", "");
translation.add(95, "Su mandíbula está temblando.", "");
translation.add(96, "Da un paso adelante...", "");
translation.add(97, "Está empezando a decir algo, pero antes de\nque logre articular una palabra, salgo de ahí.", "");
translation.add(98, "¡P-Presidente! ¡Ahh! ¡Se ha escapado!", "Miembro del consejo");
translation.add(99, "El consejo irrumpe en sonoras carcajadas.", "");
translation.add(100, "*suspiro*", "Kouhei");
translation.add(101, "Me limpio el sudor de la frente.", "");
translation.add(102, "Esta vista no está nada mal...", "");
translation.add(103, "Estoy en la azotea. La cerraron por culpa de\nuna serie de problemas, y ahora solo yo puedo\nentrar.", "");
translation.add(104, "Es un privilegio y un placer que tengo\ncomo presidente del consejo.", "");
translation.add(105, "Tengo la llave de la puerta en mi bolsillo.", "");
translation.add(106, "Me apoyo sobre la rejilla y miro hacia abajo.\nSopla un viento refrescante.", "");
translation.add(107, "Sí, soy el presidente del consejo estudiantil...", "Kouhei");
translation.add(108, "Puedo decirlo todas las veces que quiera,\npero no me siento como uno.", "");
translation.add(109, "Me río.", "");
translation.add(110, "...", "");
translation.add(111, "......", "");
translation.add(112, "Sí, la escuela ha cambiado.", "");
translation.add(113, "¿Yo también he cambiado? No tan drásticamente.", "");
translation.add(114, "La perspectiva que tenía de la cosas se amplió\nun poco más. Solo un poco.", "");
translation.add(115, "Aunque me bastó para ver mucho más.", "");
translation.add(116, "Nunca imaginé que el acoso escolar fuera\nla causa de otras atrocidades mayores.", "");
translation.add(117, "Y así lo expuse.", "");
translation.add(118, "Nunca imaginé que se aceptaran las drogas\nporque todo el mundo las tomaba.", "");
translation.add(119, "Y así lo expuse.", "");
translation.add(120, "Me nombraron presidente del consejo\npoco después.", "");
translation.add(121, "Por hacer lo que cualquiera podría haber hecho.", "");
translation.add(122, "Eso es todo... Soy un presidente pésimo,\nnada comparado con Kirimiya.", "");
translation.add(123, "Todo el mundo hace como si nunca hubiera estado\nahí.", "");
translation.add(124, "Es como si la hubieran borrado de su mente.", "");
translation.add(125, "Yo no creo que sea capaz de hacer algo así.", "");
translation.add(126, "La recordaré incluso cuando solo yo me acuerde\nde ella.", "");
translation.add(127, "Pues nos une todo el tiempo que pasamos juntos.", "");

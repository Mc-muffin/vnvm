translation.add(3, "¿Le dije algo raro anoche?", "");
translation.add(4, "Inclino la cabeza en un intento por recordar.\nA ver...", "");
translation.add(5, "¡Ahhhhh!", "");
translation.add(6, "La luz del nuevo día me hace ver lo estúpido\nque fui ayer.", "");
translation.add(7, "¡¿P-Por qué le dije aquello?!", "");
translation.add(8, "¿Es que los rayos cósmicos me dejaron tarumba?", "");
translation.add(9, "Menos mal que no me montó ninguna escenita.", "");
translation.add(10, "Si desaparezco dentro de poco tiempo,\nbuscadme en la habitación de Aya...", "");
translation.add(11, "Quizá todavía queden algunos pedacitos de mí.", "");
translation.add(12, "......", "");

translation.add(0, "¡Quieta ahí, pequeña bastarda!", "Voz");
translation.add(1, "¿Qué?", "Kouhei");
translation.add(2, "Vaya, están persiguiendo a esa chica.", "Aeka");
translation.add(3, "¡Vamos, por aquí! ¡Sígueme, gilipollas!", "Nekoko");
translation.add(4, "¡Detente, maldita mocosa de mierda!", "Hombre furioso");
translation.add(5, "(¡Es ella!).", "Kouhei");
translation.add(6, "Parece que se ha metido en una buena.", "");
translation.add(7, "¡Tú, humano!", "Nekoko");
translation.add(8, "¡No!", "");
translation.add(9, "Está viniendo hacia aquí, Kagami-kun.\n¿La conoces?", "Aeka");
translation.add(10, "Pues claro que no.\nDeberíamos irnos de aquí cuanto antes mejor.", "Kouhei");
translation.add(11, "(Antes de nada...).", "Kouhei");
translation.add(12, "¡Shiraki-san, escóndete en esa tienda de ahí!", "Kouhei");
translation.add(13, "¿Y tú qué, Kagami-kun?", "Aeka");
translation.add(14, "¡Deprisa! ¡Date prisa y corre, Shiraki-san!", "Kouhei");
translation.add(15, "V-Vale...", "Aeka");
translation.add(16, "Shiraki se escabulle en la tienda más cercana.", "");
translation.add(17, "¡Cógeme si tan mal te sientes!", "Nekoko");
translation.add(18, "¡Voy a hacer que te tragues esas palabras!", "Hombre furioso");
translation.add(19, "Nekoko me agarra del brazo.", "");
translation.add(20, "¡Tengo un aliado!", "Nekoko");
translation.add(21, "¡¿Qué?! ¡Esperaaaaaa!", "Kouhei");
translation.add(22, "¡Me he metido bien adentro de la boca del lobo!\n¡Pero he encontrado la verdad!", "Nekoko");
translation.add(23, "¡¿Y esa verdad soy yo?!\n¡Nada de lo que dices tiene sentido!", "Kouhei");
translation.add(24, "¡¿Estás con esa chica?!", "Hombre furioso");
translation.add(25, "El hombre furioso me apunta con el dedo.", "");
translation.add(26, "¡N-No! ¡Yo no sé nada! ¡No soy más que un pobre\ntranseúnte! ¡Es ella la que ha venido hacia mí!", "Kouhei");
translation.add(27, "El hombre está realmente enfadado. Desde aquí\npuedo verle una vena palpitarle en la frente.\nNo creo que me haya oído.", "");
translation.add(28, "(¡Mi única opción es huir de él!).", "Kouhei");
translation.add(29, "¡Desplazarse es el camino hacia la compasión!\n¡El mundo entero se lo tiene muy creído!", "Nekoko");
translation.add(30, "Nekoko es rápida, pero el tío\nese también corre que se las pela.", "");
translation.add(31, "¿Por qué me meto en líos cada vez que te veo?", "Kouhei");
translation.add(32, "¡No pasa nada, el hada gato está aquí!\n¡No tienes nada de qué preocuparte!", "Nekoko");
translation.add(33, "¡No has tenido ni que pedir prestadas mis tetas!", "Nekoko");
translation.add(34, "¡No tengo ni idea de lo que estás diciendo!\n¿Prestas tus tetas?", "Kouhei");
translation.add(35, "¡Qué pregunta más inoportuna!", "Nekoko");
translation.add(36, "¡Vosotros dos, dejad de cotorrear!", "Hombre furioso");
translation.add(37, "¡Escribid ahora mismo vuestro testamento,\nporque cuando os pille será demasiado tarde!", "Hombre furioso");
translation.add(38, "¡Que te den, no tenemos tiempo para escribir!", "Nekoko");
translation.add(39, "¡Tú la llevas!", "Nekoko");
translation.add(40, "¡Me las pagarás por esto!", "Hombre furioso");
translation.add(41, "¡Te voy a ahogar en una piscina!", "Hombre furioso");
translation.add(42, "¿Te pasa algo, viejo?", "Nekoko");
translation.add(43, "¿No estás acostumbrado al aire fresco?\n¿Te gusta más estar rodeado de paredes?", "Nekoko");
translation.add(44, "¿Qué? ¡¿Por qué...?!", "Hombre furioso");
translation.add(45, "¡Cázame si puedes! ¡Mentiroso! ¡Mentiroso!", "Nekoko");
translation.add(46, "¡De... deja de actuar como una puta retrasada!", "Hombre furioso");
translation.add(47, "¡Acabas de pisar una mierda de gato, despistado!\n¡Eres tan feo que asustarías a un fantasma!", "Nekoko");
translation.add(48, "¡Por favor, deja de insultarle!", "Kouhei");
translation.add(49, "Como te oiga nos meteremos en problemas.", "Kouhei");
translation.add(50, "No te preocupes, ese viejo no tiene suficiente\ncapacidad mental para recordarlo todo.", "Nekoko");
translation.add(51, "(¡Agh! ¿Dónde ha aprendido a decir todo eso?).", "Kouhei");
translation.add(52, "¿Me has oído, viejo?\n¡Hasta los MP3 tienen más memoria que tú!", "Nekoko");
translation.add(53, "¡Te voy a matar! ¡Voy a partirte el cuello!", "Hombre furioso");
translation.add(54, "¡Aún sigues ahí detrás! ¡Vamos, tú puedes!", "Nekoko");
translation.add(55, "¡Por qué me llevas a rastras por todo el camino!", "Kouhei");
translation.add(56, "¡Son los hados, el destino!", "Nekoko");
translation.add(57, "(Ruego a todos los dioses del firmamento que\ntengáis piedad de esta desafortunada alma...).", "Kouhei");
translation.add(58, "¡Por aquí! ¡No te rezagues!", "Nekoko");
translation.add(59, "He llegado al límite... Que alguien me ayude...", "Kouhei");
translation.add(60, "¡Vamos, lentorro!", "Nekoko");
translation.add(61, "Uff, uff, uff... *tos*, *tos*\nEspera... Por favor, espera...", "Hombre furioso");
translation.add(62, "Anda.", "Nekoko");
translation.add(63, "¡Ahhhhh!", "Kouhei");
translation.add(64, "Hemos llegado a un callejón sin salida...", "");
translation.add(65, "Qué... qué mala suerte para vosotros, niños.", "Hombre furioso");
translation.add(66, "El hombre está sudando a chorretones.", "");
translation.add(67, "Será mejor que os preparéis.\n¡No tenéis donde esconderos!", "Hombre furioso");
translation.add(68, "¡Hasta los genios se equivocan; los monos se\ncaen de los árboles y los demonios acuáticos\nson arrastrados por la corriente al otro mundo!", "Nekoko");
translation.add(69, "¡¿Por qué no vas TÚ solita allí\ny a mí me dejas en paz?!", "Kouhei");
translation.add(70, "¡Hora de recoger lo sembrado!", "Hombre furioso");
translation.add(71, "Antes de eso, conviene aclarar este asunto.", "");
translation.add(72, "Tengo una pregunta.", "Kouhei");
translation.add(73, "¿Qué? Que sea rápido.", "Hombre furioso");
translation.add(74, "Bueno, al menos me está escuchando.", "");
translation.add(75, "¿Quién empezó todo esto?", "Kouhei");
translation.add(76, "¡Ella!", "Hombre furioso");
translation.add(77, "¡Él!", "Nekoko");
translation.add(78, "Nekoko y el hombre se señalan simultáneamente.", "");
translation.add(79, "¡Me tocó mi precioso gorro de hada!", "Nekoko");
translation.add(80, "¡Ensució mi traje de Armani de 290 000 yenes!", "Hombre furioso");
translation.add(81, "Vamos a echarle un vistazo...", "Kouhei");
translation.add(82, "Era mi único traje bueno. ¡Míralo y llora!", "Hombre furioso");
translation.add(83, "El hombre me enseña la etiqueta interior\nde la chaqueta. Está escrito con caracteres\noccidentales...", "");
translation.add(84, "Perdone, pero ahí pone «Arnani», no «Armani».", "Kouhei");
translation.add(85, "¿Qué?...", "Hombre furioso");
translation.add(86, "El silencio hace acto de presencia.", "");
translation.add(87, "¡Devuélveme mis 340 000 yenes!", "Hombre furioso");
translation.add(88, "Sí que sube rápido de precio...", "");
translation.add(89, "¡A la mierda! ¡Todo esto es por tu culpa!", "Hombre furioso");
translation.add(90, "¡¿Por qué?!", "Kouhei");
translation.add(91, "Bah, tampoco era para tanto.\nNo es más que una falsificación.", "Nekoko");
translation.add(92, "Y bueno, como tenía prisa, le dije que a\nver si me atrapaba. Tenía derecho a hacerlo.", "Nekoko");
translation.add(93, "¡Bueno, y te he atrapado!", "Hombre furioso");
translation.add(94, "Digamos que... ¡maldita sea!", "Nekoko");
translation.add(95, "¿No parece que estos dos se están comportando\nexactamente igual?", "");
translation.add(96, "Esto..., ¿os importa si me voy a casa?", "Kouhei");
translation.add(97, "Tú te quedas.", "Nekoko");
translation.add(98, "No me importa que quieras ser el primero.", "Hombre furioso");
translation.add(99, "De acuerdo, tú te lo has buscado. *suspiro*", "Nekoko");
translation.add(100, "Tal vez debería aplicar mi sagrada sentencia\nfeérica sobre ti, simio descerebrado.", "Nekoko");
translation.add(101, "Y el veredicto es: ¡muere en el maaaaar!", "Nekoko");
translation.add(102, "Y su malévola magia feérica ataca de nuevo.", "");
translation.add(103, "¿Y qué va a hacer una pequeña mocosa como tú?\n¡No subestimes a tus mayores!", "Hombre furioso");
translation.add(104, "¡Mi poder mágico te traerá un prematuro final!", "Nekoko");
translation.add(105, "Por favor, sal por la salida de emergencia\ndel cielo cuando regreses.", "Nekoko");
translation.add(106, "Nada de lo que dice tiene sentido,\ny prefiero no encontrárselo.", "");
translation.add(107, "Inténtalo, si quieres. ¡Acabarás sumida\nen un abismo de dolor y desesperación!", "Hombre furioso");
translation.add(108, "Y ahora el tipo empieza a hablar como ella.", "");
translation.add(109, "¿Estarán sincronizados? Mmm...", "");
translation.add(110, "Aunque si la cosa se pone fea, Nekoko no tendrá\nninguna oportunidad. No es más que una cría.", "");
translation.add(111, "¿Debería detenerle?", "");
translation.add(112, "Me preparo para interponerme entre Nekoko\ny el hombre furioso.", "");
translation.add(113, "...", "Nekoko");
translation.add(114, "¡¡Ahhhhhh!!", "Hombre furioso");
translation.add(115, "¡Ajá! ¿Ves?\n¡Es la magia infalible del hada gato Nekoko!", "Nekoko");
translation.add(116, "¡¿Acaba de salir disparado?!", "");
translation.add(117, "Nekoko está ondeando aquel bastón ornamentado\npor encima de su cabeza.", "");
translation.add(118, "Las chispas saltan en uno de los extremos.", "");
translation.add(119, "Bueno, si obviamos esa estúpida pose,\n¿no es eso una vara aturdidora?\nPuede considerarse como «magia» humana.", "");
translation.add(120, "¡Mi encanto es indiscutible!", "Nekoko");
translation.add(121, "¡El hombre furioso se tambalea!", "");
translation.add(122, "¿Por qué tiene que empezar a dolerme ahora\nla herida que me hice en la guerra del Golfo?", "Hombre furioso");
translation.add(123, "Nekoko no lo duda un instante y se dirige,\narma en ristre, hacia él.", "");
translation.add(124, "¡Ahhh!", "Hombre furioso");
translation.add(125, "Fiu.", "Nekoko");
translation.add(126, "Intercambian sus posiciones en un santiamén.\nPor un momento, nadie se mueve.", "");
translation.add(127, "¡Je, je, je, je!", "Hombre furioso");
translation.add(128, "Me has pillado, pequeña... Llegarás muy lejos\nen este mundo... Puede que hasta la cima...", "Hombre furioso");
translation.add(129, "Mi vida no tiene sentido... Argh.", "Hombre furioso");
translation.add(130, "Las chispas que emite la vara aturdidora de\nNekoko caen al mismo tiempo que el hombre.\nNo se levanta.", "");
translation.add(131, "Parecía gozar de buena salud,\nasí que quizá no muera... creo.", "");
translation.add(132, "He presionado sus puntos vitales.", "Nekoko");
translation.add(133, "Este es el secreto del estilo de lucha feérico.\nEntrará en el próximo examen, así que acuérdate.", "Nekoko");
translation.add(134, "¿De qué examen está hablando?\nEn cualquier caso, es hora de salir de aquí.", "");
translation.add(135, "¡Vale, démonos el piro!", "Kouhei");
translation.add(136, "Agarro a Nekoko del brazo y nos vamos corriendo.", "");
translation.add(137, "¡Yuju! ¡Mi magia nos ha librado de una buena!", "Nekoko");
translation.add(138, "Lo que he corrido hoy.", "");
translation.add(139, "*jadeo, jadeo, jadeo*", "Kouhei");
translation.add(140, "Fiu.", "Nekoko");
translation.add(141, "Creo que podemos disminuir el ritmo.", "Kouhei");
translation.add(142, "Me gustaría saber quién leches eres en realidad.", "Kouhei");
translation.add(143, "Ey.", "Nekoko");
translation.add(144, "¿Eh?", "Kouhei");
translation.add(145, "Di «Ah».", "Nekoko");
translation.add(146, "Nekoko salta sobre mí.", "");
translation.add(147, "Mmmmmgg....", "Kouhei");
translation.add(148, "Mmmm...", "Nekoko");
translation.add(149, "¡Mmmmfffff!", "Kouhei");
translation.add(150, "¡La bendición de agradecimiento de un hada!", "Nekoko");
translation.add(151, "Nekoko se separa de un salto y sale corriendo.", "");
translation.add(152, "No debería llamarse hada gato...", "");
translation.add(153, "«Calamidad» sería un nombre mucho más apropiado.", "");
translation.add(154, "¡Un punto más gracias a lo de ahora!", "Nekoko");
translation.add(155, "¡Ah!", "Kouhei");
translation.add(156, "Creía que se había ido y justo aparece\ndetrás de mí.", "");
translation.add(157, "¿No se había ido por aquella calle?", "");
translation.add(158, "¡Pero para activar el flag del amor\naún te queda un largo trecho por superar!", "Nekoko");
translation.add(159, "Por fin se ha marchado de verdad.", "");

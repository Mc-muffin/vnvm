translation.add(0, "Por fin es sábado.", "");
translation.add(1, "Sigo exhausto por culpa de la semanita que\nhe pasado con Nekoko y su incansable búsqueda.", "");
translation.add(2, "Hoy he quedado con ella a la hora de siempre.", "");
translation.add(3, "¿Qué hora es?", "");
translation.add(4, "Casi las diez y media.\nDebo haber dormido unas trece horas.", "");
translation.add(5, "Alguien llama a la puerta. Es Aya.", "");
translation.add(6, "¿Cuánto tiempo piensas seguir durmiendo?\n¡Vamos, levántate ya!", "Aya");
translation.add(7, "Déjame un ratito más...", "Kouhei");
translation.add(8, "¡Tienes visita!", "Aya");
translation.add(9, "¿Cómo? ¿Quién es?", "Kouhei");
translation.add(10, "Aún estoy algo dormido, pero aun así no me viene\na la cabeza nadie que venga a verme a estas horas.", "");
translation.add(11, "Un bicho raro.\nMe da rabia pensar que sales con gente así.", "Aya");
translation.add(12, "¿Un bicho raro?", "Kouhei");
translation.add(13, "Oh, oh...", "");
translation.add(14, "Resulta que es verdad que huiste\nde un yakuza en la plaza de la estación.", "Aya");
translation.add(15, "¡Agh! ¿A qué viene ahora esa determinación?", "");
translation.add(16, "Eh, bueno...", "Kouhei");
translation.add(17, "Quién iba a decir que mi propio hermano\nes uno de los objetivos de la mafia.", "Aya");
translation.add(18, "N-No es verdad...", "Kouhei");
translation.add(19, "¿Podrías dejar de negar lo evidente?\nNo hay nadie que se vista como esa chica.\nY encima ha venido a verte.", "Aya");
translation.add(20, "Quieres decir...", "Kouhei");
translation.add(21, "Vamos, no digas esas cosas de nuestra invitada.", "Mamá");
translation.add(22, "¿Cómo puedes decir eso, mamá?\n¡Tendríamos que abandonar de la ciudad\nsi la gente supiera que Kouhei es un liante!", "Aya");
translation.add(23, "Vamos, vamos... Lo importante es\nque sea su amiga, no cómo vaya vestida.", "Mamá");
translation.add(24, "¡No quiero una persona así en nuestra casa!\n¡Ya no podré invitar nunca más a mis amigas!\n¡Mamá, di algo!", "Aya");
translation.add(25, "No deberías juzgar a la gente por su apariencia.", "Mamá");
translation.add(26, "¡¿Por qué no?!", "Aya");
translation.add(27, "Tengo un maaaal presentimiento acerca de esto.", "");
translation.add(28, "¡Atiende, Kouhei! ¡Escoge mejor a tus amigos\nsi no quieres convertir tu vida en una farsa!", "Aya");
translation.add(29, "Las quejas de Aya se vuelven más distantes.", "");
translation.add(30, "Me desperezo, me visto y miro afuera.", "");
translation.add(31, "Detrás de mi madre...", "");
translation.add(32, "Sí, ahí está Nekoko, ataviada como de costumbre.", "");
translation.add(33, "Más tarde os traeré zumo y galletitas.", "Mamá");
translation.add(34, "Mi madre sale de la habitación.\nParece que Nekoko está muy emocionada.", "");
translation.add(35, "¡Yuju! Tu mamá es muy maja, Kouhei.", "Nekoko");
translation.add(36, "¡¿C-Cómo has descubierto dónde vivía?!", "Kouhei");
translation.add(37, "¡No debería saberlo!", "");
translation.add(38, "¡Tenía los ojos vendados cuando le di de comer!", "");
translation.add(39, "Nunca volvió a pedirme comida, pero por lo visto\ndejó un rastro de migas o vete a saber qué.", "");
translation.add(40, "Aunque no te lo creas, las hadas de hoy\nen día saben dónde viven las personas.", "Nekoko");
translation.add(41, "¡Eso no me vale!", "Kouhei");
translation.add(42, "¡¿Porque es un hada?!", "");
translation.add(43, "No. Cálmate... Seguro que algún día me siguió.", "");
translation.add(44, "Yo estoy cansadísimo y ella está más pizpireta\nque nunca. De hecho, hasta parece un poco aburrida.", "");
translation.add(45, "Quizá le creció esa cola por tener tanta energía.", "");
translation.add(46, "Vale, olvídalo...\nEn cualquier caso, ¡deberías aprender modales!\n¿No habíamos quedado para vernos esta noche?", "Kouhei");
translation.add(47, "Qué importa. Hoy no tienes nada que hacer, ¿no?", "Nekoko");
translation.add(48, "¡SÍ que importa! ¡No puedo con mi alma!\n¡Me dará un ataque si no descanso un poco!", "Kouhei");
translation.add(49, "No te preocupes por eso.\nTe daré polvo de hadas si te pasa algo.", "Nekoko");
translation.add(50, "¿Polvo... de hadas?", "Kouhei");
translation.add(51, "¿Quieres un poco? Tengo un...", "Nekoko");
translation.add(52, "No, gracias. Y no traigas eso aquí.", "Kouhei");
translation.add(53, "¡¿Por qué?!", "Nekoko");
translation.add(54, "¡Porque lo digo yo!", "Kouhei");
translation.add(55, "Porque parecería que es mi camello particular.\nNo pienso tomarme a cachondeo algo así.", "");
translation.add(56, "Su sola presencia ha conseguido\nque mi familia me vea con otros ojos.\nYa puedo dar gracias si solo termina en eso.", "");
translation.add(57, "Aya está decepcionadísima.\nNo quiero que vaya a más.", "");
translation.add(58, "Déjalo... Te ofrecería entrar, pero ya veo\nque te has tomado la libertad de hacerlo.", "Kouhei");
translation.add(59, "¡Qué guay, la habitación de Kouhei!", "Nekoko");
translation.add(60, "Confirmo que no hay nadie\nen el pasillo y cierro la puerta.", "");
translation.add(61, "Nekoko salta un par de veces\nencima de la cama y se sienta.", "");
translation.add(62, "Vamos a ver, Nekoko-chan...", "Kouhei");
translation.add(63, "¡Guau, tienes un montón de mangas!", "Nekoko");
translation.add(64, "Escúchame.", "Kouhei");
translation.add(65, "Y eso estoy haciendo.\n¿Quieres que lo hagamos en tu cama?", "Nekoko");
translation.add(66, "¡No me estás escuchando!", "Kouhei");
translation.add(67, "Le pego un golpecito en la cabeza.", "");
translation.add(68, "¡¿Pero qué haces?!", "Nekoko");
translation.add(69, "No puedes presentarte en la casa de alguien\nsin avisar, y mucho menos para montar jaleo.", "Kouhei");
translation.add(70, "¿Es que no te alegras de verme?", "Nekoko");
translation.add(71, "Depende del momento.\nY no, ahora no me alegro de verte.", "Kouhei");
translation.add(72, "¡¿Qué?!", "Nekoko");
translation.add(73, "Ya tengo mis propios problemas en casa.\nAya... Mi hermana se ha enterado.", "Kouhei");
translation.add(74, "¿Qué problemas?", "Nekoko");
translation.add(75, "Parece que una amiga suya nos vio\ncuando nos persiguió aquel yakuza.", "Kouhei");
translation.add(76, "¿Y?", "Nekoko");
translation.add(77, "¿Cómo que «y»? ¿Es que no se da cuenta\nde la gravedad de la situación?", "");
translation.add(78, "Pues que estoy en un compromiso con mi familia.\nCualquiera se enfadaría por eso.", "Kouhei");
translation.add(79, "¿Ah,sí?", "Nekoko");
translation.add(80, "Veamos...", "");
translation.add(81, "Admito que me preocupan tus problemas familiares,\npero los míos van primero.", "");
translation.add(82, "Pues sí, es lo normal. \nAdemás, la gente se escandaliza\ncuando ve el traje que llevas.", "Kouhei");
translation.add(83, "¡Eso sí que no! ¡Este es el uniforme oficial\nde las hadas! Hay que llevarlo siempre encima,\nsobre todo en el mundo humano.", "Nekoko");
translation.add(84, "Lo sé, pero es lo que hay.\nPor eso no quiero que vengas aquí.", "Kouhei");
translation.add(85, "Grr.", "Nekoko");
translation.add(86, "No refunfuñes. Ya nos veremos por ahí fuera.", "Kouhei");
translation.add(87, "¡No! ¿Por qué debería hacerte caso, adlátere?", "Nekoko");
translation.add(88, "Suspiro con resignación.", "");
translation.add(89, "Al menos dime por qué has venido aquí.", "Kouhei");
translation.add(90, "Si me hubiera enterado antes,\nhabría corriendo en un pispás.", "");
translation.add(91, "Me voy a dormir hasta que se haga de noche.\n¿Podrías irte, por favor?", "Kouhei");
translation.add(92, "¡¿Cómo puedes decirle eso a tu invitada?!", "Nekoko");
translation.add(93, "¡No pienso discutir con alguien\nque se presenta sin avisar!", "Kouhei");
translation.add(94, "No digas eso.\nVamos a buscar el hogar de las hadas.", "Nekoko");
translation.add(95, "Ya lo haremos esta noche.\nAdemás, estamos en un segundo piso.", "Kouhei");
translation.add(96, "Aquí no hay nada parecido a la entrada.", "Kouhei");
translation.add(97, "Eso no es verdad. Ahí mismo tienes una.", "Nekoko");
translation.add(98, "Señala un montón de videojuegos apilados.", "");
translation.add(99, "Raro es el chico que hoy en día no tenga alguno.\nHasta yo los conozco.", "");
translation.add(100, "¿Qué les pasa a esos juegos?", "Kouhei");
translation.add(101, "El mundo digital y el de las hadas están\nfuertemente ligados. Quizá las hadas de allí\npuedan decirnos dónde está la entrada.", "Nekoko");
translation.add(102, "La verdad, no creo que tengan nada que ver.", "Kouhei");
translation.add(103, "¡Yo te digo que sí! ¡Intentémoslo!", "Nekoko");
translation.add(104, "Vamos, que lo único que quiere es jugar un rato.", "");
translation.add(105, "Vaaale. ¿A qué quieres jugar?\nTengo unos cuantos, aunque ninguno para chicas.", "Kouhei");
translation.add(106, "Mmmm... A este.", "Nekoko");
translation.add(107, "Coge uno que tiene un gato en la portada.", "");
translation.add(108, "Es un juego de tenis protagonizado,\nobviamente, por un gato.", "");
translation.add(109, "Tiene una jugabilidad penosa. Es uno de\nesos juegos TAN malos que solo pones cuando\nno tienes absolutamente nada más que hacer.", "");
translation.add(110, "Y es que además, ¿qué pinta un gato ahí?", "");
translation.add(111, "Ese es bastante malo...", "Kouhei");
translation.add(112, "¡Me da igual! ¡Me ha caído simpático!", "Nekoko");
translation.add(113, "Como quieras.", "");
translation.add(114, "Empiezo una partida y le paso el mando.", "");
translation.add(115, "¿Oh? Oh, oh, oh.", "Nekoko");
translation.add(116, "...", "Kouhei");
translation.add(117, "¡Grrrrrrrr!", "Nekoko");
translation.add(118, "...", "Kouhei");
translation.add(119, "¡Esto no va!", "Nekoko");
translation.add(120, "¡Ahhhh!", "Kouhei");
translation.add(121, "Lanza el mando con furia por el suelo.", "");
translation.add(122, "¡Si no sabes jugar, dímelo!\n¡Podrías haberlo roto!", "Kouhei");
translation.add(123, "¡Cómo quieres que no me vaya mal\nsi es la primera vez que juego a esto!", "Nekoko");
translation.add(124, "¿Nunca habías jugado antes?", "Kouhei");
translation.add(125, "Imagino que la principal afición de las hadas\nconsiste en reírse de los mafiosos y policías.", "");
translation.add(126, "Déjame que te enseñe.\nPara golpear la pelota tienes que...", "Kouhei");
translation.add(127, "...", "");
translation.add(128, "......", "");
translation.add(129, "............", "");
translation.add(130, "¿A-Así?", "Nekoko");
translation.add(131, "Eso es. Lo estás haciendo muy bien.", "Kouhei");
translation.add(132, "¡Así!", "Nekoko");
translation.add(133, "Veo que lo has pillado.", "Kouhei");
translation.add(134, "¡¿Y por qué sigo perdiendo?!", "Nekoko");
translation.add(135, "Ya te he dicho que es un juego muy malo...", "Kouhei");
translation.add(136, "¡No es malo! ¡Antes te lo has pasado enseguida!\n¡¿Por qué tú sí y yo no?!", "Nekoko");
translation.add(137, "Porque habrá que pillarle el truco...", "Kouhei");
translation.add(138, "No debería provocarla.", "");
translation.add(139, "¡¿Qué truco?!", "Nekoko");
translation.add(140, "No es para tanto... Ahora te lo enseño", "Kouhei");
translation.add(141, "Cojo el mando y empiezo otro partido.", "");
translation.add(142, "Observa atentamente.", "Kouhei");
translation.add(143, "Vale.", "Nekoko");
translation.add(144, "Si subes tanto a la red acabarás por fallar\ncasi todas las pelotas. Tienes que intentar\ndevolverlas desde la línea.", "Kouhei");
translation.add(145, "Ahh...", "Nekoko");
translation.add(146, "La barra se llena a base de devolver pelotas.\nCuando esté a tope, podrás ejecutar un golpe\nespecial. Mira, como este: ¡Ataque gatuno!", "Kouhei");
translation.add(147, "¡Ohhhh!", "Nekoko");
translation.add(148, "El juego muestra al gato pegando un débil puñetazo.", "");
translation.add(149, "Aunque no es un movimiento nada espectacular,\nel rival es incapaz de devolverla.", "");
translation.add(150, "Y así es como va.", "Kouhei");
translation.add(151, "¡E-Ese ataque gatuno ha sido increíble!\n¡Quiero aprender a hacerlo!", "Nekoko");
translation.add(152, "Eso es imposibl...", "Kouhei");
translation.add(153, "Eh... Ese gato está mejorado genéticamente.\nNi siquiera un hada podría copiar ese movimiento.", "Kouhei");
translation.add(154, "Mmm. Supongo que eso está fuera de mi alcance.", "Nekoko");
translation.add(155, "El caso es que tengo la sensación\nde que si no usas ese ataque, pierdes.\nHagas lo que hagas.", "Kouhei");
translation.add(156, "Entiendo.", "Nekoko");
translation.add(157, "Le devuelvo el mando a Nekoko.", "");
translation.add(158, "Eres increíble, Kouhei.\n¿Crees que llegaría al mundo digital\nsi lograra hacerlo tan bien como tú?", "Nekoko");
translation.add(159, "No deberías adentrarte tanto...\nUn científico alemán dijo que pasarse\njuegos violentos es malo para la salud mental.", "Kouhei");
translation.add(160, "Eso solo se aplica a los humanos.\nConmigo a mi lado llegaremos\nhasta el hogar de las hadas.", "Nekoko");
translation.add(161, "En ese caso...", "Kouhei");
translation.add(162, "No sé muy bien qué más responderle.", "");
translation.add(163, "¡Vale, ya verás como lo consigo!", "Nekoko");
translation.add(164, "Se concentra totalmente en la partida.\nSin embargo...", "");
translation.add(165, "¡Oye! ¡El otro gato ha hecho un punto!", "Nekoko");
translation.add(166, "Sí, solo puedes ganar si lanzas\ntu golpe especial antes que tu rival.", "Kouhei");
translation.add(167, "En menos de tres peloteos la máquina se las apaña\npara llenar la barra y realizar su golpe especial.", "");
translation.add(168, "En resumen: si cometes un error, estás perdido.\nPor eso digo que tiene una jugabilidad de pena.", "");
translation.add(169, "Mira, tienes la barra a tope. Úsalo ahora.", "Kouhei");
translation.add(170, "Pero es que no puedo...\n¡Aaarg! ¡He vuelto a perder!", "Nekoko");
translation.add(171, "Calma... Nunca te lo pasarás si te pones así.", "Kouhei");
translation.add(172, "¡Pero es que no le da donde le digo!\n¡Así es imposible!", "Nekoko");
translation.add(173, "Su frenética lucha hace que termine en mi regazo.", "");
translation.add(174, "¡Ohhh! ¡Oh, oh!", "Kouhei");
translation.add(175, "¡Voy a probarlo otra vez!", "Nekoko");
translation.add(176, "¿Otra vez? ¿Por qué no te comportas como\nuna buena chica y me dejas ponerte un juego\npara novatos?", "Kouhei");
translation.add(177, "¡No! ¡Me gusta este!", "Nekoko");
translation.add(178, "Vale, vale. Porque te ha caído simpático...", "Kouhei");
translation.add(179, "Empiezo una nueva partida.", "");
translation.add(180, "¡Oh! ¡Ah! ¡Uh!", "Nekoko");
translation.add(181, "¡Por aquí, por aquí! ¡Vamos!", "Nekoko");
translation.add(182, "La pelota vuela de un lado a otro...", "");
translation.add(183, "¡Yuju! ¡He ganado un partido!", "Nekoko");
translation.add(184, "Brinca encima de mi regazo,\npalmeando las manos con alegría.", "");
translation.add(185, "Ya entiendo cómo va.\n¡Y ahora, a por el próximo rival!", "Nekoko");
translation.add(186, "Alza el puño con convicción.", "");
translation.add(187, "Vamos, tú puedes.", "Kouhei");
translation.add(188, "Le paso el mando cuando empieza la siguiente ronda.", "");
translation.add(189, "¿Mmm?", "");
translation.add(190, "Entonces noto algo extraño.", "");
translation.add(191, "¿Qué ocurre?...", "");
translation.add(192, "¡Allá vamos!", "Nekoko");
translation.add(193, "Nekoko está realmente animada, aunque yo sigo\ndebatiéndome contra esta incómoda sensación.", "");
translation.add(194, "Empieza otra partida.", "");
translation.add(195, "Se mueve de izquierda a derecha con entusiasmo.\nEntonces me doy cuenta.", "");
translation.add(196, "¡¡Ahhh!!", "Kouhei");
translation.add(197, "¡Mi pequeñín ha salido de paseo!", "");
translation.add(198, "Y aún más, ¡está rozando la raja\nal descubierto de Nekoko!", "");
translation.add(199, "¡Lo que había sentido era que me estaba\nbajando la cremallera! ¡Pero qué rápida es!", "");
translation.add(200, "M-Mi familia está en casa, Nekoko-chan...", "Kouhei");
translation.add(201, "No importa.", "Nekoko");
translation.add(202, "¡Pues claro que importa! ¡Y mucho!", "Kouhei");
translation.add(203, "Te van a oír si gritas tanto...", "Nekoko");
translation.add(204, "Qué susurro más falso.", "");
translation.add(205, "¡Esto no va a así!\n¡Es el chico quien debería decir eso!", "Kouhei");
translation.add(206, "La miro fijamente, pero\nparece que ni se ha dado cuenta.", "");
translation.add(207, "Tranquilo, he lanzado un hechizo de ilusión.", "Nekoko");
translation.add(208, "Nuestra casa es resistente a la magia.\n¿Qué pasa si no funciona?", "Kouhei");
translation.add(209, "Nadie se dará cuenta si nos ven desde atrás.", "Nekoko");
translation.add(210, "En eso tiene razón. Pero aun así...", "");
translation.add(211, "¡Ese no es el tema!", "Kouhei");
translation.add(212, "¡Aaaaargh! ¡Cállate!\n¡Por tu culpa he vuelto a perder!", "Nekoko");
translation.add(213, "Nekoko-chan...", "Kouhei");
translation.add(214, "Me he quedado sin palabras.", "");
translation.add(215, "¿Qué? ¿Qué tal ahí abajo?", "Nekoko");
translation.add(216, "Eso da igual. El caso es que hay\nun momento y un lugar para todo...", "Kouhei");
translation.add(217, "Aunque no parece importarle.", "");
translation.add(218, "Oh, ¿de verdad?", "Nekoko");
translation.add(219, "Mueve la cadera de forma que su carne\nse frote con mi todavía flácido pene.", "");
translation.add(220, "Jejejé. ¿Qué tal, Kouhei?", "Nekoko");
translation.add(221, "Déjalo ya. Pesas mucho; venga, baja.", "Kouhei");
translation.add(222, "O es que no quieres que sepa\nlo dura que se te ha puesto, ¿mmm?", "Nekoko");
translation.add(223, "¡No!", "Kouhei");
translation.add(224, "Jo, jo. Parece que he dado en el clavo.", "Nekoko");
translation.add(225, "Arg...", "Kouhei");
translation.add(226, "¡Pues sí, me estoy excitando un poco!", "");
translation.add(227, "Leí en una revista que se trata del temor de\nser pillado por sorpresa. ¿Habré sucumbido a eso?", "");
translation.add(228, "¿No te gusta? Mira lo mucho que crece.", "Nekoko");
translation.add(229, "¡Por Dios, baja la voz!", "Kouhei");
translation.add(230, "Dejo que ella se encargue del estímulo\nde nuestras partes inferiores mientras\nyo deslizo las manos hasta sus pechos.", "");
translation.add(231, "Empiezo a pellizcarle los pezones.", "");
translation.add(232, "Ahh... Nnaa... Ahh... Nnaa...", "Nekoko");
translation.add(233, "Aunque ya se ha olvidado del juego,\nno hay manera de que suelte el mando.", "");
translation.add(234, "Mira que es cabezota.", "");
translation.add(235, "¿Qué me dices de ti? ¿Qué tal?", "Kouhei");
translation.add(236, "Nnn... Ahh... Estoy bien... Esto no es nada...", "Nekoko");
translation.add(237, "¿De verdad? Tus gemidos indican todo lo contrario.", "Kouhei");
translation.add(238, "Pues... tu cosa también se ha puesto bien dura...", "Nekoko");
translation.add(239, "Estás chorreando a espuertas, Nekoko-chan.", "Kouhei");
translation.add(240, "Es un duelo bastante extraño.", "");
translation.add(241, "Jejejejé.", "Nekoko");
translation.add(242, "Jejejejé.", "Kouhei");
translation.add(243, "¡La situación se ha invertido totalmente\nde un tiempo a esta parte!", "");
translation.add(244, "¡Te voy a ganar!", "Nekoko");
translation.add(245, "¡No pienso perder!", "Kouhei");
translation.add(246, "Je, je... Nnn...", "Nekoko");
translation.add(247, "Nekoko acelera el ritmo.", "");
translation.add(248, "Ugg. ¡Será...!", "");
translation.add(249, "No pienso dejarme ganar así como así.\n¡Tengo que demostrar que soy un hombre\nde pelo en pecho!", "");
translation.add(250, "¡No podría mirarme a los ojos si perdiera!", "");
translation.add(251, "Ah... ¡Qué me dices de esto!", "Kouhei");
translation.add(252, "Nngg... Ahh... Nngg... ¡Nnnnnnn!", "Nekoko");
translation.add(253, "¿Qué pasará si le muerdo la oreja?\nSolo hay una forma de averiguarlo...", "");
translation.add(254, "¡Nnnaaaa!", "Nekoko");
translation.add(255, "Su cuerpo se pone rígido y empieza a temblar.", "");
translation.add(256, "Ha ido mejor de lo que imaginaba.\nEn ese caso, ¡vamos a seguir!", "");
translation.add(257, "Nnn... Ahh... Nnn... Aaahhh...", "Nekoko");
translation.add(258, "Mmff, mff, mmff, mmmf, mfmf, mfmmmf.\n(¿Qué te parece esto, Nekoko-chan?).", "Kouhei");
translation.add(259, "Nnn... Ahh... Ahh... Nnn...", "Nekoko");
translation.add(260, "Ahh... ¡No puedo aguantar más! Kouhei, métemela...", "Nekoko");
translation.add(261, "Je, je. ¿Admites que has perdido?", "Kouhei");
translation.add(262, "¡Sí, lo admito!", "Nekoko");
translation.add(263, "¡Hurra! ¡Por fin he ganado a Nekoko!", "");
translation.add(264, "Vamos... Date prisa...", "Nekoko");
translation.add(265, "Muy bien, no voy a negarte ese placer.", "Kouhei");
translation.add(266, "Muevo la cadera de forma que permita\nla entrada de mi pene en su vagina.", "");
translation.add(267, "Ahh... Nnn... Ahh... ¡Está entrando!", "Nekoko");
translation.add(268, "Aunque hace nada que está desvirgada,\naún sigue muy sensible.", "");
translation.add(269, "Mejor, prefiero que sienta placer y no dolor.", "");
translation.add(270, "Como no escucho queja alguna, empiezo a moverme.", "");
translation.add(271, "¡Nnnnaaa! Ahh... Ahh...", "Nekoko");
translation.add(272, "No le pasará nada. Había oído que duele hasta\nque te acostumbras, pero ella parece llevarlo\nbastante bien.", "");
translation.add(273, "Aunque quizá sea por la droga que toma...", "");
translation.add(274, "¡Ahhhh!", "Nekoko");

translation.add(382, "Ahhh...", "Nekoko");
translation.add(383, "Titubea. Parece que he acertado.", "");
translation.add(384, "Ahh... Nnn... Kouhei... Eres un cerdo...", "Nekoko");
translation.add(385, "Dudo que encontrases un solo chico\nque no disfrutara de esta situación.", "Kouhei");
translation.add(386, "Está bien... Hagámoslo.", "Nekoko");
translation.add(387, "Allá voy.", "Kouhei");
translation.add(388, "Empiezo a darle desde abajo.", "");
translation.add(389, "Ahh... Nngg...", "Nekoko");
translation.add(390, "Pese a los pasmos que recorren todo su cuerpo,\nse niega a soltar el mando.", "");
translation.add(391, "Mientras tanto, en la pantalla, el oponente\nmanda sin piedad la pelota al espacio vacío.", "");
translation.add(392, "Uunn... Ahh... Unn... Ahh...\nVoy a perder otra vez...", "Nekoko");
translation.add(393, "Su porfía es admirable: pensar en otras cosas\nmientras está teniendo sexo es increíble.", "");
translation.add(394, "Aprovecharé para experimentar un poco con ella.", "");
translation.add(395, "Quizá si derrotas al rival mientras estás\nhaciéndolo, puedas acceder al mundo digital.", "Kouhei");
translation.add(396, "¡¿D-De verdad?!", "Nekoko");
translation.add(397, "Pues claro que no, pero no pienso decírselo.", "");
translation.add(398, "No lo sabrás si no lo intentas.", "Kouhei");
translation.add(399, "¡Entonces voy a darlo todo!... ¡Nnnnnnnn!", "Nekoko");
translation.add(400, "Se pone a jugar inmediatamente.\nEn cuanto empieza el siguiente partido,\nse la meto con fuerza.", "");
translation.add(401, "Ahh... Nnn... Ahh... Nnaa... Ahh...\n¡No te muevas! ¡Deja de moverte así!", "Nekoko");
translation.add(402, "Tienes que devolver el servicio del contrario.", "Kouhei");
translation.add(403, "¡Ya lo sé, pero es demasiado bueno!", "Nekoko");
translation.add(404, "Embisto contra su vagina sin compasión,\na pesar de sus esfuerzos por seguir con el juego.", "");
translation.add(405, "Nnn... Nnaa... Ahh... Ahh... Nnn... Ahh...", "Nekoko");
translation.add(406, "Miro cómo le va la partida. Es totalmente\nincapaz de reaccionar: su personaje está\ndando vueltas sin sentido en una esquina.", "");
translation.add(407, "N-No puedo devolverla, no puedo devolverla...\nAhh... Nngg... Nnaa... Ahh...", "Nekoko");
translation.add(408, "Le beso la nuca.", "");
translation.add(409, "Nnaa... Ahh... No chupes ahí...", "Nekoko");
translation.add(410, "Ignoro sus no muy convincentes protestas\ny empiezo a reptar la lengua por su piel.", "");
translation.add(411, "Ahh... Nnn... Ahhh...", "Nekoko");
translation.add(412, "No va mal la cosa.\nEstá empezando a prestar más atención al sexo.", "");
translation.add(413, "Estás perdiendo en los dos encuentros.", "Kouhei");
translation.add(414, "E-Eso es porque tu cosa está ardiendo, y es\ntan grande... ¡Me encanta! ¡Dame más fuerte!", "Nekoko");
translation.add(415, "Por dentro también estás muy caliente y mojada.", "Kouhei");
translation.add(416, "Nnnn. ¿Te gusta, Kouhei? ¿Te gusta?", "Nekoko");
translation.add(417, "Nada podría gustarme más...", "Kouhei");
translation.add(418, "M-Mejor, eso hará que tu semen\nesté cargado con aún más poder feérico.\n¡Disfrútalo todo lo que puedas!", "Nekoko");
translation.add(419, "Entonces aceleraré el ritmo.", "Kouhei");
translation.add(420, "Sí, hazlo... Nnn... Ahh... Ahh... Nnn...", "Nekoko");
translation.add(421, "Por fin se ha olvidado del jueguecito de marras.\nAhora no es más que una mujer sedienta de placer.", "");
translation.add(422, "Ella también empieza a moverse, como si\nmis propias acometidas no fueran suficientes.", "");
translation.add(423, "Ahh... Nnn... Ahh... Uunn... Ahh... Nngg...", "Nekoko");
translation.add(424, "Ugg... Es increíble...", "Kouhei");
translation.add(425, "El dominio que tenía sobre mí mismo se desvanece.", "");
translation.add(426, "¡Sus pliegues me están poniendo a mil!", "");
translation.add(427, "Auuu, Kouhei...\nC-Creo que voy a llegar al mundo digital...", "Nekoko");
translation.add(428, "Yo también estoy a punto...", "Kouhei");
translation.add(429, "¡Llévame contigo!\n¡Llévame al mundo digital con tu polla!", "Nekoko");
translation.add(430, "Nekoko-chan, me voy a correr...", "Kouhei");
translation.add(431, "Ahh... Nngg... Ahh... Ahh...\n¡Estoy alcanzando el mundo digital!", "Nekoko");
translation.add(432, "¡Ugg, ahhhh!", "Kouhei");
translation.add(433, "La levanto y hago que todo\nsu peso caiga sobre mi polla.", "");
translation.add(434, "Nnn... Ahh... ¡Aaaaaaah!", "Nekoko");
translation.add(435, "Uff, uff, uff...", "Kouhei");
translation.add(436, "Uunn... Ahh... Ahh... Nnn...", "Nekoko");
translation.add(437, "Su cuerpo se estremece,\npierde fuerza y cae sobre mí.", "");
translation.add(438, "Uff, uff, uff...", "Nekoko");
translation.add(439, "Uuu... Creía haber llegado, pero aún no...", "Nekoko");
translation.add(440, "¿Te refieres al mundo digital?", "Kouhei");
translation.add(441, "Sí... Mmm, se está saliendo tu poder feérico.", "Nekoko");
translation.add(442, "¡Ahh! ¡No lo saques tan rápido!", "Kouhei");
translation.add(443, "Corro a coger un pañuelo para limpiarla.", "");
translation.add(444, "Parece que eso era todo.", "");
translation.add(445, "En cualquier caso, creo que ha salido todo bien.", "");
translation.add(446, "Oye...", "Nekoko");
translation.add(447, "¿Mm? ¿Qué pasa?", "Kouhei");
translation.add(448, "Eres mi sirviente, ¿verdad?", "Nekoko");
translation.add(449, "¿Cuándo me he convertido en eso?", "Kouhei");
translation.add(450, "¡La primera vez que nos conocimos!\n¿No te acuerdas?", "Nekoko");
translation.add(451, "No. Y la verdad, no creo que haya pasado nunca.\n¿Por qué lo dices?", "Kouhei");
translation.add(452, "Verás... La tienda de objetos mágicos donde\nconsigo el polvo de hadas ha traído una nueva\nmedicina.", "Nekoko");
translation.add(453, "El polvo de hadas, ¿eh?", "Kouhei");
translation.add(454, "Debe estar hablando de la droga que toma.\nSupongo que se trata de una nueva versión.", "");
translation.add(455, "El nuevo es muy potente, y estoy segura de que\npodría alcanzar el hogar de las hadas con él.\nPero nosotras no llevamos dinero encima.", "Nekoko");
translation.add(456, "¿Quieres que te preste dinero?", "Kouhei");
translation.add(457, "No. Quiero que me lo des.", "Nekoko");
translation.add(458, "Extiende las manos hacia mí.", "");
translation.add(459, "Anda ya.", "Kouhei");
translation.add(460, "¡Cómo que no! ¡La norma dice que los\nsirvientes tienen que obedecer a sus amos!", "Nekoko");
translation.add(461, "No la he leído en ningún sitio.", "Kouhei");
translation.add(462, "¿Qué es eso de una droga más potente?\nEs evidente que también será más peligrosa.", "");
translation.add(463, "Toda esa energía debe provenir de la droga.", "");
translation.add(464, "No conozco los detalles, pero seguro que si\nestá haciendo esto es porque está enganchada.", "");
translation.add(465, "Antes ya era peligroso.\nSi encima toma algo más fuerte...", "");
translation.add(466, "¿Y prestármelo?...", "Nekoko");
translation.add(467, "Ni hablar. ¿No dices que no tienes dinero?\nNo podrías devolvérmelo.", "Kouhei");
translation.add(468, "¡Por eso odio este mundo!", "Nekoko");
translation.add(469, "Se está enfadando.", "");
translation.add(470, "Eres muy impertinente para ser un sirviente.\n¡Tu deber es ayudar a tu amo cuando lo necesita!", "Nekoko");
translation.add(471, "No vivimos en una sociedad tan jerarquizada.", "Kouhei");
translation.add(472, "Creía que sabías lo importante que es la\nmedicación para una chica. ¡Menos un punto!", "Nekoko");
translation.add(473, "Admito que el dinero es muy importante\nen el mundo humano; no como el hogar de\nlas hadas, donde todo sale gratis.", "Kouhei");
translation.add(474, "¡Bah! Es por eso que...", "Nekoko");
translation.add(475, "Además, no sabía que fuera tan fácil llegar\nhasta allí. Menuda misión la tuya si puede\nsuperarse tomando una simple droga.", "Kouhei");
translation.add(476, "¡Te equivocas!\n¡Es un requisito indispensable para mi tarea!", "Nekoko");
translation.add(477, "Como quieras, pero no te voy a dar nada.", "Kouhei");
translation.add(478, "Grrrrr.", "Nekoko");
translation.add(479, "A ver si lo deja ya...", "");
translation.add(480, "Si está en esta situación, es que es\nmuy probable que ya no le quede dinero.", "");
translation.add(481, "Como siempre está de aquí para allá,\nlo más probable es que no tenga trabajo.", "");
translation.add(482, "Y no creo que sus padres le den dinero.", "");
translation.add(483, "Si no se lo doy, no podrá comprar más drogas.", "");
translation.add(484, "Podría ser una buena oportunidad\npara que las dejara de una vez.", "");
translation.add(485, "Entonces, parece que se le ocurre algo.\nSe agarra a mí y me hace una llave de cabeza.", "");
translation.add(486, "¡Ah!", "Kouhei");
translation.add(487, "Has dicho que el dinero es muy\nimportante para los humanos, ¿no?", "Nekoko");
translation.add(488, "Auuu, me haces daño... Sí, eso he dicho..", "Kouhei");
translation.add(489, "¿Cómo se consigue más?", "Nekoko");
translation.add(490, "Ahh... Pues trabajando. Busca un trabajo.", "Kouhei");
translation.add(491, "¡Entonces consígueme uno!", "Nekoko");
translation.add(492, "¿Qué?", "Kouhei");
translation.add(493, "Todo se solucionará si gano mi propio dinero.\nAsí que encuéntrame un trabajo.", "Nekoko");
translation.add(494, "¿C-Cómo pretende que haga eso?", "");
translation.add(495, "No me imagino presentando una drogata a mi jefe.\nEn el peor de los casos me despedirían.", "");
translation.add(496, "Y aunque nadie se diera cuenta de su condición,\nno hay duda de que acabaría metiéndose en líos.", "");
translation.add(497, "Además, aunque lo hiciera bien,\ngastaría el dinero en comprar drogas.", "");
translation.add(498, "Tengo que evitarlo a toda costa.", "");
translation.add(499, "L-Lo siento, pero no puedo... S-Suéltame...", "Kouhei");
translation.add(500, "¡Sí que puedes!\n¡Puedo trabajar en el conbini donde estás!", "Nekoko");
translation.add(501, "¡¿Qué?! ¡No, no puedo!", "Kouhei");
translation.add(502, "¡Decidido! ¡Mañana iré a verte!", "Nekoko");
translation.add(503, "Acabo de decirte que...", "Kouhei");
translation.add(504, "No puedo andar por ahí sacando\nconclusiones basadas en nada.", "");
translation.add(505, "(Pero...).", "Kouhei");
translation.add(506, "No quiero ni pensarlo, pero...\n¿qué pasaría si empezara a verse\ncon hombres a cambio de un estipendio?", "");
translation.add(507, "Podría comprar un montón de drogas\nsi cayera mucho dinero en sus manos.", "");
translation.add(508, "Aunque bien es cierto que con la paga\nde la tienda no le bastaría para mucho.", "");
translation.add(509, "Además, si empieza a trabajar,\nquizá termine por volver a la realidad.", "");
translation.add(510, "Vale.", "Kouhei");
translation.add(511, "Es inútil intentar detenerla. Lo único que\npuedo hacer es rezar para que todo vaya bien.", "");
translation.add(512, "Veamos...", "Kouhei");
translation.add(513, "Le cuento en qué consiste mi trabajo.", "");
translation.add(514, "Está empezando a dolerme la cabeza.\nNo puedo evitar pensar que, si tomase un poco\nde esa droga, podría olvidarme de todo esto.", "");

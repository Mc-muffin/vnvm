translation.add(0, "(¿Mmm?).", "Kouhei");
translation.add(1, "Abro los ojos. Me siento extrañamente incómodo.", "");
translation.add(2, "La luz ilumina la habitación. Ya es de día...", "");
translation.add(3, "¿Mmm?", "");
translation.add(4, "Qué raro, las sábanas no me están tapando.", "");
translation.add(5, "Tengo frío...", "Kouhei");
translation.add(6, "Me he levantado por su culpa,\naunque aún podría dormir un poco más.", "");
translation.add(7, "Extiendo el brazo para alcanzar las sábanas...", "");
translation.add(8, "Mmm... ¿Mmm?", "Aeka");
translation.add(9, "Ahí está Aeka. Duerme tan plácidamente\nque cuesta creer que ayer estuviera a punto\nde suicidarse.", "");
translation.add(10, "(Es verdad... Ayer).", "Kouhei");
translation.add(11, "Me la traje a casa. Nunca imaginé\nque me vería envuelto en una situación así.", "");
translation.add(12, "Si no sales conmigo, me suicidaré.", "Aeka");
translation.add(13, "No sé si fue una confesión o una amenaza.", "Kouhei");
translation.add(14, "Siento haberme metido en un asunto muy serio;\nen uno donde no podré dar marcha atrás.", "");
translation.add(15, "Aunque quizá haya sido lo mejor.", "");
translation.add(16, "Agh, qué frío.", "Kouhei");
translation.add(17, "No puedo más. Le zarandeo un poco el hombro.", "");
translation.add(18, "Aeka...", "Kouhei");
translation.add(19, "Mmm... Ahh... ¿Kouhei-kun?", "Aeka");
translation.add(20, "Le quito las sábanas. Puedo sentir\nel calor que desprende a través de ellas.", "");
translation.add(21, "Kouhei-kun, tengo frío...", "Aeka");
translation.add(22, "No haberte quedado todas las sábanas para ti.", "Kouhei");
translation.add(23, "Se lamenta. Empiezo a travesear\ncon ella enredando mis piernas con las suyas.", "");
translation.add(24, "Todavía me avergüenza un poco llamarla\npor el nombre, aunque supongo que acabaré\npor acostumbrarme.", "");
translation.add(25, "Eso espero.", "");
translation.add(26, "¡Ahh! ¡Tienes las piernas heladas!", "Aeka");
translation.add(27, "¡Y también el cuerpo entero!\n¡Comparte la sábana!", "Kouhei");
translation.add(28, "Forcejea un poco, como si le estuviera haciendo\ncosquillas. Por fin termina por envolver\nlos brazos a su alrededor y quedarse quieta.", "");
translation.add(29, "Esto... Kouhei-kun...", "Aeka");
translation.add(30, "¿Sí?", "Kouhei");
translation.add(31, "Mmm... Tengo que ir al baño.", "Aeka");
translation.add(32, "¿Eh?", "Kouhei");
translation.add(33, "Entonces recuerdo que mis padres volvieron\nanoche. El reloj marca las seis; mamá pronto\nse despertará.", "");
translation.add(34, "¡Mierda!", "Kouhei");
translation.add(35, "Salto de la cama y me visto a toda pastilla.", "");
translation.add(36, "(¡Las zapatillas de Aeka están en la entrada!).", "Kouhei");
translation.add(37, "Me da igual que lo sepa Aya, pero no puedo\npermitir que mis padres descubran que una\ncompañera de clase ha pasado la noche aquí.", "");
translation.add(38, "Está a punto de amanecer...\nQuizá sea demasiado tarde.", "");
translation.add(39, "¿Ocurre algo?", "Aeka");
translation.add(40, "Cojo mi uniforme de gimnasia y se lo doy a Aeka.", "");
translation.add(41, "No puedes ir desnuda. Ponte esto.", "Kouhei");
translation.add(42, "Vale...", "Aeka");
translation.add(43, "Espero a que se vista\ny salimos en silencio de la habitación.", "");
translation.add(44, "Por aquí.", "Kouhei");
translation.add(45, "La guío con cuidado por las escaleras\nhasta el servicio.", "");
translation.add(46, "Vuelve a mi habitación cuando hayas terminado.", "Kouhei");
translation.add(47, "Vale.", "Aeka");
translation.add(48, "Voy al pasillo para recoger sus zapatillas.", "");
translation.add(49, ".........", "");
translation.add(50, "O esa era mi intención...", "");
translation.add(51, "(¡H-Han desaparecido!).", "Kouhei");
translation.add(52, "Solo están mis zapatos y los de mi familia.", "");
translation.add(53, "Los de Aeka no están aquí.\nMiro debajo del aparador, pero nada.", "");
translation.add(54, "¿Cómo es posible?", "Kouhei");
translation.add(55, "Esto... ¿debería volver a tu habitación?", "Aeka");
translation.add(56, "Parece que Aeka ha terminado.", "");
translation.add(57, "Ah, sí.", "Kouhei");
translation.add(58, "Sube las escaleras a trompicones.", "");
translation.add(59, "(Mmm... Quizá Aeka los tirase... No, no creo).", "Kouhei");
translation.add(60, "¿...?  ¡¡...!!", "Kouhei");
translation.add(61, "Antes de que pueda reaccionar,\nuna zapatilla voladora choca contra mi espalda.", "");
translation.add(62, "¡Shhh! ¡Kouhei! ¡Por aquí!", "Aya");
translation.add(63, "Aya está en mitad de las escaleras.\nMe hace señas para que me acerque.", "");
translation.add(64, "Recojo la zapatilla y subo las escaleras.", "");
translation.add(65, "Si te sientas ahí\nte pegará dolor de barriga por el frío.", "Kouhei");
translation.add(66, "¡Ah! ¡Cómo eres!", "Aya");
translation.add(67, "Normalmente Aya hubiera gritado; pero\nahora, en vez de eso, intenta golpearme\ncon lo que lleva en la mano.", "");
translation.add(68, "Ey.", "Kouhei");
translation.add(69, "Son las zapatillas de Aeka. ¿Qué hace con ellas?", "");
translation.add(70, "Anoche os las dejasteis en la entrada,\n¡así que me las llevé a mi habitación!", "Aya");
translation.add(71, "De otra forma me hubiera sentido culpable...", "Aya");
translation.add(72, "Gracias... Esta es mi hermanita.", "Kouhei");
translation.add(73, "Es una gran muestra de gratitud por mi parte,\npero lo único que hace es enfadarse.", "");
translation.add(74, "¡Bah! ¡Toma, cógelas!", "Aya");
translation.add(75, "Me quita la zapatilla de las manos\ny me la vuelve a dar junto con la otra.", "");
translation.add(76, "Espera... Aya, gracias.", "Kouhei");
translation.add(77, "Debería devolvérselas cuanto antes.", "");
translation.add(78, "No ha sido nada...", "Aya");
translation.add(79, "Me mira con una expresión extraña\ny vuelve a su habitación.", "");
translation.add(80, "Aeka está esperándome sentada en mi cama.", "");
translation.add(81, "El ruido de afuera indica que el mundo\nestá empezando a moverse de nuevo.", "");
translation.add(82, "Lo siento, he tenido que buscar tus zapatillas.", "Kouhei");
translation.add(83, "Oh... Gracias.", "Aeka");
translation.add(84, "Creo que me voy a ir a casa.", "Aeka");
translation.add(85, "¿Quieres que te acompañe?", "Kouhei");
translation.add(86, "Empiezo a sacar algo más de ropa.", "");
translation.add(87, "No, no hace falta.\nYa es de día, no debería pasarme nada.", "Aeka");
translation.add(88, "Pero has pasado fuera toda la noche...\nTus padres estarán preocupados.", "Kouhei");
translation.add(89, "Su semblante se ensombrece.", "");
translation.add(90, "La verdad, no creo que lo estén...", "Aeka");
translation.add(91, "Sonríe con tristeza, como cuando está en clase.", "");
translation.add(92, "A mis padres no les importa lo que haga,\nasí que no se preocupan por mí... Ja, ja.", "Aeka");
translation.add(93, "(Así que... no solo es en clase).", "Kouhei");
translation.add(94, "La verdad es que no sé nada sobre su familia.", "");
translation.add(95, "Y no debería insistir mucho sobre el tema,\nal menos mientras ella no quiera contarme nada.", "");
translation.add(96, "Había pensado ir temprano a la escuela\na buscar mi uniforme.", "Aeka");
translation.add(97, "(¡Oh!).", "Kouhei");
translation.add(98, "Aunque no sé si lo encontraré...", "Aeka");
translation.add(99, "Entonces recuerdo el correo de Antoinette.", "");
translation.add(100, "Era sobre su uniforme.", "");
translation.add(101, "Estaba enfadada con Aeka\nporque es callada y muy mona...", "");
translation.add(102, "Así que se lo escondió.", "");
translation.add(103, "Lo puso en una papelera...", "");
translation.add(104, "Lo sacó justo antes\nde que llegase el camión de la basura...", "");
translation.add(105, "Leer aquel mensaje me dio arcadas.", "");
translation.add(106, "Por eso fui incapaz de decírselo anoche...", "");
translation.add(107, "Aeka escucha mi historia.\nAl principio me mira sorprendida,\npero luego parece simplemente abatida.", "");
translation.add(108, "Me pregunto... por qué se molesta tanto...", "Aeka");
translation.add(109, "Posiblemente sea una pregunta retórica.\nNo creo que espere que le dé una respuesta.", "");
translation.add(110, "Aun así, me siento como un traidor\npor tener un contacto así con Antoinette.", "");
translation.add(111, "Pero, créeme.\nNo tenía ni idea de lo que pasó el otro día.", "Kouhei");
translation.add(112, "Además, ¿por qué me sigue mandando correos?\n¿Es que quiere que me una a ella?", "Kouhei");
translation.add(113, "O quizá... quizá se haya dado cuenta de que\nhe estado pasando demasiado tiempo con Aeka\ny quiera mantenerme vigilado.", "");
translation.add(114, "Intento poner alguna excusa, pero\nlo único que hace Aeka es mirarme con asombro.", "");
translation.add(115, "¿Eh? Ni se me había ocurrido\nque tuvieses algo que ver con ella.", "Aeka");
translation.add(116, "Si hubiera sido el caso, no estaría aquí,\ny tampoco me hubieras contado lo del uniforme.", "Aeka");
translation.add(117, "Lo traicionero es que debería habérselo\ndicho enseguida... Pero bueno, dejémoslo así.", "");
translation.add(118, "Si continuara con esto...", "");
translation.add(119, "Podría herirla si le doy a entender\nque sospecha de mí.", "");
translation.add(120, "Gracias por contármelo...\nIré a buscarlo mientras todavía esté a tiempo.", "Aeka");
translation.add(121, "Se pone las zapatillas y se levanta.", "");
translation.add(122, "La acompaño hasta la salida.", "");
translation.add(123, "El sol está despuntando\ny el bullicio empieza a inundar la calle.", "");
translation.add(124, "Parece que nadie nos ha pillado.", "");
translation.add(125, "Te veré en la azotea durante el descanso.", "Kouhei");
translation.add(126, "Vale... Iré aunque no encuentre el uniforme.\nAdemás, Kouhei-kun...", "Aeka");
translation.add(127, "Gracias. De verdad.", "Aeka");
translation.add(128, "De... de nada.", "Kouhei");
translation.add(129, "Me sorprende su repentino cambio de actitud.", "");
translation.add(130, "Nos vemos.", "Aeka");
translation.add(131, "Da media vuelta y se aleja.", "");
translation.add(132, "Mm... Sí, adiós. Ten cuidado.", "Kouhei");
translation.add(133, "Vale... Me voy.", "Aeka");
translation.add(134, "Se gira y me dedica una cálida sonrisa.", "");
translation.add(135, "Cuando llego a clase Aeka ya está en su sitio.", "");
translation.add(136, "Por suerte lleva el uniforme.\nParece que se las arregló para encontrarlo.", "");
translation.add(137, "Me alegro, aunque no puedo decírselo.\nAcordamos no dirigirnos la palabra en clase.", "");
translation.add(138, "Buenas.", "Kouhei");
translation.add(139, "Qué tal. ¿Viste el programa de anoche?", "Takeshi");
translation.add(140, "No, estuve ocupado con el trabajo.", "Kouhei");
translation.add(141, "¿Qué? ¡Pero si te dije que lo grabases!", "Misaki");
translation.add(142, "Bla, bla, bla...\nLa mañana transcurre como otra cualquiera.", "");
translation.add(143, "Antoinette y su séquito lanzan\nocasionales miradas de preocupación a Aeka.", "");
translation.add(144, "(Debían querer reírse de ella al aparecer sin\nuniforme. Pero parece que su plan ha fracasado).", "Kouhei");
translation.add(145, "...", "Aeka");
translation.add(146, "Ella las ignora y se limita a atender en clase.", "");
translation.add(147, "Me giro hacia Antoinette para ver su reacción.", "");
translation.add(148, "¿...? Jajá.", "Kyoka");
translation.add(149, "Me guiña un ojo cuando se percata\nde que la estoy observando.", "");
translation.add(150, "(Mierda... No debería haber hecho eso).", "Kouhei");
translation.add(151, "Parece que ha malinterpretado\nmi gesto de inquietud.", "");
translation.add(152, "(Espero que no haya pensado lo que no es...).", "Kouhei");
translation.add(153, "No quiero tener nada que ver con ella desde que\nvolvió con Jingai... Por favor, ni se te ocurra\ncoquetear conmigo.", "");
translation.add(154, "Hora de comer...", "");
translation.add(155, "¡Ven aquí, Shiraki-san!", "Kyoka");
translation.add(156, "¿Vas a comprar algo? Vamos juntas.", "Kyoka");
translation.add(157, "Hoy está muy arrogante.", "");
translation.add(158, "Se suele comportar así desde que volvió Jingai.", "");
translation.add(159, "Eh, pero...", "Aeka");
translation.add(160, "¿Sí? ¿Es que tienes un compromiso? Qué raro...", "Kyoka");
translation.add(161, "Antoinette sabe que Aeka no tiene ninguno.", "");
translation.add(162, "Dicen que las mujeres pueden ser muy retorcidas,\npero esto no tiene nada que ver con el género.\nEs que Antoinette simplemente es así.", "");
translation.add(163, "Bueno, la verdad es que no...", "Aeka");
translation.add(164, "...", "Kouhei");
translation.add(165, "Aeka mira a su alrededor. Nuestros ojos se\nencuentran, pero me limito a levantarme como\nsi no la conociese.", "");
translation.add(166, "¿Serías capaz de hacer otra cosa\naun cuando una amiga te invita?", "Kyoka");
translation.add(167, "Lo siento... Eh... ¿Qué debería comprar?", "Aeka");
translation.add(168, "Saca una hoja de su bloc y empieza a escribir\nlas cosas que las compañeras le van diciendo.", "");
translation.add(169, "La misma cantinela de siempre.\nNo sé si hubiera sido mejor no haberme fijado...", "");
translation.add(170, "(Y esto es solo la punta del iceberg).", "Kouhei");
translation.add(171, "Le doy vueltas al tema\nmientras llego hasta la puerta del aula.", "");
translation.add(172, "¿Tú también vas a comprar comida, Kouhei-kun?\n¿Por qué no nos acompañas?", "Kyoka");
translation.add(173, "Cómo no, antes de que pudiera salir de clase,\nAntoinette tenía que abrir la boca.", "");
translation.add(174, "Nah, es igual.", "Kouhei");
translation.add(175, "Vamos, no seas tímido. ¿Qué quieres?", "Kyoka");
translation.add(176, "Se está haciendo tarde, debería darme prisa.", "Kouhei");
translation.add(177, "Venga, comamos juntos. ¿Porfi?", "Kyoka");
translation.add(178, "¡Iré yo solo a por la mía! ¡Deja de incordiarme!", "Kouhei");
translation.add(179, "Maldita sea, he perdido la compostura.", "");
translation.add(180, "...", "Kouhei");
translation.add(181, "Controlo mi rabia.\nLos ojos de Antoinette se estrechan.", "");
translation.add(182, "Ey, Kouhei-kun...\n¿Leíste el mensaje que te envié ayer?", "Kyoka");
translation.add(183, "¿Eh? ¿Un mensaje?", "Kouhei");
translation.add(184, "Por un momento no comprendo lo que quiere decir.\nEntonces recuerdo el correo\nsobre el uniforme de Aeka.", "");
translation.add(185, "Será mejor que me haga el loco...", "");
translation.add(186, "Lo siento, me he vuelto a olvidar el móvil.\nYa te contestaré cuando llegue a casa.", "Kouhei");
translation.add(187, "Mmmm... ¿En serio?", "Kyoka");
translation.add(188, "¿No quieres nada de la cafetería, Kagami-kun?\nEntonces me voy ya.", "Aeka");
translation.add(189, "¡Y nada de panecillos normales! ¡Date prisa!", "Estudiante E (chica)");
translation.add(190, "¡Te mataremos como te equivoques con el cambio!", "Estudiante F (chica)");
translation.add(191, "Lo siento, Nanjou-san, ¡pero tengo prisa!", "Kouhei");
translation.add(192, "¡Ey! ¡Argh! ¡Kouhei-kun!", "Kyoka");
translation.add(193, "Salgo disparado en busca de comida.", "");
translation.add(194, "Por culpa de Antoinette llego tarde a la guerra.", "");
translation.add(195, "(Por lo menos no me ha pillado en medio...).", "Kouhei");
translation.add(196, "Como acordamos, espero a Aeka\ndelante de la puerta de la azotea.", "");
translation.add(197, "Al cabo de un rato,\nescucho sus pasos en las escaleras.", "");
translation.add(198, "Siento haberte hecho esperar, Kouhei-kun.", "Aeka");
translation.add(199, "Abre la puerta gracias a la llave maestra.", "");
translation.add(200, "Una vez en la azotea, nos sentamos en el banco.", "");
translation.add(201, "Parece que lograste llegar a tiempo esta mañana.", "Kouhei");
translation.add(202, "Sí, gracias a ti.", "Aeka");
translation.add(203, "No tuve ningún problema\ny pude cambiarme tranquilamente.", "Aeka");
translation.add(204, "Me alegro. Pero Antoinette y su banda\nestán ojo avizor; no podemos distraernos.", "Kouhei");
translation.add(205, "Sí... Ah, por cierto.", "Aeka");
translation.add(206, "Aeka pone un par de pasteles sobre el banco.", "");
translation.add(207, "Toma. He comprado uno para ti.", "Aeka");
translation.add(208, "(¡¿Qué?!).", "Kouhei");
translation.add(209, "Es para agradecerte... ¿Eh? ¿Kouhei-kun?", "Aeka");
translation.add(210, "Ja... Jajajá.", "Kouhei");
translation.add(211, "Coloco los míos junto a ella.\nEn realidad, yo también había comprado dos.", "");
translation.add(212, "Por lo visto habíamos pensado lo mismo.", "Kouhei");
translation.add(213, "Vaya... He hecho que vuelvas a preocuparte por mí.", "Aeka");
translation.add(214, "Parece un poco deprimida.", "");
translation.add(215, "No pasa nada.\nVenga, vamos a ver si conseguimos comérnoslo todo.", "Kouhei");
translation.add(216, "Sería una tontería deprimirse\nen plena hora de descanso.", "");
translation.add(217, "Los dos sacamos la comida\nde nuestras mochilas y empezamos a merendar.", "");
translation.add(218, "Ey.", "Aeka");
translation.add(219, "¿Mmm?", "Kouhei");
translation.add(220, "Esta noche te lavaré el uniforme,\nasí que mañana por la mañana te lo podré devolver.", "Aeka");
translation.add(221, "No te preocupes por eso, no tengo que ponérmelo\nhasta pasado mañana, en Educación Física.", "Kouhei");
translation.add(222, "Desvío la mirada en dirección a la puerta.", "");
translation.add(223, "Por cierto, esa puerta...", "Kouhei");
translation.add(224, "¿Qué le pasa?", "Aeka");
translation.add(225, "Deberías cerrarla cuando estés aquí arriba.", "Kouhei");
translation.add(226, "Es evidente que Antoinette se interesará\nmucho más por ella después de lo de hoy.", "");
translation.add(227, "Seguramente intentarán hacerle lo mismo de ayer.", "");
translation.add(228, "¿Eh? Pero entonces tú no podrías subir.", "Aeka");
translation.add(229, "Ya me encargaría de conseguir una de esas llaves.\nHasta entonces, ya pensaremos en algo.", "Kouhei");
translation.add(230, "Vale...", "Aeka");
translation.add(231, "Asiente y se levanta enseguida.", "");
translation.add(232, "Corre hacia la puerta.", "");
translation.add(233, "Y luego regresa.", "");
translation.add(234, "Puedes cerrarla por este lado sin usar una llave.", "Aeka");
translation.add(235, "Vaya... Bien, eso ayudará.", "Kouhei");
translation.add(236, "Eso espero...", "Aeka");
translation.add(237, "C-Claro.", "Kouhei");
translation.add(238, "Habla muy bajito, pero el significado\nde esas palabras me golpean con toda su fuerza.", "");
translation.add(239, "Pero, de algún modo...", "Aeka");
translation.add(240, "¿Sí?", "Kouhei");
translation.add(241, "Al tener los dos la llave de la puerta\nque nos permite estar juntos...", "Aeka");
translation.add(242, "De algún modo, da la sensación\nde que estamos saliendo de verdad.", "Aeka");
translation.add(243, "Parece que eso le da un poco de vergüenza.", "");
translation.add(244, "Oh... ¿Te refieres a que es como tener\nla llave de la puerta de un apartamento?", "Kouhei");
translation.add(245, "Sí...", "Aeka");
translation.add(246, "Tiene razón. Eso es algo\nque puedes esperar de tu amor verdadero.", "");
translation.add(247, "Aunque para mí no hay nada más bonito\nque compartir la llave de una habitación.", "");
translation.add(248, "Me pregunto si esto de salir juntos\nestá haciendo que nos conozcamos mejor.", "");
translation.add(249, "No sé qué responderle.\nSimplemente me como el bocadillo en silencio.", "");
translation.add(250, "Lo muerdo, lo mastico, lo degluto.\nY después bebo un trago de leche.", "");
translation.add(251, "El hormigón de la azotea está seco;\nel cielo está despejado.", "");
translation.add(252, "Oigo varias voces provenientes de abajo...", "");
translation.add(253, "Pero ninguna de ellas me cuenta qué debería decir.", "");
translation.add(254, "Me estiro después de terminarme la leche.", "");
translation.add(255, "Miro a Aeka. Está plegando la primera bolsa\nde papel y metiéndola dentro de la otra.\nParece como si lo hiciera automáticamente.", "");
translation.add(256, "Qué metódica...\n¿Debería preguntar o limitarme a mirar?", "");
translation.add(257, "Eh... ¿Qué estás haciendo?", "Kouhei");
translation.add(258, "¿Mmm? Ah... Es una costumbre que tengo.", "Aeka");
translation.add(259, "Pues resulta que sí lo hacía inconscientemente...\nSe queda contemplándome.", "");
translation.add(260, "¡...!", "Aeka");
translation.add(261, "¿Mm? ¡Ah!", "Kouhei");
translation.add(262, "¡¿Eh?! ¿Qué? Ah, lo siento.", "Kouhei");
translation.add(263, "Oh...", "Aeka");
translation.add(264, "Aeka estaba mirando la parte inferior de mi cara.", "");
translation.add(265, "De repente, se acerca a pocos centímetros.", "");
translation.add(266, "Lamido...", "");
translation.add(267, "Mmm... Dulce.", "Aeka");
translation.add(268, "¡¿Qué?! ¿Azúcar?", "Kouhei");
translation.add(269, "Me ha sorprendido que me lamiera los labios así.\nPero bueno, no ha estado nada mal.", "");
translation.add(270, "Mmm... Solo un poco más...", "Aeka");
translation.add(271, "Lo vuelve a hacer.", "");
translation.add(272, "Vale...", "Kouhei");
translation.add(273, "Está lamiendo hasta el último cristal de azúcar\nde mis labios. He reaccionado sin pensar.", "");
translation.add(274, "Es un beso sutil, pero suficiente para que\nme empiece a hormiguear la zona que ha tocado.", "");
translation.add(275, "¡Ah! Ahh...", "Aeka");
translation.add(276, "Se tensa brevemente, pero entonces...", "");
translation.add(277, "Mmm... Mmm...", "Aeka");
translation.add(278, "La atraigo hacia mí.\nAeka coloca las manos sobre mi pecho.", "");
translation.add(279, "Nos besamos una y otra vez.", "");
translation.add(280, "Mmm... Nnn...", "Aeka");
translation.add(281, "Nos vamos dando picos.\nMe sumerjo en el placer que me proporcionan.", "");
translation.add(282, "Kouhei-kun...", "Aeka");
translation.add(283, "Ansío sus labios como si\nestuviera hambriento de contacto humano.", "");
translation.add(284, "¿Esto es a lo que refieren\ncuando hablan de la fase oral en psicología?", "");
translation.add(285, "Bah, a quién le importa. No pienso analizarlo.", "");
translation.add(286, "Ahh.", "Kouhei");
translation.add(287, "Al igual que Aeka,\nseparo los labios y saco la lengua.", "");
translation.add(288, "La enrollo con la suya,\ninvitándola a entrar en mi boca.", "");
translation.add(289, "Mmmf... Uaa...", "Aeka");
translation.add(290, "Su apasionada respiración me pone a mil.", "");
translation.add(291, "Alcanzo uno de sus pechos con una mano, mientras\nque con la otra le acaricio la espalda por debajo\nde la camisa.", "");
translation.add(292, "¡Ah!", "Aeka");
translation.add(293, "Se sacude con fuerza, recostándose sobre mi torso.", "");
translation.add(294, "¿Estás bien? ¿No te ha gustado?", "Kouhei");
translation.add(295, "Por lo visto he ido demasiado rápido.", "");
translation.add(296, "No es eso... Pero, ahí...", "Aeka");
translation.add(297, "Apunta a una zona situada cerca de la escalera,\ndonde pega la sombra.\nParece que está un poco avergonzada.", "");
translation.add(298, "Sigamos ahí... ¿vale?", "Aeka");
translation.add(299, "¿Por qué?", "Kouhei");
translation.add(300, "Estoy desconcertado.", "");
translation.add(301, "Sé que ha empezado con un simple beso...", "");
translation.add(302, "Y aun así, aquí estoy, dispuesto a por más.", "");
translation.add(303, "Ahí está mi manta...\nY hoy está un poco más calentita...", "Aeka");
translation.add(304, "Entiendo...", "Kouhei");
translation.add(305, "Ahora que lo pienso,\nese lugar es como su escondrijo particular.", "");
translation.add(306, "Nos quedamos callados.\nNo insistiré mientras ella no cambie de parecer.", "");
translation.add(307, "Me quedo mirando cómo estira la manta.", "");
translation.add(308, "Nos sentamos encima cuando termina.", "");
translation.add(309, "...", "Aeka");
translation.add(310, "Sin más preámbulos, reanudamos el besuqueo.", "");
translation.add(311, "Sigo palpando su cuerpo por encima del uniforme.", "");
translation.add(312, "Se acurruca más a mí como respuesta.", "");
translation.add(313, "Pronto se sienta sobre mis rodillas\ny empieza a besarme por toda la cara.", "");
translation.add(314, "Nnn... Ahh...", "Aeka");
translation.add(315, "Se desata la cinta del uniforme.", "");
translation.add(316, "(Se está desvistiendo lentamente... Es increíble).", "Kouhei");
translation.add(317, "Desabrocho la cremallera de su falda y la blusa\ny empiezo a masajear la piel que aparece.", "");
translation.add(318, "Ahh... Mmm...", "Aeka");
translation.add(319, "Su cuerpo se contrae un poco como respuesta.\nDeja de besarme y se quita la ropa\nque se interpone entre mí y su pecho.", "");
translation.add(320, "Junto con la camisa desaparece el sujetador,\nrevelando sus senos.", "");
translation.add(321, "Ahh... Uahh...", "Aeka");
translation.add(322, "Así medio desvestida está más atractiva que nunca.", "");
translation.add(323, "La tentación es tan grande\nque soy incapaz de describirla con palabras.\nMis labios y mis manos se abalanzan sobre su piel.", "");
translation.add(324, "Ahh... Ahh...", "Aeka");
translation.add(325, "Aeka.", "Kouhei");
translation.add(326, "Alzo la mirada y la veo apoyada sobre mis hombros,\ncon los ojos cerrados, dejándose llevar.", "");
translation.add(327, "Qué guapa es.\nTengo la sensación de estar tan absorto en mi\npropio placer que ni siquiera me fijo en ella.", "");
translation.add(328, "(No sé si nos dará tiempo).", "Kouhei");
translation.add(329, "Vuelvo en mí. Parece que Aeka está insatisfecha.", "");
translation.add(330, "La siento en mi regazo.", "");
translation.add(331, "Mmm...", "Aeka");
translation.add(332, "Le acaricio la espalda con delicadeza.", "");
translation.add(333, "Se levanta un poco; me besa las orejas y la nuca.", "");
translation.add(334, "Ey.", "Kouhei");
translation.add(335, "Mmm... Jajá.", "Aeka");
translation.add(336, "Deslizo una mano hasta posarla en su trasero.", "");
translation.add(337, "(Me pregunto quién manda sobre quién).", "Kouhei");
translation.add(338, "Nnn...", "Kouhei");
translation.add(339, "De repente me siento agobiado.", "");
translation.add(340, "Oh, lo siento... ¿Cómo debería hacerlo?", "Aeka");
translation.add(341, "Empieza a quitarme el cinturón.", "");
translation.add(342, "Pero se lo impido.", "");
translation.add(343, "Argh... Y-Ya lo haré yo.", "Kouhei");
translation.add(344, "Intento que no me estorbe mientras\nme bajo los pantalones y los calzoncillos.", "");
translation.add(345, "Oh.", "Aeka");
translation.add(346, "Ahh...", "Kouhei");
translation.add(347, "Nuestros muslos se chocan. La sensación húmeda\nque siento hace que me ponga cachondo otra vez.", "");
translation.add(348, "Es increíble...", "Aeka");
translation.add(349, "Tú también...", "Kouhei");
translation.add(350, "¿Quieres tocar?", "Aeka");
translation.add(351, "Vale...", "Kouhei");
translation.add(352, "No lo entiendo, pero no quiero mirarla a los ojos.\nBeso su ombligo destapado.", "");
translation.add(353, "¡Ahh! Kou... hei... kun...", "Aeka");
translation.add(354, "Le toco un pecho con la mano izquierda.\nLa derecha se encamina hacia su entrepierna.", "");
translation.add(355, "Uhh, nng...", "Aeka");
translation.add(356, "Hasta que por fin la encuentra.\nTiene las bragas pegajosas y muy húmedas.", "");
translation.add(357, "Aeka se apoya sobre mis hombros\ny mueve las caderas como si frotara\nsu entrepierna con mi estómago.", "");
translation.add(358, "Nnnn...", "Kouhei");
translation.add(359, "Le quito la tela que cubre su ingle.", "");
translation.add(360, "El estímulo que provoca su entrepierna desnuda\ncontra mi muslo es suficiente para ponérmelo duro.\nMi pene choca suavemente contra su piel.", "");
translation.add(361, "¡Ah!", "Aeka");
translation.add(362, "Kouhei-kun... Ahh...", "Aeka");
translation.add(363, "Mi miembro empieza a tocar sus pliegues.", "");
translation.add(364, "Ambos sabemos en qué situación nos encontramos.", "");
translation.add(365, "Nnnnn... Nnn... Nnngg...", "Aeka");
translation.add(366, "Comienza a mover las caderas.", "");
translation.add(367, "Aún no hemos hecho nada, pero...", "");
translation.add(368, "Sé que busca sentir mucho más placer,\npero el ambiente sigue un poco cargado.", "");
translation.add(369, "(Mmmm... Bueno, como sea).", "Kouhei");
translation.add(370, "Las chicas también son seres humanos;\nno tiene nada de raro que quieran sentirse bien.", "");
translation.add(371, "Siempre había creído que el deseo sexual\npertenecía a los hombres. Ni se me ocurrió\npensar que las chicas también lo tuvieran.", "");
translation.add(372, "Pero pensaba eso porque mi experiencia\ncon ellas era muy limitada.\nEn realidad, todos queremos lo mismo.", "");
translation.add(373, "Decido aceptar sus insinuaciones por lo que son.", "");
translation.add(374, "Coloca una mano sobre la que yo tenía en su seno.\nAhora ya no se sabe quién está haciendo el masaje.", "");
translation.add(375, "Ahh... Auhh...", "Aeka");
translation.add(376, "Aunque, por otro lado,\nquizá lo haga para confirmarlo.", "");
translation.add(377, "Para confirmar que es una chica\ncapaz de mantener relaciones sexuales.", "");
translation.add(378, "O quizá para comprobar si la trato como una chica.", "");
translation.add(379, "Al fin y al cabo, solo lo estamos haciendo\nporque ella está arriesgando su vida en esto.", "");
translation.add(380, "(Y pese a todo, la acepté).", "Kouhei");
translation.add(381, "¡¡Op!!", "");
translation.add(382, "¡Ahh!", "Aeka");
translation.add(383, "¡Oohh!", "Kouhei");
translation.add(384, "Nuestras membranas se deslizan unas con la otras.\nAmbos sentimos una oleada de gozo.", "");
translation.add(385, "Siento mi glande abriéndose paso\na través de su carne, penetrando en el interior.", "");
translation.add(386, "Aeka está chorreando a mares. Se impulsa\nhacia mí, entrelazándose en torno a mi miembro.", "");
translation.add(387, "Ahn... Kouhei-kun... es genial...", "Aeka");
translation.add(388, "(Maldita sea...\nNos va a costar mucho si continuamos así...).", "Kouhei");
translation.add(389, "Mi pene empieza a palpitar.\nEstá comenzando a gobernar mis sentidos.", "");
translation.add(390, "No sabía que fuera tan incómodo\nquerer correrse pero no poder hacerlo.", "");
translation.add(391, "(¡Tengo que moverme!).", "Kouhei");
translation.add(392, "Aeka me abraza con fuerza, como si estuviera\ncansada. Estaba a punto de cambiar de posición,\npero...", "");
translation.add(393, "Ahhh... No puedo aguantarlo más...", "Aeka");
translation.add(394, "Psss...", "");
translation.add(395, "Se aferra a mí, temblando.", "");
translation.add(396, "¿Qué? Un momento... ¿te acabas de correr?", "Kouhei");
translation.add(397, "Ahh... N-No lo sé...", "Aeka");
translation.add(398, "Lo siento... Déjame estar así un poco.", "Aeka");
translation.add(399, "(Mmm... Para mí que está exhausta...).", "Kouhei");
translation.add(400, "Me quedo con ganas de más,\npero entonces suena el timbre de la quinta hora.", "");
translation.add(401, "No dejo de pensar en clase sobre lo que ha pasado.\nSoy incapaz de concentrarme.", "");
translation.add(402, "Y cuando estaba a punto de irme a casa,\nun obstáculo me bloquea el camino.", "");
translation.add(403, "¿Te encuentras bien, Kouhei-kun?", "Kyoka");
translation.add(404, "¿Eh? ¿A qué te refieres?", "Kouhei");
translation.add(405, "Antoinette se reclina sobre mi escritorio.\nHacía tiempo que no se detenía a hablar conmigo.", "");
translation.add(406, "Sus compinches se arrastran detrás de ella.", "");
translation.add(407, "Hoy no estás muy animado.", "Kyoka");
translation.add(408, "¿Eso crees?", "Kouhei");
translation.add(409, "Pareces alicaído... Estoy muy preocupada por ti.", "Kyoka");
translation.add(410, "Solo estoy un poco cansado, nada más.\nSiento haberte alarmado.", "Kouhei");
translation.add(411, "¡No digas eso! ¡Harás que me preocupe aún más!", "Kyoka");
translation.add(412, "(Eso no, por Dios).", "Kouhei");
translation.add(413, "Por cierto, estás hecho polvo.", "Kyoka");
translation.add(414, "¡Así que vamos,\nhoy tienes que salir a relajarte con nosotras!", "Kyoka");
translation.add(415, "Te va a encantar.", "Kyoka");
translation.add(416, "Acerca su cara a la mía.", "");
translation.add(417, "Porque vendrás... ¿verdad?", "Kyoka");
translation.add(418, "Su sobrecogedora aura\ny su colonia me están intimidando.", "");
translation.add(419, "Lo siento... pero no puedo.", "Kouhei");
translation.add(420, "He estado a punto de perder la compostura.", "");
translation.add(421, "Mi fuerza de voluntad casi ha claudicado.", "");
translation.add(422, "Su actitud arrogante no ha vencido por los pelos.", "");
translation.add(423, "¡Quééé! ¡Qué aburrido! ¡No sales lo suficiente!", "Kyoka");
translation.add(424, "Antoinette empieza a hacer aspavientos.", "");
translation.add(425, "He venido hasta aquí para invitarte,\n¿y ahora me vas a decir que no?", "Kyoka");
translation.add(426, "Lo siento...", "Kouhei");
translation.add(427, "Me quedo mirando al frente.\nNo hay mucho más que pueda hacer frente a ella.", "");
translation.add(428, "Es que... tienes novio. No sería justo para él.", "Kouhei");
translation.add(429, "Eso.", "");
translation.add(430, "Sé que se trata de Jingai, pero una chica\ncon novio no debería flirtear así con otros chicos.", "");
translation.add(431, "¡Qué quieres decir con eso! ¡No tiene gracia!", "Kyoka");
translation.add(432, "Es una razón perfectamente razonable,\naunque ella no lo ve así.", "");
translation.add(433, "Bueeeno, hoy te lo perdono. Vámonos, chicas.", "Kyoka");
translation.add(434, "Ey, ey, ¿adónde vamos, Nanjou-san?", "Estudiante E (chica)");
translation.add(435, "Mmm... Pues no lo sé... Oh.", "Kyoka");
translation.add(436, "Justo cuando está a punto de salir,\nAntoinette me mira por encima del hombro.", "");
translation.add(437, "¡Kouhei-kun! ¡Aunque hoy no puedas\nacompañarnos, pienso pedírtelo otra vez!", "Kyoka");
translation.add(438, "Ja, ja. Gracias...", "Kouhei");
translation.add(439, "(Rezaré para que sea dentro de mucho tiempo).", "Kouhei");
translation.add(440, "Parece que Sayama se ha puesto de mal humor.", "");
translation.add(441, "Normalmente se mantiene neutral respecto a\nAntoinette, pero por lo visto hoy ha decidido\ninvolucrarse.", "");
translation.add(442, "Se acerca rápidamente a mí y empieza a susurrarme.", "");
translation.add(443, "Kagami... ¿es que no te das cuenta\nde lo privilegiado que eres por caerle bien?", "Misaki");
translation.add(444, "Por tu propio bien,\ndeberías ser más cortés con ella.", "Misaki");
translation.add(445, "Sé que solo intenta darme un buen consejo,\npero eso es porque es incapaz de verlo desde\nmi perspectiva.", "");
translation.add(446, "Lamento haberte preocupado tanto.\nYa lo intentaré la próxima vez, ¿vale?", "Kouhei");
translation.add(447, "Hablo con calma, pero mi sonrisa revela\nque en realidad no quiero salir con ella.", "");
translation.add(448, "Bah, haz lo que quieras.", "Misaki");
translation.add(449, "Conoce esa sonrisa y su significado.", "");
translation.add(450, "¡Pero no eches a perder otra oportunidad así!", "Misaki");
translation.add(451, "Vale, vale... Hasta luego.", "Kouhei");
translation.add(452, "Sayama sale del aula después de Antoinette.", "");
translation.add(453, "(Bueno, por fin he llegado a casa).", "Kouhei");
translation.add(454, "Aquella noche...", "");
translation.add(455, "(Oh, no he hablado con Aeka desde el descanso).", "Kouhei");
translation.add(456, "Detengo mis quehaceres. Eso me preocupa.", "");
translation.add(457, "Hemos tenido sexo tres veces en menos de dos días.\nParece que solo estemos saliendo por eso.", "");
translation.add(458, "Y en la tercera vez volvimos a clase en silencio.", "");
translation.add(459, "(Si tanto me preocupa, debería\nhaber subido a la azotea después de clase).", "Kouhei");
translation.add(460, "No creo que haya pasado nada,\npero prefiero asegurarme.", "");
translation.add(461, "¡Decidido!", "Kouhei");
translation.add(462, "Enciendo el ordenador y me pongo a buscar\nun mensaje en concreto del boletín de la escuela.", "");
translation.add(463, "Veamos... Ah, aquí está.", "Kouhei");
translation.add(464, "Le quito el cargador al móvil y marco un número.", "");
translation.add(465, "Eh, hola. Siento llamar tan tarde. Soy Kagami.\n¿Es esta la casa de Shiraki?", "Kouhei");
translation.add(466, "Ah... ¿Kouhei-kun?", "Aeka");
translation.add(467, "¿Aeka? ¿Estás en casa?", "Kouhei");
translation.add(468, "¡Sí, estoy en casa! ¿Y tú?", "Aeka");
translation.add(469, "Parece bastante contenta.", "");
translation.add(470, "Bien, bien, me alegro.", "Kouhei");
translation.add(471, "¿Eh? Te noto un poco raro.", "Aeka");
translation.add(472, "¿De verdad?", "Kouhei");
translation.add(473, "¿Quieres algo?", "Aeka");
translation.add(474, "Y aquí estamos, en un punto muerto.", "");
translation.add(475, "(¿Y ahora qué digo?).", "Kouhei");
translation.add(476, "«Te he llamado para comprobar si seguías viva...».", "");
translation.add(477, "Parecería un acosador.", "");
translation.add(478, "(¿Cómo puedo continuar?).", "Kouhei");
translation.add(479, "De repente, tengo una idea.", "");
translation.add(480, "Aeka, ¿tienes planes para mañana?", "Kouhei");
translation.add(481, "¿Eh? ¿Planes? No, ninguno.", "Aeka");
translation.add(482, "Entonces... ¿qué te parece si salimos por ahí?", "Kouhei");
translation.add(483, "Vaya... He ido directo al grano.\nNoto un escalofrío que me recorre la espalda.", "");
translation.add(484, "Pero el tono de Aeka cambia de forma imprevista.", "");
translation.add(485, "¡¿E-En serio?!", "Aeka");
translation.add(486, "Bueno, sí. Me refiero a tú y yo, nosotros...", "Kouhei");
translation.add(487, "No puedo evitar sentir un poco de vergüenza.", "");
translation.add(488, "S-Sí... Claro...", "Aeka");
translation.add(489, "Prácticamente puedo oír las palpitaciones\nde su corazón a través del auricular.\nEstá mucho más contenta de lo que pensaba.", "");
translation.add(490, "Gracias por invitarme.", "Aeka");
translation.add(491, "Bien. Por lo menos no he quedado como\nun idiota por haber llamado a estas horas.", "");
translation.add(492, "Mañana te llamaré. Dime tu número de teléfono.", "Kouhei");
translation.add(493, "Mmmm...", "Aeka");
translation.add(494, "Ahora parece un poco menos contenta.", "");
translation.add(495, "La verdad es que... no tengo móvil.", "Aeka");
translation.add(496, "¿En serio? Vaya, lo siento.", "Kouhei");
translation.add(497, "Oh, no pasa nada. No me importa.", "Aeka");
translation.add(498, "Como hasta yo tengo móvil,\nhabía supuesto que todos en clase tenían uno.", "");
translation.add(499, "Además, juraría haberla visto darle un número\na otras chicas... Supongo que debí confundirme.", "");
translation.add(500, "Vale. Entonces quedemos en algún sitio.", "Kouhei");
translation.add(501, "Muy bien. ¿A qué hora?", "Aeka");
translation.add(502, "¿Qué tal a las diez bajo el árbol de la estación?", "Kouhei");
translation.add(503, "Oh, vale. Sé dónde está.", "Aeka");
translation.add(504, "Entonces nos vemos mañana.", "Kouhei");
translation.add(505, "Por si acaso le he dicho que anotase mi número.", "");
translation.add(506, "Vale, nos vemos mañana. Buenas noches.", "Aeka");
translation.add(507, "Buenas noches.", "Kouhei");
translation.add(508, "Tengo la sensación de que su voz ha sonado\nextrañamente alegre justo antes de colgar...", "");
translation.add(509, "Me tumbo sobre la cama.\nSus palabras no dejan de resonar en mis oídos.", "");
translation.add(510, "«Gracias por invitarme».", "Aeka");
translation.add(511, "Entonces recuerdo el juego que me dejó Tsubaki.", "");
translation.add(512, "Puedo imaginarme lo que me diría si se lo contara:", "");
translation.add(513, "«¡Perfecto, Kouhei-kun! ¡Has entrado en su rama!».", "Hirofumi");
translation.add(514, "Aeka y yo... ¿de verdad vivimos en la realidad?", "");
translation.add(515, "En todo caso, lo descubriré cuando la vea.", "");
translation.add(516, "Y aquí aparece de nuevo la alucinación auditiva.\nEs como una fuerza que se activa para prevenir\nel aburrimiento.", "");
translation.add(517, "Cachún...\nEl tren me llevará a cualquier sitio que desee.", "");
translation.add(518, "Cachún... Se acabaron las preguntas.\nCachún... Es hora de dormir.", "");

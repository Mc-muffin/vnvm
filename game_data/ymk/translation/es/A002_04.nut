translation.add(0, "Shiraki, ¿puedes venir un momento?", "Profesor");
translation.add(1, "Oh... sí.", "Aeka");
translation.add(2, "Estamos en clase de tutoría.", "");
translation.add(3, "Aeka se levanta y se acerca al profesor.", "");
translation.add(4, "(Es muy guapa, sin duda).", "Kouhei");
translation.add(5, "Salimos tanto el sábado como el domingo.\n¿En qué estuve pensando ayer toda la noche?", "");
translation.add(6, "Hay algo que me da mala espina...\nPese a todo, el día termina sin percances.", "");
translation.add(7, "Te dije que podías coger tres a la vez.", "Takeshi");
translation.add(8, "Claro...", "Kouhei");
translation.add(9, "Le doy la razón\nIogi se acerca a mi pupitre al final de clase.", "");
translation.add(10, "Pero me asustó que la gente me viese allí.", "Takeshi");
translation.add(11, "¿Y dices que fue delante de la estación?", "Kouhei");
translation.add(12, "Parece que lo presenció de primera mano.", "");
translation.add(13, "Le escucho sin prestarle demasiada atención.\nEstoy vigilando a Aeka.", "");
translation.add(14, "Vuelve a su sitio, al parecer un poco confundida.\nEstá rebuscando algo en su mochila.", "");
translation.add(15, "(Algo no va bien...).", "Kouhei");
translation.add(16, "Es como si tuviese que darle algo al profesor,\npero fuese incapaz de encontrarlo.", "");
translation.add(17, "Está examinando cada rincón de la mochila.", "");
translation.add(18, "Puedo verla palidecer desde aquí.", "");
translation.add(19, "Pero bueno, quería probarlos.", "Takeshi");
translation.add(20, "¿Qué compraste al final?", "Kouhei");
translation.add(21, "Bueno... Ya sabes.", "Takeshi");
translation.add(22, "Pone dos dedos en sus labios.", "");
translation.add(23, "¿Se refiere a cigarrillos?", "");
translation.add(24, "Vaya...", "");
translation.add(25, "Asiento vagamente y vuelvo mi mirada hacia Aeka.", "");
translation.add(26, "(Parece que se ha metido en un lío).", "Kouhei");
translation.add(27, "Empieza a revolver su pupitre cuando no encuentra\nlo que busca en la mochila.", "");
translation.add(28, "Caen varios libros al suelo.", "");
translation.add(29, "Está buscando algo desesperadamente.", "");
translation.add(30, "El profesor suspira y se cruza de brazos.", "");
translation.add(31, "Los alumnos que se sientan junto a ella\nla están observando, sonrientes.", "");
translation.add(32, "(¿Cómo pueden reírse\nde alguien que lo está pasando tan mal?).", "Kouhei");
translation.add(33, "Pero... si yo antes me comportaba igual.", "");
translation.add(34, "El séquito de Antoinette rodea a Aeka.", "");
translation.add(35, "Están sonriendo... Qué panda de engreídas.", "");
translation.add(36, "(Tengo un mal presentimiento...).", "Kouhei");
translation.add(37, "Pero a ver, ¿no dicen que se\npueden estar tres días sin dormir?", "Takeshi");
translation.add(38, "¿De verdad?\nDudo que eso sea muy beneficioso para la salud.", "Kouhei");
translation.add(39, "El profesor le está diciendo algo a Aeka.", "");
translation.add(40, "Sí, señor...", "Aeka");
translation.add(41, "Responde con la voz quebrada.", "");
translation.add(42, "Tan pronto como el profesor abandona el aula,\nAeka continúa rebuscando en su pupitre.", "");
translation.add(43, "Te aclara la cabeza. Casi parece como si\npudieses aprobar los exámenes como si nada.", "Takeshi");
translation.add(44, "Vaya... Entonces quizá debería probarlo.", "Kouhei");
translation.add(45, "Está claro que no pienso hacerlo,\npero le digo que sí para tenerlo contento.", "");
translation.add(46, "Shiraki-san.", "Kyoka");
translation.add(47, "*Clanc*", "");
translation.add(48, "Entonces, Antoinette se levanta de su silla.", "");
translation.add(49, "Lleva un sobre en una mano.\nEs demasiado sobrio para ser suyo.", "");
translation.add(50, "Toda la clase enmudece.", "");
translation.add(51, "Aeka está temblando.\nSe gira lentamente hacia Antoinette.", "");
translation.add(52, "¿Es esto lo que estás buscando?", "Kyoka");
translation.add(53, "Ondea el sobre delante de sus ojos.", "");
translation.add(54, "Su voz es amable.", "");
translation.add(55, "Y sus movimientos muy refinados.", "");
translation.add(56, "Ah...", "Aeka");
translation.add(57, "Se le ilumina el rostro\ny extiende la mano hacia el sobre.", "");
translation.add(58, "Pero Antoinette levanta el brazo,\ne impide de forma que pueda cogerlo.", "");
translation.add(59, "Muy bien, te lo devolveré.", "Kyoka");
translation.add(60, "Ah... Gracias.", "Aeka");
translation.add(61, "Espera a que Aeka termine de agradecérselo.", "");
translation.add(62, "Pero lo que había dentro estaba...", "Kyoka");
translation.add(63, "Pone el sobre boca abajo.", "");
translation.add(64, "De él empiezan a revolotear trozos de papel\nque flotan hasta caer al suelo.", "");
translation.add(65, "Ah...", "Aeka");
translation.add(66, "Pedazos de lo que una vez\nfue un billete de diez mil yenes.", "");
translation.add(67, "Y por lo que veo, no había solo uno.", "");
translation.add(68, "Los ojos de Aeka siguen el trayecto de los trozos.", "");
translation.add(69, "No sé, pero parece que está todo. Qué bien, ¿no?", "Kyoka");
translation.add(70, "Aeka se deja caer al suelo.", "");
translation.add(71, "Eres la única que paga su matrícula en efectivo.", "Kyoka");
translation.add(72, "Antoinette finge sorpresa.", "");
translation.add(73, "Aeka no dice ni una palabra.\nEmpieza a recoger los pedazos de los billetes.", "");
translation.add(74, "¿No crees que llevar tanto dinero\na la escuela es demasiado imprudente?", "Kyoka");
translation.add(75, "Baja la vista hacia Aeka.", "");
translation.add(76, "Tiene las manos en la cadera\ny sonríe como si fuera una santa.", "");
translation.add(77, "Parece que está vez te has librado del castigo.", "Kyoka");
translation.add(78, "Aeka no dice nada, aunque debe saberlo.", "");
translation.add(79, "Sabe que ha sido Antoinette,\no al menos una de sus compinches.", "");
translation.add(80, "Recoge los trozos uno por uno.", "");
translation.add(81, "La clase vuelve a la normalidad.", "");
translation.add(82, "Siempre igual, como si nadie pudiera intervenir.", "");
translation.add(83, "«No tengo nada que ver con esto. Estoy a salvo».", "");
translation.add(84, "«No podemos hacer nada cuando se trata de ella».", "");
translation.add(85, "«Esto no me pasará a mí. Estoy a salvo».", "");
translation.add(86, "Todos piensan eso de forma inconsciente.\nY este es el resultado:", "");
translation.add(87, "«Esto no tiene nada que ver conmigo. Lo ignoraré».", "");
translation.add(88, "Es un proceso que se ha dado\ntantas veces que ya es automático.", "");
translation.add(89, "(Nunca me di cuenta... Mentira:\nsí que lo hice, pero no le daba importancia).", "Kouhei");
translation.add(90, "Forma parte del día a día.", "");
translation.add(91, "Nadie se siente culpable,\npor lo que nadie se lo plantea.", "");
translation.add(92, "Lo único que importa es el statu quo.", "");
translation.add(93, "*Suspiro*...", "Kyoka");
translation.add(94, "Antoinette exhala un suspiro teatral.", "");
translation.add(95, "Quizá se haya dado cuenta de que no está\nconsiguiendo el efecto que esperaba.", "");
translation.add(96, "Abandona la clase con su séquito pisándole\nlos talones, como si no hubiese pasado nada.", "");
translation.add(97, "Aeka continúa recogiendo los pedacitos.", "");
translation.add(98, "Los recoge con la mano derecha\ny los pone en la izquierda.", "");
translation.add(99, "Ya nadie pulula a su alrededor.", "");
translation.add(100, "El ambiente de la clase vuelve a la normalidad.", "");
translation.add(101, "Hoy se han pasado de la raya, hasta para ella.", "Takeshi");
translation.add(102, "Sí, eso parece. Ja, ja.", "Kouhei");
translation.add(103, "Debo estar sonriendo.", "");
translation.add(104, "Iogi pone cara de sorpresa,\npero entonces él también sonríe.", "");
translation.add(105, "(¿Quién soy ahora mismo?).", "Kouhei");
translation.add(106, "No sé si soy yo la persona que está aquí, ahora.", "");
translation.add(107, "Siento como si estuviera siguiendo\nla conversación con Iogi desde fuera.", "");
translation.add(108, "Cambiamos de tema en\nla hamburguesería de la estación.", "");
translation.add(109, "¿No crees que este combinado\nde setas y anchoas es muy raro?", "Takeshi");
translation.add(110, "Ja, pues sí. Creo que se han pasado.", "Kouhei");
translation.add(111, "Nos reímos juntos.", "");
translation.add(112, "(¿Quién eres?).", "Kouhei");
translation.add(113, "(¿Quién eres tú, que eres capaz de ignorar de esta\nforma a una persona con la que has hecho el amor?).", "Kouhei");
translation.add(114, "Frrrrrrrrr.", "");
translation.add(115, "El sonido de una cinta de celo al desenrollarse.", "");
translation.add(116, "Nada importante.", "");
translation.add(117, "Noto la fría y particular sensación del vinilo.", "");
translation.add(118, "Lo siento...", "Kouhei");
translation.add(119, "No te preocupes, Kouhei-kun.", "Aeka");
translation.add(120, "Ha conseguido juntar uno de los billetes,\npero todavía quedan muchos trozos sueltos.", "");
translation.add(121, "Pero...", "Kouhei");
translation.add(122, "Con esta, ¿cuántas veces me habré disculpado?", "");
translation.add(123, "Ya te lo he dicho, no te preocupes.", "Aeka");
translation.add(124, "(Es demasiado buena persona...).", "Kouhei");
translation.add(125, "No puedo mirarla a los ojos.", "");
translation.add(126, "No digo nada más; me limito a cortar más celo e\nintentar encontrar el siguiente trozo de billete.", "");
translation.add(127, "Cuando terminé de hablar con Iogi,\nestaba que no podía por ir a verla.", "");
translation.add(128, "Pero no pude porque todos me hubieran visto.\nNo soy más que un cobarde.", "");
translation.add(129, "Recogí mis cosas y salí del aula, como siempre.", "");
translation.add(130, "Subí a la azotea sin pensármelo dos veces.", "");
translation.add(131, "Aunque estuve ojo avizor a\ncualquiera que pudiera observarme.", "");
translation.add(132, "(Esa es otra de mis facetas).", "Kouhei");
translation.add(133, "Solo estoy tratando de deshacerme de la culpa\npor inmiscuirme en los asuntos de los demás.", "");
translation.add(134, "Aeka subió a la azotea prácticamente llorando.", "");
translation.add(135, "Llevaba los trozos en una bolsa de plástico.", "");
translation.add(136, "Sentí un terrible pinchazo de dolor en el pecho.", "");
translation.add(137, "Nos dirigimos a la biblioteca,\nasegurándonos de que nadie nos viera.", "");
translation.add(138, "Entramos por separado.", "");
translation.add(139, "Sabías...", "Aeka");
translation.add(140, "Empieza a hablar.", "");
translation.add(141, "Los costes mensuales de mi matrícula valen más\nde lo que gana mi madre en su trabajo a media\njornada.", "Aeka");
translation.add(142, "Sé que me está hablando a mí porque\nno estoy diciendo nada.", "");
translation.add(143, "Pero sigo sin poder levantar la mirada.", "");
translation.add(144, "Nunca deja de quejarse... como esta mañana.", "Aeka");
translation.add(145, "Miro fijamente mis manos,\nocupadas reconstruyendo un billete.", "");
translation.add(146, "Frrrrrrr. Corto otro trozo de celo.", "");
translation.add(147, "Y aun así... siempre me dado el dinero.\nSignifica mucho para ella.", "Aeka");
translation.add(148, "Pego los trozos al celo.", "");
translation.add(149, "¿No es gracioso? ¿No te hace reír?", "Aeka");
translation.add(150, "No, no es gracioso.", "Kouhei");
translation.add(151, "Respondo con brusquedad. Debo parecer enfadado.", "");
translation.add(152, "Entiendo... No crees que sea gracioso...\nNo... No lo es... Es tal como dices.", "Aeka");
translation.add(153, "Alzo la vista.\nVeo sus dulces ojos cubiertos de lágrimas.", "");
translation.add(154, "Algo que no hizo frente a Antoinette.\nReprimió sus emociones y resistió...", "");
translation.add(155, "(¿Quién ha podido decir estas palabras?).", "Kouhei");
translation.add(156, "(¡¿Cómo he podido decirle eso?!).", "Kouhei");
translation.add(157, "Soy incapaz de responder a mi propia pregunta.", "");
translation.add(158, "Lo... lo siento.", "Kouhei");
translation.add(159, "No... Es tal como dices...", "Aeka");
translation.add(160, "El silencio se abate sobre nosotros.\nContinuamos pegando los pedazos con celo.", "");
translation.add(161, "Frrrrrrrrr. Otro trozo.", "");
translation.add(162, "Esto...", "Aeka");
translation.add(163, "Parece que no le gusta el silencio.", "");
translation.add(164, "Cuando era niña, mi padre olía mucho a tinta.", "Aeka");
translation.add(165, "Vaya...", "Kouhei");
translation.add(166, "Recuerdo lo que ocurrió ayer.", "");
translation.add(167, "El estruendo de la furia irracional y el crujido\nde algo que se encuentra más allá de esa puerta de\nhierro.", "");
translation.add(168, "Entiendo cómo se sienten sus padres.", "");
translation.add(169, "Liberan su frustración en el otro.", "");
translation.add(170, "Y todo para no tener que pagarlo con Aeka.", "");
translation.add(171, "Por aquel entonces,\nmi padre trabajaba en una imprenta...", "Aeka");
translation.add(172, "Oh...", "Kouhei");
translation.add(173, "Por eso asociaba el olor de la tinta con el suyo.", "Aeka");
translation.add(174, "...", "Kouhei");
translation.add(175, "Pero ahora solo apesta a alcohol...", "Aeka");
translation.add(176, "Rememoro el sonido que oí ayer.", "");
translation.add(177, "(No tiene nada que ver conmigo.\nEs un sonido que está entre Aeka y yo).", "Kouhei");
translation.add(178, "Se encuentra en algún lugar de mi nuca.\nLo ignoro como si fuera algo que no me incumbiese.", "");
translation.add(179, "Aeka se ha quedado callada.\nFrrrrrrrrr. Otro trozo de celo.", "");
translation.add(180, "Ha logrado encajar varias piezas y ahora\nestá colocando un trozo de celo sobre ellas.", "");
translation.add(181, "Hasta ahora no era más que un pedazo inútil.", "");
translation.add(182, "Soy demasiado cruel contigo...", "Kouhei");
translation.add(183, "¿Por qué dices eso? Eres muy amable.", "Aeka");
translation.add(184, "La única persona amable que hay aquí eres tú.", "");
translation.add(185, "Pero si se lo dijera me sentiría como un hipócrita.\nCierro la boca y con eso regresa el silencio.", "");
translation.add(186, "(¿Por qué... por qué soy tan patético?).", "Kouhei");
translation.add(187, "Nunca perdonaré a Antoinette por esto.\nAunque si me hubiera enfrentado a ella,\nprobablemente se hubiera reído en mi cara.", "");
translation.add(188, "Ha sido cruel hasta la náusea.\nEstoy furioso por lo que ha hecho.", "");
translation.add(189, "Pero con quien estoy más furioso es conmigo mismo.", "");
translation.add(190, "(Volví a hacer lo mismo).", "Kouhei");
translation.add(191, "(Me quedé en silencio y\nfingí que el tema no iba conmigo).", "Kouhei");
translation.add(192, "Lo que me hace ser como Antoinette.", "");
translation.add(193, "Y eso me saca de quicio...\nEstoy cabreado conmigo mismo.", "");
translation.add(194, "Lo siento... Debería haber hecho algo.", "Kouhei");
translation.add(195, "Oh, no. Para nada.", "Aeka");
translation.add(196, "Habla con suavidad. Corto otro trozo de celo.", "");
translation.add(197, "Porque nadie más hace nada por mí\nen un momento como este...", "Aeka");
translation.add(198, "Plop, plop. Las piezas caen en su sitio.", "");
translation.add(199, "Ahora me estás ayudando, ¿no?", "Aeka");
translation.add(200, "Todos acabarán por darse cuenta\nmientras sigas a mi lado, ¿no crees?", "Aeka");
translation.add(201, "Y, aun así, aquí estás...", "Aeka");
translation.add(202, "Me sonríe.", "");
translation.add(203, "Aeka continúa hablando mientras yo\nsigo uniendo las piezas del billete.", "");
translation.add(204, "Aquí estás, ayudándome...", "Aeka");
translation.add(205, "Frrrrrrrrrr. Más celo.", "");
translation.add(206, "Me basta con esto.\nEstás siendo muy amable conmigo...", "Aeka");
translation.add(207, "No digo nada.", "");
translation.add(208, "Sé que cualquier cosa que pudiera haber dicho\nno hubiera sido más que una excusa de mi propia\ncobardía.", "");
translation.add(209, "Aeka cree que soy una buena persona...\nNo, está haciendo lo imposible por creérselo.", "");
translation.add(210, "Debe sentirse obligada después de\nque me coaccionara de aquella manera.", "");
translation.add(211, "Se convence a sí misma de que cualquiera\nque salga con ella tiene buen corazón.", "");
translation.add(212, "Pero... soy un cobarde.", "");
translation.add(213, "...", "Aeka");
translation.add(214, "...", "Kouhei");
translation.add(215, "Los dos estamos haciendo todo lo posible\npara seguir adelante con la relación.", "");
translation.add(216, "E intentar sanar la parte de Aeka que se ha roto.\nLa parte que yo mismo he quebrado.", "");
translation.add(217, "Nos centramos en arreglar los billetes...", "");
translation.add(218, "Cachún... Cachún... Cachún...", "");
translation.add(219, "La alucinación reaparece\ncuando estoy tumbado sobre la cama.", "");
translation.add(220, "Cachún... Cachún... Cachún...", "");
translation.add(221, "No, creo que la he estado oyendo desde que Aeka\ny yo nos separamos en la biblioteca.", "");
translation.add(222, "Cachún... Cachún... Cachún...", "");
translation.add(223, "Conseguimos encajar todos los billetes,\naunque quedaron muy arrugados y llenos de celo.", "");
translation.add(224, "No puede decirse que tengamos mucha maña en eso.", "");
translation.add(225, "Cachún... Cachún... Cachún...", "");
translation.add(226, "Y aun así, Aeka me lo agradeció tantas veces...", "");
translation.add(227, "Me agradeció que la ayudara.", "");
translation.add(228, "Cachún... Cachún... Cachún...", "");
translation.add(229, "La alucinación turba mis sentidos.", "");
translation.add(230, "(¿Debería encontrarme a gusto en este sonido?).", "Kouhei");
translation.add(231, "Cachún... Cachún... Cachún...", "");
translation.add(232, "Fingía no darme cuenta, pero soy uno de ellos.", "");
translation.add(233, "Cachún... Cachún... Cachún...", "");
translation.add(234, "«Nadie quiere verse acosado».", "Aeka");
translation.add(235, "«Así que no le des tantas vueltas, Kouhei-kun».", "Aeka");
translation.add(236, "«El simple hecho de que estés a mi lado\nya me hace feliz...».", "Aeka");
translation.add(237, "Esas fueron las palabras que murmuró. Parecía tan\nsolitaria... Y aun así sigo culpándome a mí mismo,\nsin hacer nada para remediarlo.", "");
translation.add(238, "Es algo tan perjudicial como ignorarla del todo.", "");
translation.add(239, "Esto no puede seguir así...", "");
translation.add(240, "Cachún... Cachún... Cachún...", "");
translation.add(241, "La alucinación hace caer mi consciencia\nen la oscuridad.", "");
translation.add(242, "(Esto... no puede seguir así...).", "Kouhei");
translation.add(243, "Cachún... Cachún... Cachún...", "");
translation.add(244, "Cachún... Cachún... Cachún...", "");
translation.add(245, "Cachún... Cachún... Cachún...", "");
translation.add(246, "Cachún... Cachún... Cachún...", "");

translation.add(0, "......", "");
translation.add(1, "............", "");
translation.add(-1, "", "");
translation.add(3, "Incluso medio dormido, logro agarrar los palillos\ny llevarme el desayuno a la boca.", "");
translation.add(4, "¿Sabías que mis padres viven en una casa\nde campo con un montón de animales raros?", "Papá");
translation.add(5, "Nunca supimos si las bolas marrones que había\nsobre la mesa eran patatas de verdad...", "Papá");
translation.add(6, "Le doy un mordisco a una patata. Estoy tan\nsobado que no sabría decir ni a qué sabe.", "");
translation.add(7, "Sobre todo porque mamá tenía la costumbre de\nllenarla de excrementos de caballo. Comer en \naquella mesa era como jugar a la ruleta rusa.", "Papá");
translation.add(8, "Dejo de masticar inmediatamente.", "");
translation.add(9, "¡¿Estarás de broma, verdad?!", "");
translation.add(10, "Entonces mi padre muestra su verdadero rostro.", "");
translation.add(11, "Adopta una pose ridícula y salta por la ventana\ncomo una estrella de Hollywood.", "");
translation.add(12, "¡Ahhhhhhh! ¡Agua! ¡Necesito agua!", "Kouhei");
translation.add(13, "Vamos, vamos, vamos, cariño.", "Mamá");
translation.add(14, "Mi madre sonríe y vierte algo de té como\nsi aquello fuera lo más normal del mundo.", "");
translation.add(15, "Corro desesperadamente hacia la cocina\ny me llevo la taza a los labios.", "");
translation.add(16, "Por cierto, yo también crecí en un área rural.", "Mamá");
translation.add(17, "Nunca logramos distinguir cuál era la taza\ndel té y cuál la de la orina del perro.", "Mamá");
translation.add(18, "Cada mañana era como una nueva aventura.", "Mamá");
translation.add(19, "Antes de que pueda reaccionar, el líquido que\nestoy sorbiendo comienza a transformarse en una\nnauseabunda, repugnante y horrible sustancia.", "");
translation.add(20, "¡¡Aghhhhh!!", "Kouhei");
translation.add(21, "Entonces mi madre muestra su verdadero rostro.", "");
translation.add(22, "Desaparece en una nube de humo,\ncomo en una película de ninjas.", "");
translation.add(23, "¡Puaj! ¡Mierda de caballo y meado de perro!", "Kouhei");
translation.add(24, "Me tiro por el suelo, en un desesperado\nintento por limpiarme la boca de todo eso.", "");
translation.add(25, "¡Pero te quieres levantar de una vez!", "Aya");
translation.add(26, "Alguien me despierta.", "");
translation.add(27, "Siento algo frío que choca contra mi cara.\nMe levanto de la silla, sobresaltado.", "");
translation.add(28, "¡Ahh! ¡¿Y ahora qué?! Primero un desayuno\nde mierda, ¿y ahora una mujer de hielo?", "Kouhei");
translation.add(29, "Mmm... ¿Eh?", "Kouhei");
translation.add(30, "Me doy cuenta de que estoy sentado en la mesa\ncon un bol de arroz entre las manos.", "");
translation.add(31, "La luz matinal duele como de costumbre;\nmi cuerpo fatigado se desplaza con torpeza.", "");
translation.add(32, "Aya está apretando una taza contra mi cara.", "");
translation.add(33, "¡No me vengas con esas!", "Aya");
translation.add(34, "¡No te quedes dormido en la mesa!\n¡Me saca de quicio!", "Aya");
translation.add(35, "Ahora comprendo lo que ha pasado...", "");
translation.add(36, "Vaya... Yo no podría llegar a este extremo.", "Mamá");
translation.add(37, "¿Anoche te quedaste despierto hasta tarde?", "Mamá");
translation.add(38, "Mi madre me sonríe dubitativa.\nMi padre se vierte más té en la taza.", "");
translation.add(39, "Ver eso me resulta muy inquietante\ndespués del sueño que acabo de tener.", "");
translation.add(40, "No, nada de eso...", "Kouhei");
translation.add(41, "Te pusiste a navegar por sitios que no debías,\n¿eh? ¡Bah! ¡Es asqueroso!", "Aya");
translation.add(42, "Aya está convencida de que tiene razón.", "");
translation.add(43, "Por desgracia para ella,\nno soy un chico que visite páginas como esas.", "");
translation.add(44, "O al menos, no anoche.", "");
translation.add(45, "Vaya. Y ¿qué tiene Internet de asqueroso, Aya?", "Kouhei");
translation.add(46, "¡Pues que está lleno de pornografía!", "Aya");
translation.add(47, "Hay muchos otros sitios que no tienen.", "Kouhei");
translation.add(48, "De hecho, que pienses en eso cuando alguien\nmenciona la red, dice más de ti que de mí.", "Kouhei");
translation.add(49, "¡¿Qué?!", "Aya");
translation.add(50, "Basta con verle la cara para saber que\nse está enfadando. Qué inocente es.", "");
translation.add(51, "Aprovecharé la ocasión.\nCreo que voy a inventarme otra trola.", "");
translation.add(52, "Por cierto, Aya...", "Kouhei");
translation.add(53, "¡Qué!", "Aya");
translation.add(54, "El navegador registra las páginas que visitas...", "Kouhei");
translation.add(55, "¡¿Y qué?!", "Aya");
translation.add(56, "No me importa que uses el ordenador de alguien\ncuando no está, pero...", "Kouhei");
translation.add(57, "Por lo menos borra el historial cuando termines.", "Kouhei");
translation.add(58, "Sobre todo si has ido a ESE tipo de sitios.", "Kouhei");
translation.add(59, "Me lo estoy inventando todo sobre la marcha.", "");
translation.add(60, "¡Agh!", "Aya");
translation.add(61, "Los ojos de Aya se ensanchan;\nsu cuerpo se tensa.", "");
translation.add(62, "Su reacción es tan exagerada que me sorprende\nincluso a mí.", "");
translation.add(63, "Me inclino hacia ella; se echa hacia atrás.", "");
translation.add(64, "¿Aya-chan? No me digas que...", "Kouhei");
translation.add(65, "N-No. Yo solo busqué información\nen una tienda de ropa interior...", "Aya");
translation.add(66, "Se le ha escapado.\nEn la habitación reina un silencio sepulcral.", "");
translation.add(67, "Todos nos quedamos mirándola.\nAya se da cuenta de lo que acaba de decir\ny se tapa la boca con las manos.", "");
translation.add(68, "No la estamos juzgando;\nsentimos más bien compasión por ella.", "");
translation.add(69, "Mi hermana es más bien delgadita. E incluso\nse podría decir que no termina de crecer. Es\nevidente que se trata de un tema que la irrita.", "");
translation.add(70, "Pero tú NO necesitas ropa interior.\nBueno, salvo esa que ayuda a las niñas a crecer.", "Kouhei");
translation.add(71, "Si lo que quieres es aparentar tener más pecho,\n¿por qué no te pones relleno?", "Mamá");
translation.add(72, "Preocúpate por beber más leche\ny menos por la ropa interior.", "Papá");
translation.add(73, "Pretendíamos ser comprensivos, pero...", "");
translation.add(74, "¡Buuaaaaaaa!", "Aya");
translation.add(75, "Se cubre el pecho con las manos como si\npretendiera taparse sus tetas inexistentes.", "");
translation.add(76, "¡A-Aya!", "Mamá");
translation.add(77, "¡Dejadme en paz!", "Aya");
translation.add(78, "No, no es eso. Es que esta mañana\nse me ha olvidado prepararte la comida.", "Mamá");
translation.add(79, "¡Buuuaaaaaaaaaa!", "Aya");
translation.add(80, "Aya se escabulle hasta el salón.", "");
translation.add(81, "Debe tener muchísima prisa si no ha querido\nterminarse su tortilla favorita.", "Kouhei");
translation.add(82, "No la molestéis. A las chicas de su edad\nles gusta estar solas de vez en cuando.", "Papá");
translation.add(83, "Y dicho esto, mi padre empieza a comerse\nla tortilla de Aya.", "");
translation.add(84, "Ah, sí, escenas así son el pan de cada día.", "");
translation.add(85, "Debería terminar de desayunar\ny prepararme para ir a la escuela.", "");
translation.add(86, "El buen tiempo que hace hoy se ve enturbiado\npor la malhumorada expresión de Aya.", "");
translation.add(87, "Nos vamos los dos juntos, pero no me dice ni mu.\nSe dedica a seguirme en silencio.", "");
translation.add(88, "Ey, Aya, ¿te han llegado ya\ntodo ese montón de bragas que has comprado?", "Kouhei");
translation.add(89, "¡No me hables!", "Aya");
translation.add(90, "Vamos, no te enfades. Pero si vamos\nen la misma dirección. Charlemos un rato.", "Kouhei");
translation.add(91, "¡No me sigas!", "Aya");
translation.add(92, "¡¿Por qué tienes que hablar de temas así\nde buena mañana?!", "Aya");
translation.add(93, "No quiero que me vean con mi hermano.\nMe da vergüenza.", "Aya");
translation.add(94, "¿Mmm? ¿Por qué te tendría que dar vergüenza?", "Kouhei");
translation.add(95, "Ni que arrastrases cadenas o andases desnuda.", "Kouhei");
translation.add(96, "¿Qué tiene de raro?\nSolo somos dos estudiantes que van a la escuela.", "");
translation.add(97, "A veces no tengo ni idea de cómo piensa Aya.", "");
translation.add(98, "Quiero decir que, vale, no tenemos la\nmisma sangre, pero hemos crecido juntos.\nY todavía sigo sin comprenderla.", "");
translation.add(99, "¡No me gusta hablar de esas cosas! No me pasa\nsolo contigo, sino también con papá y mamá...", "Aya");
translation.add(100, "Solo estábamos siendo nosotros mismos.\nTú has sido la única que se ha enfadado.", "Kouhei");
translation.add(101, "Ni siquiera has desayunado... Oh.", "Kouhei");
translation.add(102, "¿Estás a régimen? Deberías pensártelo dos\nveces si no quieres acabar en los huesos.", "Kouhei");
translation.add(103, "Sobre todo por la zona del pecho.", "Kouhei");
translation.add(104, "¡¡Aaaaaarrrrggghhh!!", "Aya");
translation.add(105, "Creía que intentaría darme una patada,\npero en vez de eso se abalanza sobre mí\ndispuesta a pegarme una buena bofetada.", "");
translation.add(106, "Pero tengo previstos todos sus movimientos.", "");
translation.add(107, "No creas que te vas a salir con la tuya.", "Kouhei");
translation.add(108, "No soy tan ingenuo.", "");
translation.add(109, "Evito con elegancia el tortazo de Aya,\ncomo un torero en la plaza...", "");
translation.add(110, "¡¡Ahh!!", "Kouhei");
translation.add(111, "Quién diría que por culpa de mi bonita evasión\nacabaría estampándome contra la pared que hay\njunto a la calzada.", "");
translation.add(112, "Cómo duele... Seguro que no hubiera aprobado\nel examen de cosas guais.", "");
translation.add(113, "Y encima de mí...", "");
translation.add(114, "¡¡Fssssssssss!!", "Gato");
translation.add(115, "Parece que he despertado al gato\nque estaba descansando sobre la pared.", "");
translation.add(116, "Sus maullidos delatan una hostilidad palpable.", "");
translation.add(117, "¡Tranquilo, minino!\nNo ha sido más que un desafortunado accidente.\nNo quería meterme contigo...", "Kouhei");
translation.add(118, "¡Ja! ¿Por qué no te pones a jugar con él?\n¡Sería muy propio de ti!", "Aya");
translation.add(119, "Aya se marcha.\nEl gato continúa con su mirada fija en mí.", "");
translation.add(120, "......", "");
translation.add(121, "............", "");
translation.add(122, "Nada más llegar a la escuela, lo primero que\nhago es lavarme los arañazos que me ha hecho\nel bicho ese en la mano.", "");
translation.add(123, "Al principio no molestaban mucho,\npero ahora ya están empezando a escocer.", "");
translation.add(124, "Buenas, Kouhei.\nSí que has venido pronto... ¿Y esos arañazos?", "Takeshi");
translation.add(125, "Parece que Iogi también ha llegado.", "");
translation.add(126, "El gato ese suele comportarse bastante bien,\naunque parece que hoy estaba de mal humor.", "Kouhei");
translation.add(127, "¿Ya estás peleándote con gatos a estas horas?\n¿Te ha dado muchos puntos de experiencia?\n¿Has podido derrotar a los enemigos finales?", "Takeshi");
translation.add(128, "Bueno, es que mi hermana...", "Kouhei");
translation.add(129, "Ah, tu hermana. Siempre se trata de ella, ¿eh?", "Takeshi");
translation.add(130, "Recuerdo aquello que ocurrió hace un mes.\nTodo fue por su culpa.", "Takeshi");
translation.add(131, "Nuestra conversación se ve interrumpida por un\ngrupo de chicas que cruzan delante de nosotros.\nTodas pertenecen a cursos superiores.", "");
translation.add(132, "Aunque parece que no tienen asuntos\nque tratar en nuestra clase.", "");
translation.add(133, "Sí, todavía nos queda bastante.", "Ayudante");
translation.add(134, "Parece que están comprobando el suministro\nde jabón. Deben ser del consejo estudiantil.", "");
translation.add(135, "De acuerdo. No tenemos nada más que hacer\nen esta planta. Redacta el informe y déjalo\nen la sala de profesores.", "Presidenta del consejo");
translation.add(136, "Esa es la presidenta del consejo estudiantil.", "");
translation.add(137, "¿Qué? ¿No vienes, Kirimiya-senpai?", "Ayudante");
translation.add(138, "Primero tengo que ir a la sala del consejo.", "Presidenta del consejo");
translation.add(139, "Vale...", "Ayudante");
translation.add(140, "Y con todos sus asuntos zanjados,\nla presidenta da media vuelta y se marcha.", "");
translation.add(141, "Al pasar junto a nosotros me ha parecido\nver que nos ha saludado con la cabeza.", "");
translation.add(142, "Parece que no han sido imaginaciones mías,\nIogi también se ha dado cuenta.", "");
translation.add(143, "*suspiro*\nAhora no podré concentrarme en toda la mañana.", "Takeshi");
translation.add(144, "Y tengo una buena excusa.\n¡Kirimiya está buenísima!", "Takeshi");
translation.add(145, "¿A que sí?", "Takeshi");
translation.add(146, "Ah, claro...", "Kouhei");
translation.add(147, "Tengo que admitir que es muy guapa.", "");
translation.add(148, "Las personas como ella existen de verdad...\nYa me entiendes, gente que nace con un don.", "Takeshi");
translation.add(149, "Se llama Kirimiya Mizuki.", "");
translation.add(150, "Aunque suene muy típico, es una chica\nque combina belleza e inteligencia,\nademás de ser muy elegante y refinada.", "");
translation.add(151, "Raro es el estudiante que no la reconozca\naunque sea de vista. También se oyen mil\nchismorreos acerca de ella.", "");
translation.add(152, "Ahora que Kirimiya sabe que existimos,\nquizá convendría que me andara con ojo.", "Takeshi");
translation.add(153, "Aunque dudo mucho que alguna vez se fije en mí.", "Takeshi");
translation.add(154, "Tienes razón...", "Kouhei");
translation.add(155, "Es verdad. Lo más probable es que Kirimiya\nya se haya olvidado de nosotros.", "");
translation.add(156, "¿Qué te pasa? ¡Cuando un bellezón así pasa\na tu lado deberías demostrar más interés!", "Takeshi");
translation.add(157, "Estaba pensando en mis cosas en ese momento.\nIogi debe haberse imaginado que me daba igual.", "");
translation.add(158, "Y no me importa tanto como para explicárselo.\nDejaré que piense lo que quiera.", "");
translation.add(159, "......", "");
translation.add(160, "............", "");
translation.add(161, "Las clases de hoy son un aburrimiento.", "");
translation.add(162, "La única cosa curiosa ha sido cuando el profesor\nha salido a hablar en mitad de la clase.", "");
translation.add(163, "Lo que no le impedía dejar de vigilarnos.", "");
translation.add(164, "Algunos chicos parecen disfrutar de la lección.\nDeben tener algún tipo de relación especial\ncon la asignatura.", "");
translation.add(165, "O quizá lleven encima un amuleto\ncontra el aburrimiento.", "");
translation.add(166, "A mí me deja tan machacado que me deprime\npensar que mañana tendré que volver otra vez.", "");
translation.add(167, "Y entonces...", "Estudiante F (chica)");
translation.add(168, "¿De verdad? ¿No te lo estás inventando?", "Estudiante E (chica)");
translation.add(169, "¡Yo también lo he oído!", "Estudiante C (chica)");
translation.add(170, "Ahora comprendo por qué la gente se entretiene\nen clase con cualquier tontería.", "");
translation.add(171, "Lo entiendo, pero no lo comparto; me pone\nde los nervios. Les echo una furiosa mirada\npara ver si se dan por aludidas.", "");
translation.add(172, "Lo que es realmente triste es que no sea capaz\nde decirles que se callen.", "");
translation.add(173, "Yo a lo mío, escucho al profesor\ncon atención mientras tomo apuntes.", "");
translation.add(174, "No es que me apasione ser tan educado.", "");
translation.add(175, "Pero ese continuo runrún me está molestando un\nmontón, y no quiero que el profesor se enfade.", "");
translation.add(176, "Esa es exactamente la actitud que ha forjado\nde mí la imagen de «estudiante modelo».", "");
translation.add(177, "El profesor sigue a lo suyo, porque sabe que\ncon una advertencia no conseguirá que se callen\ndurante toda la hora.", "");
translation.add(178, "Así que, si queremos desplazar el objeto\ndesde el punto A al punto B, debemos aplicar\nla fórmula que acabo de apuntar...", "Profesor de Física");
translation.add(179, "Aunque me habría gustado ver algo de motivación\npor desplazar el objeto en un primer momento...", "Profesor de Física");
translation.add(180, "Olvidad lo que acabo de decir. Continuemos...", "Profesor de Física");
translation.add(181, "El profesor respira hondo y sigue con la clase.", "");
translation.add(182, "Ha comenzado...", "");
translation.add(183, "Esta fórmula fue descubierta por el matemático\nde origen griego, Xeno.", "Profesor de Física");
translation.add(184, "En aquellos momentos se hallaba\nen un increíble estado de inspiración.", "Profesor de Física");
translation.add(185, "Varios de sus críticos aseguran que al estudiar\nsus trabajos debe tenerse en cuenta su carácter\ny el entorno donde se formó.", "Profesor de Física");
translation.add(186, "Pero, dado que muchos de ellos son muy viejos,\na veces resulta muy complicado preguntarles\nsu opinión...", "Profesor de Física");
translation.add(187, "¡Esto... no! Parece que nos hemos\ndesviado del tema principal. Centrémonos.", "Profesor de Física");
translation.add(188, "El profesor sacude la cabeza en un intento\npor aclararse y evitar hablar sobre asuntos\nque no tienen nada que ver con la materia.", "");
translation.add(189, "Otra vez...", "");
translation.add(190, "Me dijeron que este profesor se especializó en\nliteratura, y que por culpa de un malentendido\nha acabado enseñando Física.", "");
translation.add(191, "Comprendo que sea una situación difícil para él,\npero eso no quiere decir que haga la asignatura\nmenos aburrida.", "");
translation.add(192, "Buf. Pero es que...", "Estudiante F (chica)");
translation.add(193, "... no nos importa nada.", "Estudiante E (chica)");
translation.add(194, "Y esos estúpidos murmullos de fondo no ayudan nada.", "");
translation.add(195, "Me imagino los gritos de esas arpías torturadas\npor la Inquisición española.", "");
translation.add(196, "¡¡Ahhhh!!", "Aeka");
translation.add(197, "Un espantoso alarido rompe el ambiente de sopor\nque el profesor había tejido con tanta destreza.", "");
translation.add(198, "Me despierta de mi letargo tan bruscamente\nque estoy a punto de caerme de la silla.", "");
translation.add(199, "Parece que a los demás les ha pasado lo mismo.", "");
translation.add(200, "Todos se giran hacia el origen del grito.", "");
translation.add(201, "Que resulta ser Shiraki Aeka.", "");
translation.add(202, "Ehh... Ahh...", "Aeka");
translation.add(203, "Tiene la boca tan abierta que parece que\nle va a salir el alma en cualquier momento.\nEs una imagen patética.", "");
translation.add(204, "Auuuhh...", "Aeka");
translation.add(205, "Se queda de pie, rígida como una piedra,\nhasta que pierde altura y...", "");
translation.add(206, "... se derrumba.", "");
translation.add(207, "Ni siquiera intentar apoyarse en el pupitre\npara amortiguar la caída.", "");
translation.add(208, "Silencio.", "");
translation.add(209, "Es un silencio abrumador.\nLos estudiantes se preguntan qué ha pasado\ncon miradas inquisitivas.", "");
translation.add(210, "¿Qué te pasa, Shiraki?", "Profesor de Física");
translation.add(211, "El profesor rompe el mutismo. Habla como si\nestuviera preocupado, pero el tono de su voz\nrevela más enfado que inquietud.", "");
translation.add(212, "Ahh... Auu...", "Aeka");
translation.add(213, "Trata de levantarse.", "");
translation.add(214, "Se ha desplomado de una forma muy extraña,\ncomo si se hubiera golpeado con algo.", "");
translation.add(215, "Intenta incorporarse,\npero los brazos no le responden.", "");
translation.add(216, "Es como si hubiera perdido las fuerzas.", "");
translation.add(217, "Urgh... Ah... Yo...", "Aeka");
translation.add(218, "Uuuhh...", "Aeka");
translation.add(219, "Logra apoyarse sobre una rodilla,\npero pierde el equilibrio y se cae.", "");
translation.add(220, "Quiere decir algo desesperadamente,\npero no logra emitir nada coherente.\nEstá babeando.", "");
translation.add(221, "Pero, ¡¿qué demonios ha pasado?!", "");
translation.add(222, "Shiraki...", "Profesor de Física");
translation.add(223, "¿Eres creyente?", "Profesor de Física");
translation.add(224, "¿Tienes un testamento por si ocurriera lo peor?\n¿Quieres que lo escriba yo por ti?", "Profesor de Física");
translation.add(225, "El profesor por fin se da cuenta de que algo\nva mal, y ahora está sinceramente preocupado.\nEstá intentado ayudarla.", "");
translation.add(226, "Ergh... Aahh... E-Estoy bien...", "Aeka");
translation.add(227, "No, no lo está, para nada.\nLe cuesta muchísimo vocalizar.", "");
translation.add(228, "Aun así, logra arrastrarse hasta apoyarse\nen una pata del pupitre.", "");
translation.add(229, "Y ahí se queda, sentada en el suelo.", "");
translation.add(230, "Está sonriendo.", "");
translation.add(231, "Mira fijamente a ninguna parte. No creo que\nse encuentre en el mismo mundo que nosotros.", "");
translation.add(232, "Es por... por mi gom...", "Aeka");
translation.add(233, "¿Tu «gom»?", "Profesor de Física");
translation.add(234, "Se-se me había ca-caído la... go-go-gom...", "Aeka");
translation.add(235, "Y... est-esta... ba-ba in... inten-in...", "Aeka");
translation.add(236, "Mmm. Eso no nos da mucha información.\nDeberíamos iniciar una larga investigación,\npero antes me gustaría preguntarte algo...", "Profesor de Física");
translation.add(237, "¿Intentas decirnos que se te había caído\ntu goma de borrar?", "Profesor de Física");
translation.add(238, "S-Sí...", "Aeka");
translation.add(239, "Asiente como una marioneta.", "");
translation.add(240, "¿Se... se le había caído la goma de borrar?", "");
translation.add(241, "¿Y ha pegado ese grito solo por eso?", "");
translation.add(242, "Ya, claro. ¿Y qué más?", "");
translation.add(243, "Ah, vale. Entonces no me extraña.", "Estudiante C (chico)");
translation.add(244, "Sí, es un asunto muy serio. Yo he perdido un\nmontón de gomas desde que empezaron las clases.", "Estudiante D (chico)");
translation.add(245, "¡Conozco a una chica que perdió la suya y su\nmadre le dio una buena zurra con un soldador!", "Estudiante F (chica)");
translation.add(246, "Una antigua leyenda capadocia dice que\nsi dejas caer una goma de borrar en clase,\nsufrirás una muerte prematura.", "Estudiante B (chica)");
translation.add(247, "Nunca me hubiera imaginado que perder una goma\nfuera un hecho tan dramático.", "");
translation.add(248, "La tensión se desvanece,\ndando paso al tumulto habitual.", "");
translation.add(249, "Shiraki se sostiene vacilante sobre sus pies,\npero acaba por perder de nuevo el equilibrio.", "");
translation.add(250, "Una de las piernas forma una curva en el aire,\ndejando sus bragas al descubierto. Pone una mano\nsobre el pupitre, tirándolo también al suelo.", "");
translation.add(251, "Se escucha un ruido sordo.\nSe ha golpeado la parte trasera de la cabeza.", "");
translation.add(252, "¡Au! Duele...", "Aeka");
translation.add(253, "Todo lo que había en la mesa cae desparramado\npor el suelo, retrasando aún más la lección.", "");
translation.add(254, "Ha terminado la primera hora;\npor fin tengo algo de tiempo para descansar.", "");
translation.add(255, "De todas formas,\nnadie gritaría así por una goma de borrar.", "");
translation.add(256, "Me giro en su dirección.\nParece que Iogi está pensando lo mismo.", "");
translation.add(257, "Esto no tiene sentido, ¿eh?", "Takeshi");
translation.add(258, "¿A que no? ¿Qué ha pasado exactamente, entonces?", "Kouhei");
translation.add(259, "Creo que vi algo antes de que Shiraki gritase.", "Takeshi");
translation.add(260, "¿El qué?", "Kouhei");
translation.add(261, "La chica que había detrás de ella\nla golpeó con una especie de bolígrafo.", "Takeshi");
translation.add(262, "Aun así, su reacción fue demasiado exagerada.", "Takeshi");
translation.add(263, "¿Un bolígrafo?...", "");
translation.add(264, "Ahora lo recuerdo.", "");
translation.add(265, "Una vez entré en una tienda\nque vendía aparatos de autodefensa.", "");
translation.add(266, "Había una cosa llamada «taser»,\ny era parecida a un bolígrafo grande.", "");
translation.add(267, "Ponía que podía generar un pulso eléctrico\nde hasta 50 000 voltios.", "");
translation.add(268, "Miro de reojo a Shiraki.\n¿Y si le han atacado con algo así?", "");
translation.add(269, "No pienso comentar esto a nadie. Aunque incluso\nyo, que me da igual lo que les pase a los demás,\nme doy cuenta de lo mucho que está sufriendo.", "");
translation.add(270, "Es demasiado ingenua y despreocupada.", "");
translation.add(271, "Desde que la transfirieron\nsiempre le ha costado muchísimo hacer amigos.", "");
translation.add(272, "Las chicas así suelen llevar una vida\nmuy dura en el colegio.", "");
translation.add(273, "Enseguida perdí el interés en ella\ncuando me di cuenta de eso.", "");
translation.add(274, "Si alguien de la clase intimase demasiado con\nShiraki, también acabaría en el punto de mira.", "");
translation.add(275, "La opción más sensata es no involucrarse.", "");
translation.add(276, "Aparto la mirada de ella.", "");
translation.add(277, "Cachún... Cachún...", "");
translation.add(278, "Mi mente olvida por completo a Shiraki.", "");
translation.add(279, "Cachún... Cachún...", "");
translation.add(280, "Ya vuelve otra vez esa alucinación...\nNo consigo que se vaya por mucho que lo intente.", "");
translation.add(281, "Apareció más o menos durante la cuarta hora.", "");
translation.add(282, "Ey, Kouhei, ¿te pasa algo?\n¿No sales a comprar nada?", "Kyoka");
translation.add(283, "Estoy sentado en mi pupitre, absorto en mis\npensamientos, cuando Antoinette se acerca a mí.", "");
translation.add(284, "Bah, no llegaría a tiempo.", "Kouhei");
translation.add(285, "El ruido del vaivén del tren me ha dejado tan\ndormido que ya no tendría sentido participar\nen aquella matanza.", "");
translation.add(286, "Así que he decidido olvidarme de la comida.\nEsperaré a que la tormenta amaine\ny pediré el menú normal y corriente.", "");
translation.add(287, "Y parece que Antoinette se ha presentado\nante mí solo para despertarme de mi ensueño.", "");
translation.add(288, "Hoy pediré el menú normal.", "Kouhei");
translation.add(289, "¿Mmm? Entonces...", "Kyoka");
translation.add(290, "Antoinette pone una mano sobre mi pupitre\ny se inclina hacia mí.", "");
translation.add(291, "Un aroma a rosas inunda mis sentidos.", "");
translation.add(292, "¿Qué te parece si voy yo a comprártelo?", "Kyoka");
translation.add(293, "De todas formas tenemos que bajar;\nno me importa hacerlo.", "Kyoka");
translation.add(294, "Su perfume es muy femenino, pero es demasiado\ndulce para un chico hambriento como yo.", "");
translation.add(295, "Te estás arrimando demasiado, Nanjou.", "");
translation.add(296, "Nah, gracias por la oferta, pero paso.", "Kouhei");
translation.add(297, "¡De verdad que no me importa!\nHoy por ti, mañana por mí, y todo eso.", "Kyoka");
translation.add(298, "Debo admitir que es una oferta muy tentadora.", "");
translation.add(299, "Pero hay algo que me preocupa.", "");
translation.add(300, "No es que no me guste hablar\ncon tanta familiaridad con Kyoka.", "");
translation.add(301, "Es que no logro comprender cómo\nhe conseguido que me preste tanta atención.", "");
translation.add(302, "¿Por qué no? No te cobraría comisión.", "Kyoka");
translation.add(303, "Y es más práctico si solo baja una persona.", "Kyoka");
translation.add(304, "Además, creo que tiene novio.", "");
translation.add(305, "Actúa conmigo con demasiada confianza\npara ser un simple compañero de clase.", "");
translation.add(306, "Tampoco debería mezclarme con ella...", "");
translation.add(307, "Y además DICE que bajará a comprar la comida,\npero no me la imagino participando en ese jaleo.", "");
translation.add(308, "Seguro que tiene la intención\nde enviar a otra persona.", "");
translation.add(309, "Por ejemplo...", "");
translation.add(310, "Veamos, uno de salmón, otro de lomo y...\n¿un bocadillo de jamón?", "Aeka");
translation.add(311, "Shiraki está anotando los pedidos\nde las chicas que la rodean.", "");
translation.add(312, "Y apostaría cualquier cosa\na que Antoinette planeaba hacer lo mismo.", "");
translation.add(313, "No creo que mi pedido representase mucha carga\na la persona encargada de ir a buscarlos.", "");
translation.add(314, "Pero me sentiría mal si lo hiciera.", "");
translation.add(315, "Eres muy amable, Nanjou-san,\npero hoy prefiero tomar fideos.", "Kouhei");
translation.add(316, "Quizá en otra ocasión, ¿vale?", "Kouhei");
translation.add(317, "Como quieras. Ya comeremos juntos\ncuando vayamos los dos a comprar algo.", "Kyoka");
translation.add(318, "Asiento. Algo en mi interior me dice que\nojalá no tenga que aceptar nunca esa oferta.", "");
translation.add(319, "Se va.\nNo quiero verla otra vez, así que me levanto.", "");
translation.add(320, "A ver, otra vez: uno de salmón, otro de lomo...", "Aeka");
translation.add(321, "Ah, y a mí uno al estilo de Hiroshima.", "Estudiante F (chica)");
translation.add(322, "Guau, hoy se están pasando de la raya.", "");
translation.add(323, "Uh, er, uno de salmón al estilo de Hiroshima...", "Aeka");
translation.add(324, "Contamos contigo, Shiraki-san.", "Estudiante F (chica)");
translation.add(325, "Se pone a garabatear otra vez en el papel.\nNo deja de sonreír pese a su evidente estado\nde confusión y a las constantes interrupciones.", "");
translation.add(326, "Está sonriendo como si estuviese rodeada\npor sus mejores amigas, y eso hace que me dé\ntodavía más pena.", "");
translation.add(327, "En cuanto terminan de pedir lo que quieren,\ntodas las chicas dejan de prestarle atención\ny se marchan.", "");
translation.add(328, "......", "");
translation.add(329, "............", "");
translation.add(330, "............", "");
translation.add(331, "......", "");
translation.add(332, "Sigue estando petado...", "");
translation.add(333, "La cafetería sigue llenísima de estudiantes.\nNo queda ni un asiento libre en todo el local.", "");
translation.add(334, "Espero que se vacíe conforme la gente termine\nde comer. Me voy a una máquina expendedora y\nme compro un zumo.", "");
translation.add(335, "(Quizás debería pedirle a mamá\nque me prepare alguna vez la merienda...).", "Kouhei");
translation.add(336, "Nunca lo he hecho; me sabría muy mal por ella.\nYa tiene bastante con preparar dos comidas\ncada mañana.", "");
translation.add(337, "Aunque debería pensármelo si de ello depende\nel comer o no comer...", "");
translation.add(338, "Observo a la horda famélica\nque hay delante del vendedor de bocadillos.", "");
translation.add(339, "Esto... ¡Por favor!", "Aeka");
translation.add(340, "Eh, tres bocadillos de jamón, y... ¡Ah!", "Aeka");
translation.add(341, "Escucho su lastimera voz siendo engullida\npor la turba furibunda.", "");
translation.add(342, "Menuda es Shiraki.\nMe pongo a escudriñar entre el muro de carne...", "");
translation.add(343, "Guau, a este paso nos saldría más a cuenta ir a\ncomprar la comida en la tienda que hay cerca de\nde la escuela.", "Estudiante B (chico)");
translation.add(344, "Y no sé por qué, todos siguen empeñados\nen venir aquí. ¿Qué sentido tiene esto?", "Estudiante B (chico)");
translation.add(345, "¿Eh? ¿Ya vuelven?", "");
translation.add(346, "¡Hambre, conviértete en la fuerza\nque me impulsará hacia la victoria!", "Estudiante A (chica)");
translation.add(347, "¡Devoraré a los que me rodean como un tigre\nante su presa!", "Estudiante B (chica)");
translation.add(348, "¡Ahh!", "Aeka");
translation.add(349, "Shiraki sale despedida de la multitud\ncomo el corcho de una botella de champán.", "");
translation.add(350, "Q-Quiero uno de salmón al estilo de Hiroshima,\ntres de jamón...", "Aeka");
translation.add(351, "¡Con mi súper poder aplastaré a cualquiera que\nse interponga en mi camino, aunque haya salvado\na un cachorrito en una noche de tormenta!", "Estudiante A (chico)");
translation.add(352, "¡¡Así!! ¡¡Ahhhhhh!!", "Estudiante A (chico)");
translation.add(353, "¡Ayy!", "Aeka");
translation.add(354, "Intenta penetrar en la horda, pero es repelida\npor el gigantesco trasero de un estudiante.", "");
translation.add(355, "¡Ajá, un enemigo!\n¡Todos los que se acercan a mí son mis enemigos!", "Estudiante A (chica)");
translation.add(356, "¡Agh!", "Aeka");
translation.add(357, "Una estudiante golpea a Aeka con un movimiento\nsacado de algún juego de lucha.\nLa pobre da vueltas en dirección a la pared.", "");
translation.add(358, "Ahh...", "Aeka");
translation.add(359, "Se precipita contra el muro, aplastada como una\nmariposa en la exposición de un coleccionista.\nLentamente se va despegando hasta caer al suelo.", "");
translation.add(360, "Vaya, vaya...", "");
translation.add(361, "No la tratan mejor fuera de clase.", "");
translation.add(362, "Me acerco a ella.", "");
translation.add(363, "Shiraki-san, aún no lo has comprado todo, ¿no?", "Kouhei");
translation.add(364, "¿Qué se suponía que tenía que traer? Veamos...", "");
translation.add(365, "Un bocadillo de salmón al estilo de Hiroshima,\nuno de lomo y tres de jamón.", "");
translation.add(366, "¿A quién se le ocurre pedir todo eso?", "Estudiante C (chico)");
translation.add(367, "Un chico me mira con incredulidad\nantes de continuar con la refriega.\nParece que no me equivocaba.", "");
translation.add(368, "Recuerdo los pedidos de aquellas chicas.", "");
translation.add(369, "¿Debería decirle que no lo tiene todo?", "");

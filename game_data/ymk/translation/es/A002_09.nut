translation.add(0, "No sé si es porque he dormido muy bien,\npero parece que me he recuperado del todo.", "");
translation.add(1, "No he sabido nada de Aeka desde que me apalizaron.", "");
translation.add(-1, "", "");
translation.add(3, "Ese grito, justo antes de quedarme inconsciente...", "");
translation.add(4, "(Estoy seguro de que estaba muy preocupada por mí).", "Kouhei");
translation.add(5, "Decido llamarla al móvil.", "");
translation.add(6, "(Por favor, cógelo...).", "Kouhei");
translation.add(7, "«El teléfono al que llama está\ndesconectado o fuera de cobertura».", "Voz");
translation.add(8, "Supongo que lo tendrá apagado.", "");
translation.add(9, "(No me queda otra opción que ir a buscarla).", "Kouhei");
translation.add(10, "Me pongo la ropa y recojo mis útiles.", "");
translation.add(11, "Encuentro a mamá en la sala de espera.", "");
translation.add(12, "Vaya, ¿ya te has levantado?", "Mamá");
translation.add(13, "Así es. ¿Me estabas esperando?", "Kouhei");
translation.add(14, "Sí, aunque primero tengo que pagar la factura.\n¿Puedes esperarme en el coche?", "Mamá");
translation.add(15, "Claro.", "Kouhei");
translation.add(16, "De camino a casa nos detenemos a comprar un poco.\nLlegamos algo tarde.", "");
translation.add(17, "Algo está haciendo ruido...", "");
translation.add(18, "¿Eh?", "Kouhei");
translation.add(19, "(¡Es Aeka!).", "Kouhei");
translation.add(20, "Lo cojo y respondo la llamada.", "");
translation.add(21, "¿Hola? ¡¿Aeka?!", "Kouhei");
translation.add(22, "Creo que deben haberme oído\nhasta en la otra punta de la ciudad.", "");
translation.add(23, "«Kouhei-kun... ¿estás en casa?».", "Aeka");
translation.add(24, "Sí, acabo de llegar.", "Kouhei");
translation.add(25, "He salido esta tarde del hospital,\npero no logrado localizarte. ¡Lo siento mucho!", "Kouhei");
translation.add(26, "Tarda un rato en responder...", "");
translation.add(27, "«Kouhei-kun... *Snif* Lo siento tanto...».", "Aeka");
translation.add(28, "Sus sollozantes palabras me ensombrecen el rostro.", "");
translation.add(29, "Quizá porque me da la sensación\nde que ha terminado por rendirse.", "");
translation.add(30, "No llores, Aeka. Estoy bien.", "Kouhei");
translation.add(31, "«No... Si no te hubiera pedido algo así...».", "Aeka");
translation.add(32, "¿De qué estás hablando?\n¡Voy a verte ahora mismo! ¿Dónde estás?", "Kouhei");
translation.add(33, "«*Sollozo* No vale la pena seguir...\n¿Por qué no lo dejamos de una vez?».", "Aeka");
translation.add(34, "¡Aeka! ¿Me estás escuchando? ¡Eh!", "Kouhei");
translation.add(35, "«Por favor, mira tu buzón...».", "Aeka");
translation.add(36, "¿Cómo?", "Kouhei");
translation.add(37, "«Lo siento, Kouhei-kun... Adiós...».", "Aeka");
translation.add(38, "Bip, bip, bip...", "");
translation.add(39, "(¡Maldita sea, tengo que darme prisa!).", "Kouhei");
translation.add(40, "Esa despedida no ha sonado a una separación\ntemporal, sino a una para toda la eternidad.", "");
translation.add(41, "¡No puedo aceptarlo!", "");
translation.add(42, "Recojo lo que considero más necesario\ny bajo corriendo las escaleras.", "");
translation.add(43, "¡Kouhei! Qué crees que estás...", "Aya");
translation.add(44, "Aya enmudece cuando ve mi expresión desesperada.", "");
translation.add(45, "¡Salgo un momento! ¡Volveré enseguida!", "Kouhei");
translation.add(46, "Me apresuro en ponerme las zapatillas.", "");
translation.add(47, "«Mira tu buzón...».", "Aeka");
translation.add(48, "Abro el buzón. Dentro hay un sobre azul.", "");
translation.add(49, "(¡Era esto a lo que se refería!).", "Kouhei");
translation.add(50, "Cojo el sobre y salgo corriendo hacia la escuela.", "");
translation.add(51, "(¡Solo hay un sitio donde puede estar!).", "Kouhei");
translation.add(52, "Dentro del sobre hay una llave y una pequeña nota.", "");
translation.add(53, "La llave de la puerta de la azotea...", "");
translation.add(54, "«Para Kagami Kouhei.", "");
translation.add(55, "Gracias por protegerme.", "");
translation.add(56, "Pero no quiero verte sufrir nunca más.", "");
translation.add(57, "No quiero que seas infeliz por mi culpa.", "");
translation.add(58, "Por eso debemos separarnos.", "");
translation.add(59, "Aunque te quiero con toda mi alma.\nMe alegro tanto de haberme enamorado de ti.", "");
translation.add(60, "Creía que estaba sola,\npero me demostraste que no era verdad.", "");
translation.add(61, "Si no te hubiera conocido, seguramente\nhubiera olvidado lo dolorosa que es la soledad.", "");
translation.add(62, "Gracias por haberme conocido. Gracias. Y adiós.", "");
translation.add(63, "P.D.: Al final olvidé hacerte\nuna copia de la llave de la puerta.", "");
translation.add(64, "Te daré la mía para que la uses cuando quieras.\nSiento no poder hacer nada más por ti».", "");
translation.add(65, "(Pero cómo... cómo puedes ser tan egoísta...).", "Kouhei");
translation.add(66, "Querría gritar de frustración.", "");
translation.add(67, "Aeka no comprende nada de nada.", "");
translation.add(68, "¡No decidas ponerle fin tú sola!", "");
translation.add(69, "Lo que más me dolería sería que estuvieras\nen un lugar donde no pudiera alcanzarte.", "");
translation.add(70, "El hecho de que me haya dado su llave...", "");
translation.add(71, "Significa que ha dejado de considerar\nla escuela como parte de ella.", "");
translation.add(72, "Esta decisión me da muy mala espina...", "");
translation.add(73, "(¡No dejaré que cometas una estupidez! ¡Lo juro!).", "Kouhei");
translation.add(74, "(Y aquí estoy...).", "Kouhei");
translation.add(75, "Por fin he llegado a la escuela.", "");
translation.add(76, "En la oscuridad se perfila como una inmensa sombra.", "");
translation.add(77, "Por la noche, la sensación de vacuidad\ndel edificio es absolutamente espeluznante.", "");
translation.add(78, "Aeka... Por favor, que no te haya pasado nada...", "Kouhei");
translation.add(79, "Estoy convencido de que está en la azotea.", "");
translation.add(80, "No sé cómo habrá logrado subir sin la llave.", "");
translation.add(81, "Pero sé que está allí.", "");
translation.add(82, "(Tengo que entrar...).", "Kouhei");
translation.add(83, "Intento acceder por puertas y ventanas,\npero todas están cerradas a cal y canto.", "");
translation.add(84, "(¡Precisamente ahora!).", "Kouhei");
translation.add(85, "¡¿Es que solo estamos nosotros dos?!", "");
translation.add(86, "(¿Qué se supone que voy a hacer?).", "Kouhei");

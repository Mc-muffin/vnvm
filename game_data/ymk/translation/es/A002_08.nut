translation.add(0, "Eh, caraculo.\n¿Tú y Shiraki os lo estáis montando o qué?", "Gaito");
translation.add(1, "Aún puede olerse algo del hedor de ayer.", "");
translation.add(2, "Durante el descanso, el novio de Antoinette\nentra en clase y se acerca a mí.", "");
translation.add(3, "Suponía que ya se había enterado,\npero eso no lo hace menos molesto.", "");
translation.add(4, "Si eso es lo crees, ¿por qué no lo dejamos ahí?", "Kouhei");
translation.add(5, "No me gusta. ¿Qué tienes tú de especial?", "Gaito");
translation.add(6, "¡...!", "Kouhei");
translation.add(7, "Antes de que pueda reaccionar,\nme agarra del cuello y me levanta.", "");
translation.add(8, "¡Detente!", "Aeka");
translation.add(9, "¡No te metas en esto!", "Estudiante E (chica)");
translation.add(10, "¡No! ¡Déjale ir!", "Aeka");
translation.add(11, "Aeka intenta acercarse a mí,\npero las chicas evitan que lo haga.", "");
translation.add(12, "¡No me gusta nada! ¡Eh, chicos, venid aquí!", "Gaito");
translation.add(13, "Me da media vuelta y me empuja.", "");
translation.add(14, "¡Ah!", "Kouhei");
translation.add(15, "Nadie tiene intención de cogerme,\nasí que caigo de bruces al suelo.", "");
translation.add(16, "¡Bien hecho!", "Estudiante E (chico)");
translation.add(17, "Vamos, levántate, Kagami.", "Estudiante F (chico)");
translation.add(18, "Los otros chicos me retienen por los brazos.", "");
translation.add(19, "Uhh...", "Kouhei");
translation.add(20, "Bien, ¡ahora lanzadlo hacia aquí!", "Gaito");
translation.add(21, "Se ha colocado en posición de lucha.", "");
translation.add(22, "(¿A qué viene esa estúpida postura?).", "Kouhei");
translation.add(23, "Parece como si la hubiese aprendido\npor experiencia...", "");
translation.add(24, "¡Vale!", "Estudiante E (chico)");
translation.add(25, "¡Vamos, caraculo!", "Gaito");
translation.add(26, "¡Y... ahora!", "Estudiante E,F (chicos)");
translation.add(27, "Me obligan a encararme hacia él y me empujan.", "");
translation.add(28, "¡¡...!!", "Kouhei");
translation.add(29, "¡Ja!", "Gaito");
translation.add(30, "Un instante después,\nsiento su puño golpeándome el abdomen.", "");
translation.add(31, "Por un momento todo se torna oscuro.\nEntonces, el techo de la clase se aparece ante mí.", "");
translation.add(32, "*¡Cof, cof!*", "Kouhei");
translation.add(33, "¡Increíble! ¡Ni lo he visto!", "Estudiante (chico) F");
translation.add(34, "Vale, ¡una vez más!", "Gaito");
translation.add(35, "¡Eh! ¡Despierta!", "Estudiante E (chico)");
translation.add(36, "Ahh... Dejadlo ya...", "Kouhei");
translation.add(37, "Por supuesto, no van a hacerlo.", "");
translation.add(38, "Los chicos me levantan otra vez del suelo.\nLos espectadores vitorean con entusiasmo.", "");
translation.add(39, "¡Dale bien fuerte, Gaito!", "Kyoka");
translation.add(40, "¡Lo sé, lo sé!", "Gaito");
translation.add(41, "¡Gaito-kun! Una reata, ¡hazle una reata!", "Estudiante D (chico)");
translation.add(42, "¡Eh, qué buena idea! ¡Vamos!", "Gaito");
translation.add(43, "¡De acuerdo! ¿Preparado?", "Estudiante E (chico)");
translation.add(44, "¡Listo cuando tú lo estés!", "Gaito");
translation.add(45, "Jingai se echa un poco hacia atrás\ny empieza a balancear el brazo izquierdo.", "");
translation.add(46, "(¿Qué? ¿Una reata? ¿Qué leches es eso?).", "Kouhei");
translation.add(47, "¡Ohhhh!", "Estudiante E (chico)");
translation.add(48, "¡Ahh!", "Kouhei");
translation.add(49, "¡¡Ahora!!", "Estudiantes E, F (chicos)");
translation.add(50, "Una vez más, me giran y me lanzan hacia Jingai.", "");
translation.add(51, "Ah... Ah...", "Kouhei");
translation.add(52, "De nuevo, vuelo por los aires...", "");
translation.add(53, "El codo de Jingai conecta\nentre mi barbilla y el cuello.", "");
translation.add(54, "¡Quiaaa!", "Gaito");
translation.add(55, "Creo que oigo romperse algo dentro de mi cuerpo.", "");
translation.add(56, "(Joder, esto no va bien...).", "Kouhei");
translation.add(57, "Vuelvo a caerme al suelo.", "");
translation.add(58, "(Esto puede acabar mal... muy mal...).", "Kouhei");
translation.add(59, "El impacto me lanza contra varios pupitres,\naunque apenas siento nada.", "");
translation.add(60, "Oigo el griterío del público cada vez más lejano.", "");
translation.add(61, "¡Perfecto!", "Gaito");
translation.add(62, "¡Muy bien! ¡No tienes rival!", "Estudiante (chico) E");
translation.add(63, "...", "Kouhei");
translation.add(64, "Las voces se desvanecen en la nada...", "");
translation.add(65, ".........", "");
translation.add(66, "Silencio.", "");
translation.add(67, "¡Kouhei-kun! ¡¡Noooooooo!!", "Aeka");
translation.add(68, "¿Esa es... la voz de Aeka?", "");
translation.add(69, "El alarido atraviesa el silencio sepulcral\nque había engullido mis sentidos.", "");
translation.add(70, "Ah... Es demasiado fuerte. Estoy seguro de que...", "");
translation.add(71, "Aprieta los puños con mucha, mucha más fuerza.", "Mujer");
translation.add(72, "No tiene sentido decirme eso\ncuando me he quedado sin fuerzas.", "");
translation.add(73, "...", "Kouhei");
translation.add(74, "Poco a poco empiezan a movérseme los dedos.", "");
translation.add(75, "...", "Kouhei");
translation.add(76, "¿Eh?", "Estudiante F (chico)");
translation.add(77, "¿A qué viene esto? ¡Menudo subnormal!", "Gaito");
translation.add(78, "¡Este lugar no es seguro, llevémoslo a otro sitio!", "Estudiante E (chico)");
translation.add(79, "¡Kouhei-kun! ¡Detente!", "Aeka");
translation.add(80, "(¿Aeka?...).", "Kouhei");
translation.add(81, "Oigo a Aeka gritar mi nombre. Entonces,\ncomo si alguien hubiera pulsado un botón,\npierdo el conocimiento.", "");
translation.add(82, "Escucho los sollozos de una chica...", "");
translation.add(83, "¿Quién es?...", "Kouhei");
translation.add(84, "Kouhei, ¿estás despierto?", "Mamá");
translation.add(85, "Oh... ¿Mamá?...", "Kouhei");
translation.add(86, "Una pared blanca.\nMi madre sentada en una silla. Está pálida.", "");
translation.add(87, "Qué alegría verte despierto otra vez. Vamos, Aya.", "Mamá");
translation.add(88, "Extiende un brazo a su lado.", "");
translation.add(89, "¿Aya?...", "Kouhei");
translation.add(90, "Mi hermanita aparece a un flanco de la cama.", "");
translation.add(91, "*Sollozo* Qué contenta estoy...", "Aya");
translation.add(92, "¿Por qué lloras?... ¿Estamos en un hospital?", "Kouhei");
translation.add(93, "Sí... Me dijeron que te caíste\npor las escaleras de la escuela.", "Mamá");
translation.add(94, "(¿Qué? Ah... ya lo entiendo. Mierda).", "Kouhei");
translation.add(95, "Así que han encubierto lo que pasó en clase.", "");
translation.add(96, "Oh, claro, claro...", "Kouhei");
translation.add(97, "¿...?", "Aya");
translation.add(98, "(Será mejor que les siga la corriente).", "Kouhei");
translation.add(99, "No serviría de nada contarles la verdad.", "");
translation.add(100, "Es que andaba distraído; no lo recuerdo muy bien...", "Kouhei");
translation.add(101, "Sí; cuando te vieron llamaron a una ambulancia.", "Mamá");
translation.add(102, "Siento haberte preocupado... Y a ti también, Aya.", "Kouhei");
translation.add(103, "...", "Aya");
translation.add(104, "Parece que has sufrido varias contusiones leves.\n¿Te duele por algún sitio?", "Mamá");
translation.add(105, "Me duele por todo, aunque\nquizá sea porque me acabo de levantar.", "Kouhei");
translation.add(106, "Mamá asiente.", "");
translation.add(107, "El médico dice que te recuperarás\ndespués de una buena noche de descanso.", "Mamá");
translation.add(108, "Eso espero.", "Kouhei");
translation.add(109, "Mañana vendré a recogerte. Duerme mucho.", "Mamá");
translation.add(110, "No hace falta, puedo ir a casa solo...\nBuenas noches.", "Kouhei");
translation.add(111, "No deberías forzarte tanto...", "Aya");
translation.add(112, "Parece que Aya quiere decirme algo más,\npero mamá la saca de la habitación.", "");
translation.add(113, "Y entonces, vuelvo a quedarme dormido.", "");

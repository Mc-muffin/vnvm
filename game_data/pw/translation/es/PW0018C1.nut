translation.add(0, "Me!? Eeek, Arata, you know exactly how I feel!", "Liliana");
translation.add(1, "Iris' power depends on affinity. Match your\nbreathing with mine.", "Arata");
translation.add(2, "Roger that! I'll be sure to stick really close to\nyour breathing!", "Liliana");
translation.add(3, "I'm counting on you, Lun Lun.", "Arata");
translation.add(4, "Okay! Just relax like you're on a passenger\nferry!", "Liliana");
translation.add(5, "All right!", "Arata");
translation.add(6, "Engage!!", "Liliana & Arata");
translation.add(7, "We search the first floor of the mansion. Since we\ndon't know what'll happen we do so cautiously.", "");
translation.add(8, "We search through four of the rooms, finding\nnothing.", "");
translation.add(9, "Arata...... Watch out!", "Liesel");
translation.add(10, "I turn at Liesel's warning. Before me stands a\nGuardener.", "");
translation.add(11, "There's one! Let's go, Lun Lun!", "Arata");
translation.add(12, "Leave it to me!", "Liliana");
translation.add(13, "All right!", "Arata");
translation.add(14, "Go us! Our match was perfect!", "Liliana");
translation.add(15, "We beat the Guardener, but there's nothing here.", "");
translation.add(16, "It wasn't guarding anything. It seems it was just\nhere to attack intruders.", "");
translation.add(17, "There doesn't seem to be anything here. Let's move\nelsewhere.", "Suzushiro");
translation.add(18, "Just then, an enemy suddenly appears.", "");
translation.add(19, "Arata! You ready?", "Liliana");
translation.add(20, "Of course!!", "Arata");
translation.add(21, "Good!", "Arata");
translation.add(22, "Easy, easy!", "Liliana");
translation.add(23, "We beat another Guardener, but there's nothing\nhere either.", "");
translation.add(24, "We keep searching the rest of the floor, but we\ndon't find a thing.", "");
translation.add(25, "I would say it's about time to switch partners.", "Suzushiro");
translation.add(26, "We've searched the entire first floor. I think you\nshould select a new partner.", "Suzushiro");
translation.add(27, "Bah. But that's okay, I'm in the lead anyway!", "Liliana");
translation.add(28, "Lun Lun and I separate.", "");
translation.add(29, "Okay, next......", "Arata");
translation.add(30, "Angela", "");
translation.add(31, "Suzushiro", "");
translation.add(32, "Lun Lun", "");
translation.add(33, "Liesel", "");

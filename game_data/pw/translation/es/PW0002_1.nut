translation.add(0, "Encontraos, oh bellas princesas. Cubrid vuestros\ncuerpos con un Vestido, y honrad el banquete con\nvuestra presencia.", "");
translation.add(1, "Un único príncipe.", "");
translation.add(2, "Y una única princesa para bailar con el príncipe.", "");
translation.add(3, "Brilla, oh corona real, que eres el objetivo\nfinal del vals.", "");
translation.add(4, "El vals ha comenzado.", "");
translation.add(5, "Ahora que la luz brilla en todo el reino.", "");
translation.add(6, "Encantada de conocerte, princesa de la espada.", "Woman in red");
translation.add(7, "Soy Angela Victoire, princesa del reino de fuego.", "Woman in red");
translation.add(8, "Y ahora...", "Woman in red");
translation.add(9, "¿Bailamos?", "Woman in red");
translation.add(10, "...", "Woman in white");
translation.add(11, "...", "Woman in red");
translation.add(12, "Dos mujeres trazadas encima del cielo vespertino.", "");
translation.add(13, "Una de ellas viste de un blanco tan intenso que\nparece artificial bajo este cielo carmín.", "");
translation.add(14, "La otra viste de un rojo aún más intenso que\nel propio cielo.", "");
translation.add(15, "Las separan unos veinte pasos.", "");
translation.add(16, "La atmósfera es oscura y hostíl en comparación\ncon los colores tan brillantes de sus trajes.", "");
translation.add(17, "Y otra cosa más.", "");
translation.add(18, "Ambas mujeres sustienen un arma.", "");
translation.add(19, "La mujer de blanco sostiene una espada.\nMientras que la de rojo lo que sostiene es una lanza.", "");
translation.add(20, "La espada está ornamentada, engastada con joyas.", "");
translation.add(21, "Debe medir cerca de un metro, y no parece\nadecuada para la constitución de una mujer.", "");
translation.add(22, "Pero la mujer de rojo no parece impresionada\nen absoluto.", "");
translation.add(23, "Sostiene una lanza metálica, que en vertical,\nes más alta que ella misma.", "");
translation.add(24, "Es un arma diseñada para usarse\na lomos de un caballo.", "");
translation.add(25, "No es un arma que usaría la infantería.\nEs demasiado grande y pesada para luchar con ella.", "");
translation.add(26, "Sin embargo es un arma muy peligrosa...\nY la mujer de blanco es consciente de ello.", "");
translation.add(27, "Sabe que la enorme lanza es bastante apropiada\npara la mujer de rojo.", "");
translation.add(28, "Te he estado esperando.", "Woman in red");
translation.add(29, "La primera batalla debe ser siempre una\nque valga la pena.", "Woman in red");
translation.add(30, "Debe ser entre los luchadores más poderosos.\nY tú, princesa de Soldia, eres adecuada.", "Woman in red");
translation.add(31, "Y como no podría ser de otra forma,\naceptas el desafío, ¿verdad?", "Woman in red");
translation.add(32, "...", "Woman in white");
translation.add(33, "Su arma se mantiene inmóvil, pero sus ojos\nya han empezado el duelo.", "");
translation.add(34, "La mujer de blanco mantiene el silencio.\nY en vez de articular palabra alguna, alza\nla punta su espada.", "");
translation.add(35, "Je, je, je... Perfecto.", "Woman in red");
translation.add(36, "Con el familiar chirrido del metal, la mujer de\nrojo prepara su lanza.", "");
translation.add(37, "Y ese es el pistoletazo de salida.", "");
translation.add(38, "...", "Woman in white");
translation.add(39, "...", "Woman in red");
translation.add(40, "Un silencio mortecino de cierne sobre ellas.", "");
translation.add(41, "Sus cuerpos se tensan, como haría la cuerda de\nun arco durante el preludio del disparo. Sus\narmas brillan.", "");
translation.add(42, "Un mundo enmudecido. La calma antes de la tormenta.", "");
translation.add(43, "Y entonces, la acometida...", "");
translation.add(44, "Una furiosa batalla.\nEl metal desgañitando contra el metal.", "");
translation.add(45, "Y la situación se reestablece a su estado original.\nCon las mujeres inmóviles y encarándose.", "");
translation.add(46, "Un espectador habría sido incapaz de seguir el\nintercambio de golpes que acaban de acontecer.", "");
translation.add(47, "Las mujeres se encontraron seis veces durante\nel estruendo.", "");
translation.add(48, "En cuatro ocasiones se pararon entre sí.\nY las otras dos han quedado reflejadas como\nun par de agujeros enormes en el suelo.", "");
translation.add(49, "Je. Felicidades...", "Woman in red");
translation.add(50, "¡Uhh...!", "Woman in white");
translation.add(51, "La mujer de blanco está sudando.", "");
translation.add(52, "A primera vista, las mujeres combaten en la misma\nliga. Pero ambas se han dado cuenta del nivel\ncontra el que se enfrentan. **", "");
translation.add(53, "Este poder está muy por encima del que poseía\nel Guardener. Es el poder de dos princesas\nbailando el vals.", "");
translation.add(54, "En verdad eres la más poderosa de todas ellas,\nla princesa de Soldia. Ninguna otra podría haber\nresistido mi lanza. Ja, ja... Qué alegría.", "Woman in red");
translation.add(55, "Sus ojos rojos se estrechan.", "");
translation.add(56, "Entonces... ¿Qué me dices de esto...?", "Woman in red");
translation.add(57, "El ribete de su vestido se desliza por el suelo.", "");
translation.add(58, "Con un paso muy elegante que contrasta enormemente\ncon la bárbara acometida de antes, la mujer se\nacuclilla.", "");
translation.add(59, "¿¡Uh!?", "Woman in white");
translation.add(60, "Es una posición inicial mucho más complicada que\nla de antes. La mujer de blanco se da cuenta de\nlo que significa y se estremece.", "");
translation.add(61, "Sept...", "Woman in red");
translation.add(62, "Sus rojos ropajes se enredan con el viento.", "");
translation.add(63, "Una vez más, sus armas colisionan.", "");
translation.add(64, "...", "Woman in red");
translation.add(65, "...", "Woman in white");
translation.add(66, "Guh... ¡Rrrgh...!", "Woman in white");
translation.add(67, "La mujere de blanco rompe el silencio con un\ngemido.", "");
translation.add(68, "Su cara se retuerce de dolor.\nSu traje ha acabado rasgado por varios sitios.", "");
translation.add(69, "El viento se lleva los deshechos de la brillante\ntela blanca. **", "");
translation.add(70, "El \"Alma\" de la que está hecha el vestido,\nreluce y se disuelve. **", "");
translation.add(71, "La dueña del vestido que estaba protegiendo,\nestá ahora expuesta.", "");
translation.add(72, "Ese era el plan de la mujer de rojo.\nAunque no parece complacida con su victoria.", "");
translation.add(73, "Tsk...", "Woman in red");
translation.add(74, "Sus cejas se fruncen en una mueca de irritación.\nUna fina línea de sangre se desliza por su frente.", "");
translation.add(75, "Allowing me to strike your flesh so you could cut\nmy bones... What a lowly tactic.", "Woman in red");
translation.add(76, "Pero hacía mucho tiempo desde que alguien había\nsido capaz de alcanzar mi piel.", "Woman in red");
translation.add(77, "¡Ja, ja, ja, ja...! Esta rabia... Esta emoción...\n¡Las he estado esperando durante mucho tiempo!\n¡Tienes mi gratitud, princesa de la espada!", "Woman in red");
translation.add(78, "Excitada por su furia y regocijo, apunta con la\npunta de su lanza al pecho de la mujer de blanco.", "");
translation.add(79, "¡Venga! ¡Bailemos!\n¡Hagamos bailar a las estrellas del cielo!", "Woman in red");
translation.add(80, "¡Ahhh...!", "Woman in white");
translation.add(81, "La mujer de blanco aprieta los dientes y prepara\nsu espada, pero no puede ocultar el daño que ha\nsufrido.", "");
translation.add(82, "Sus heridas son meros rasguños, pero el vestido\nque la protegía, se ha esfumado por completo.", "");
translation.add(83, "La mujer de blanco se refleja en el rojo metal\nde la lanza.", "");
translation.add(84, "Y ahora, princesa de Soldia,\n¡muestrame aún más poder que el de antes!", "Woman in red");
translation.add(85, "Si no, ¡el fin de tu vida coincidirá con el\ninicio de esta lanza!", "Woman in red");
translation.add(86, "¡Guhhhhhh!", "Woman in white");
translation.add(87, "Está bien. ¡Parad!", "?");
translation.add(88, "¿¡...!?", "Woman in white");
translation.add(89, "¿¡...!?", "Woman in red");
translation.add(90, "Las dos mujeres se giran en dirección a la nueva\nvoz.", "");
translation.add(91, "Siento interrumpiros en este momento tan culminante,\npero debemos pediros que os detengáis,\nprincesas de Soldia y Agnis.", "Right boy");
translation.add(92, "...", "Left boy");
translation.add(93, "Dos ojos imperativos.", "");
translation.add(94, "¿¡Quiénes sois vosotros!?", "Woman in red");
translation.add(95, "Ah, mis disculpas.\nHe olvidado presentarme.", "Right boy");
translation.add(96, "Yo soy Pigeon. Pigeon Alnemyte.", "Right boy");
translation.add(97, "Crow Alnemyte...", "Left boy");
translation.add(98, "Somos los \"Pequeños Irrebatibles\", los árbitros\ndel vals actual.", "Pigeon");
translation.add(99, "Los dos chicos se inclinan en una reverencia.", "");
translation.add(100, "Aparentan ser niños. Cada uno sostiene una llave\nenorme. Tienen el pelo corto, pero no está claro\nsi son chicos o chicas.", "");
translation.add(101, "Se mueven sincronizados casi como la imagen\nreflejada en un espejo.", "");
translation.add(102, "¿Árbitros?", "Woman in red");
translation.add(103, "En efecto, árbitros.\nSe podría decir que somos los jueces del vals.", "Pigeon");
translation.add(104, "Y como tales, debemos asegurar la justicia y\npreservar las normas. No permitiremos luchas\nque no estén planificadas.", "Pigeon");
translation.add(105, "Estatuto del vals de las princesas, artículo 4.\nDebe asistir un árbitro en cada duelo.", "Crow");
translation.add(106, "Así que váis a tener que posponer vuestra lucha\nhasta otra ocasión, ¿entendido?", "Pigeon");
translation.add(107, "Estatuto del vals de las princesas, artículo 3.\nTodas las participantes del vals están sujetas al\njuicio de los árbitros.", "Crow");
translation.add(108, "Así es que si no seguís nuestras instrucciones,\nseréis penalizadas.", "Pigeon");
translation.add(109, "...", "Woman in red");
translation.add(110, "...Bah. Ya no estoy de humor.", "Woman in red");
translation.add(111, "...", "Woman in white");
translation.add(112, "Las armas se retiran, empezando por la lanza\ny terminando por la espada.", "");
translation.add(113, "Ah, good of you to cooperate. But then, you are a\nnoblewoman, Angela Victoire Blendin.", "Pigeon");
translation.add(114, "Bien entonces...", "Woman in red");
translation.add(115, "¡Decidamos la fecha para nuestro próximo combate\nahora mismo!", "Woman in red");
translation.add(116, "Valiéndome del estatuto del vals de las princesas,\nyo, Angela de Agnis, ¡solicito un duelo con la\nprincesa de la espada!", "Woman in red");
translation.add(117, "¡¡...!!", "Woman in white");
translation.add(118, "Menudas prisas.", "Pigeon");
translation.add(119, "¿Algún problema?", "Woman in red");
translation.add(120, "No. Para nada.", "Pigeon");
translation.add(121, "Estatuto del vals de las princesas, artículo 7.\nNo se puede rechazar un duelo.", "Crow");
translation.add(122, "Pero la fecha y el emplazamiento del duelo los\nestablecerá la princesa retada. La fecha debe ser\nantes de la media noche dos días después del reto. **", "Crow");
translation.add(123, "Así que, princesa de la espada.\nDíganos la fecha y el lugar.", "Pigeon");
translation.add(124, "...", "Woman in white");
translation.add(125, "¿Eh...? ¿Es que te ha comido la lengua el gato?\n¿O es simplemente que estás asustada?", "Pigeon");
translation.add(126, "Eh...", "Woman in white");
translation.add(127, "Dentro de dos días a media noche.\n¿Te va bien?", "Woman in red");
translation.add(128, "¿¡...!?", "Woman in white");
translation.add(129, "...", "Woman in white");
translation.add(130, "La mujer de blanco parece sorprendida por un momento,\npero entonces asiente en silencio.", "");
translation.add(131, "Parece que está de acuerdo.", "Woman in red");
translation.add(132, "Ah... Parece de verdad que le de vergüenza.\nPero no importa. **", "Pigeon");
translation.add(133, "Entonces el duelo tendrá lugar dentro de dos días\na media noche.", "Pigeon");
translation.add(134, "Aceptado y anotado.", "Crow");
translation.add(135, "...", "Woman in white");
translation.add(136, "Dentro de dos días a media noche... Era ese el\nmomento que esperaba la mujer herida.", "");
translation.add(137, "Echa una ojeada curiosa a su oponente.\n¿Por qué ha sugerido ese momento ella misma?", "");
translation.add(138, "La mujer de rojo responde a la informulada pregunta.", "");
translation.add(139, "A nosotros, los guerreros de Agnis, nos orgullece\nderrotar a nuestros adversarios cuando se encuentran\nen las mejores condiciones.", "Woman in red");
translation.add(140, "Así que aprovecha el tiempo antes de nuestro duelo\npara sanar tus heridas.", "Woman in red");
translation.add(141, "Las heridas que te he inflingido... Y también\nla herida de tu hombro.", "Woman in red");
translation.add(142, "¡¡...!!", "Woman in white");
translation.add(143, "Estatuto del vals de las princesas, artículo 7,\nsuplemento 1. Una vez estblecida la fecha,\nno se puede cambiar.", "Crow");
translation.add(144, "Estatuto del vals de las princesas, artículo 8.\nSi alguno de los bandos no se presenta al duelo,\nse considerará como una derrota para dicho bando.", "Crow");
translation.add(145, "Así es. Así que sed puntuales, ¿vale?", "Pigeon");
translation.add(146, "El lugar de encuentro será aquí, el tejado del\nedificio de la escuela Hakuhou. Os guiaremos a\nla arena desde aquí.", "Pigeon");
translation.add(147, "Y eso es todo. Hemos terminado con nuestro trabajo.", "Pigeon");
translation.add(148, "Por cierto... No te he preguntado cómo te llamas.", "Woman in red");
translation.add(149, "Estoy convencida de que puede hablar.\n¿Podría honrarnos con vuestro nombre?", "Woman in red");
translation.add(150, "...", "Woman in white");
translation.add(151, "Casi parece como si la mujer de rojo quisiese\ncomprobar si realmente puede hablar la de blanco.", "");
translation.add(152, "........I....", "Woman in white");
translation.add(153, "Iris...", "Woman in white");
translation.add(154, "¿Iris...?", "Woman in red");
translation.add(155, "Je, je, je... ¡¡Ja, ja, ja, ja, ja...!!", "Woman in red");
translation.add(156, "De todos los posibles...\n¡El nombre del primer emperador de todos!", "Woman in red");
translation.add(157, "¡Admiro tu coraje!\n¡Ahora tengo aún más ganas de derrotarte!", "Woman in red");
translation.add(158, "...", "Woman in white");
translation.add(159, "Nos vemos entonces, Iris.\nEsperaré impaciente nuestra batalla.", "Woman in red");
translation.add(160, "Con una despedida grácil, la mujer de rojo\ndesaparece en el ahora oscuro cielo azul.", "");
translation.add(161, "...", "Woman in white");
translation.add(162, "Siguiendo su ejemplo, la mujer llamada Iris,\ntambién se desvanece en la nada.", "");
translation.add(163, "Vaya, vaya.\nHay algunos participantes la mar de interesantes\nen este vals.", "Pigeon");
translation.add(164, "...", "Crow");
translation.add(165, "Ah, sí. El suyo será el primero de los duelos,\nasí que debemos celebrar la ceremonia de apertura.", "Pigeon");
translation.add(166, "Ejem... Allá vamos, Crow.", "Pigeon");
translation.add(167, "Meet, oh lovely princesses!", "Pigeon");
translation.add(168, "...", "Crow");
translation.add(169, "......Meet, oh lovely princesses!", "Pigeon");
translation.add(170, "...", "Crow");
translation.add(171, "Venga, Crow. Tienes que hacer tu parte.", "Pigeon");
translation.add(172, "Está bien...", "Crow");
translation.add(173, "Ejem. Una vez más:", "Pigeon");
translation.add(174, "Meet, oh lovely princesses!", "Pigeon");
translation.add(175, "Envelop your bodies in a Dress, and grace the\nbanquet with your presence.", "Crow");
translation.add(176, "Dance with the lightest of feet, fight with the\nbest of valour, and show the seven kingdoms your\nsplendour!", "Pigeon");
translation.add(177, "One prince only.", "Crow");
translation.add(178, "One princess only to dance with the prince!", "Pigeon");
translation.add(179, "Glitter, oh royal Crown that is the final goal of\nthe Waltz.", "Crow");
translation.add(180, "Shine your light on the strongest and most sublime\nprincess of them all!", "Pigeon");
translation.add(181, "The Waltz is on.", "Crow");
translation.add(182, "Now that the light shines on the entire land!", "Pigeon");
translation.add(183, "El vals de las princesas...", "Crow & Pigeon");
translation.add(184, "Raise the curtain!", "Crow & Pigeon");
translation.add(185, "Ba-thump...", "");
translation.add(186, "Ba-thump...", "");
translation.add(187, "Duele...", "");
translation.add(188, "La presión hace que me resulte difícil respirar.", "");
translation.add(189, "Tengo miedo...", "");
translation.add(190, "Pero no puedo escapar.\nNo puedo siquiera gritar.", "");
translation.add(191, "Si aparto la mirada un solo momento, me matará.", "");
translation.add(192, "Lo único que puedo hacer es mantenerme en mi\nposición y aguantar el miedo.", "");
translation.add(193, "Uff... Uff... Uff...", "Arata");
translation.add(194, "Y poco a poco, el miedo...", "");
translation.add(195, "Se desvance en pro de un terror aún mayor...", "");
translation.add(196, "¡¡¡¡AHHHHHHHHHHHHHHHHHHHHHHH!!!!", "Arata");
translation.add(197, "¿Eh? ¿Estás despierto...?", "Shizuka");
translation.add(198, "Ehh... ¿S-Shizuka?", "Arata");
translation.add(199, "Estabas teniendo una pesadilla.", "Shizuka");
translation.add(200, "¿U-Una pesadilla? ¿Dónde estoy...?", "Arata");
translation.add(201, "En casa.", "Shizuka");
translation.add(202, "Me sorprendí mucho cuando me enteré de que habías\nperdido el conocimiento en la escuela.", "Shizuka");
translation.add(203, "El médico dijo que habías cogido un resfriado por\nfalta de sueño. No deberías quedarte despierto\nhasta tan tarde.", "Shizuka");
translation.add(204, "¿M-Me he quedado inconsciente...?", "Arata");
translation.add(205, "¿No lo recuerdas?\nTe llevaron a la enfermería de la escuela,\ny luego un taxi te trajo a casa.", "Shizuka");
translation.add(206, "...", "Arata");
translation.add(207, "Creo que deberías seguir durmiendo...", "Shizuka");
translation.add(208, "Pero... Tengo que hacer la cena...", "Arata");
translation.add(209, "No seas tonto. Vuelve a dormirte.", "Shizuka");
translation.add(210, "Pero hermanita... ¿y qué hay del dojo?\n¿Has tenido que cancelar las clases por mí?", "Arata");
translation.add(211, "No te preocupes por eso. Eres el paciente.\nAhora tu única preocupación debería ser relajarte.", "Shizuka");
translation.add(212, "...", "Arata");
translation.add(213, "Voy a hablar con mamá. No te preocupes por nada\ny descansa.", "Shizuka");
translation.add(214, "Está bien...", "Arata");
translation.add(215, "Ah, te traeré unas pocas rodajas de manzana.\n¿O prefieres mejor ciruelas en almíbar?", "Shizuka");
translation.add(216, "Oye... ¿hermanita?", "Arata");
translation.add(217, "¿Eh?", "Shizuka");
translation.add(218, "Hoy me estás tratando extrañamente bien.", "Arata");
translation.add(219, "Tonto... Siempre te trato bien.", "Shizuka");
translation.add(220, "Gracias, hermanita...", "Arata");
translation.add(221, "Mmm...", "Shizuka");
translation.add(222, "...", "Arata");
translation.add(223, "¿Ha sido todo un sueño...?", "Arata");
translation.add(224, "Supongo que tenía que ser un sueño...", "Arata");
translation.add(225, "Me convertí en una chica, y me enfrenté a otra...\nY a otro monstruo.", "");
translation.add(226, "¿Eh...?\n¿Pero de dónde vendrá un sueño así? **", "");
translation.add(227, "Recuerdo haber subido corriendo las escaleras\npara ayudar a Chris...", "");
translation.add(228, "¡Auu...! Me duele la cabeza...", "");
translation.add(229, "Supongo que sí que he pillado un resfriado...", "");
translation.add(230, "Un sueño... Ha sido todo un sueño...", "");
translation.add(231, "La campana ha señalizado el comienzo...", "??");
translation.add(232, "Well then, I should be going.", "??");
translation.add(233, "Espera un poco más.\nSuzushiro estará contigo muy pronto.", "??");
translation.add(234, "Chris... Mi amado príncipe...", "??");
translation.add(235, "¡Se ha escapado! ¡Defensas! ¡Que alguien le pare!", "Kazuhiro");
translation.add(236, "¡Uoooooooohhhh!", "Male student");
translation.add(237, "¡Anda!", "Chris");
translation.add(238, "¿Eh...?", "Male student");
translation.add(239, "¡¡Kyaaaaaaaa!!", "The girls");
translation.add(240, "¡Sí! ¡Tres puntos, colega!", "Nishimoto");
translation.add(241, "¡Y con ese ya van treinta!", "Arai");
translation.add(242, "Fiu...", "Chris");
translation.add(243, "¡Kyaaa! Chris, ¡eres genial!", "Kazuko");
translation.add(244, "Guaaau... Increíble.\nHa volado por el aire...", "Nodoka");
translation.add(245, "Qué mono...", "Riko");
translation.add(246, "¿Eh?", "Nodoka");
translation.add(247, "¿Eeeh? ¿Q-Qué?", "Riko");
translation.add(248, "Oh, nothing, it's just that I haven't heard you\nsay something like that ever before, so you\nsurprised me...", "Nodoka");
translation.add(249, "N, no, I didn't mean it like that...", "Riko");
translation.add(250, "Aah, you're blushing! Hahaha!", "Nodoka");
translation.add(251, ".......", "Riko");
translation.add(252, "But yeah, he really is handsome. When I first saw\nhim, I thought he was a real prince.", "Nodoka");
translation.add(253, ".......F, Fukamori said the same yesterday.......", "Riko");
translation.add(254, "Did he?", "Nodoka");
translation.add(255, ".................", "Arata");
translation.add(256, "Kyaa! Chris has the ball again!", "Arai");
translation.add(257, "Oh no you don't!", "Kazuhiro");
translation.add(258, "Hmph...", "Chris");
translation.add(259, "You can't keep it up forever!", "Kazuhiro");
translation.add(260, "I'm on to your movements!!", "Kazuhiro");
translation.add(261, "'Scuse me.", "Chris");
translation.add(262, "Huh!?", "Kazuhiro");
translation.add(263, "...Whew.", "Chris");
translation.add(264, "Kyaaa! Chris!!!", "The girls");
translation.add(265, "Woooow! He's like a pro player!", "Suzuki");
translation.add(266, "Eh, what...?", "Kazuhiro");
translation.add(267, "You suck, Sugimoto! You and your 'I'm on to you'!", "Kazuko");
translation.add(268, "Heeeeeeh?", "Kazuhiro");
translation.add(269, "You were standing too tall, Kazuhiro. You should\nchange your stance.", "Chris");
translation.add(270, "I, I see! Well done, Chris, I was thinking the\nsame thing, hahahahaha!", "Kazuhiro");
translation.add(271, "I'll stop you next time!", "Kazuhiro");
translation.add(272, "We'll see.", "Chris");
translation.add(273, ".................", "Arata");
translation.add(274, "It was just a moment.", "");
translation.add(275, "He feinted just before he stepped past him. I\ndon't even think Kazuhiro realized what happened.", "");
translation.add(276, "He's good. Really, really good.", "");
translation.add(277, "No wonder he's able to fight those monsters... No,\nno! That was just a dream!", "");
translation.add(278, "Ah, jeez! I wish I could remember properly. I have\nno idea where that dream came from.", "");
translation.add(279, "What are you doing all by yourself!", "Nodoka");
translation.add(280, "N, Nodoka?", "Arata");
translation.add(281, "You must be bored. You're on the side line for the\nfirst time in your life.", "Nodoka");
translation.add(282, "Mom made me take rest today, so I have little\nchoice.", "Arata");
translation.add(283, "Well, duh. You collapsed yesterday, remember?", "Nodoka");
translation.add(284, "...Hey, did I really collapse?", "Arata");
translation.add(285, "You sure did. Don't you remember?", "Nodoka");
translation.add(286, "No.", "Arata");
translation.add(287, "A, are you okay? No major injuries?", "Nodoka");
translation.add(288, "It's weird for you to get a fever.", "Nodoka");
translation.add(289, "Yeah. I almost never catch a cold.", "Arata");
translation.add(290, "That's right, idiots don't catch colds. Maybe you\nhave some terrible illness?", "Nodoka");
translation.add(291, "I guess so...... Hey!", "Arata");
translation.add(292, "Whoopsie! Well, at least you look healthy.", "Nodoka");
translation.add(293, "But don't overdo it. Your mom said you should take\nit easy today, got it?", "Nodoka");
translation.add(294, "......Yeaaahhh......", "Arata");
translation.add(295, "...But... No, something isn't quite right.", "");
translation.add(296, "Did I really collapse because of a cold?", "");
translation.add(297, "Did I really dream everything up?", "");
translation.add(298, "Meeting Chris after school. Finding some weird\nmonster on the roof. Chris fighting it.", "");
translation.add(299, "A monster? That I fought?", "Chris");
translation.add(300, "Gee, how old are you again?", "Chris");
translation.add(301, "You can dream whatever you want, but don't take\nthe dream out of bed.", "Chris");
translation.add(302, "...Step aside, please, I need to prepare for\nclass.", "Chris");
translation.add(303, "Well, I can't imagine it NOT being a dream. It's\njust ludicrous. However...", "");
translation.add(304, "Kyaaaaaaaa!", "The girls");
translation.add(305, "There they go again.", "");
translation.add(306, "Before my eyes, Chris gently lifts off the\nground.", "");
translation.add(307, "His small body rises up, together with the ball.\nThe defense is powerless.", "");
translation.add(308, "He draws a golden arc through the air...", "");
translation.add(309, "I definitely saw him doing the same thing\nyesterday.", "");
translation.add(310, "...All right!", "Arata");
translation.add(311, "Huh??", "Nodoka");
translation.add(312, "Time for a little challenge.", "Arata");
translation.add(313, "Huh!? H, hey, Arata!!", "Nodoka");
translation.add(314, "Referee, change of players! Kazuhiro, move your\nass.", "Arata");
translation.add(315, "What!? You're supposed to be benched...", "Kazuhiro");
translation.add(316, "Never mind that. I'll take it from here.", "Arata");
translation.add(317, "I can't decide just by looking and listening.", "");
translation.add(318, "Bring it on, Chris!", "Arata");
translation.add(319, "........!", "Chris");
translation.add(320, "  Heh.", "");
translation.add(321, "Oooh, Here comes a new challenger! How will our\nChris make Arata bite the dust, I wonder!", "Kazuko");
translation.add(322, "What do you think, Nonomiya?", "Kazuko");
translation.add(323, "Arata!! I'm gonna tell your mom and sis!", "Nodoka");
translation.add(324, ".................", "Chris");
translation.add(325, "No pulling any punches.", "Arata");
translation.add(326, "Bah.", "Chris");
translation.add(327, "He looks really sure of himself.", "");
translation.add(328, "Just you wait. I'll make you get serious.", "");
translation.add(329, "Priiiiiiiii!", "");
translation.add(330, "Kyaaa! It's going to Chris again!", "Suzuki");
translation.add(331, "Good! Here goes!", "Arata");
translation.add(332, "Kuh!?", "Chris");
translation.add(333, "He's taken aback when he finds me in his path, but\nthen Chris starts dribbling left and right.", "");
translation.add(334, "Bom... Bom... Bom...", "");
translation.add(335, "Here it comes! That feint!", "");
translation.add(336, ".................", "Chris");
translation.add(337, "Will he go left? Will he go right?", "");
translation.add(338, "Left... Right... Left... Right...", "");
translation.add(339, "Well, it's got to be one of the two. It's all or\nnothing! Right!!", "");
translation.add(340, "The moment I move, Chris steps forward.", "");
translation.add(341, "!!", "Chris");
translation.add(342, "He's moving in the same direction as me. I got it\nright!!", "");
translation.add(343, "I stretch out my hand. The distance is almost\nzero. I'm pretty sure I can take the ball from\nhim.", "");
translation.add(344, "But the next moment, the transfer student is lost\nfrom my field of vision, leaving only the scenery\nsliding to the left.", "");
translation.add(345, "Left!? No...", "");
translation.add(346, "U, up!?", "");
translation.add(347, "I look up, at the roof of the gym. There he is, up\nin the air with the ball.", "");
translation.add(348, "It's like gravity doesn't affect him.", "");
translation.add(349, "....!!", "Arata");
translation.add(350, "I make my body brake, though it protests. I follow\nhis movement.", "");
translation.add(351, "I am SO going to catch him!", "");
translation.add(352, "I stretch my arms, I stretch my back.", "");
translation.add(353, "And I ram the ground away from me with all my\nmight!", "");
translation.add(354, "I'm not letting you!!", "Arata");
translation.add(355, "What!?", "Chris");
translation.add(356, "My hand gets hold of something-------", "");
translation.add(357, "The scenery flips over.", "");
translation.add(358, "Ow... Ow, ow, ow...", "Arata");
translation.add(359, "Kyaaa!? Chris!!??", "Arai");
translation.add(360, "Huh?", "Arata");
translation.add(361, "I lever myself up. Below me I see a golden-haired\nboy, clutching at his shoulder.", "");
translation.add(362, "Crap. Did I fall on top of him?", "");
translation.add(363, "Ugggh...", "Chris");
translation.add(364, "I, I'm sorry! Are you all right!", "Arata");
translation.add(365, ".....!! I'm fine! Don't touch me!", "Chris");
translation.add(366, "I am about to touch his shoulder, but Chris knocks\nmy hand away.", "");
translation.add(367, "Ah...", "Arata");
translation.add(368, "His shoulder... He got a wound there yesterday. It\nwas his right one, if I recall correctly.", "");
translation.add(369, "Heey! Fukamori! What are you doing to Chris!", "Kazuko");
translation.add(370, "That didn't count! That was foul play!", "Nishimoto");
translation.add(371, "Foul play, foul play!", "Suzuki");
translation.add(372, "Er, uh... Foul! Back to the base line!", "Referee");
translation.add(373, ".................", "Arata");
translation.add(374, "Wh, what? How long are you going to sit on top of\nme?", "Chris");
translation.add(375, "Hey, are you injured?", "Arata");
translation.add(376, "No... You didn't hit me that hard.", "Chris");
translation.add(377, "Not that. I mean since yesterday.", "Arata");
translation.add(378, ".......No.", "Chris");
translation.add(379, "Really?", "Arata");
translation.add(380, "Yes, really! Get off my case!", "Chris");
translation.add(381, ".................", "Arata");
translation.add(382, "Don't space out! Get off me alrea---", "Chris");
translation.add(383, "Hop.", "Arata");
translation.add(384, "!!!!!!!!!!!!!!!!!!!!!!!!!!!!", "Chris");
translation.add(385, "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!", "");
translation.add(386, "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!", "");
translation.add(387, ".....................................................", "");
translation.add(388, ".............................................", "");
translation.add(389, ".......................................", "");
translation.add(390, "See? You're bandaged up.", "Arata");
translation.add(391, "See, that wound is probably from yesterday...", "Arata");
translation.add(392, "Bofgh!?", "Arata");
translation.add(393, "Wh, what are you doing?", "Arata");
translation.add(394, "You complete idiot!!!!!!!!!!!!", "Chris");
translation.add(395, "Whumpfgh!?", "Arata");
translation.add(396, ".......Kyuuu.......", "");
translation.add(397, "Kyaaaaaaaaaaaaaaaaaaa!!!!!!!", "The girls");
translation.add(398, "Fukamori, you pervert!", "Nishimoto");
translation.add(399, "Wh, whoa, his skin is so smooth!", "Arai");
translation.add(400, "I'll never wash my eyes again!", "Kazuko");
translation.add(401, "................", "Kanada");
translation.add(402, "Wham!!", "");
translation.add(403, "Waaaaaaahhhh! Ricchan!!", "Nodoka");
translation.add(404, "Kanada got a nosebleed and collapsed!!", "Male student");
translation.add(405, "Medic! Medic!!", "Kazuhiro");

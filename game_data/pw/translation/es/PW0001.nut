translation.add(0, "Érase una vez una gran nación.", "");
translation.add(1, "Estaba dividida en seis reinos,\ndirigidos cada una de ellos por un rey.", "");
translation.add(2, "Y un único emperador que los gobernaba a todos.", "");
translation.add(3, "El emperador tenía una esposa que había\nelegido de entre las seis princesas del reino.", "");
translation.add(4, "La emperatriz daría a luz a un príncipe,\nque a su vez se convertiría en el nuevo emperador.", "");
translation.add(5, "Las seis princesas lucharían por el puesto\nde emperatriz bailando para el príncipe.", "");
translation.add(6, "En el Vals de las princesas.", "");
translation.add(7, "La princesa vencedora podría reclamar\nla mano del príncipe.", "");
translation.add(8, "«Y ahora, princesitas, ¿estáis preparadas?».", "");
translation.add(9, "Los gritos se alzan en el aire, mezclándose\ncon el repiqueteo del acero contra el acero.", "");
translation.add(10, "Un destello de luz y la sensación\ndel frío metal rozando mi piel.", "");
translation.add(11, "La hoja se desliza por el aire portando \nla muerte por doquiera que pase.", "");
translation.add(12, "Y aun así mantengo mi atención en otro punto.", "");
translation.add(13, "Siento el viento en mi cara.\nEl viento que anuncia la caída del atardecer.", "");
translation.add(14, "Su vestido níveo ondeando con la brisa.\nSu áureo cabello perfilando su semblante.", "");
translation.add(15, "Y brillando a través de sus hebras,\ndos insondables ojos glaucos.", "");
translation.add(16, "Cuya intensa mirada casi me hace estremecer.", "");
translation.add(17, "Pupilas esmeralda...", "");
translation.add(18, "¡RINGGGGGGGGG!", "");
translation.add(19, "...", "Arata");
translation.add(20, "¡¡RIIIIIIIIIIIIIINGGGGGGGGGGGGGGGGG!!", "");
translation.add(21, "...", "Arata");
translation.add(22, "¡¡¡RIIIIIIIIIIIIIINNNNNNNNNNNGGGGGGGGGGGGGGGGG!!!", "");
translation.add(23, "...", "Arata");
translation.add(24, "Son las 7:41...", "");
translation.add(25, "Eh...", "Arata");
translation.add(26, "¡MIERDA! ¡Me he quedado dormido!", "Arata");
translation.add(27, "¡Dormido! ¡Dormido! ¡Dormido!", "Arata");
translation.add(28, "Menudo jaleo.", "Shizuka");
translation.add(29, "¡Shizuka! ¡Buenos días!", "Arata");
translation.add(30, "Buenos días, Arata.\n¿A qué viene tanta actividad de buena mañana?", "Shizuka");
translation.add(31, "¡Hoy me tocan las tareas de clase!\n¡Y voy a llegar tardísimo!", "Arata");
translation.add(32, "¡Fris, fras, fris, fras!", "");
translation.add(33, "Lavado de cara... ¡Listo!", "");
translation.add(34, "Ras, ras, ras, ras. ¡Glú, glú, glú!", "");
translation.add(35, "Cepillado de dientes... ¡Listo!", "");
translation.add(36, "Ponerme la ropa... ¡Listo!", "");
translation.add(37, "Ale, ¡hasta luego!", "Arata");
translation.add(38, "Para el carro.", "Shizuka");
translation.add(39, "¡Mmmgg!", "Arata");
translation.add(40, "En cuanto giro la cabeza, una enorme rebanada\nde pan se estampa contra mi boca.", "");
translation.add(41, "Mmm. Pues no sabe nada mal.", "");
translation.add(42, "El desayuno es fundamental para empezar bien el día.\nInviertes tres minutos preparándolo y uno comiéndolo.", "Shizuka");
translation.add(43, "Mgg, mgg. (Ajá, ajá).", "Arata");
translation.add(44, "Toma, ya está listo.", "Shizuka");
translation.add(45, "¡Canasta!", "Arata");
translation.add(46, "Ñam, ñam, ñam. Glup, glup, glup.", "");
translation.add(47, "No deberías ir tan acelerado.", "Shizuka");
translation.add(48, "Si actúas precipitadamente, no te quedará tiempo\npara tomar decisiones con el debido cuidado.", "Shizuka");
translation.add(49, "Si te preparases las cosas antes de dormir,\ntendrías más tiempo por las mañanas.\nDe todas formas...", "Shizuka");
translation.add(50, "Que sí, que sí. Que ya lo serghmgmgm... ¿Uhm?\n¡¡Hfffggggggg!!", "Arata");
translation.add(51, "...", "Shizuka");
translation.add(52, "Toma, un poco de té.", "Shizuka");
translation.add(53, "Gulp, gulp, gulp...", "Arata");
translation.add(54, "¡Ahhh! ¡Gracias!", "Arata");
translation.add(55, "Ah, por cierto. Hoy no habrá entrenamiento.\nEl señor Nonomiya se ha fastidiado la espalda.", "Shizuka");
translation.add(56, "¿Hmffmmff? (¿En serio?) Mgg mmff mfffmfff mfff.\n(Supongo que se va haciendo viejo.)", "Arata");
translation.add(57, "Hoy me encargo del grupo junior. Llegaré a casa\ntarde, así que ocúpate tú de la cena.", "Shizuka");
translation.add(58, "¡Hamgggg! (¡Entendido!)", "Arata");
translation.add(59, "Y ten cuidado de no merodear mucho por ahí.\nHan habido algunos incidentes muy extraños\nen el vecindario últimamente.", "Shizuka");
translation.add(60, "¿Momffgg?", "Arata");
translation.add(61, "Paredes rotas, postes de teléfono partidos. Ese tipo\nde cosas. La gente cree que ha sido algún conductor\ndescuidado. Pero todavía no han capturado a nadie.", "Shizuka");
translation.add(62, "Alertaré también a los chicos del dojo.\nLa verdad es que estoy bastante preocupada.", "Shizuka");
translation.add(63, "Mm, gulp. Está bien, te ayudaré.", "Arata");
translation.add(64, "No, no te preocupes. Puedo hacerlo sola.", "Shizuka");
translation.add(65, "¡Oye! Que me he vuelto bastante fuerte, ¿eh?\n¡Te consigo ganar una de cada diez!", "Arata");
translation.add(66, "Te sobre valoras. ¿No será una de cada cien?", "Shizuka");
translation.add(67, "Además, no me fío un pelo de un dormilón como tú.\nEstate quieto, que voy a peinarte.", "Shizuka");
translation.add(68, "Au, duele.", "Arata");
translation.add(69, "Eso es buena señal.", "Shizuka");
translation.add(70, "Ya, claro...", "Arata");
translation.add(71, "¡Ding-dong!\n¡Ding-dong!", "");
translation.add(72, "Solo hay una persona que tocaría así.", "Shizuka");
translation.add(73, "¡Maldición! ¡Debe ser Nodoka!", "Arata");
translation.add(74, "Mumgg... ¡Gulp! ¡Todo listo! ¡Hora de irme!", "Arata");
translation.add(75, "Oye... que todavía no he terminado de pein...", "Shizuka");
translation.add(76, "¡Gracias por el desayuno, hermanita!", "Arata");
translation.add(77, "¡Ding-Dong!\n¡Ding-Dong!", "");
translation.add(78, "¡DING! ¡DONG!", "");
translation.add(79, "¡Aarrrggg! ¡Ya vooooy!", "Arata");
translation.add(80, "No te precipites.\nY ten cuidado con lo del conductor.", "Shizuka");
translation.add(81, "Vale. ¡Lo tendré!", "Arata");
translation.add(82, "Ains...", "Shizuka");
translation.add(83, "*Bostezo*...\n¿Qué pasa? ¿Por qué tanto ruido?", "Nanae");
translation.add(84, "¿Eh...? ¡Mamá!", "Shizuka");
translation.add(85, "Nos días, Shizuka.", "Nanae");
translation.add(86, "Lo siento.\nNunca se me habría ocurrido que pudieses\nestar todavía en la cama...", "Shizuka");
translation.add(87, "¿Eh?", "Nanae");
translation.add(88, "Mira la hora...", "Shizuka");
translation.add(89, "Oh...", "Nanae");
translation.add(90, "¡¡Aaaaaahhhhh!! ¡Voy a llegar tarde!", "Nanae");
translation.add(91, "¡Llegas taaaaaaaaaaaaaarde!", "Nodoka");
translation.add(92, "En mi pueblo, 'tarde' lleva solo una 'a'.", "Arata");
translation.add(93, "¡Llegas tardisisísimo! ¡DIEZ minutarros tarde!\n¿¡Pero qué hora te crees que es!?", "Nodoka");
translation.add(94, "Hace tan solo ocho minutos estaba todavía en la\ncama. Soy la leche de rápido.", "Arata");
translation.add(95, "¿¡A quién le importa eso!?\n¿¡No te dije ayer que te pusieses el despertador!?", "Nodoka");
translation.add(96, "¡Y mira los pelos que llevas!\n¡Pareces un espantapájaros!", "Nodoka");
translation.add(97, "Tanto dará. No soy una chica y esas cosas.", "Arata");
translation.add(98, "¡Ah, no! ¡Los chicos también tenéis que cuidaros!", "Nodoka");
translation.add(99, "Te lo apañaré en un periquete, ¡no te muevas!", "Nodoka");
translation.add(100, "¡Ah! Ey, ¡ya basta! ¡Ni que fueses mi hermana!", "Arata");
translation.add(101, "De todas formas, se me volverá\na despeinar mientras corremos.", "Arata");
translation.add(102, "¡Quietecito y calladito!", "Nodoka");
translation.add(103, "¡Hngg! ¡Hnngg!", "Nodoka");
translation.add(104, "¡Au! ¡Au, au!", "Arata");
translation.add(105, "¡Arg! ¿Cómo puede estar así de enredado?", "Nodoka");
translation.add(106, "¡Aaaarghh! ¡Corta el rollo! ¡Vamos a llegar tarde!", "Arata");
translation.add(107, "¡Que no!", "Nodoka");
translation.add(108, "¡Ahh! ¡Ahhh! ¡Me haces daño!", "Arata");
translation.add(109, "¡A escaparse...! ¡Hop!", "Arata");
translation.add(110, "¿¡Ahh!?", "Nodoka");
translation.add(111, "Si tienes tantas ganas de tocar pelo, que sea\nel tuyo. Hazte alguna coletita más o algo.", "Arata");
translation.add(112, "¿¡Ahhhh!?", "Nodoka");
translation.add(113, "¡Oyeee! ¿¡Qué haces tirándome del pelo!?\n¡Idiota! ¡Pervertido!", "Nodoka");
translation.add(114, "No hay tiempo para eso. Tenemos que darnos prisa.", "Arata");
translation.add(115, "¡Ahhh! ¡No me estires del brazo!", "Nodoka");
translation.add(116, "¡VA!", "Arata");
translation.add(117, "¡¡Aahhhhhh!! ¡Vas demasiado rápido! ¡¡DEMASIADOOO!!", "Nodoka");
translation.add(118, "Uy espera... Que me he dejado las zapatillas.", "Arata");
translation.add(119, "¡Uuaaaaa!", "Nodoka");
translation.add(120, "¡TONTOOOOOO!", "Nodoka");
translation.add(121, "¿Un estudiante de intercambio?", "Arata");
translation.add(122, "...Sí... Un estudiante... de intercambio...\nUf, uf...", "Nanae");
translation.add(123, "Señorita Fukam...\nQuiero decir, profesora...\n¿Se encuentra bien?", "Nodoka");
translation.add(124, "He tenido que venir corriendo porque Arata y Shizuka\nno se han dignado a despertar a su propia madre...", "Nanae");
translation.add(125, "Pensábamos que ya estarías despierta y arreglada...\n¡Oye, espera! Estamos en la escuela. Aquí no eres\nmi madre. Únicamente mi profesora.", "Arata");
translation.add(126, "Lo sé, lo sé.", "Nanae");
translation.add(127, "Profesora, ibas a hablarnos del estudiante\nde intercambio...", "Nodoka");
translation.add(128, "Ah sí. Pues veréis...", "Nanae");
translation.add(129, "¡Vais a alucinar! ¡Es un miembro de una familia\naristócrata de Inglaterra! Bueno, algo así.", "Nanae");
translation.add(130, "Anda, ¿un extranjero?", "Arata");
translation.add(131, "¡Y a las chicas os va a encantar!\nEs un chico, ¡un príncipe rubio!", "Nanae");
translation.add(132, "¿¡Es RUBIO!?", "Arata");
translation.add(133, "¿¡Y príncipe!?", "Nodoka");
translation.add(134, "¿En serio?", "Arata");
translation.add(135, "Bueno. La verdad es que no lo he visto todavía,\nasí que no sé si es rubio.", "Nanae");
translation.add(136, "Madre mía.", "Nodoka");
translation.add(137, "Igual hasta está super cachas. Es un extranjero\ndespués de todo.", "Arata");
translation.add(138, "Pues sí.", "Nanae");
translation.add(139, "¿¡Qué!? ¡No, no no! Nada de eso. Seguro.", "Nodoka");
translation.add(140, "Madre mía, me pregunto cómo será.\nEstoy muy emocionada.", "Nanae");
translation.add(141, "¿Qué...? ¡Tengo que contárselo a los demás!", "Kazuko");
translation.add(142, "¿Qué? ¿¡Un extranjero!?", "Kazuhiro");
translation.add(143, "¿¡Un príncipe montando a caballo!?", "Chica de clase");
translation.add(144, "Un... A ¿Un príncipe...?", "Chica de clase");
translation.add(145, "¡Sí! Mide algo así como 1.90m.\n¡Y es todo un hombretón!", "Kazuhiro");
translation.add(146, "Yo he oído que dirige el equipo de\nbéisbol de primera división.", "Chico de clase");
translation.add(147, "¿Juega en primera división pese a ser británico?", "Chica de clase");
translation.add(148, "¡Increíble!", "Chico de clase");
translation.add(149, "¿Eh? ¿Que es un genio de CI 300?\n¿Y que se saltará un curso e irá directamente\na la universidad el año que viene?", "Chica de clase");
translation.add(150, "¿Que quéé? ¿¡Es tan inteligente!?", "Kazuhiro");
translation.add(151, "¿Qué son esas cosas que están diciendo todos?", "Nodoka");
translation.add(152, "¡VA a unirse al club de béisbol ya veréis!", "Kazuhiro");
translation.add(153, "Me pregunto si le gustará el judo.", "Chico de clase");
translation.add(154, "Ah pues siendo un extranjero, debería estar en la\nlegión extranjera, ¿no? ¿Qué es una legión extranjera?", "Chica de clase");
translation.add(155, "¡Ni hablar! ¡Es un príncipe!, ¿recuerdas?\n¡Un prín-ci-pe! ¿La legión extranjera? ¡Y un cuerno!", "Kazuko");
translation.add(156, "¡Eh! ¡Tateyama! ¿¡Qué significa todo esto!?", "Arata");
translation.add(157, "¿¡Qué!? ¡Lo dices como si fuese culpa MÍA!", "Kazuko");
translation.add(158, "¡Es que es todo culpa tuya!", "Arata");
translation.add(159, "¡Les he contado exáctamente lo que he oído!", "Kazuko");
translation.add(160, "¡Lo que importa es que lo has ido contando por ahí!", "Arata");
translation.add(161, "Bah, tanto dará. ¿Y dónde está el susodicho\nestudiante de intercambio?", "Kazuhiro");
translation.add(162, "¡Eso! ¡¿Cuándo vendrá?!\nLa clase ya ha terminado.", "Kazuko");
translation.add(163, "¿Me ves con cara de saberlo?", "Arata");
translation.add(164, "Muy bien. Entonces tendremos que mandar a un\nexplorador. Hoy eres tú el encargado, así que,\n¡ya tardas!", "Kazuhiro");
translation.add(165, "¿¡Que qué!?", "Arata");
translation.add(166, "Y tú también Nodoka.", "Kazuko");
translation.add(167, "¿¡Yo también!?", "Nodoka");
translation.add(168, "¡Os animaremos desde aquí!\n¡Que tengáis buena suerte!", "Kazuhiro");
translation.add(169, "¡Estaremos esperando las buenas nuevas!", "Kazuko");
translation.add(170, "¡Adiós!", "Todos los estudiantes");
translation.add(171, "Pues sí que están alterados con el tema.", "Arata");
translation.add(172, "Pero puedo entenderlo. Yo también estoy emocionada.\nMe pregunto cómo será...", "Nodoka");
translation.add(173, "Ah. Yo creo que será un tipo normal.\nNo me espero alguien especial.", "Arata");
translation.add(174, "Al final seguro que es medio japonés, y será\ncomo nosotros. La vida juega esas malas pasadas.", "Arata");
translation.add(175, "Así que malas pasadas, ¿eh?", "Nodoka");
translation.add(176, "Pero no deberías hablar así delante de él.\nLo acaban de transferir y debe estar muy nervioso.", "Nodoka");
translation.add(177, "Ya, ya... Claro que no lo haré. Es obvio...", "Arata");
translation.add(178, "¡¿Uaaaa?!", "");
translation.add(179, "¡Iiiiic!", "Nodoka");
translation.add(180, "Au, au, au...", "Arata");
translation.add(181, "Me empotro contra alguien. Pillado por sorpresa,\nme estampo contra el suelo, llevándome a Nodoka\npor el camino.", "");
translation.add(182, "¡Ves con más cuidado!", "Nodoka");
translation.add(183, "Lo siento. ¿Estáis bien?", "¿?");
translation.add(184, "Sí. Estamos bien, no te preocupes.", "Arata");
translation.add(185, "Lo sentimos también nosotros.", "Nodoka");
translation.add(186, "Ah, no os preocupéis. ¿Podéis levantaros?", "¿?");
translation.add(187, "S-Sí...", "Nodoka");
translation.add(188, "¿¡Eh!?", "");
translation.add(189, "Ehh...", "Arata");
translation.add(190, "¿Pero qué...?", "");
translation.add(191, "Un cabello dorado y radiante.", "");
translation.add(192, "Unos ojos verdes intensos. Una piel perfecta.", "");
translation.add(193, "La luz proveniente de la ventana situada detrás de\nél hace que parezca una super-estrella en el escenario.", "");
translation.add(194, "Parece casi como si hubiese salido de un cuento\nde hadas...", "");
translation.add(195, "Es... Es el príncipe...", "Nodoka");
translation.add(196, "Sí. Un príncipe.", "");
translation.add(197, "Venga, dadme la mano.", "Chico rubio");
translation.add(198, "...", "Nodoka");
translation.add(199, "¡S-Sí!", "");
translation.add(200, "¿Una ilusión óptica? ¿O es que de verdad llevaba\nun manto rojo y una corona?", "");
translation.add(201, "Y es que ni siquiera parecían fuera de lugar.\nEran tan radiantes y resplandecían tanto...", "");
translation.add(202, "¿Y tú? ¿Te encuentras bien?", "Chico rubio");
translation.add(203, "Eh... Sí, sí. Estoy bien.", "Arata");
translation.add(204, "Ajá. Menos mal.", "Chico rubio");
translation.add(205, "...", "Arata");
translation.add(206, "¡¡Aaaaaaaah!!", "Las chicas");
translation.add(207, "¡¡Buaaaaah!!", "Los chicos");
translation.add(208, "Vale... ¡Silencio ya! ¡Silencio!", "Nanae");
translation.add(209, "Bien. Entonces,\n¿podrías hacer una breve autopresentación?", "Nanae");
translation.add(210, "Encantado de conoceros a todos.", "Chris");
translation.add(211, "Me llamo Chris Northfield y estaré en vuestra\nclase a partir de hoy.", "Chris");
translation.add(212, "Estaré por aquí solamente un mes, pero espero\nllevarme bien con todos.", "Chris");
translation.add(213, "¡¡Guauuuuuuu!!", "Todos los estudiantes");
translation.add(214, "Es rubio platino...", "Chica de clase");
translation.add(215, "¡Una belleza rubia de intercambio!", "Kazuko");
translation.add(216, "¡Y habla japonés perfectamente!", "Chico de clase");
translation.add(217, "¡Uf! ¡Ahora si que estoy efusivo!", "Kazuhiro");
translation.add(218, "Qué guay...", "Chica de clase");
translation.add(219, "¡Pregunta!", "Chica de clase");
translation.add(220, "¡Eso, eso! ¡Y yo tengo otra!", "Chica de clase");
translation.add(221, "¡Por turnos! ¡Iremos de izquierda a derecha!", "Kazuko");
translation.add(222, "¿Cuáles son tus aficiones?", "Chica de clase");
translation.add(223, "¿Qué deportes practicas?", "Kazuhiro");
translation.add(224, "Eh... ¿Eres de sangre noble?", "Chica de clase");
translation.add(225, "¡Parad! ¡He dicho que uno por uno!", "Kazuko");
translation.add(226, "Tateyama, detente TÚ también. Lamento interrumpiros\nel momento, pero ya haréis las preguntas al terminar\nla clase.", "Nanae");
translation.add(227, "Y ahora, a ver dónde te sentamos...", "Nanae");
translation.add(228, "¡Hay un asiento libre aquí!", "Kazuko");
translation.add(229, "¡Ey! ¡Ahí me siento yo!", "Kazuhiro");
translation.add(230, "Hay uno libre detrás de Fukamori y Nonomiya.", "Nanae");
translation.add(231, "Fukamori, desplázate un asiento. Así Chris podrá\nsentarse ahí. Nonomiya, deja que Chris use tus\nlibros de texto.", "Nanae");
translation.add(232, "¿¡Eehhh!? ¿A-Aquí?", "Nodoka");
translation.add(233, "Hoy es vuestro turno, ¿no? Chris, si hay algo\nque no entiendas, pregúntales a esos dos.", "Nanae");
translation.add(234, "Sí, señorita.", "Chris");
translation.add(235, "Está bien. Y con eso concluimos la tutoría.\nEmpecemos con la clase.", "Nanae");
translation.add(236, "Abrid el libro por la página 148.\nKanada, ¿puedes intentar leer?", "Nanae");
translation.add(237, "¡S-Sí!", "Kanada");
translation.add(238, "Gracias por lo de antes.\nAsí que estamos en la misma clase.\nEncantado de conocerte.", "Chris");
translation.add(239, "¡S-Sí! ¡Encantada!", "Nodoka");
translation.add(240, "...", "Arata");
translation.add(241, "Encantado de conocerte.", "Chris");
translation.add(242, "¿Eh...? Ah. Eh... Sí, claro.", "Arata");
translation.add(243, "¡Vale! ¡Hora de las preguntas!", "Kazuko");
translation.add(244, "¿Cuáles son tus hobbies?", "Chica de clase");
translation.add(245, "Me gusta la lectura y los deportes.", "Chris");
translation.add(246, "¿Qué tipo de deportes?", "Chico de clase");
translation.add(247, "¿Montar a caballo?", "Chica de clase");
translation.add(248, "Sí. He hecho algo de eso.", "Chris");
translation.add(249, "¿En serio? Guau, ¡qué guay!", "Chica de clase");
translation.add(250, "Bueno. Tampoco tanto.", "Chris");
translation.add(251, "¡Yo, yo!\n¿Y dónde estás viviendo ahora?", "Kazuko");
translation.add(252, "Estoy viviendo en una casa que pertenece a un\nviejo amigo de mi padre. Aunque está algo lejos\nde aquí.", "Chris");
translation.add(253, "Hablas muy bien japonés.\n¿Dónde aprendiste a hablarlo?", "Chica de clase");
translation.add(254, "Mi padre es letrado del idioma japonés.\nAsí que he crecido con él prácticamente.", "Chris");
translation.add(255, "¿Eres artista?", "Chica de clase");
translation.add(256, "Jaja. No. Lo siento.", "Chris");
translation.add(257, "¿Ya has decidido a qué club vas a unirte?\n¿A nuestro club de béisbol por ejemplo?", "Kazuhiro");
translation.add(258, "O al club de judo.", "Chico de clase");
translation.add(259, "Me gustan las artes marciales japonesas, supongo.", "Chris");
translation.add(260, "Ah. ¡Entonces deberías hablar con Nodoka!\n¡Su padre lleva un dojo de kendo!", "Kazuko");
translation.add(261, "¿¡Eh!?", "Nodoka");
translation.add(262, "¿Es eso cierto, Nodoka?", "Chris");
translation.add(263, "¿¡No-no-nodoka!?", "Nodoka");
translation.add(264, "Lo siento. Quizá ha sido un poco rudo por mi\nparte llamarte por tu nombre en vez de por\ntu apellido.", "Chris");
translation.add(265, "Ah. ¡No, no! ¡Para nada! ¡Claro que no!", "Nodoka");
translation.add(266, "Ah, vale.\nEntonces te llamaré Nodoka de ahora en adelante.", "Chris");
translation.add(267, "¡V-Vale!", "Nodoka");
translation.add(268, "¡Entonces llámame Kazuko!", "Kazuko");
translation.add(269, "¡A mí Yuuki!", "Chica de clase");
translation.add(270, "Yo soy Hiromi...", "Chica de clase");
translation.add(271, "¡Y yo Keiko!", "Chica de clase");
translation.add(272, "Vale.", "Chris");
translation.add(273, "Oye Nodoka, enséñale a Chris tu casa.\nHa dicho que estaba interesado.", "Kazuko");
translation.add(274, "¿Te importaría, Nodoka?", "Chris");
translation.add(275, "¿¡Eh!? Eh, ¡claro!", "Nodoka");
translation.add(276, "Gracias. Muy amable por tu parte.", "Chris");
translation.add(277, "No es justo, Nodoka.\n¡Yo también quiero invitarle!", "Suzuki");
translation.add(278, "¿Y si le enseñamos los alrededores entre todos\nal terminar las clases?", "Arai");
translation.add(279, "¡Mayoría absoluta!", "Nishimoto");
translation.add(280, "Os lo agradezco. Será un placer.", "Chris");
translation.add(281, "¡Vaaale!", "Las chicas");
translation.add(282, "...", "Arata");
translation.add(283, "¿T-Te encuentras bien, Fukamori?", "Kanada");
translation.add(284, "¿Uhm...? ¿Kanada? ¿Qué quieres?", "Arata");
translation.add(285, "Uhm... Es que pareces algo decaído, así que\npensaba que igual te encontrabas mal...", "Kanada");
translation.add(286, "No. Lo que pasa es que...", "Arata");
translation.add(287, "Es muy guapo.", "Arata");
translation.add(288, "¿¡G-Guapo!?", "Kanada");
translation.add(289, "Sí. Es que le rodea esa extraña aura que le hace\nparecer super guay. ¿No crees que es guapo?", "Arata");
translation.add(290, "S-Sí, claro... Lo que pasa es que...\n¿Así que también pensáis lo mismo los chicos...?", "Kanada");
translation.add(291, "¿Eh? Diría que su aura la vemos todos igual.", "Arata");
translation.add(292, "...", "Kanada");
translation.add(293, "S-Sí... Le rodea un aura...", "");
translation.add(294, "¿Uhm...?", "Arata");
translation.add(295, "Me ha parecido ver algo por el rabillo del ojo.", "");
translation.add(296, "Giro la cabeza hacia la ventana.\nAunque, por supuesto, allí no hay nada.", "");
translation.add(297, "Y justo cuando voy a apartar la mirada,\nel estudiante de intercambio también mira\nen esa dirección.", "");
translation.add(298, "Anda. ¿Así que no eran imaginaciones mías?", "");
translation.add(299, "Vuelvo a mirar otra vez, pero lo único que veo\nes el cielo azul.", "");
translation.add(300, "¿...?", "Arata");
translation.add(301, "¿Q-Qué ocurre?", "Riko");
translation.add(302, "Ah. Nada.", "Arata");
translation.add(303, "En fin.", "");
translation.add(304, "...", "Chris");
translation.add(305, "......", "¿¿??");
translation.add(306, "¡¡¡...!!!", "¿¿??");
translation.add(307, "Uhm...", "Mujer de rojo");
translation.add(308, "El viento que sopla sobre la terraza juguetea\ncon los flecos de un vestido.", "");
translation.add(309, "Sobre la terraza, de pie, una mujer de rojos ropajes\nque contrastan abruptamente con el cielo azul.", "");
translation.add(310, "Viste con un esbelto traje que le llega hasta los\ntobillos y una diadema de plata.", "");
translation.add(311, "Por la forma en la que se sostiene sobre la losa,\nparece casi como una princesa preparada para ir\na un baile.", "");
translation.add(312, "Pero el objeto que aferra en su mano detruye por\ncompleto esa imagen de cuento de hadas.", "");
translation.add(313, "Es una lanza que de normal usarían caballeros\na lomos de su corcel. Es más alta que ella misma\ny su punta afilada como las garras de un león.", "");
translation.add(314, "Y esa punta atraviesa a la monstruosidad con forma\nde perro. Monstruosidad que acaba absorbida por\nllamas azules.", "");
translation.add(315, "La bestia se convierte lentamente en cenizas\nmientras sus piernas se baten en un vano intento\npor escapar.", "");
translation.add(316, "La mujer espera hasta que la bestia se ha consumido\npor completo, para mirar posteriormente al vacío\ncielo que hay encima en lo alto.", "");
translation.add(317, "Muy bien. ¿Quién es el siguiente?", "Mujer de rojo");
translation.add(318, "Como respuesta, el cielo empieza a agitarse.", "");
translation.add(319, "Y dónde no había nada hacía escasos momentos,\naparecen más de esas bestias.", "");
translation.add(320, "Una, dos y tres. Aparecen en un abrir y cerrar\nde ojos. Y se disponen a rodear a la mujer\ntrazando un círculo lentamente.", "");
translation.add(321, "No parece muy impresionada.\nUna esquina de su boca se mueve ligeramente.", "");
translation.add(322, "Je, je...", "Mujer de rojo");
translation.add(323, "Atravesados por la lanza, los cuerpos de las\nbestias son consumidos por el fuego azul.", "");
translation.add(324, "En cuanto el fuego desaparece, lo único que queda\nes un anillo de cenias sobre las baldosas.", "");
translation.add(325, "Y en el centro de ese anillo está la mujer,\nimpasible, como si no hubiese pasado nada.", "");
translation.add(326, "Es como si la furiosa tormenta de fuego de hace\nun momento no tuviese nada que ver con ella.\nSe limita a disfrutar de la brisa.", "");
translation.add(327, "Son tan débiles...", "Mujer de rojo");
translation.add(328, "Dudo seriamente que estas criaturas inferiores\nfuesen a tener el Velo.", "Mujer de rojo");
translation.add(329, "Aún así, si hacían de guardianes, la princesa\nque vigilaban debe ser muy débil.", "Mujer de rojo");
translation.add(330, "No debería hacerme demasiadas ilusiones.", "Mujer de rojo");
translation.add(331, "¿Uhm?", "Mujer de rojo");
translation.add(332, "La mujer divisa una sombra moviéndose al otro\nlado de la valla que rodea la terraza.", "");
translation.add(333, "Arremete contra ella. Su lanza rasga la piedra,\npero no ha sido lo bastante rápida para alcanzar\nla sombra.", "");
translation.add(334, "Bueno. Pues dejaré a esta sabandija a la\npequeñina a por la que les habían mandado.", "Chica de rojo");
translation.add(335, "Sus ojos, de un rojo aún más intenso que su\nvestido, se enfocan en su lanza.", "");
translation.add(336, "Jaja. Quieres divertirte de verdad, ¿eh?", "Mujer de rojo");
translation.add(337, "El metal curvado de la lanza ensalza\nsu sonrisa sádica.", "");
translation.add(338, "Ya han terminado las clases...", "");
translation.add(339, "Oye, Chris. ¿Dónde quieres ir luego?", "Kazuko");
translation.add(340, "¡Propongo que vayamos a la cafetería Daiichi!\n¡Hacen un té exquisito!", "Suzuki");
translation.add(341, "¡Ah, muy buena idea!\nLa cafetería Daiichi de nuestro pueblo\nes bastante famosa.", "Nodoka");
translation.add(342, "¿Os referís al edificio que hay junto\na la torre del reloj!", "Chris");
translation.add(343, "¡Sip! ¡Es como un castillito pequeño!\n¿Verdad que es maravilloso?", "Nishimoto");
translation.add(344, "No es COMO un castillo. He oído que es uno de\nverdad. El castillo al que el dueño solía ir y\nque se lo trajo a Japón.", "Arai");
translation.add(345, "¿Eh? ¿En serio?", "Suzuki");
translation.add(346, "Parece una historia muy interesante.\nEchemos un vistazo...", "Chris");
translation.add(347, "¿¡...!?", "Chris");
translation.add(348, "¿Chris...?", "Nodoka");
translation.add(349, "Lo siento. Acabo de recordar que ya me había\ncomprometido a quedar con alguien.\n¡Disculpadme!", "Chris");
translation.add(350, "¿¡Quéééé!?", "Chicas");
translation.add(351, "Lo siento mucho, de verdad.\nMuchas gracias por enseñarme los alrededores.\n¡Nos vemos mañana!", "Chris");
translation.add(352, "¡Oye, Chris!", "Nodoka");
translation.add(353, "Vaya. Se ha marchado.", "Kazuko");
translation.add(354, "Esta sensación... ¿¡Guardianes...!?", "Chris");
translation.add(355, "Fiu. Al fin he terminado. Maldita Nodoka,\n¡dejarme todo el marrón de la limpieza a mí!\n¡Le haré pagar por esto!", "Arata");
translation.add(356, "Oye, que yo tengo más motivos para estar enfadado\nque tú. ¿Por qué tengo que ayudar yo? Ni siquiera\nme toca hoy el turno de la limpieza.", "Kazuhiro");
translation.add(357, "¡Gracias! ¡Se nota que eres mi mejor amigo!", "Arata");
translation.add(358, "Estando las chicas como están, supongo que no\nhay nada que hacer. Es RUBIO, después de todo.", "Kazuhiro");
translation.add(359, "¡Pues sí! ¡Y además es bastante guapo!", "Arata");
translation.add(360, "Además, yo hasta me creo que sea príncipe de verdad.\nCiertamente no es muy normal.", "Kazuhiro");
translation.add(361, "Un príncipe, ¿eh...?", "Arata");
translation.add(362, "¿Y le has visto la cara? Hasta me entran ganas\nde desmayarme cada vez que lo miro.", "Kazuhiro");
translation.add(363, "\"¡Ahhh! ¡Príncipe! ¡Mire hacia aquí!\"", "Kazuhiro");
translation.add(364, "Sí. Es bastante apuesto...", "Arata");
translation.add(365, "...", "Arata");
translation.add(366, "¿Hola...? ¿Tierra llamando a Arata?", "Kazuhiro");
translation.add(367, "¡Arataaa! ¡Regresa con nosotros! ¡Eyyyy!", "Kazuhiro");
translation.add(368, "¡Wah!", "Arata");
translation.add(369, "Anda. Mira qué tarde es...\nTengo que ir a entrenar. Te dejo el resto a ti.", "Kazuhiro");
translation.add(370, "Claro. Gracias de nuevo.", "Arata");
translation.add(371, "¡Nos vemos!\n¡Tengo que encargarme de cierta pelota blanca!", "Kazuhiro");
translation.add(372, "*Suspiro*\nEs realmente guapo.", "Arata");
translation.add(373, "Es casi como si no fuese real... No, no puede ser.\nAlgo tan bello tiene que ser verdadero.", "");
translation.add(374, "Es tan rubio que su cabello prácticamente emite\nluz por sí mismo. Como si él mismo fuese el sol...\nNo. Más bien la luna.", "");
translation.add(375, "Sí. A ese mismo color me estaba refiriendo...\n¿¡Eh!?", "");
translation.add(376, "¡¡Plas!!", "");
translation.add(377, "¿¡Uooa!?", "Chris");
translation.add(378, "¡Gwuaah!", "Arata");
translation.add(379, "Au...", "Arata");
translation.add(380, "Auuu...", "Chris");
translation.add(381, "L-Lo siento. Tenía prisa y...", "Chris");
translation.add(382, "Vaya, ¡pero si es él en persona!", "Arata");
translation.add(383, "¡T-Tú otra vez!", "Chris");
translation.add(384, "Anda. Nas. Qué cosas encontrarte aquí.", "Arata");
translation.add(385, "¡Lo siento, pero tengo prisa...! ¿¡Uaaaa!?", "Chris");
translation.add(386, "¿¡Uaaahh!?", "Arata");
translation.add(387, "Unhhh... Lo siento.", "Chris");
translation.add(388, "Au, au, au...\n¿A dónde vas con tantísima prisa?", "Arata");
translation.add(389, "¡A-A ningún sitio! ¡Déjame! ¡Hngggg!", "Chris");
translation.add(390, "¡Yeeeooowww! ¡Para, para! ¡Mi pierna!\n¡La tengo atrapada!", "Arata");
translation.add(391, "Tenemos las piernas enredadas entre sí,\ny por eso no me puedo levantar.", "");
translation.add(392, "Espera un momento, las desenredaré.\nY... ¡Ale-hop!", "Arata");
translation.add(393, "¿¡Eh...!?", "Chris");
translation.add(394, "Y ahora así... Y después...", "Arata");
translation.add(395, "¡Au! ¡¡Au, auu!!", "Chris");
translation.add(396, "¿Eh? ¿Quizá así...?", "Arata");
translation.add(397, "¡Ahhhhhh! ¿Eh...? ¡Ahhhh!", "Chris");
translation.add(398, "¡Ey! ¡Deja de hacer el moñas y PÍRATE!", "Chris");
translation.add(399, "Va, cállate.\nTe quejas tanto que casi pareces una chica.", "Arata");
translation.add(400, "¡¿Pero bue...?!", "Chris");
translation.add(401, "¡Au!", "Arata");
translation.add(402, "¡¿Qué estás haciendo?!", "Arata");
translation.add(403, "¿¡A quién estás llamando chica!?", "Chris");
translation.add(404, "¿Qué?", "Arata");
translation.add(405, "¡Mira mi vestimenta! ¿Es que estás CIEGO?\n¡¿De qué te parezco una chica?!", "Chris");
translation.add(406, "De repente, se ha enfadado mucho.", "");
translation.add(407, "Tras sus ojos verdes arde una furia incontrolable.", "");
translation.add(408, "...", "Chris");
translation.add(409, "...", "Arata");
translation.add(410, "L-Lo siento.", "Arata");
translation.add(411, "B-Bueno, está bien.\nSiempre que haya que quedado claro ese punto.", "Chris");
translation.add(412, "¡Arg...! ¡No tengo tiempo para estar\nhaciendo el tonto aquí!", "Chris");
translation.add(413, "¡O-Oye! ¿¡A dónde vas!?", "Arata");
translation.add(414, "¡Cállate, y no me sigas!", "Chris");
translation.add(415, "¡¿Eh...?!", "Arata");
translation.add(416, "Y con eso dicho (o más bien escupido)\nel estudiante de intercambio se gira y\nsale corriendo a través del pasillo.", "");
translation.add(417, "...", "Arata");
translation.add(418, "¿¡Es que le falta un tornillo o algo!?", "Arata");
translation.add(419, "Enfadándose así sin más, ¡y empujando a la gente!\n¿Es que no le echan aceite o algo?", "Arata");
translation.add(420, "Madre mía, qué tipo tan violento.\nMe da igual que sea rubio o lo que sea.\n¡No le pienso dejar MIS libros nunca más!", "Arata");
translation.add(421, "...", "Arata");
translation.add(422, "Fiu...", "Arata");
translation.add(423, "Bueno. Al gritar un poco ya me he relajado.", "");
translation.add(424, "Pero tío. Ya se lo diré a la cara\nmañana cuando lo vea.", "");
translation.add(425, "En fin. Tocará marcharse...\n¿Eh? ¿Qué es esto?", "Arata");
translation.add(426, "Mientras me espolvoreo los pantalones,\nme fijo en algo brillante que yace en\nel suelo del pasillo.", "");
translation.add(427, "Es una pieza metálica pequeña. Algo que desentona\naquí en la escuela. ¿Qué podría ser?", "");
translation.add(428, "¿Un anillo? Me pregunto si será suyo.", "Arata");
translation.add(429, "No tiene ninguna decoración. Es meramente una \nfranja de metal.", "");
translation.add(430, "Aunque posee un pequeño patrón gravado en él.\nDebe de ser alguna reliquia familiar o algo así.", "");
translation.add(431, "Supongo que es normal que la gente ostentosa\ngaste accesorios ostentosos.", "Arata");
translation.add(432, "¡Pero va en contra de las normas de la escuela!\n¡Si la profesora se enterase se enfadaría!", "Arata");
translation.add(433, "Pero no voy a chorizárselo.\nSe lo devolveré mañana.", "");
translation.add(434, "Aún así... Es un buen anillo.", "Arata");
translation.add(435, "Cuando lo miras, es como si te entrasen\nganas de atesorártelo.", "");
translation.add(436, "No sabría decir. Igual es a esto a lo que se\nrefiere la gente con la atracción que produce\nel trabajo de orfebrería caro de alto nivel.", "");
translation.add(437, "Estoy convencido de que no se molestará por que\nme lo pruebe un momentito.", "Arata");
translation.add(438, "Intento colocarme el anillo en la mano izquierda.\nNo me entra en el dedo mayor, pero encaja a la\nperfección en el anular.", "");
translation.add(439, "Uhm.", "Arata");
translation.add(440, "Extiendo mi mano en dirección a la luz del\nsol poniéndose, que brilla a través de la ventana.", "");
translation.add(441, "El intrincado patrón gravado refleja la luz\nde forma que hace que parezca que emita luz\npor sí mismo.", "");
translation.add(442, "Tiene el mismo color que el hierro fundido que\nvi en una ocasión en una excursión a un horno\nde fundición...", "");
translation.add(443, "¿Eh...? ¡¿Pero qué...?!", "Arata");
translation.add(444, "¡¿Qué le pasa a esta cosa?!, ¡está quemando!\n¡Au! ¡Au! ¡Ahhhh!", "Arata");
translation.add(445, "¡N-No puedo quitármelo! ¡Urgm!", "Arata");
translation.add(446, "No voy a poder quitármelo... ¿Por qué?", "Arata");
translation.add(447, "Eh...", "Arata");
translation.add(448, "¡¡¿Ehhh?!!", "Arata");
translation.add(449, "¿¡Qué-qué-qué-eh-eh...!?", "Arata");
translation.add(450, "De repente, acabo cegado por una luz que inunda\nmi campo de visión.", "");
translation.add(451, "Empezando por mi mano, mi cuerpo entero acaba\nabsorbido completamente por la luz.", "");
translation.add(452, "¿¡Uahhh!? Uhh... ¡¡Ahhhhhhhh!!", "Arata");
translation.add(453, "¡¡¡Ahhhhhhhhhhhh!!!", "Arata");
translation.add(454, "Uf... Uf...", "Arata");
translation.add(455, "¿Ha sido solo un momento?\n¿O ha sido una hora entera?", "");
translation.add(456, "Miro fatigado el anillo.\nYa no emite luz.", "");
translation.add(457, "Uno, dos, tres, cuatro, cinco...", "Arata");
translation.add(458, "Increíble. Parece que todavía conservo los cinco\ndedos. Y el resto de mi cuerpo parece también\nintacto.", "");
translation.add(459, "¿Q-Qué leches acaba de pasar?\n¿Qué es este anillo?", "Arata");
translation.add(460, "¿Uhm...?", "Arata");
translation.add(461, "Se oyen ruidos en la lejanía. El ruido es como\nel de dos objetos metálicos entrechocando.", "");
translation.add(462, "Viene... ¿De arriba?", "Arata");
translation.add(463, "¿¡Ehhh!?", "Arata");
translation.add(464, "¿Qué... qué está pasando?", "Arata");
translation.add(465, "...", "Arata");
translation.add(466, "¡Solo hay una forma de averiguarlo!", "");
translation.add(467, "¿De dónde viene esta niebla?", "Arata");
translation.add(468, "Al mirar por la ventana, veo que los alrededores\nestán enturbiados por una densa niebla.", "");
translation.add(469, "Quizá es cosa del sol que se está poniendo.\nPero la niebla esta es de un color rojizo que\nno había visto nunca.", "");
translation.add(470, "El edificio de la escuela que hay junto a este\nno se alcanza a ver, ni tampoco el terreno de la\nescuela.", "");
translation.add(471, "Rojo.", "");
translation.add(472, "Es como una gran masa rojiza.", "");
translation.add(473, "Todo lo que alcanzo a ver es el contorno de las\ndos campanas idénticas que representativas de\nnuestra escuela.", "");
translation.add(474, "¿Sigo todavía en la escuela?", "Arata");
translation.add(475, "Logro percibir el sonido del\neco de mis propias pisadas.", "");
translation.add(476, "Es como si ya nos hubiésemos adentrado en la noche,\ny fuese el único que quedase en la escuela.", "");
translation.add(477, "¡Guh...!", "Arata");
translation.add(478, "Para desechar la sensación escalofriante,\nme pongo a correr.", "");
translation.add(479, "¿Qué...? ¿Qué está ocurriendo aquí...?", "Arata");
translation.add(480, "Al abrir la puerta del tejado, el espectáculo\nhace que me quede de piedra.", "");
translation.add(481, "De hecho es tan sumamente extraño que me resulta\ndifícil pensar en alguna pregunta que no sea una\ncompleta y total locura.", "");
translation.add(482, "¿Q-Qué está pasando...?", "");
translation.add(483, "Alguien está corriendo, saltando, embistiendo\ny volteando tan rápido que mis ojos no pueden\nseguir el ritmo.\nSomeone is running, jumping, charging and tumbling\nso fast that my eyes can't keep up.", "");
translation.add(484, "De hecho son dos alguienes.", "");
translation.add(485, "En cuanto se encuentran, salta una chispa como\nsi alguien hubiese lanzado fuegos artificiales.", "");
translation.add(486, "Por el sonido que hace, la angustiada forma debe\nde ser un objeto metálico enorme.", "");
translation.add(487, "Cada vez que las formas golpean la valla que\nrodea la terraza, rebotan como bolas en una\nmesa de billar.", "");
translation.add(488, "Pero los movimientos no son aleatorios.\nAmbas formas se mueven en un arduo intento por\ndestruirse mutuamente.", "");
translation.add(489, "Obviamente están luchando.", "");
translation.add(490, "¿Por qué...?", "");
translation.add(491, "No lo sé.", "");
translation.add(492, "Ni siquiera sé quién está luchando...", "");
translation.add(493, "La sombra más grande parece tener cuatro patas.\nNo es excesivamente alta y a veces gruñe.", "");
translation.add(494, "¿Un perro...?", "");
translation.add(495, "No. Los perros no son así de grandes.", "");
translation.add(496, "Además de que los perros no tienen púas como\nesas.", "");
translation.add(497, "El monstruo de cuatro ojos está atacando a la\nforma de dos piernas que sostiene algún tipo de\narma entre sus manos.", "");
translation.add(498, "Mientras corre, el cabello dorado platino \nse desliza por detrás de él. Lleva un uniforme\nnegro.", "");
translation.add(499, "¿Quién es ese...?", "");
translation.add(500, "Bueno. Menuda pregunta más estúpida.\nSolo puede ser una persona.", "");
translation.add(501, "Es el tipo que han transferido\nhoy a nuestra clase...", "");
translation.add(502, "¡Es el estudiante de intercambio!", "Arata");
translation.add(503, "¡¡Haaa!!", "Chris");
translation.add(504, "¡¡Grrrrrrrr!!", "Monstruo cuadrúpedo");
translation.add(505, "Colisionan de nuevo.", "");
translation.add(506, "Se separan, aterrizan y\ncargan de nuevo contra el otro.", "");
translation.add(507, "Otro choque.", "");
translation.add(508, "En ocasiones, el ritmo del acero entrechocando\nparece que estuviese compuesto por algún músico.", "");
translation.add(509, "¡Ughh...!", "Chris");
translation.add(510, "Tras el doceavo choque, el ritmo empieza a flaquear.", "");
translation.add(511, "El estudiante de intercambio se para, y cae\nsobre una rodilla.", "");
translation.add(512, "Se recupera con presteza, pero el titubeo resulta\nser fatal.", "");
translation.add(513, "Sin dudarlo un momento, la bestia acorta\ndistancias rápidamente.", "");
translation.add(514, "¡Lo van a matar!", "");
translation.add(515, "Cierro los ojos instintivamente, tratando de\nevitar la escena que va a acontecer delante\nde mis ojos.", "");
translation.add(516, "Pero entonces, se oye un choque inusualmente intenso.", "");
translation.add(517, "Y en vez de los gritos del estudiante de intercambio,\noigo a la bestia aullar.", "");
translation.add(518, "¡¡...!!", "Arata");
translation.add(519, "Con vacilación, abro lentamente los ojos.", "");
translation.add(520, "No ha sido la bestia, sino el estudiante de\nintercambio el que ha acabado con su oponente.", "");
translation.add(521, "Uff... Uff... Uff...", "Chris");
translation.add(522, "Del cuerpo de la bestia brota una llama azul,\ny empieza a disolverse.", "");
translation.add(523, "El sonido es similar al de hojas secas\nquemándose, un sonido extrañamente calmado\ndadas las circunstancias.", "");
translation.add(524, "¿Ha ganado...? ¿Ha derrotado a esa...?\n¿A una bestia como esa?", "");
translation.add(525, "Entonces ocurre.", "");
translation.add(526, "La puerta de acero de la terraza se desliza\npor entre mis dedos entumecidos.", "");
translation.add(527, "¿¡Uaaa!??", "Arata");
translation.add(528, "¿¡Qué!?", "Chris");
translation.add(529, "No logro aferrarme a la manija a tiempo.", "");
translation.add(530, "El estudiante de intercambio fija su mirada\nhostil sobre mí, como si yo también fuera un\nenemigo.", "");
translation.add(531, "¡¿Qué demonios estás haciendo aquí?!", "Chris");
translation.add(532, "No atino a emitir ninguna palabra.", "");
translation.add(533, "Paralizado ante el miedo que provoca alguien\ncon el poder de derrotar a un monstruo como ese.", "");
translation.add(534, "¡Pero q...! ¡Ese anillo!", "Chris");
translation.add(535, "¡¿Eh?!", "Arata");
translation.add(536, "Ha divisado el anillo que llevo en la mano\nizquierda, que empieza otra vez a resplandecer\ndébilmente.", "");
translation.add(537, "Eh... Bueno... Verás...", "Arata");
translation.add(538, "¿De dónde lo has sacado...?", "Chris");
translation.add(539, "¡En ese momento...!", "");
translation.add(540, "Ya veo... Así que te las has arreglado para\nentrar al Guarden... Joder, menudo follón.", "Chris");
translation.add(541, "El estudiante de intercambio murmura algo que no\nlogro a entender mientras camina en mi dirección.", "");
translation.add(542, "¿Q-Quién...? ¿Quién eres?", "Arata");
translation.add(543, "Intento que suene con un tono severo, pero\nretrocedo mientras hablo. Y a punto de salir por\npatas.", "");
translation.add(544, "Cogeré el anillo de nuevo. Gracias por traérmelo.", "Chris");
translation.add(545, "¡N-No hace falta que me lo digas!", "Arata");
translation.add(546, "No pretendía quedármelo.\nClaro que iba a devolverlo.", "");
translation.add(547, "Agarro el anillo con la mano derecha, y estiro\ncon todas mis fuerzas.", "");
translation.add(548, "Pero sigue sin querer salir.", "");
translation.add(549, "Ugghhhh... Ugghhhh... ¡Urghhh!", "Arata");
translation.add(550, "¡¿Qué estás haciendo?! ¡Rápido!", "Chris");
translation.add(551, "¡Lo intento! ¡Pero es que no sale!", "Arata");
translation.add(552, "¡¿Qué?! ¡Déjame ver!", "Chris");
translation.add(553, "Me tira del brazo hacia él.", "");
translation.add(554, "¡Ahhh!", "Arata");
translation.add(555, "Salto, e involuntariamente me libero de él.", "");
translation.add(556, "¿¡Eh!? ¡¡Uh...!!", "Chris");
translation.add(557, "¿Eh?", "Arata");
translation.add(558, "No ha sido con mucha fuerza, pero en cuanto\nle empujo, su cara se retuerce de dolor y se\ndesploma.", "");
translation.add(559, "¿E-Eh...?", "Arata");
translation.add(560, "¡¡Ahhh...!!", "Chris");
translation.add(561, "Gime de dolor, mientras se aferra fuertemente el\nhombro izquierdo con los dedos. Un líquido rojo\noscuro empieza a salir.", "");
translation.add(562, "¡¡...!!", "Arata");
translation.add(563, "El fluido empieza a gotear sobre las baldosas.", "");
translation.add(564, "No es que salga a borbotones, pero el hecho\nde que esté sangrando hace que me olvide de\nmi ansiedad.", "");
translation.add(565, "¿¡Estás herido!?", "Arata");
translation.add(566, "No... No es nada. ¡Puedo con ello!\n¡Un enemigo de un nivel tan bajo no puede herirme!", "Chris");
translation.add(567, "Corro hacia él, olvidándome por completo de\nque iba a huír hace escasos momentos.", "");
translation.add(568, "De repente, una sombra salta sobre nosotros.", "");
translation.add(569, "Alzo la vista hacia la forma que bloquea\nla luz del sol. Cuatro motas rojas de luz\nme miran fijamente.", "");
translation.add(570, "Como si estuviesen avisando, las cuatro motas \nse estrechan...", "");
translation.add(571, "¡Cuidad...!", "Arata");
translation.add(572, "Antes de poder terminar la frase, el infierno\nse desata.", "");
translation.add(573, "Siento una presión enorme. Entonces, una garra\ndescomunal barre el aire que hay donde se\nencontraba mi cabeza hace una fracción de segundo.", "");
translation.add(574, "Entonces, yo y el estudiante de intercambio,\nque obviamente ha sido el que me ha desplazado,\nnos estampamos contra el suelo.", "");
translation.add(575, "Ugh... Unhh...", "Arata");
translation.add(576, "El dolor es intenso.", "");
translation.add(577, "Veo una pila de escombros y sobresaliendo\nde ellos, la parte trasera del monstruo.", "");
translation.add(578, "...", "Arata");
translation.add(579, "Ha hecho pedazos el cemento.\nCon eso queda patente lo fuerte que es.", "");
translation.add(580, "Por un instante, me he imaginado a mí mismo\nyaciendo ahí, aplastado como un tomate maduro.", "");
translation.add(581, "¡Ven! ¡Corre!", "Chris");
translation.add(582, "El estudiante de intercambio tira de mi brazo\nviolentamente.", "");
translation.add(583, "Mira a la bestia de refilón, que está intentando\ndesprenderse de los despojos, y me hace entrar\nen el edificio de la escuela.", "");
translation.add(584, "El pasillo está se llena con el sonido de nuestras\nvoces y de las pisadas al correr.", "");
translation.add(585, "¡No te rezagues! ¡¿O es que quieres morir?!", "Chris");
translation.add(586, "¡Ya lo sé!", "Arata");
translation.add(587, "Es inaceptable.\nHay muchas cosas que soy incapaz aceptar.", "");
translation.add(588, "Pero no tengo claro qué es lo que debería objetar\nahora mismo.", "");
translation.add(589, "¡Ahí!", "Chris");
translation.add(590, "Por ahora lo único que puedo hacer es seguir\nsus indicaciones al pie de la letra.", "");
translation.add(591, "¿Qué está pasando...?", "Arata");
translation.add(592, "¡¿Qué está pasando aquí?!", "Arata");
translation.add(593, "¡Uff...! ¡Uff...! ¡Uff...!", "Arata");
translation.add(594, "Creo que lo hemos despistado.", "Chris");
translation.add(595, "El pórtico no se abrirá en un rato...\nDeberíamos parar a descansar un poco.", "Chris");
translation.add(596, "Uff... ¡Uff...!", "Arata");
translation.add(597, "Para llegar hasta aquí hemos tenido que subir y\nbajar montones de escaleras sin descanso alguno.\nY hemos ido a la carrera todo el rato.", "");
translation.add(598, "Estoy hecho trizas. El corazón me va a estallar.\nTengo la garganta muy reseca, y me cuesta respirar.", "");
translation.add(599, "Tras un rato, mi cerebro vuelve a funcionar.\nY empiezo a digerir los eventos que han pasado\nen los últimos minutos.", "");
translation.add(600, "Había un monstruo. Una bestia cuadrúpeda\ndesconocida. Y el estudiante de intercambio\nse la cargó.", "");
translation.add(601, "Y después apareció otro monstruo de esos, y casi\nme corta la cabeza.", "");
translation.add(602, "Y luego... Luego...", "");
translation.add(603, "¡¡¡Aahhhhhhhhhhhh!!!", "Arata");
translation.add(604, "¡¿Nos ha alcanzado?!", "Chris");
translation.add(605, "¡¡Tenemos que decírselo a los demás!!", "Arata");
translation.add(606, "¡¿Q-Qué?!", "Chris");
translation.add(607, "¡Si no nos damos prisa podrían acabar en peligro!\n¡Tenemos que avisarles!", "Arata");
translation.add(608, "¡Eh! Tranquilízate.", "Chris");
translation.add(609, "¡Ya me calmaré luego!\n¡Ahora no es el momento!", "Arata");
translation.add(610, "¡¡Te digo que te tranquilices!!", "Chris");
translation.add(611, "¡No puedo!", "Arata");
translation.add(612, "¡Aquí no hay nadie más a parte de nosotros dos!", "Chris");
translation.add(613, "No puedo tranquilizarme cuando... ¿Qué?", "Arata");
translation.add(614, "Mira afuera. Ya no estamos en el edificio de la\nescuela donde nos encontrábamos antes.", "Chris");
translation.add(615, "Esta es una realidad alternativa, alejado del\nespacio habitual. Se llama \"Guarden\".", "Chris");
translation.add(616, "Es una realidad simulada, que pretende mantener\nalejadas de este mundo las influencias no deseadas.", "Chris");
translation.add(617, "...", "Arata");
translation.add(618, "Esto...\n¿Me lo puedes repetir otra vez pero en cristiano?", "Arata");
translation.add(619, "Vale... En resumen...", "Chris");
translation.add(620, "Lo fundamental es que este es un lugar que\nse parece a nuestra escuela, pero sin serlo.\nAquí no hay estudiantes.", "Chris");
translation.add(621, "...", "Arata");
translation.add(622, "Bueno. No esperaba que lo entendieses.\nPor ahora...", "Chris");
translation.add(623, "¡Increíble! ¡¡Es como en una película de fantasía!!", "Arata");
translation.add(624, "¿Ehhh?", "Chris");
translation.add(625, "Ya me extrañaba que se hubiese hecho de noche\ntan rápido, y que no hubiese nadie por aquí.", "Arata");
translation.add(626, "Y también que hemos bajado como diez pisos,\n¡y el edificio todavía sigue!", "Arata");
translation.add(627, "Así que es todo porque estamos en algún universo\nextraño. Vale. Entendido. ¡Qué guay!", "Arata");
translation.add(628, "¿Lo... entiendes?", "Chris");
translation.add(629, "¿Eh? ¿Qué?", "Arata");
translation.add(630, "Quiero decir... Me sorprende que te lo tomes tan\nbien. Parece que me crees y no te veo perturbado.", "Chris");
translation.add(631, "¿Qué sentido tendría no creerlo?\nLo que he visto ya es bastante\nconvincente de por sí.", "Arata");
translation.add(632, "Bueno. Mejor así que tenerte histérico perdido...", "Chris");
translation.add(633, "Oye, oye...\n¿Y qué eran esos monstruos?\n¿Son esos los guardianes?", "Arata");
translation.add(634, "Los guardianes de este sitio se llaman 'Guardeners'.\nY sí, son los monstruos que has visto antes.", "Chris");
translation.add(635, "Su cometido es purgar de este mundo a los intrusos\nde otros mundos como yo.", "Chris");
translation.add(636, "¿I-Intrusos? ¿De otros mundos?", "Arata");
translation.add(637, "Literalmente. Otros mundos que no sean este.\nLugares a los que no se puede acceder ni por\ntierra ni por aire.", "Chris");
translation.add(638, "In...", "Arata");
translation.add(639, "¡¡Increíble!! ¿De verdad?\nQuiero decir, ¿¡todo esto es real!?", "Arata");
translation.add(640, "¡Guaaaau! ¡Increíble! ¡Esto es la leche!", "Arata");
translation.add(641, "¡Oye! ¡Relájate!", "Chris");
translation.add(642, "Ah. Lo siento. Creo que me he dejado llevar.", "Arata");
translation.add(643, "Mi hermana siempre me dice que tengo que\ncontrolarme y mantener los pies en la tierra.", "Arata");
translation.add(644, "Pero bueno. Volvamos al tema...\n¿Así que eres de otro mundo? ¿Dónde está?", "Arata");
translation.add(645, "Y, eh... Veamos. ¿Por qué viniste a mi mundo?", "Arata");
translation.add(646, "No hay necesidad de que sepas eso.", "Chris");
translation.add(647, "¡Ehhhh! ¡No me seas estrecho!", "Arata");
translation.add(648, "¡Para nada!", "Chris");
translation.add(649, "Me limito a decirte lo que necesitas saber para\nsalir ileso de aquí. No tengo tiempo para hablar\ncontigo.", "Chris");
translation.add(650, "Ohhh...", "Arata");
translation.add(651, "¡Y nada de eso tampoco! Tío, por todo lo que\nhas pasado, ¡¿y aún no comprendes la situación\nen la que estás?!", "Chris");
translation.add(652, "¡Recuerda lo que acaba de pasar!\nEse monstruo va detrás de ti, ¿sabes?", "Chris");
translation.add(653, "¡...!", "Arata");
translation.add(654, "Normalmente, los humanos de este mundo no tienen\nnada que temer de los Guardeners.", "Chris");
translation.add(655, "Pero este Guardener ha decidido que tú eres\nuna entidad ajena que debe ser purgada.", "Chris");
translation.add(656, "Dentro de quince minutos el pórtico volverá a\nabrirse.", "Chris");
translation.add(657, "No voy a poder garantizar tu seguridad en el caso\nde que te vuelvan a atacar.", "Chris");
translation.add(658, "Tendrás que cuidar de ti mismo. Si no...", "Chris");
translation.add(659, "Morirás...", "Chris");
translation.add(660, "...", "Arata");
translation.add(661, "Tienes razón... He estado a punto de palmarla\nantes.", "Arata");
translation.add(662, "Bien. Parece que por fin te das cuenta de la\nsituación en la que te encuentras.", "Chris");
translation.add(663, "Debería agradecértelo.", "Arata");
translation.add(664, "¿Eh?", "Chris");
translation.add(665, "He estado a punto de morir, y tú me has\nsalvado la vida. Debo darte las gracias.\n¡Gracias, estudiante de intercambio!", "Arata");
translation.add(666, "Ehh...", "Chris");
translation.add(667, "Venga, una abrazada.", "Arata");
translation.add(668, "*Sacudida*, *Sacudida*.", "");
translation.add(669, "...", "Chris");
translation.add(670, "¡E-Estúpido!", "Chris");
translation.add(671, "¡¿Y eso?!", "Arata");
translation.add(672, "¡No es momento para dar las gracias! ¡Idiota!", "Chris");
translation.add(673, "¡Mi mamá siempre me dice que debo agradecer\nlas cosas a los demás lo más pronto posible!", "Arata");
translation.add(674, "¡Olvídate de eso! ¡Te digo que debes de ser\nconsciente del problema en el que te encuentras!\n¡Y preocuparte de ti mismo, en vez de por los demás!", "Chris");
translation.add(675, "Piensa en lo que ha pasado antes.\n¡Tienes que centrarte en protegerte a ti mismo\nen vez de intentar alertar a los demás!", "Chris");
translation.add(676, "¡Ahora mismo estarías muerto si no te hubiese\nsalvado! ¡¿Es que no te sientes en peligro?!", "Chris");
translation.add(677, "¡Eres un estúpido! ¡Un estúpido integral!\n¡Y clasificarte como tal es insultar a los estúpidos!", "Chris");
translation.add(678, "¡D-Deja de llamame estúpido! ¡Ya me lo has dicho\ncomo cinco veces! ¿Qué pasaría si me volviese uno\nde verdad?", "Arata");
translation.add(679, "¡Deja de contar las veces que te llamo estúpido,\nestúpido!", "Chris");
translation.add(680, "¡Argh! ¡Y con esa van seis!\n¡Yo soy cola, tu pegamento! ¡Falso!", "Arata");
translation.add(681, "Eh, ¡¿falso?!", "Chris");
translation.add(682, "¡Sí! ¡Eres completamente diferente a como finges\nser en clase!", "Arata");
translation.add(683, "Te pasas el rato enfadado, sin dejar de\ninsultar y de restregar a la gente sus errores.", "Arata");
translation.add(684, "¡Me enfado porque tú haces que me enfade!", "Chris");
translation.add(685, "¡Falso! ¡Interesado! ¡Efecto especial!", "Arata");
translation.add(686, "¡¿Qué se supone que significa eso?!", "Chris");
translation.add(687, "*Suspiro*", "Chris");
translation.add(688, "¿Eh?", "Arata");
translation.add(689, "Ya está bien.\nSi sigo hablando contigo, voy a acabar mal\nde la cabeza.", "Chris");
translation.add(690, "Limítate a descansar para que puedas moverte\ncuando haga falta.", "Chris");
translation.add(691, "El estudiante de intercambio aparta la mirada,\ny se queda mirando al frente sin decir nada más.", "");
translation.add(692, "...", "Arata");
translation.add(693, "Bicho raro.", "");
translation.add(694, "Pero aunque raro, no es un mal tipo.", "");
translation.add(695, "Antes, aunque posiblemente estuviese cagándose\nen los pantalones, me salvó la vida.", "");
translation.add(696, "Si fuese un mal tipo enmascarado por esa cara\nbonita de príncipe, no habría hecho eso.", "");
translation.add(697, "Chris...", "Chris");
translation.add(698, "¿Eh?", "Arata");
translation.add(699, "Me llamo Chris Northfield.\nPero llámame Chris a secas.", "Chris");
translation.add(700, "¿Y a qué viene ESO ahora?", "Arata");
translation.add(701, "No dejas de llamarme \"tú\" y \"estudiante de\nintercambio\". No te acuerdas de mi nombre, ¿verdad?", "Chris");
translation.add(702, "Eh...", "Arata");
translation.add(703, "¡S-Sí que eres rudo para ser un\nestudiante de intercambio!", "Arata");
translation.add(704, "¡Pero no es que me hubiese olvidado completamente!\n¡Sabía que era bastante largo y que empezaba por 'K'!", "Arata");
translation.add(705, "...", "Chris");
translation.add(706, "Por cierto. Yo me llamo Arata Fukamori.", "Arata");
translation.add(707, "Ya lo sé. Ya me lo habían dicho antes.", "Chris");
translation.add(708, "Puedes llamarme Arata a secas, Chris.", "Arata");
translation.add(709, "...", "Chris");
translation.add(710, "Jeje... Está bien, Arata.", "");
translation.add(711, "¡Lo has pillado!", "Arata");
translation.add(712, "Anda, si sabe sonreír y todo.", "");
translation.add(713, "Lo sabía. Tampoco es tan malo.", "");
translation.add(714, "¡De hecho, es bastante agradable!", "");
translation.add(715, "Si completamos esta parte, podremos salir del\nGuarden. Con suerte...", "Chris");
translation.add(716, "Guau...\nLa salida tiene la forma de un pórtico enorme.", "Arata");
translation.add(717, "En cuanto volvamos, la primera cosa que haremos\nes ver cómo quitarte el anillo del dedo. ¿Entendido?", "Chris");
translation.add(718, "Entendido.", "Arata");
translation.add(719, "He intentado quitármelo una vez más, pero no hay\nforma.", "");
translation.add(720, "¿Quizá con un poco de jabón?", "");
translation.add(721, "Oye, cuando volvamos, cuéntame un poco más sobre\nel mundo del que vienes y todo eso.", "Arata");
translation.add(722, "No diré una palabra, por supuesto. ¿Porfi?", "Arata");
translation.add(723, "Si recuerdo algo cuando salgamos...", "Chris");
translation.add(724, "¡Oye! ¡No trates de engañarme, que no soy tonto!", "Arata");
translation.add(725, "En ese preciso instante, el enorme pórtico empieza\na abrirse.", "");
translation.add(726, "Y sorprendentemente, hemos llegado hasta aquí\nsin ninguna sorpresa desagradable.", "");
translation.add(727, "Si fuese una película de miedo, en este momento\nde respiro sería cuando aparecía un monstruo\nrompiendo alguna ventana o algo.", "");
translation.add(728, "¡¿Uuaaaah?!", "Arata");
translation.add(729, "¡¡¡Grrrrrr!!!", "Guardener");
translation.add(730, "¿¡Ahhh!?", "Arata");
translation.add(731, "¡Guhh!", "Chris");
translation.add(732, "¡¡Embody!!", "Chris");
translation.add(733, "Acabo cegado por un momento por la luz que cae.\nOigo el repiqueteo del acero al chocar.", "");
translation.add(734, "¡Urrrrggghhh!", "Chris");
translation.add(735, "Salen chispas disparadas cada vez que el acero\nse encuentra.", "");
translation.add(736, "Por un lado las garras del monstruo. Y\nencontrándose con ellas, una espada bastarda\nde dos manos en la mano de Chris.", "");
translation.add(737, "Pero el equilibrio de poder dura solo un momento.", "");
translation.add(738, "Chris no tiene nada que hacer con la tremenda\nembestida de la bestia. La suela de sus zapatos\nse desgasta sobre el cemento.", "");
translation.add(739, "Urrgh...", "Chris");
translation.add(740, "¡Chris!", "Arata");
translation.add(741, "Argh...", "Chris");
translation.add(742, "¡¡Rhaaaaaaa!!", "Chris");
translation.add(743, "Un instante después, el dorado de su cabello se\nentremezcla con el plateado de su espada en un\nmovimiento desbocado.", "");
translation.add(744, "¡¿...?!", "Guardener");
translation.add(745, "Chris gira como si fuese una peonza.", "");
translation.add(746, "Desvía los colmillos del monstruo hacia abajo,\nacomodándolos en el suelo, para después hacer\ndanzar su espada.", "");
translation.add(747, "¡¡¡Grrrrrrrr!!!", "Guardener");
translation.add(748, "El Guardener aúlla ferozmente y rueda por el\npasillo.", "");
translation.add(749, "¡G-Genial! ¡Lo has conseguido!", "Arata");
translation.add(750, "Uhhh... ¡Uuaaahh!", "Chris");
translation.add(751, "¿¡...!? ¡Chris!", "Arata");
translation.add(752, "Estaba convencido de que había esquivado el\nataque del Guardener, pero se está agarrando\ncon fuerza el hombro.", "");
translation.add(753, "Y su uniforme está cubierto de sangre.", "");
translation.add(754, "¡¡Ahh!!", "Arata");
translation.add(755, "Tú hombro. ¡Esa herida es de antes!", "Arata");
translation.add(756, "¡Cuanta sangre! Pero había parado de sangrar,\n¡y no parecía dolerle ya!", "");
translation.add(757, "Ugh... ¡Tenemos que escapar!\n¡Volverá en cualquier momento!", "Chris");
translation.add(758, "No espera a que le responda. Se limita a correr\npor las escaleras, sujetándose el hombro mientras\ncorre.", "");
translation.add(759, "Por detrás se oyen los sonidos emitidos por el\nenfurecido Guardener que se libera de los\nescombros.", "");
translation.add(760, "Tengo la sensación de que nos va a alcanzar en\ncualquier momento. Empiezo a correr más rápido.", "");
translation.add(761, "¡Y ya casi habíamos llegado! ¡Qué mala pata!", "Chris");
translation.add(762, "¿¡Dónde vas!? ¡La salida no es por ahí!", "Arata");
translation.add(763, "¡No podemos esperar un buen momento para pasar\na través del pórtico en estas condiciones!\n¡Tenemos que despistarlo primero!", "Chris");
translation.add(764, "¿No puedes cortarlo cómo habías hecho con el\notro?", "Arata");
translation.add(765, "Usé mi carta del triunfo en esa batalla...\nNo puedo usarla dos veces...", "Chris");
translation.add(766, "¿¡Qué!?", "Arata");
translation.add(767, "¡Ya te lo he dicho, no puedo protegerte!\nAhora mismo, ¡tenemos que esperar otra oportunidad!", "Chris");
translation.add(768, "¡Pero qué...!", "Chris");
translation.add(769, "En pocos minutos hemos salido al jardín\nsobre techo del edificio.", "");
translation.add(770, "No hemos alcanzado el otro lado todavía en cuanto\nla puerta por la que hemos pasado explota en una\nnube de cemento pulverizado.", "");
translation.add(771, "Ahora hay un agujero enorme en la pared. Y parado\nen mitad de la nube de polvo una figura titiritante.", "");
translation.add(772, "¿¡Ya nos ha alcanzado!?", "Chris");
translation.add(773, "¡Déjanos en paz!", "Arata");
translation.add(774, "¡Joder! ¡Es más rápido que antes!", "Chris");
translation.add(775, "Chris dobla y alza la espada contra la bestia.", "");
translation.add(776, "¿¡Chris!?", "Arata");
translation.add(777, "Yo me encargo. Mientras tanto usa la otra salida\ny llega hasta el pórtico.", "Chris");
translation.add(778, "¿¡QUÉ!?", "Arata");
translation.add(779, "No podemos correr más que él. Lo mejor es\nencararle mientras todavía tenga espacio para\nluchar.", "Chris");
translation.add(780, "¿Pero por qué tengo que huir y tú quedarte?", "Arata");
translation.add(781, "Lucho mejor sin tenerte por aquí.", "Chris");
translation.add(782, "¡Te ayudaré!", "Arata");
translation.add(783, "¡Ya te he dicho que solo serías un estorbo!", "Chris");
translation.add(784, "Pero... Pero sigo sin poder huir yo solo.", "Arata");
translation.add(785, "¿¡Por qué no!?", "Chris");
translation.add(786, "¡No puedo abandonarte aquí!", "Arata");
translation.add(787, "¡...!", "Chris");
translation.add(788, "¡Tú mismo has dicho que ya habías usado tu\nmejor baza antes!", "Arata");
translation.add(789, "Y estás herido...\n¿¡Cómo piensas ganar tú solo!?", "Arata");
translation.add(790, "...", "Chris");
translation.add(791, "No te preocupes. No es una herida demasiado\nprofunda. Y todavía tengo otros trucos además de\nla carta de triunfo. Puedo derrotar a este bicho.", "Chris");
translation.add(792, "¡Pero...!", "Arata");
translation.add(793, "¿No habías sido tú el que me había dicho que\ntenías los pies en tierra?", "Chris");
translation.add(794, "Pues usa esos mismos pies para salir de esta.", "Chris");
translation.add(795, "P-Pero...", "Arata");
translation.add(796, "Yo soy más rápido que tú. Y tu habilidad ofensiva\nes nula. Acabaría teniendo que protegerte.", "Chris");
translation.add(797, "Si de verdad quieres ayudarme, ya deberías\nsaber lo que hacer en esta situación.", "Chris");
translation.add(798, "...", "Arata");
translation.add(799, "Puedes ganar ¿no? Vas a ganar, ¿verdad?", "Arata");
translation.add(800, "Claro que sí. Te alcanzaré en cuanto termine.", "Chris");
translation.add(801, "¿De verdad? ¡¿Lo prometes?!", "Arata");
translation.add(802, "¡Vete ya de aquí!", "Chris");
translation.add(803, "Chris se golpea el pecho con el puño y se gira.\nGira la cabeza, y me dice:", "");
translation.add(804, "¡Escucha, no dejes de correr!\n¡No te rezagues y no mires atrás!", "Chris");
translation.add(805, "Vale... ¡Entendido!", "Arata");
translation.add(806, "Chris se prepara para la batalla.\nYo me giro y empiezo a correr.", "");
translation.add(807, "¡Arata...! ¡Gracias por lo que me has dicho antes!", "Chris");
translation.add(808, "¡...!", "Arata");
translation.add(809, "Escasos segundos después de esas últimas palabras\nempiezo a escuchar el ya familiar sonido de\ncolmillo contra espada.", "");
translation.add(810, "*Jadeo* *Jadeo* *Jadeo*", "Arata");
translation.add(811, "Corro por pasillos y escaleras.", "");
translation.add(812, "Sin nadie en los alrededores corro como si\nfuese un sueño.", "");
translation.add(813, "Percibo mis pasos y respiración con una intensidad\ninusitada. Y de fondo, el sonido amortiguado de la\nbatalla.", "");
translation.add(814, "Corro más deprisa, como intentando escapar de\nese sonido.", "");
translation.add(815, "¡Tranquilo! ¡Tranquilo! ¡Tranquilo...!", "Arata");
translation.add(816, "No paro de repetir esa palabra como si fuese\nalgún tipo de conjuro.", "");
translation.add(817, "Estoy haciendo lo correcto.\n¡Tengo que correr! ¡Correr!", "");
translation.add(818, "¡Chris estaba convencido de que iba a ganar!\n¡No tengo por qué preocuparme!", "");
translation.add(819, "¡...Gracias por lo que me has dicho antes!", "Chris");
translation.add(820, "¡Urk...!", "");
translation.add(821, "¡Maldición! ¿¡Por qué ha tenido que decir eso!?", "");
translation.add(822, "¡Sonaba casi como una despedida!\n¡Como si no fuese a volver!", "");
translation.add(823, "...", "");
translation.add(824, "...", "");
translation.add(825, "No. ¡No, no, no!", "");
translation.add(826, "¡Tranquilo! ¡Corre! ¡Corre!", "");
translation.add(827, "Si vuelvo, no seré de ninguna ayuda.\n¡Y podría morir! ¡Y probablemente hasta duela!\n¡Y haya un montón de sangre!", "");
translation.add(828, "¡Chris también estaba sangrando un montón...!", "");
translation.add(829, "Urgh...", "Arata");
translation.add(830, "¡¡Urrrgggh...!!", "Arata");
translation.add(831, "¡¡A la mierda!!", "Arata");
translation.add(832, "Frena, frena, frena. Giro de 180 grados.\nCorre en la otra dirección.", "");
translation.add(833, "¡¿Cómo es posible que piense que voy a huir?!", "Arata");
translation.add(834, "¡Que más da si puedo o no puedo ayudar!", "");
translation.add(835, "No le puedo dejar atrás, joder. ¡No puedo!", "Arata");
translation.add(836, "¡¡Grrr!!", "Guardener");
translation.add(837, "¡¡Ahhhh!!", "Chris");
translation.add(838, "El delicado cuerpo de Chris se dispara hacia su\nenemigo, directo como una flecha.", "");
translation.add(839, "*Crash* La línea recta se convierte en un\nsemicírculo.", "");
translation.add(840, "Chris acaba derribado por la fuerza del golpe.", "");
translation.add(841, "El Guardener se mueve para seguir con su cola,\npero Chris lo estaba esperando.", "");
translation.add(842, "Conforme la cola atiza el cemento, Chris se\nreincorpora de un salto.", "");
translation.add(843, "¡¡¡TOMA!!!", "Chris");
translation.add(844, "Clava la espada. Desde esa altura, debería haber\nsido capaz de atravesar el punto vulnerable de\nla bestia...", "");
translation.add(845, "Sin embargo...", "");
translation.add(846, "¡Grrrrrrrrrr!", "Guardener");
translation.add(847, "¿¡Qué!?", "Chris");
translation.add(848, "Bloquea la hoja con una de sus garras.", "");
translation.add(849, "Solo tiene garras en los extremos de las patas.\nÉsta está adherida a una de sus patas frontales.", "");
translation.add(850, "Ha doblado la pata y el cuerpo de una forma\nimposible. Está sosteniéndose únicamente con sus\npatas traseras.", "");
translation.add(851, "Ningún animal normal podría haberse colocado\nen una postura como esa.", "");
translation.add(852, "Pero eso solo dura un instante. Al instante\nsiguiente, el Guardener continúa su transformación.", "");
translation.add(853, "Su cuerpo se deforma como si estuviese hecho de\narcilla. Y al final de la transformación ha\nacabado con una forma completamente diferente.", "");
translation.add(854, "¿¡Pero qué demonios...!?", "Chris");
translation.add(855, "Ahora Chris tiene que levantar la mirada.\nEs mucho más alto de lo que era antes.", "");
translation.add(856, "Ahora tiene dos piernas y dos brazos.\nY su brazo derecho sostiene un arma cilíndrica.", "");
translation.add(857, "Es obvio que está imitando a un humano.", "");
translation.add(858, "¡¡Grrrrrrrrr!!", "Guardener");
translation.add(859, "¿¡Puede copiar técnicas!? ¿Ha copiado la mía?\nNo... Ese arma es...", "Chris");
translation.add(860, "¿¡Ya te has enfrentado a una \"princesa\"!?", "Chris");
translation.add(861, "La figura empieza a caminar.\nParece que se le da bastante bien.", "");
translation.add(862, "¡¡...!!", "Chris");
translation.add(863, "Es del mismo tamaño que era antes.\nDe hecho, ahora parece un poco más pequeño.", "");
translation.add(864, "Pero Chris se aleja, sobrecogido por la\nnueva forma tan imponente.", "");
translation.add(865, "Aún sin pararse a comprobarlo, sabe que el\nmonstruo es mucho más fuerte de lo que era.", "");
translation.add(866, "Sin embargo...", "Chris");
translation.add(867, "Sigues siendo un Guardener solo...", "Chris");
translation.add(868, "¡No tengo por qué escapar!", "Chris");
translation.add(869, "El concierto de choques comienza de nuevo,\nesta vez con un tono especialmente alto.", "");
translation.add(870, "La versión humanoide usa un arma larga, cuyo\nextremo termina en punta. Recuerda a una lanza.", "");
translation.add(871, "La usa retirándola, y embistiendo hacia delante.", "");
translation.add(872, "Pero lo que sorprende de verdad es la velocidad\na la que lo hace. Acomete dos... No, tres veces\nen un abrir y cerrar de ojos.", "");
translation.add(873, "Chris se las arregla para evitar los ataques,\npero logra esquivarlos por los pelos.", "");
translation.add(874, "Las tornas se han cambiado por completo. Ahora\nya no es Chris el que tiene la iniciativa.", "");
translation.add(875, "Ugh... Gahh...", "Chris");
translation.add(876, "Aunque todavía puede, Chris está empezando a\nagotarse progresivamente.", "");
translation.add(877, "Ahora Chris se encuentra arrinconado al filo del\nvacío entre las dos superficies, con la espalda\ncontra la valla.", "");
translation.add(878, "Desde arriba, no se llega a divisar el suelo.\nUn abismo del cual solo se puede apreciar una\ndensa niebla. Caer significa una muerte segura.", "");
translation.add(879, "Era solo cuestión de tiempo. Uno de las muchas\nmiles de acometidas de la lanza, encuentra por\nfin su objetivo.", "");
translation.add(880, "¡¿Ghaaaaa?!", "Chris");
translation.add(881, "Tratar de defenderse contra eso es inútil.\nEl golpe es demasiado fuerte. La espada se\nsepara de la mano de Chris.", "");
translation.add(882, "¡¡Guaaaaaaaahhhh!!", "Chris");
translation.add(883, "Chris acaba disparado dejando un rastro sobre el\nsuelo con sus zapatillas.", "");
translation.add(884, "Aunque el golpe no lo mata, tiene que apoyarse\nsobre la valla para mantenerse parcialmente erguido.", "");
translation.add(885, "¡¡Gaahhh!!", "Chris");
translation.add(886, "La valla se deforma con la fuerza del\nGuardener humanoide.", "");
translation.add(887, "Chris se las arregla para mantenerse encima del\npuente.", "");
translation.add(888, "¡Uff, uff, uff...!", "Chris");
translation.add(889, "Pero la situación va de mal en peor.\nYa no hay espacio para retroceder.", "");
translation.add(890, "Parece que el monstruo también se percata de\nque se han cambiado las tornas.", "");
translation.add(891, "Empieza a caminar, con una postura que irradia\nvictoria. Avanza lentamente hacia su víctima.", "");
translation.add(892, "Ugh... ¡Maldición!", "Chris");
translation.add(893, "Chris agarra su espada, y hace frente a su enemigo\nuna vez más. Tiene que aguantarse sobre ella\npara mantenerse de pie.", "");
translation.add(894, "Tiene que mantenerse de pie, cubrirse el hombro,\ny encima luchar.", "");
translation.add(895, "Y pese a la desesperanzada situación, sus ojos\nesmeralda no pierden un ápice de su chispa.", "");
translation.add(896, "¿¡Eso es todo lo que puedes hacer...!?", "Chris");
translation.add(897, "¿Chris, es no puedes ni derrotar\na un enemigo tan débil!?", "Chris");
translation.add(898, "¡¡¿Y te llamas a ti mismo príncipe?!!", "Chris");
translation.add(899, "¡Grrrr!", "Guardener");
translation.add(900, "¡¡Guuuuhhhhhh!!", "Chris");
translation.add(901, "Chris se tambalea por el golpe.", "");
translation.add(902, "Pero incluso ahora, una fuerza nunca vista\nsostiene su deteriorado cuerpo, negándose a\ndejarle caer al suelo.", "");
translation.add(903, "Ufff... Ufff... Ufff...", "Chris");
translation.add(904, "Aun sin el vestido... Aun sin su protección...\n¡¡Voy a ser capaz de cargarme a este cabronazo!!", "Chris");
translation.add(905, "Porque yo... Yo...", "Chris");
translation.add(906, "¡¡Soy el príncipe de estas tierras!!", "Chris");
translation.add(907, "¡¡Guaaahhh!!", "Chris");
translation.add(908, "Una mierda... Menuda mierda...", "Chris");
translation.add(909, "¡¡Mierda!!", "Chris");
translation.add(910, "¡¡¡Mierda!!!", "Chris");
translation.add(911, "¡¡Ghaaaaaaaa!!", "Chris");
translation.add(912, "Uhh... Uhhhh...", "Chris");
translation.add(913, "Es... ¿el fin...?", "Chris");
translation.add(914, "¡¡Aguanta!!", "Arata");
translation.add(915, "¿¡Eh...!?", "Chris");
translation.add(916, "¿¡...!?", "Guardener");
translation.add(917, "Llego sin darme cuenta.\nMi cuerpo se mueve sin tener que decírselo,\npor su propia voluntad.", "");
translation.add(918, "¡¡Hiyaaaahh!!", "Arata");
translation.add(919, "Busco algo con lo que golpearle. En cuanto logro\nsacarlo, acabo con un extintor de incendios en\nlas manos.", "");
translation.add(920, "Alzo el tanque de metal rojo por encima de la\ncabeza y embisto. No estoy lo suficiente sereno\nni como para notar todo su peso.", "");
translation.add(921, "Lo cierto es que es ridículo. Después de todo el\nentrenamiento en el dojo, no tengo ninguna clase\nde estilo ni habilidad.", "");
translation.add(922, "Le cedo el control a mi rabia, que me controla,\ncomo un hombre poseído...", "");
translation.add(923, "¡¡Rrrhhhhhaaaaaaaaaaaahhhhh!!", "Arata");
translation.add(924, "¡He golpeado al monstruo!", "");
translation.add(925, "¡Grrr!", "Guardener");
translation.add(926, "¿¡...!?", "Arata");
translation.add(927, "Siento un calambre en los brazos.\nEs como si hubiese golpeado\nun bloque de piedra macizo.", "");
translation.add(928, "¡Está durísimo...! Pero ha hecho algo de efecto.", "");
translation.add(929, "El polvo que se derrama del extintor hace que\nresulte difícil de ver, pero la bestia se ha\ncaído sobre una rodilla.", "");
translation.add(930, "¡Funciona!", "");
translation.add(931, "¡Imbécil! ¡¿Por qué has vuelto?!", "Chris");
translation.add(932, "¡He venido a salvarte, tío!", "Arata");
translation.add(933, "¡Eso es una locura!", "Chris");
translation.add(934, "Espera. ¡Voy a acabar con él!", "Arata");
translation.add(935, "Levanto nuevamente el bidón rojo sobre la cabeza.", "");
translation.add(936, "¡Así! Otra vez, ¡¡con pasión!!", "Arata");
translation.add(937, "¡Eso no...! ¡Rápido! ¡¡Huye!!", "Chris");
translation.add(938, "¿Eh...?", "Arata");
translation.add(939, "Se oye un golpe seco.", "");
translation.add(940, "El suelo desaparece y se convierte en el cielo.", "");
translation.add(941, "Estoy volando.", "");
translation.add(942, "Vuelo como si fuese un avión.", "");
translation.add(943, "Me siento ligero y esponjoso...", "");
translation.add(944, "Y esta sensación, afable y apacible...", "");
translation.add(945, "Se merma abruptamente.", "");
translation.add(946, "¿¿¡¡Guofff!!??", "Arata");
translation.add(947, "Anda, mira... fuegos artificiales.", "");
translation.add(948, "De repente, el mundo entero se convierte\nen fuegos artificiales.", "");
translation.add(949, "¡¡...!!", "");
translation.add(950, "La gravedad actúa de nuevo.\nEl cemento se me antoja bastante desagradable.", "");
translation.add(951, "¡¡...!!", "");
translation.add(952, "Doy vueltas. Diez... veinte...\nLa piel quema... **", "");
translation.add(953, "Finalmente el tormento acaba.", "");
translation.add(954, "La gravedad vuelve a desaparecer.", "");
translation.add(955, "Ahhh...", "Arata");
translation.add(956, "Es como si el tiempo se hubiese ralentizado.", "");
translation.add(957, "Veo la valla enfrente de mí. Que se desplaza,\nmuy suavamente.", "");
translation.add(958, "Tengo que aferrarme, y rápido.\nPero mi cuerpo no se mueve.", "");
translation.add(959, "No... Se mueve... Pero no como me gustaría.\nLa valla se desplaza con firmeza.", "");
translation.add(960, "No... Tengo que agarrarme... Tengo que...\nO si no, yo... Yo...", "");
translation.add(961, "Ahh...", "");
translation.add(962, "El cielo inunda mi campo de visión.\nYa no hay nada que agarrar.", "");
translation.add(963, "Esto no va bien...", "");
translation.add(964, "Nada bien...", "");
translation.add(965, "¡¡Arata!!", "Chris");
translation.add(966, "¿Una mano...?", "");
translation.add(967, "Sí... Tengo que estrecharla.\nTengo que estrechar esa mano.", "");
translation.add(968, "¡Tengo que afferarme a esa mano...!", "");
translation.add(969, "¡¡Vestido...!! ¡¡Dame poder...!!", "Chris");
translation.add(970, "Solo esta vez... ¡Vestido!", "Chris");
translation.add(971, "¡¡¡Dame el poder!!!", "Chris");
translation.add(972, "¡Esa mano...! ¡¡Tengo que alcanzarla!!", "");
translation.add(973, "...", "");
translation.add(974, "Ven...", "¿?");
translation.add(975, "Qué gustito...", "");
translation.add(976, "Ven...", "¿?");
translation.add(977, "Qué mano más... cálida...", "");
translation.add(978, "Toma la espada...", "¿?");
translation.add(979, "...", "Mujer de blanco");
translation.add(980, "¡Grrrrrrrr!", "Guardener");
translation.add(981, "...", "Mujer de blanco");
translation.add(982, "La belleza de su espada afilada y descubierta...", "Mujer de rojo");
translation.add(983, "Sus ojos verdes, que brillan con la lujuria\nde la batalla...", "Mujer de rojo");
translation.add(984, "Y rodeándola, una brillante y blanca Alma...", "Mujer de rojo");
translation.add(985, "Ja, ja, ja... Ya me he cansado de esperar,\nprincesa de la espada.", "Mujer de rojo");
translation.add(986, "Para compensar el aburrimiento, voy a tener que\ndivertirme lo que quiera con ese cuerpo tuyo.\nJa, ja, ja...", "Mujer de rojo");
translation.add(987, "Los gritos se propagan por el aire, alternándose\ncon el repiqueteo del acero contra el acero.", "");
translation.add(988, "El choque de las armas es tan violento que el\nsuelo se estremece.", "");
translation.add(989, "El cemento empieza a descomponerse y los trozos\nsalen disparados por el aire. Y en medio de la\nlluvia invertida de cemento, la lanza del monstruo.", "");
translation.add(990, "Un golpe, tan fuerte como todos los anteriores,\npero que no llega a alcanzar su destino.", "");
translation.add(991, "La espada detiene el avance de la lanza y en un\nsolo movimiento, asesta un tajo en el brazo del\nmonstruo.", "");
translation.add(992, "El monstruo ruge de dolor y furia para un momento\ndespués, lanzar otro ataque.", "");
translation.add(993, "Un haz de luz, y la sensación del frío acero\nrozando mi piel.", "");
translation.add(994, "Pero no me asusta.", "");
translation.add(995, "Es como un combate de entrenamiento más.\nSoslayar la punta de la lanza es elemental.", "");
translation.add(996, "Es fácil. Es como bailar.", "");
translation.add(997, "Y ahora estamos en el pasadizo central,\nque está bañado en la luz roja.", "");
translation.add(998, "Este jardín sobre tejado se asemeja perfectamente\na un campo de batalla.", "");
translation.add(999, "Con pétalos blancos bailando por encima.", "");
translation.add(1000, "Es una escena cautivadora.", "");
translation.add(1001, "Una espada plateada y un pelo dorado.", "");
translation.add(1002, "Ojos verdes brillando con una luz apacible.", "");
translation.add(1003, "Es bella... Tan bella que quita la respiración.", "");
translation.add(1004, "Una princesa de blanco...", "");
translation.add(1005, "¡¡¡...!!!", "Guardener");
translation.add(1006, "El baile llega a su fin.", "");
translation.add(1007, "El monstruo aulla balbuceante, tratando de\nasestar otro golpe.", "");
translation.add(1008, "Pero su contorno se desfallece y parte de él\nya se ha convertido en polvo.", "");
translation.add(1009, "El polvo se va volando junto al viente como\nharían las semillas del diente de león.", "");
translation.add(1010, "Es el precursor de la victoria.\nAsí como el fin de la larga batalla...", "");
translation.add(1011, "Termina con un último tajo de espada.", "");
translation.add(1012, "...", "Mujer de blanco");
translation.add(1013, "El viento acaricia su pelo. Me mira directamente.", "");
translation.add(1014, "Está al otro lado de una ventana roja...", "");
translation.add(1015, "Es como si la estuviese viendo reflejada en el\nmetal rojizo de una cuchilla desenvainada.", "");
translation.add(1016, "Esa...", "Arata");
translation.add(1017, "¿Esa... soy yo...?", "Arata");
translation.add(1018, "Ugh...", "Mujer de blanco");
translation.add(1019, "El mundo empieza a girar.", "");
translation.add(1020, "Y en la parte reflectante de la espada,\nla mujer trastabillea.", "");
translation.add(1021, "¿Qué significa...? ¿Qué me ha pasado...?", "");
translation.add(1022, "Ja, ja, ja... Muy bien hecho.", "Mujer de rojo");
translation.add(1023, "¿¡...!?", "Mujer de blanco");
translation.add(1024, "Un placer conocerte, princesa de la espada.", "Mujer de rojo");
translation.add(1025, "Soy Angela Victoire, princesa de Agnis,\nel reino de fuego.", "Mujer de rojo");
translation.add(1026, "Y ahora...", "Mujer de rojo");
translation.add(1027, "¿Bailamos?", "Mujer de rojo");

translation.add(246, "Gooooooo!! Lightning shoot!!", "Liliana");
translation.add(247, "Whew......", "Liliana");
translation.add(248, "What was that, anyway?", "Liliana");
translation.add(249, "......It's not nice to selfishly steal someone\nelse's prey.", "??");
translation.add(250, "!?", "Liliana");
translation.add(251, "Heh heh heh......", "Angela");
translation.add(252, "(Angela Victoire Blendin! The dragon princess from\nAgnis whose whereabouts were unknown!)", "Liliana");
translation.add(253, "(That crazy thing was a Guardener that reacted to\nher!?)", "Liliana");
translation.add(254, "But no matter. I'm in luck.", "Angela");
translation.add(255, "I chased after the appetizer, and stumbled into\nthe main dish.", "Angela");
translation.add(256, "..................", "Liliana");
